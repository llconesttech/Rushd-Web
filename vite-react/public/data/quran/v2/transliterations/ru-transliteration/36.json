{
  "number": 36,
  "name": "سُورَةُ يسٓ",
  "englishName": "Yaseen",
  "englishNameTranslation": "Yaseen",
  "revelationType": "Meccan",
  "numberOfAyahs": 83,
  "ayahs": [
    {
      "number": 3706,
      "text": "Йӓ -Сӥ н \r",
      "numberInSurah": 1,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3707,
      "text": "Ұа А л- Қ ур' ӓ ни А л-Хак ӥ ми \r",
      "numberInSurah": 2,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3708,
      "text": "'И нн ака Ламина А л-Мурсал ӥ на \r",
      "numberInSurah": 3,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3709,
      "text": "`Алá Ҫ и р ӓ ҭ и н Муста қ ӥ ми н \r",
      "numberInSurah": 4,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3710,
      "text": "Та н з ӥ ла А л-`Аз ӥ зи А р- Ра х ӥ ми \r",
      "numberInSurah": 5,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3711,
      "text": "Литу н ҙи р а Қ аұмӓа н М ӓ 'У н ҙи р а 'Ӓб ӓ 'ууһу м Фаһу м Ғ ӓфил ӱ на \r",
      "numberInSurah": 6,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3712,
      "text": "Ла қ а д Ха қ қ а А л- Қ а ұ лу `Ал á 'Акćа р иһи м Фаһу м Лӓ Йу'умин ӱ на \r",
      "numberInSurah": 7,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3713,
      "text": "'И нн ӓ Җа`алнӓ Ф ӥ 'А`нӓ қ иһи м 'А ғ лӓлӓа н Фаһийа 'Илá А л-'Аҙ қ ӓ ни Фаһу м Му қ мах ӱ на \r",
      "numberInSurah": 8,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3714,
      "text": "Ұа Җа`алнӓ Ми н Ба й ни 'Айдӥһи м Саддӓа н Ұа Мин Ҳ алфиһи м Саддӓа н Фа'а ғ шайнӓһу м Фаһу м Лӓ Йу б ҫ ир ӱ на \r",
      "numberInSurah": 9,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3715,
      "text": "Ұа Саұ ӓ 'ун `Алайһи м 'А'а н ҙартаһу м 'А м Ла м Ту н ҙи р һу м Лӓ Йу'умин ӱ на \r",
      "numberInSurah": 10,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3716,
      "text": "'И нн амӓ Ту н ҙи р у Мани А ттаба`а А ҙ-Ҙик ра Ұа Ҳ ашийа А р- Ра хмана Бил- Ғ а й би ۖ Фабашши р һу Бима ғ фи ра ти н Ұа 'А җ р и н Ка р ӥ ми н \r",
      "numberInSurah": 11,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3717,
      "text": "'И нн ӓ Нахну Нухйи А л-Маұтá Ұа Нактубу Мӓ Қ аддамӱ Ұа 'Ӓćӓ ра һу м ۚ Ұа Кулла Ша й 'ин 'Х ҫ айн ӓ һу Ф ӥ 'Им ӓ ми н Муб ӥ ни н \r",
      "numberInSurah": 12,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3718,
      "text": "Ұа А đ р и б Лаһу м Маćалӓан 'А ҫ х ӓ ба А л- Қ арйати 'Иҙ Җ ӓ 'аһӓ А л-Мурсал ӱ на \r",
      "numberInSurah": 13,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3719,
      "text": "'Иҙ 'Арсалн ӓ 'Илайһиму А ćна й ни Факаҙҙабӱһумӓ Фа`аззазнӓ Биćӓлиćи н Фа қ ӓл ӱ 'И нн ӓ 'Илайку м Мурсал ӱ на \r",
      "numberInSurah": 14,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3720,
      "text": "Қ ӓлӱ М ӓ 'А н ту м 'Иллӓ Баша ру н Миćлунӓ Ұа М ӓ 'А н зала А р- Ра хм ӓ ну Ми н Ша й 'ин 'Ин 'А н ту м 'Иллӓ Такҙиб ӱ на \r",
      "numberInSurah": 15,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3721,
      "text": "Қ ӓлӱ Ра ббунӓ Йа`ламу 'И нн ӓ 'Илайку м Ламурсал ӱ на \r",
      "numberInSurah": 16,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3722,
      "text": "Ұа Мӓ `Алайн ӓ 'Иллӓ А л-Балӓ ғ у А л-Муб ӥ ну \r",
      "numberInSurah": 17,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3723,
      "text": "Қ ӓл ӱ 'И нн ӓ Та ҭ аййарнӓ Бику м ۖ Ла'и н Ла м Та н таһӱ Ланарҗума нн аку м Ұа Лайамасса нн аку м Ми нн ӓ `Аҙ ӓ бун 'Ал ӥ му н \r",
      "numberInSurah": 18,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3724,
      "text": "Қ ӓлӱ Ҭ ӓ 'и ру ку м Ма`аку м ۚ 'А'и н Ҙукки р ту м ۚ Бал 'А н ту м Қ а ұ му н Мус р иф ӱ на \r",
      "numberInSurah": 19,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3725,
      "text": "Ұа Җ ӓ 'а Мин 'А қ ҫ á А л-Мадӥнати Ра җулу н Йас`á Қ ӓ ла Йӓ Қ а ұ ми А ттаби`ӱ А л-Мурсал ӥ на \r",
      "numberInSurah": 20,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3726,
      "text": "А ттаби`ӱ Ма н Лӓ Йас'алуку м 'А җ рӓ а н Ұа Һу м Муһтад ӱ на \r",
      "numberInSurah": 21,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3727,
      "text": "Ұа Мӓ Лийа Л ӓ 'А`буду А л-Лаҙӥ Фа ҭ а ра нӥ Ұа 'Ила й һи Турҗа` ӱ на \r",
      "numberInSurah": 22,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3728,
      "text": "'А'атта ҳ иҙу Ми н Дӱниһ и~ 'Ӓлиһатан 'И н Йу р и д ни А р- Ра хм ӓ ну Биđур р и н Лӓ Ту ғ ни `А нн ӥ Шафӓ`атуһу м Шай' ӓ а н Ұа Лӓ Йу н қ иҙ ӱ ни \r",
      "numberInSurah": 23,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3729,
      "text": "'И нн ӥ 'Иҙӓа н Лафӥ Đ ал ӓ ли н Муб ӥ ни н \r",
      "numberInSurah": 24,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3730,
      "text": "'И нн ӥ 'Ӓма н ту Би ра ббику м Фӓсма` ӱ ни \r",
      "numberInSurah": 25,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3731,
      "text": "Қ ӥ ла А д ҳ ули А л-Җа нн ата ۖ Қ ӓ ла Йӓ Ла й та Қ аұмӥ Йа`лам ӱ на \r",
      "numberInSurah": 26,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3732,
      "text": "Бимӓ Ғ афа ра Лӥ Ра ббӥ Ұа Җа`аланӥ Мина А л-Мук ра м ӥ на \r",
      "numberInSurah": 27,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3733,
      "text": "Ұа М ӓ 'А н залнӓ `Алá Қ аұмиһ и Ми н Ба`диһ и Ми н Җу н ди н Мина А с-Сам ӓ 'и Ұа Мӓ Ку нн ӓ Му н зил ӥ на \r",
      "numberInSurah": 28,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3734,
      "text": "'И н Кӓнат 'Иллӓ Ҫ айхата н Ұӓхидата н Фа'иҙӓ Һу м Ҳ ӓмид ӱ на \r",
      "numberInSurah": 29,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3735,
      "text": "Йӓ Хас ра тан `Алá А л-`Иб ӓ ди ۚ Мӓ Йа'тӥһи м Ми н Ра с ӱ лин 'Иллӓ Кӓнӱ Биһ и Йастаһзи'ӱ н \r",
      "numberInSurah": 30,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3736,
      "text": "'Ала м Йа ра ұ Ка м 'Аһлакнӓ Қ а б лаһу м Мина А л- Қ ур ӱ ни 'А нн аһу м 'Илайһи м Лӓ Йарҗи` ӱ на \r",
      "numberInSurah": 31,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3737,
      "text": "Ұа 'И н Куллу н Ла мм ӓ Җам ӥ `у н Ладайнӓ Мухđар ӱ на \r",
      "numberInSurah": 32,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3738,
      "text": "Ұа 'Ӓйату н Лаһуму А л-'Арđу А л-Майтату 'Ахйайнӓһӓ Ұа 'А ҳ ра җ нӓ Минһӓ Хаббӓа н Фаминһу Йа'кул ӱ на \r",
      "numberInSurah": 33,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3739,
      "text": "Ұа Җа`алнӓ Фӥһӓ Җа нн ӓ ти н Ми н На ҳ ӥ ли н Ұа 'А`н ӓ би н Ұа Фаҗҗарнӓ Фӥһӓ Мина А л-`Уй ӱ ни \r",
      "numberInSurah": 34,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3740,
      "text": "Лийа'кулӱ Ми н Ćама р иһ и Ұа Мӓ `Амилат/һу 'Айдӥһи м ۖ 'Афалӓ Йашкур ӱ на \r",
      "numberInSurah": 35,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3741,
      "text": "Су б х ӓ на А л-Лаҙӥ Ҳ ала қ а А л-'Азұ ӓ җа Куллаһӓ Ми мм ӓ Ту н биту А л-'Арđу Ұа Мин 'А н фусиһи м Ұа Ми мм ӓ Лӓ Йа`лам ӱ на \r",
      "numberInSurah": 36,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3742,
      "text": "Ұа 'Ӓйату н Лаһуму А л-Ла й лу Насла ҳ у Минһу А н -Наһ ӓ ра Фа'иҙӓ Һу м Муžлим ӱ на \r",
      "numberInSurah": 37,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3743,
      "text": "Ұа А ш-Ша м су Та җ р ӥ Лимуста қ ар р и н Лаһӓ ۚ Ҙӓлика Та қ д ӥ р у А л-`Аз ӥ зи А л-`Ал ӥ ми \r",
      "numberInSurah": 38,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3744,
      "text": "Ұа А л- Қ ама ра Қ аддарн ӓ һу Манӓзила Хаттá ` Ӓ да Кӓл`урҗ ӱ ни А л- Қ ад ӥ ми \r",
      "numberInSurah": 39,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3745,
      "text": "Лӓ А ш-Ша м су Йа н ба ғ ӥ Лаһ ӓ 'А н Ту д р ика А л- Қ ама ра Ұа Лӓ А л-Ла й лу Сӓби қ у А н -Наһ ӓ р и ۚ Ұа Куллу н Фӥ Фалаки н Йасбах ӱ на \r",
      "numberInSurah": 40,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3746,
      "text": "Ұа 'Ӓйату н Лаһу м 'А нн ӓ Хамалнӓ Ҙур р ӥйатаһу м Фӥ А л-Фулки А л-Машх ӱ ни \r",
      "numberInSurah": 41,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3747,
      "text": "Ұа Ҳ ала қ нӓ Лаһу м Ми н Миćлиһ и Мӓ Йаркаб ӱ на \r",
      "numberInSurah": 42,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3748,
      "text": "Ұа 'И н Наша' Ну ғ р и қ һу м Фалӓ Ҫ а р ӥ ҳ а Лаһу м Ұа Лӓ Һу м Йу н қ аҙ ӱ на \r",
      "numberInSurah": 43,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3749,
      "text": "'Иллӓ Ра хмата н Ми нн ӓ Ұа Матӓ`ӓан 'Илá Х ӥ ни н \r",
      "numberInSurah": 44,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3750,
      "text": "Ұа 'Иҙӓ Қ ӥ ла Лаһуму А тта қ ӱ Мӓ Ба й на 'Айдӥку м Ұа Мӓ Ҳ алфаку м Ла`аллаку м Турхам ӱ на \r",
      "numberInSurah": 45,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3751,
      "text": "Ұа Мӓ Та'тӥһи м Мин 'Ӓйати н Мин 'Ӓй ӓ ти Ра ббиһи м 'Иллӓ Кӓнӱ `Анһӓ Му` р иđ ӥ на \r",
      "numberInSurah": 46,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3752,
      "text": "Ұа 'Иҙӓ Қ ӥ ла Лаһу м 'А н фи қ ӱ Ми мм ӓ Ра за қ акуму А лл ӓһу Қ ӓ ла А л-Лаҙ ӥ на Кафарӱ Лиллаҙ ӥ на 'Ӓман ӱ 'Ану ҭ `иму Ма н Лаұ Йаш ӓ 'у А лл ӓһу 'А ҭ `амаһ у~ 'Ин 'А н ту м 'Иллӓ Фӥ Đ ал ӓ ли н Муб ӥ ни н \r",
      "numberInSurah": 47,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3753,
      "text": "Ұа Йа қ ӱл ӱ на Матá Һӓҙӓ А л-Ұа`ду 'И н Ку н ту м Ҫ ӓди қ ӥ на \r",
      "numberInSurah": 48,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3754,
      "text": "Мӓ Йа н žур ӱ на 'Иллӓ Ҫ айхата н Ұӓхидата н Та' ҳ уҙуһу м Ұа Һу м Йа ҳ и ҫ ҫ им ӱ на \r",
      "numberInSurah": 49,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3755,
      "text": "Фалӓ Йаста ҭ ӥ` ӱ на Таұ ҫ ийата н Ұа Л ӓ 'Ил á 'Аһлиһи м Йарҗи` ӱ на \r",
      "numberInSurah": 50,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3756,
      "text": "Ұа Нуфи ҳ а Фӥ А ҫ - Ҫ ӱ р и Фа'иҙӓ Һу м Мина А л-'А җ дӓćи 'Илá Ра ббиһи м Йа н сил ӱ на \r",
      "numberInSurah": 51,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3757,
      "text": "Қ ӓлӱ Йӓ Ұайланӓ Ма н Ба`аćанӓ Ми н Мар қ адинӓ ۜ ۗ Һӓҙӓ Мӓ Ұа`ада А р- Ра хм ӓ ну Ұа Ҫ ада қ а А л-Мурсал ӱ на \r",
      "numberInSurah": 52,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3758,
      "text": "'И н Кӓнат 'Иллӓ Ҫ айхата н Ұӓхидата н Фа'иҙӓ Һу м Җам ӥ `у н Ладайнӓ Мухđар ӱ на \r",
      "numberInSurah": 53,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3759,
      "text": "Фӓлйа ұ ма Лӓ Туžламу Нафсу н Шай' ӓ а н Ұа Лӓ Ту җ за ұ на 'Иллӓ Мӓ Ку н ту м Та`мал ӱ на \r",
      "numberInSurah": 54,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3760,
      "text": "'И нн а 'А ҫ х ӓ ба А л-Җа нн ати А л-Йа ұ ма Фӥ Шу ғ ули н Фӓкиһ ӱ на \r",
      "numberInSurah": 55,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3761,
      "text": "Һу м Ұа 'Азұӓҗуһу м Фӥ Ž ил ӓ лин `Алá А л-'Ар ӓ 'ики Муттаки' ӱ на \r",
      "numberInSurah": 56,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3762,
      "text": "Лаһу м Фӥһӓ Фӓкиһату н Ұа Лаһу м Мӓ Йадда` ӱ на \r",
      "numberInSurah": 57,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3763,
      "text": "Сал ӓ му н Қ аұлӓа н Ми н Ра бби н Ра х ӥ ми н \r",
      "numberInSurah": 58,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3764,
      "text": "Ұа А м тӓзӱ А л-Йа ұ ма 'Аййуһӓ А л-Му җ р им ӱ на \r",
      "numberInSurah": 59,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3765,
      "text": "'Ала м 'А`һа д 'Илайку м Йӓ Бан ӥ 'Ӓдама 'А н Лӓ Та`будӱ А ш-Шай ҭ ӓ на ۖ 'И нн аһ у Лаку м `Ад ӱ ұу н Муб ӥ ну н \r",
      "numberInSurah": 60,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3766,
      "text": "Ұа 'Ани А `будӱнӥ ۚ Һӓҙӓ Ҫ и р ӓ ҭ у н Муста қ ӥ му н \r",
      "numberInSurah": 61,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3767,
      "text": "Ұа Ла қ а д 'Аđалла Ми н ку м Җибиллӓа н Каćӥ р ӓан ۖ 'Афала м Такӱнӱ Та` қ ил ӱ на \r",
      "numberInSurah": 62,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3768,
      "text": "Һаҙиһ и Җаһа нн аму А ллатӥ Ку н ту м Тӱ`ад ӱ на \r",
      "numberInSurah": 63,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3769,
      "text": "А ҫ лаұһӓ А л-Йа ұ ма Бимӓ Ку н ту м Такфур ӱ на \r",
      "numberInSurah": 64,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3770,
      "text": "Ал-Йа ұ ма На ҳ тиму `Ал á 'Афұӓһиһи м Ұа Тукаллимун ӓ 'Айдӥһи м Ұа Таш/һаду 'Арҗулуһу м Бимӓ Кӓнӱ Йаксиб ӱ на \r",
      "numberInSurah": 65,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3771,
      "text": "Ұа Лаұ Наш ӓ 'у Ла ҭ амаснӓ `Ал á 'А`йуниһи м Фӓстаба қ ӱ А ҫ - Ҫ и р ӓ ҭ а Фа'а нн á Йу б ҫ ир ӱ на \r",
      "numberInSurah": 66,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3772,
      "text": "Ұа Лаұ Наш ӓ 'у Ламаса ҳ нӓһу м `Алá Макӓнатиһи м Фамӓ А ста ҭ ӓ`ӱ Муđӥйӓа н Ұа Лӓ Йарҗи` ӱ на \r",
      "numberInSurah": 67,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3773,
      "text": "Ұа Ма н Ну`а мм и р һу Нунаккис/һу Фӥ А л- Ҳ ал қ и ۖ 'Афалӓ Йа` қ ил ӱ на \r",
      "numberInSurah": 68,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3774,
      "text": "Ұа Мӓ `Алла м н ӓ һу А ш-Ши` ра Ұа Мӓ Йа н ба ғ ӥ Лаһ у~ ۚ 'Ин Һуұа 'Иллӓ Ҙик ру н Ұа Қ ур' ӓ ну н Муб ӥ ну н \r",
      "numberInSurah": 69,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3775,
      "text": "Лийу н ҙи р а Ма н К ӓ на Хаййӓа н Ұа Йахи қ қ а А л- Қ а ұ лу `Алá А л-Кӓфи р ӥ на \r",
      "numberInSurah": 70,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3776,
      "text": "'Аұала м Йа ра ұ 'А нн ӓ Ҳ ала қ нӓ Лаһу м Ми мм ӓ `Амилат 'Айдӥн ӓ 'Ан`ӓмӓа н Фаһу м Лаһӓ Мӓлик ӱ на \r",
      "numberInSurah": 71,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3777,
      "text": "Ұа Ҙаллалнӓһӓ Лаһу м Фаминһӓ Ра кӱбуһу м Ұа Минһӓ Йа'кул ӱ на \r",
      "numberInSurah": 72,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3778,
      "text": "Ұа Лаһу м Фӥһӓ Манӓфи`у Ұа Машӓ р ибу ۖ 'Афалӓ Йашкур ӱ на \r",
      "numberInSurah": 73,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3779,
      "text": "Ұа А тта ҳ аҙӱ Ми н Д ӱ ни А лл ӓ һи 'Ӓлиһата н Ла`аллаһу м Йу н ҫ ар ӱ на \r",
      "numberInSurah": 74,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3780,
      "text": "Лӓ Йаста ҭ ӥ` ӱ на На ҫ ра һу м Ұа Һу м Лаһу м Җу н ду н Мухđар ӱ на \r",
      "numberInSurah": 75,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3781,
      "text": "Фалӓ Йахзу н ка Қ аұлуһу м ۘ 'И нн ӓ На`ламу Мӓ Йуси р р ӱ на Ұа Мӓ Йу`лин ӱ на \r",
      "numberInSurah": 76,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3782,
      "text": "'Аұала м Йа ра А л-'И н с ӓ ну 'А нн ӓ Ҳ ала қ н ӓ һу Ми н Ну ҭ фати н Фа'иҙӓ Һуұа Ҳ а ҫ ӥ му н Муб ӥ ну н \r",
      "numberInSurah": 77,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3783,
      "text": "Ұа Đ а ра ба Ланӓ Маćалӓа н Ұа Насийа Ҳ ал қ аһ у ۖ Қ ӓ ла Ма н Йухйӥ А л-`Иž ӓ ма Ұа Һийа Ра м ӥ му н \r",
      "numberInSurah": 78,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3784,
      "text": "Қ ул Йухйӥһӓ А л-Лаҙ ӥ 'А н ша'аһ ӓ 'Аұұала Мар ра ти н ۖ Ұа Һуұа Бикулли Ҳ ал қ ин `Ал ӥ му н \r",
      "numberInSurah": 79,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3785,
      "text": "Ал-Лаҙӥ Җа`ала Лаку м Мина А ш-Шаҗа р и А л-'А ҳ đа р и Нӓ рӓ а н Фа'иҙ ӓ 'А н ту м Минһу Тӱ қ ид ӱ на \r",
      "numberInSurah": 80,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3786,
      "text": "'Аұала й са А л-Лаҙӥ Ҳ ала қ а А с-Самӓұ ӓ ти Ұа А л-'Арđа Би қ ӓди р ин `Ал á 'А н Йа ҳ лу қ а Миćлаһу м ۚ Балá Ұа Һуұа А л- Ҳ алл ӓ қ у А л-`Ал ӥ му \r",
      "numberInSurah": 81,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3787,
      "text": "'И нн ам ӓ 'А м ру һ у~ 'Иҙ ӓ 'А р ӓ да Шай' ӓ ан 'А н Йа қ ӱ ла Лаһ у Ку н Файак ӱ ну \r",
      "numberInSurah": 82,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3788,
      "text": "Фасу б х ӓ на А л-Лаҙӥ Бийадиһ и Малак ӱ ту Кулли Ша й 'и н Ұа 'Ила й һи Турҗа` ӱ на \r",
      "numberInSurah": 83,
      "juz": 23,
      "manzil": 5,
