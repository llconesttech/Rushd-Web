{
  "number": 78,
  "name": "سُورَةُ النَّبَإِ",
  "englishName": "An-Naba",
  "englishNameTranslation": "The Announcement",
  "revelationType": "Meccan",
  "numberOfAyahs": 40,
  "ayahs": [
    {
      "number": 5673,
      "text": "`А мм а Йатас ӓ 'ал ӱ на \r",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5674,
      "text": "`Ани А н -Наба'и А л-`Аž ӥ ми \r",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5675,
      "text": "Ал-Лаҙӥ Һу м Ф ӥ һи Му ҳ талиф ӱ на \r",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5676,
      "text": "Каллӓ Сайа`лам ӱ на \r",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5677,
      "text": "Ćу мм а Каллӓ Сайа`лам ӱ на \r",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5678,
      "text": "'Ала м На җ `али А л-'Арđа Миһӓдӓа н \r",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5679,
      "text": "Ұа А л-Җиб ӓ ла 'Аұтӓдӓа н \r",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5680,
      "text": "Ұа Ҳ ала қ нӓку м 'Азұӓҗӓа н \r",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5681,
      "text": "Ұа Җа`алнӓ Наұмаку м Субӓтӓа н \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5682,
      "text": "Ұа Җа`алнӓ А л-Ла й ла Либӓсӓа н \r",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5683,
      "text": "Ұа Җа`алнӓ А н -Наһ ӓ ра Ма`ӓшӓа н \r",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5684,
      "text": "Ұа Банайнӓ Фаұ қ аку м Са б `ӓа н Шидӓдӓа н \r",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5685,
      "text": "Ұа Җа`алнӓ Си р ӓҗӓа н Ұа Һһӓҗӓа н \r",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5686,
      "text": "Ұа 'А н залнӓ Мина А л-Му` ҫ и р ӓ ти М ӓ 'а н Ćаҗҗӓҗӓа н \r",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5687,
      "text": "Лину ҳ р иҗа Биһи Хаббӓа н Ұа Набӓтӓа н \r",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5688,
      "text": "Ұа Җа нн ӓ тин 'Алфӓфӓа н \r",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5689,
      "text": "'И нн а Йа ұ ма А л-Фа ҫ ли К ӓ на Мӥ қ ӓтӓа н \r",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5690,
      "text": "Йа ұ ма Йу н фа ҳ у Фӥ А ҫ - Ҫ ӱ р и Фата'т ӱ на 'Афұӓҗӓа н \r",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5691,
      "text": "Ұа Футихати А с-Сам ӓ 'у Факӓнат 'А б ұӓбӓа н \r",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5692,
      "text": "Ұа Суййи ра ти А л-Җиб ӓ лу Факӓнат Са рӓ бӓа н \r",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5693,
      "text": "'И нн а Җаһа нн ама Кӓнат Ми р ҫ ӓдӓа н \r",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5694,
      "text": "Лил ҭ ҭ ӓ ғ ӥ на Ма' ӓ бӓа н \r",
      "numberInSurah": 22,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5695,
      "text": "Лӓбиć ӥ на Фӥһ ӓ 'Ах қ ӓбӓа н \r",
      "numberInSurah": 23,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5696,
      "text": "Лӓ Йаҙӱ қ ӱ на Фӥһӓ Бардӓа н Ұа Лӓ Ша рӓ бӓа н \r",
      "numberInSurah": 24,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5697,
      "text": "'Иллӓ Хамӥмӓа н Ұа Ғ ассӓ қ ӓа н \r",
      "numberInSurah": 25,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5698,
      "text": "Җаз ӓ 'а н Ұифӓ қ ӓа н \r",
      "numberInSurah": 26,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5699,
      "text": "'И нн аһу м Кӓнӱ Лӓ Йарҗ ӱ на Хисӓбӓа н \r",
      "numberInSurah": 27,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5700,
      "text": "Ұа Каҙҙабӱ Би'ӓйӓтинӓ Киҙҙӓбӓа н \r",
      "numberInSurah": 28,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5701,
      "text": "Ұа Кулла Ша й 'ин 'Ах ҫ айн ӓ һу Китӓбӓа н \r",
      "numberInSurah": 29,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5702,
      "text": "Фаҙӱ қ ӱ Фала н Назӥдаку м 'Иллӓ `Аҙӓбӓа н \r",
      "numberInSurah": 30,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5703,
      "text": "'И нн а Лилмутта қ ӥ на Мафӓзӓа н \r",
      "numberInSurah": 31,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5704,
      "text": "Хад ӓ 'и қ а Ұа 'А`нӓбӓа н \r",
      "numberInSurah": 32,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5705,
      "text": "Ұа Каұӓ`иба 'Ат рӓ бӓа н \r",
      "numberInSurah": 33,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5706,
      "text": "Ұа Ка'сӓа н Диһӓ қ ӓа н \r",
      "numberInSurah": 34,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5707,
      "text": "Лӓ Йасма` ӱ на Фӥһӓ Ла ғ ұа н Ұа Лӓ Киҙҙӓбӓа н \r",
      "numberInSurah": 35,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5708,
      "text": "Җаз ӓ 'а н Ми н Ра ббика `А ҭ ӓ 'ан Хисӓбӓа н \r",
      "numberInSurah": 36,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5709,
      "text": "Ра бби А с-Самӓұ ӓ ти Ұа А л-'Арđи Ұа Мӓ Байнаһумӓ А р- Ра хм ӓ ни ۖ Лӓ Йа м лик ӱ на Минһу Ҳ и ҭ ӓбӓа н \r",
      "numberInSurah": 37,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5710,
      "text": "Йа ұ ма Йа қ ӱ му А р-Р ӱ ху Ұа А л-Мал ӓ 'икату Ҫ аффӓа н ۖ Лӓ Йатакаллам ӱ на 'Иллӓ Ман 'Аҙина Лаһу А р- Ра хм ӓ ну Ұа Қ ӓ ла Ҫ аұӓбӓа н \r",
      "numberInSurah": 38,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5711,
      "text": "Ҙӓлика А л-Йа ұ му А л-Ха қ қ у ۖ Фама н Ш ӓ 'а А тта ҳ аҙа 'Илá Ра ббиһ и Ма' ӓ бӓа н \r",
      "numberInSurah": 39,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5712,
      "text": "'И нн ӓ 'А н ҙарнӓку м `Аҙӓбӓа н Қ а р ӥбӓа н Йа ұ ма Йа н žу ру А л-Мар'у Мӓ Қ аддамат Йад ӓ һу Ұа Йа қ ӱ лу А л-Кӓфи р у Йӓ Лайтанӥ Ку н ту Ту рӓ бӓа н \r",
      "numberInSurah": 40,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    }
  ],
