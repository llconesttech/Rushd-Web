{
  "number": 74,
  "name": "سُورَةُ المُدَّثِّرِ",
  "englishName": "Al-Muddaththir",
  "englishNameTranslation": "The Cloaked One",
  "revelationType": "Meccan",
  "numberOfAyahs": 56,
  "ayahs": [
    {
      "number": 5496,
      "text": "Й ӓ 'Аййуһӓ А л-Муддаććи р у \r",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5497,
      "text": "Қ у м Фа'а н ҙи р \r",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5498,
      "text": "Ұа Ра ббака Факабби р \r",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5499,
      "text": "Ұа Ćийӓбака Фа ҭ аһһи р \r",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5500,
      "text": "Ұа А р- Ру җ за Фӓһ җ ур \r",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5501,
      "text": "Ұа Лӓ Та м ну н Тастакćи р у \r",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5502,
      "text": "Ұа Ли ра ббика Фӓ ҫ б и р \r",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5503,
      "text": "Фа'иҙӓ Ну қ и ра Фӥ А н -Нӓ қ ӱ р и \r",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5504,
      "text": "Фаҙӓлика Йаұма'иҙи н Йа ұ мун `Ас ӥ р у н \r",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5505,
      "text": "`Алá А л-Кӓфи р ӥ на Ғ а й ру Йас ӥ р и н \r",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5506,
      "text": "Ҙарнӥ Ұа Ман Ҳ ала қ ту Ұахӥдӓа н \r",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5507,
      "text": "Ұа Җа`алту Лаһ у Мӓлӓа н Ма м дӱдӓа н \r",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5508,
      "text": "Ұа Бан ӥ на Шуһӱдӓа н \r",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5509,
      "text": "Ұа Маһһа д ту Лаһ у Та м һӥдӓа н \r",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5510,
      "text": "Ćу мм а Йа ҭ ма`у 'Ан 'Аз ӥ да \r",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5511,
      "text": "Калл ӓ ۖ 'И нн аһ у К ӓ на Ли'йӓтинӓ `Анӥдӓа н \r",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5512,
      "text": "Са'урһи қ уһу Ҫ а`ӱдӓа н \r",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5513,
      "text": "'И нн аһ у Факка ра Ұа Қ адда ра \r",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5514,
      "text": "Фа қ утила Ка й фа Қ адда ра \r",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5515,
      "text": "Ćу мм а Қ утила Ка й фа Қ адда ра \r",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5516,
      "text": "Ćу мм а Наžа ра \r",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5517,
      "text": "Ćу мм а `Абаса Ұа Баса ра \r",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5518,
      "text": "Ćу мм а 'А д ба ра Ұа А стакба ра \r",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5519,
      "text": "Фа қ ӓ ла 'Ин Һӓҙ ӓ 'Иллӓ Сих ру н Йу'уćа ру \r",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5520,
      "text": "'Ин Һӓҙ ӓ 'Иллӓ Қ а ұ лу А л-Баша р и \r",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5521,
      "text": "Са'у ҫ л ӥ һи Са қ а ра \r",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5522,
      "text": "Ұа М ӓ 'А д р ӓ ка Мӓ Са қ а ру \r",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5523,
      "text": "Лӓ Ту б қ ӥ Ұа Лӓ Таҙа ру \r",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5524,
      "text": "Лаұұӓхату н Лилбаша р и \r",
      "numberInSurah": 29,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5525,
      "text": "`Алайһӓ Тис`ата `Аша ра \r",
      "numberInSurah": 30,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5526,
      "text": "Ұа Мӓ Җа`алн ӓ 'А ҫ х ӓ ба А н -Н ӓ р и 'Иллӓ Мал ӓ 'иката н ۙ Ұа Мӓ Җа`алнӓ `Иддатаһу м 'Иллӓ Фитната н Лиллаҙ ӥ на Кафарӱ Лийастай қ ина А л-Лаҙ ӥ на 'Ӱтӱ А л-Кит ӓ ба Ұа Йазд ӓ да А л-Лаҙ ӥ на 'Ӓман ӱ 'Ӥмӓнӓа н ۙ Ұа Лӓ Йарт ӓ ба А л-Лаҙ ӥ на 'Ӱтӱ А л-Кит ӓ ба Ұа А л-Му'умин ӱ на ۙ Ұа Лийа қ ӱ ла А л-Лаҙ ӥ на Фӥ Қ улӱбиһи м Ма ра đу н Ұа А л-Кӓфир ӱ на Мӓҙ ӓ 'А р ӓ да А лл ӓһу Биһаҙӓ Маćалӓа н ۚ Каҙӓлика Йуđиллу А лл ӓһу Ма н Йаш ӓ 'у Ұа Йаһдӥ Ма н Йаш ӓ 'у ۚ Ұа Мӓ Йа`ламу Җун ӱ да Ра ббика 'Иллӓ Һуұа ۚ Ұа Мӓ Һийа 'Иллӓ Ҙикрá Лилбаша р и \r",
      "numberInSurah": 31,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5527,
      "text": "Каллӓ Ұа А л- Қ ама р и \r",
      "numberInSurah": 32,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5528,
      "text": "Ұа А л-Ла й ли 'Иҙ 'А д ба ра \r",
      "numberInSurah": 33,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5529,
      "text": "Ұа А ҫ - Ҫ у б хи 'Иҙ ӓ 'Асфа ра \r",
      "numberInSurah": 34,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5530,
      "text": "'И нн аһӓ Ла'ихдá А л-Куба р и \r",
      "numberInSurah": 35,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5531,
      "text": "Наҙӥ р ӓа н Лилбаша р и \r",
      "numberInSurah": 36,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5532,
      "text": "Лима н Ш ӓ 'а Ми н ку м 'А н Йата қ аддама 'Аұ Йата'а ҳ ҳ а ра \r",
      "numberInSurah": 37,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5533,
      "text": "Куллу Нафси н Бимӓ Касабат Ра һӥнату н \r",
      "numberInSurah": 38,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5534,
      "text": "'Илл ӓ 'А ҫ х ӓ ба А л-Йам ӥ ни \r",
      "numberInSurah": 39,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5535,
      "text": "Фӥ Җа нн ӓ ти н Йатас ӓ 'ал ӱ на \r",
      "numberInSurah": 40,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5536,
      "text": "`Ани А л-Му җ р им ӥ на \r",
      "numberInSurah": 41,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5537,
      "text": "Мӓ Салакаку м Фӥ Са қ а ра \r",
      "numberInSurah": 42,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5538,
      "text": "Қ ӓлӱ Ла м Наку Мина А л-Му ҫ алл ӥ на \r",
      "numberInSurah": 43,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5539,
      "text": "Ұа Ла м Наку Ну ҭ `иму А л-Миск ӥ на \r",
      "numberInSurah": 44,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5540,
      "text": "Ұа Ку нн ӓ На ҳ ӱ đу Ма`а А л- Ҳ ӓ 'иđ ӥ на \r",
      "numberInSurah": 45,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5541,
      "text": "Ұа Ку нн ӓ Нукаҙҙибу Бийа ұ ми А д-Д ӥ ни \r",
      "numberInSurah": 46,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5542,
      "text": "Хатт á 'Атӓнӓ А л-Йа қ ӥ ну \r",
      "numberInSurah": 47,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5543,
      "text": "Фамӓ Та н фа`уһу м Шафӓ`ату А ш-Шӓфи` ӥ на \r",
      "numberInSurah": 48,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5544,
      "text": "Фамӓ Лаһу м `Ани А т-Таҙки ра ти Му` р иđ ӥ на \r",
      "numberInSurah": 49,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5545,
      "text": "Ка'а нн аһу м Хуму ру н Муста н фи ра ту н \r",
      "numberInSurah": 50,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5546,
      "text": "Фар ра т Ми н Қ асұа ра ти н \r",
      "numberInSurah": 51,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5547,
      "text": "Бал Йу р ӥ ду Куллу А м р и'и н Минһу м 'А н Йу'утá Ҫ ухуфӓа н Мунашша ра та н \r",
      "numberInSurah": 52,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5548,
      "text": "Каллӓ ۖ Ба л Лӓ Йа ҳ ӓф ӱ на А л-'Ӓ ҳ и ра та \r",
      "numberInSurah": 53,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5549,
      "text": "Калл ӓ 'И нн аһ у Таҙки ра ту н \r",
      "numberInSurah": 54,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5550,
      "text": "Фама н Ш ӓ 'а Ҙака ра һ у \r",
      "numberInSurah": 55,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5551,
      "text": "Ұа Мӓ Йаҙкур ӱ на 'Илл ӓ 'А н Йаш ӓ 'а А лл ӓһу ۚ Һуұа 'Аһлу А т-Та қ ұá Ұа 'Аһлу А л-Ма ғ фи ра ти \r",
      "numberInSurah": 56,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    }
  ],
