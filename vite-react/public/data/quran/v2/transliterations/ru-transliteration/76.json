{
  "number": 76,
  "name": "سُورَةُ الإِنسَانِ",
  "englishName": "Al-Insaan",
  "englishNameTranslation": "Man",
  "revelationType": "Medinan",
  "numberOfAyahs": 31,
  "ayahs": [
    {
      "number": 5592,
      "text": "Һал 'Атá `Алá А л-'И н с ӓ ни Х ӥ ну н Мина А д-Даһ р и Ла м Йаку н Шай' ӓ а н Маҙкӱ рӓ а н \r",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5593,
      "text": "'И нн ӓ Ҳ ала қ нӓ А л-'И н с ӓ на Ми н Ну ҭ фатин 'А м ш ӓ җи н На б тал ӥ һи Фаҗа`алн ӓ һу Самӥ`ӓа н Ба ҫ ӥ р ӓа н \r",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5594,
      "text": "'И нн ӓ Һадайн ӓ һу А с-Саб ӥ ла 'И мм ӓ Шӓки р ӓа н Ұа 'И мм ӓ Кафӱ рӓ а н \r",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5595,
      "text": "'И нн ӓ 'А`та д нӓ Лилкӓфи р ӥ на Салӓсилӓа н Ұа 'А ғ лӓлӓа н Ұа Са`ӥ р ӓа н \r",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5596,
      "text": "'И нн а А л-'А б р ӓ ра Йаш ра б ӱ на Ми н Ка'си н К ӓ на Мизӓҗуһӓ Кӓфӱ рӓ а н \r",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5597,
      "text": "`Айнӓа н Йаш ра бу Биһӓ `Иб ӓ ду А лл ӓһи Йуфаҗҗирӱнаһӓ Тафҗӥ р ӓа н \r",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5598,
      "text": "Йӱф ӱ на Би н -Наҙ р и Ұа Йа ҳ ӓф ӱ на Йаұмӓа н К ӓ на Шар ру һ у Муста ҭ ӥ р ӓа н \r",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5599,
      "text": "Ұа Йу ҭ `им ӱ на А ҭ - Ҭ а` ӓ ма `Алá Хуббиһ и Мискӥнӓа н Ұа Йатӥмӓа н Ұа 'Асӥ р ӓа н \r",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5600,
      "text": "'И нн амӓ Ну ҭ `имуку м Лиұа җ һи А лл ӓ һи Лӓ Ну р ӥ ду Ми н ку м Җаз ӓ 'а н Ұа Лӓ Шукӱ рӓ а н \r",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5601,
      "text": "'И нн ӓ На ҳ ӓ фу Ми н Ра ббинӓ Йаұмӓан `Абӱсӓа н Қ а м ҭ а р ӥ р ӓа н \r",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5602,
      "text": "Фаұа қ ӓһуму А лл ӓһу Шар ра Ҙӓлика А л-Йа ұ ми Ұа Ла қ қ ӓһу м Наđ ра та н Ұа Сурӱ рӓ а н \r",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5603,
      "text": "Ұа Җазӓһу м Бимӓ Ҫ абарӱ Җа нн ата н Ұа Ха р ӥ р ӓа н \r",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5604,
      "text": "Муттаки' ӥ на Фӥһӓ `Алá А л-'Ар ӓ 'ики ۖ Лӓ Йа ра ұ на Фӥһӓ Ша м сӓа н Ұа Лӓ За м һа р ӥ р ӓа н \r",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5605,
      "text": "Ұа Дӓнийатан `Алайһи м Ž илӓлуһӓ Ұа Ҙуллилат Қ у ҭ ӱфуһӓ Таҙлӥлӓа н \r",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5606,
      "text": "Ұа Йу ҭ ӓ фу `Алайһи м Би'ӓнийати н Ми н Фиđđати н Ұа 'Акұ ӓ би н Кӓнат Қ аұӓ р ӥ р а \r",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5607,
      "text": "Қ аұӓ р ӥ р а Ми н Фиđđати н Қ аддарӱһӓ Та қ дӥ р ӓа н \r",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5608,
      "text": "Ұа Йус қ а ұ на Фӥһӓ Ка'сӓа н К ӓ на Мизӓҗуһӓ За н җабӥлӓа н \r",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5609,
      "text": "`Айнӓа н Фӥһӓ Туса мм á Салсабӥлӓа н \r",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5610,
      "text": "Ұа Йа ҭ ӱ фу `Алайһи м Ұилд ӓ ну н Му ҳ аллад ӱ на 'Иҙӓ Ра 'айтаһу м Хаси б таһу м Лу'улу'уӓа н Ма н ćӱ рӓ а н \r",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5611,
      "text": "Ұа 'Иҙӓ Ра 'а й та Ćа мм а Ра 'а й та На`ӥмӓа н Ұа Мулкӓа н Кабӥ р ӓа н \r",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5612,
      "text": "`Ӓлийаһу м Ćий ӓ бу Су н дусин Ҳ уđ ру н Ұа 'Иста б ра қ у н ۖ Ұа Хулл ӱ 'Асӓұи р а Ми н Фиđđати н Ұа Са қ ӓһу м Ра ббуһу м Ша рӓ бӓа н Ҭ аһӱ рӓ а н \r",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5613,
      "text": "'И нн а Һӓҙӓ К ӓ на Лаку м Җаз ӓ 'а н Ұа К ӓ на Са`йуку м Машкӱ рӓ а н \r",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5614,
      "text": "'И нн ӓ Нахну Наззалнӓ `Ала й ка А л- Қ ур' ӓ на Та н зӥлӓа н \r",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5615,
      "text": "Фӓ ҫ би р Лихукми Ра ббика Ұа Лӓ Ту ҭ и` Минһу м 'Ӓćимӓан 'Аұ Кафӱ рӓ а н \r",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5616,
      "text": "Ұа А ҙкур А сма Ра ббика Бук ра та н Ұа 'А ҫ ӥлӓа н \r",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5617,
      "text": "Ұа Мина А л-Ла й ли Фӓсҗу д Лаһ у Ұа Саббихһу Лайлӓа н Ҭ аұӥлӓа н \r",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5618,
      "text": "'И нн а Һ ӓ 'уул ӓ ' Йухибб ӱ на А л-`Ӓҗилата Ұа Йаҙар ӱ на Ұар ӓ 'аһу м Йаұмӓа н Ćа қ ӥлӓа н \r",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5619,
      "text": "Нахну Ҳ ала қ нӓһу м Ұа Шада д н ӓ 'Ас ра һу м ۖ Ұа 'Иҙӓ Ши'нӓ Баддалн ӓ 'А м ćӓлаһу м Та б дӥлӓа н \r",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5620,
      "text": "'И нн а Һаҙиһ и Таҙки ра ту н ۖ Фама н Ш ӓ 'а А тта ҳ аҙа 'Илá Ра ббиһ и Сабӥлӓа н \r",
      "numberInSurah": 29,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5621,
      "text": "Ұа Мӓ Таш ӓ ' ӱ на 'Илл ӓ 'А н Йаш ӓ 'а А лл ӓһу ۚ 'И нн а А лл ӓһа К ӓ на `Алӥмӓан Хакӥмӓа н \r",
      "numberInSurah": 30,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5622,
      "text": "Йу д ҳ илу Ма н Йаш ӓ 'у Фӥ Ра хматиһ и Ұа ۚ А ž- Ž ӓлим ӥ на 'А`адда Лаһу м `Аҙӓбӓан 'Алӥмӓа н \r",
      "numberInSurah": 31,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    }
  ],
