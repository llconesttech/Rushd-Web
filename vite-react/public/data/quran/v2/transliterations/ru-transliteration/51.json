{
  "number": 51,
  "name": "سُورَةُ الذَّارِيَاتِ",
  "englishName": "Adh-Dhaariyat",
  "englishNameTranslation": "The Winnowing Winds",
  "revelationType": "Meccan",
  "numberOfAyahs": 60,
  "ayahs": [
    {
      "number": 4676,
      "text": "Ұа А ҙ-Ҙӓ р ий ӓ ти Ҙарұа н \r",
      "numberInSurah": 1,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4677,
      "text": "Фӓлхӓмил ӓ ти Ұи қ рӓ а н \r",
      "numberInSurah": 2,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4678,
      "text": "Фӓлҗӓ р ий ӓ ти Йус рӓ а н \r",
      "numberInSurah": 3,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4679,
      "text": "Фӓлму қ ассим ӓ ти 'А м рӓ а н \r",
      "numberInSurah": 4,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4680,
      "text": "'И нн амӓ Тӱ`ад ӱ на Ла ҫ ӓди қ у н \r",
      "numberInSurah": 5,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4681,
      "text": "Ұа 'И нн а А д-Д ӥ на Лаұӓ қ и`у н \r",
      "numberInSurah": 6,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4682,
      "text": "Ұа А с-Сам ӓ 'и Ҙ ӓ ти А л-Хубуки \r",
      "numberInSurah": 7,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4683,
      "text": "'И нн аку м Лафӥ Қ а ұ ли н Му ҳ талифи н \r",
      "numberInSurah": 8,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4684,
      "text": "Йу'уфаку `Анһу Ман 'Уфика \r",
      "numberInSurah": 9,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4685,
      "text": "Қ утила А л- Ҳ ар рӓ ҫ ӱ на \r",
      "numberInSurah": 10,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4686,
      "text": "Ал-Лаҙ ӥ на Һу м Фӥ Ғ а м ра ти н Сӓһ ӱ на \r",
      "numberInSurah": 11,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4687,
      "text": "Йас'ал ӱ на 'Айй ӓ на Йа ұ му А д-Д ӥ ни \r",
      "numberInSurah": 12,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4688,
      "text": "Йа ұ ма Һу м `Алá А н -Н ӓ р и Йуфтан ӱ на \r",
      "numberInSurah": 13,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4689,
      "text": "Ҙӱ қ ӱ Фитнатаку м Һӓҙӓ А л-Лаҙӥ Ку н ту м Биһ и Таста`җил ӱ на \r",
      "numberInSurah": 14,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4690,
      "text": "'И нн а А л-Мутта қ ӥ на Фӥ Җа нн ӓ ти н Ұа `Уй ӱ ни н \r",
      "numberInSurah": 15,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4691,
      "text": "' Ӓ ҳ иҙ ӥ на М ӓ 'Ӓтӓһу м Ра ббуһу м ۚ 'И нн аһу м Кӓнӱ Қ а б ла Ҙӓлика Мухсин ӥ на \r",
      "numberInSurah": 16,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4692,
      "text": "Кӓнӱ Қ алӥлӓа н Мина А л-Ла й ли Мӓ Йаһҗа` ӱ на \r",
      "numberInSurah": 17,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4693,
      "text": "Ұа Бил-'Асх ӓ р и Һу м Йаста ғ фир ӱ на \r",
      "numberInSurah": 18,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4694,
      "text": "Ұа Ф ӥ 'А м ұӓлиһи м Ха қ қ у н Лилсс ӓ 'или Ұа А л-Махр ӱ ми \r",
      "numberInSurah": 19,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4695,
      "text": "Ұа Фӥ А л-'Арđи 'Ӓй ӓ ту н Лилмӱ қ ин ӥ на \r",
      "numberInSurah": 20,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4696,
      "text": "Ұа Ф ӥ 'А н фусику м ۚ 'Афалӓ Ту б ҫ ир ӱ на \r",
      "numberInSurah": 21,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4697,
      "text": "Ұа Фӥ А с-Сам ӓ 'и Р из қ уку м Ұа Мӓ Тӱ`ад ӱ на \r",
      "numberInSurah": 22,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4698,
      "text": "Фаұа ра бби А с-Сам ӓ 'и Ұа А л-'Арđи 'И нн аһ у Лаха қ қ у н Миćла М ӓ 'А нн аку м Та н ҭ и қ ӱ на \r",
      "numberInSurah": 23,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4699,
      "text": "Һал 'Ат ӓ ка Хадӥćу Đ а й фи 'И б рӓ һ ӥ ма А л-Мук ра м ӥ на \r",
      "numberInSurah": 24,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4700,
      "text": "'Иҙ Да ҳ алӱ `Ала й һи Фа қ ӓлӱ Салӓмӓа н ۖ Қ ӓ ла Сал ӓ му н Қ а ұ му н Му н кар ӱ на \r",
      "numberInSurah": 25,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4701,
      "text": "Фа рӓ ғ а 'Ил á 'Аһлиһ и Фаҗ ӓ 'а Би`и җ ли н Сам ӥ ни н \r",
      "numberInSurah": 26,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4702,
      "text": "Фа қ ар ра баһ у~ 'Илайһи м Қ ӓ ла 'Алӓ Та'кул ӱ на \r",
      "numberInSurah": 27,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4703,
      "text": "Фа'аұҗаса Минһу м Ҳ ӥфата н ۖ Қ ӓлӱ Лӓ Та ҳ аф ۖ Ұа Башшар ӱ һу Би ғ ул ӓ мин `Ал ӥ ми н \r",
      "numberInSurah": 28,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4704,
      "text": "Фа'а қ балати А м ра 'атуһу Фӥ Ҫ ар ра ти н Фа ҫ аккат Ұа җ һаһӓ Ұа Қ ӓлат `Аҗ ӱ зун `А қ ӥ му н \r",
      "numberInSurah": 29,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4705,
      "text": "Қ ӓлӱ Каҙӓлики Қ ӓ ла Ра ббуки ۖ 'И нн аһ у Һуұа А л-Хак ӥ му А л-`Ал ӥ му \r",
      "numberInSurah": 30,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4706,
      "text": "Қ ӓ ла Фамӓ Ҳ а ҭ буку м 'Аййуһӓ А л-Мурсал ӱ на \r",
      "numberInSurah": 31,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4707,
      "text": "Қ ӓл ӱ 'И нн ӓ 'Урсилн ӓ 'Илá Қ а ұ ми н Му җ р им ӥ на \r",
      "numberInSurah": 32,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4708,
      "text": "Линурсила `Алайһи м Хиҗӓ ра та н Ми н Ҭ ӥ ни н \r",
      "numberInSurah": 33,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4709,
      "text": "Мусаұұаматан `И н да Ра ббика Лилмус р иф ӥ на \r",
      "numberInSurah": 34,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4710,
      "text": "Фа'а ҳ ра җ нӓ Ма н К ӓ на Фӥһӓ Мина А л-Му'умин ӥ на \r",
      "numberInSurah": 35,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4711,
      "text": "Фамӓ Ұаҗа д нӓ Фӥһӓ Ғ а й ра Ба й ти н Мина А л-Муслим ӥ на \r",
      "numberInSurah": 36,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4712,
      "text": "Ұа Та ра кнӓ Фӥһ ӓ 'Ӓйата н Лиллаҙ ӥ на Йа ҳ ӓф ӱ на А л-`Аҙ ӓ ба А л-'Ал ӥ ма \r",
      "numberInSurah": 37,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4713,
      "text": "Ұа Фӥ Мӱс á 'Иҙ 'Арсалн ӓ һу 'Илá Фи р `а ұ на Бисул ҭ ӓ ни н Муб ӥ ни н \r",
      "numberInSurah": 38,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4714,
      "text": "Фатаұаллá Би ру книһ и Ұа Қ ӓ ла Сӓхи р ун 'Аұ Ма җ н ӱ ну н \r",
      "numberInSurah": 39,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4715,
      "text": "Фа'а ҳ аҙн ӓ һу Ұа Җунӱдаһ у Фанабаҙнӓһу м Фӥ А л-Йа мм и Ұа Һуұа Мул ӥ му н \r",
      "numberInSurah": 40,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4716,
      "text": "Ұа Фӥ ` Ӓ дин 'Иҙ 'Арсалнӓ `Алайһиму А р- Р ӥ ха А л-`А қ ӥ ма \r",
      "numberInSurah": 41,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4717,
      "text": "Мӓ Таҙа ру Ми н Ша й 'ин 'Атат `Ала й һи 'Иллӓ Җа`алат/һу Кӓлр ра м ӥ ми \r",
      "numberInSurah": 42,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4718,
      "text": "Ұа Фӥ Ćам ӱ да 'Иҙ Қ ӥ ла Лаһу м Таматта`ӱ Хаттá Х ӥ ни н \r",
      "numberInSurah": 43,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4719,
      "text": "Фа`атаұ `Ан 'А м р и Ра ббиһи м Фа'а ҳ аҙат/һуму А ҫ - Ҫ ӓ`и қ ату Ұа Һу м Йа н žур ӱ на \r",
      "numberInSurah": 44,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4720,
      "text": "Фамӓ А ста ҭ ӓ`ӱ Ми н Қ ий ӓ ми н Ұа Мӓ Кӓнӱ Му н та ҫ и р ӥ на \r",
      "numberInSurah": 45,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4721,
      "text": "Ұа Қ а ұ ма Нӱхи н Ми н Қ а б лу ۖ 'И нн аһу м Кӓнӱ Қ аұмӓа н Фӓси қ ӥ на \r",
      "numberInSurah": 46,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4722,
      "text": "Ұа А с-Сам ӓ 'а Банайнӓһӓ Би'ай й ди н Ұа 'И нн ӓ Ламӱси` ӱ на \r",
      "numberInSurah": 47,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4723,
      "text": "Ұа А л-'Арđа Фа ра шнӓһӓ Фани`ма А л-Мӓһид ӱ на \r",
      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4724,
      "text": "Ұа Ми н Кулли Ша й 'ин Ҳ ала қ нӓ Заұҗа й ни Ла`аллаку м Таҙаккар ӱ на \r",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4725,
      "text": "Фафи р р ӱ 'Илá А лл ӓһи ۖ 'И нн ӥ Лаку м Минһу Наҙ ӥ р у н Муб ӥ ну н \r",
      "numberInSurah": 50,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4726,
      "text": "Ұа Лӓ Та җ `алӱ Ма`а А лл ӓһи 'Илаһӓан 'Ӓ ҳ а ра ۖ 'И нн ӥ Лаку м Минһу Наҙ ӥ р у н Муб ӥ ну н \r",
      "numberInSurah": 51,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4727,
      "text": "Каҙӓлика М ӓ 'Атá А л-Лаҙ ӥ на Ми н Қ а б лиһи м Ми н Ра с ӱ лин 'Иллӓ Қ ӓлӱ Сӓхи р ун 'Аұ Ма җ н ӱ ну н \r",
      "numberInSurah": 52,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4728,
      "text": "'Атаұӓ ҫ аұ Биһ и ۚ Бал Һу м Қ а ұ му н Ҭ ӓ ғ ӱ на \r",
      "numberInSurah": 53,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4729,
      "text": "Фатаұалла `Анһу м Фам ӓ 'А н та Бимал ӱ ми н \r",
      "numberInSurah": 54,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4730,
      "text": "Ұа Ҙакки р Фа'и нн а А ҙ-Ҙикрá Та н фа`у А л-Му'умин ӥ на \r",
      "numberInSurah": 55,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4731,
