{
  "number": 75,
  "name": "سُورَةُ القِيَامَةِ",
  "englishName": "Al-Qiyaama",
  "englishNameTranslation": "The Resurrection",
  "revelationType": "Meccan",
  "numberOfAyahs": 40,
  "ayahs": [
    {
      "number": 5552,
      "text": "Л ӓ 'У қ симу Бийа ұ ми А л- Қ ийӓмати \r",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5553,
      "text": "Ұа Л ӓ 'У қ симу Би н -Нафси А л-Лаұұӓмати \r",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5554,
      "text": "'Айахсабу А л-'И н с ӓ ну 'Алла н На җ ма`а `Иžӓмаһ у \r",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5555,
      "text": "Балá Қ ӓди р ӥ на `Ал á 'А н Нусаұұийа Банӓнаһ у \r",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5556,
      "text": "Бал Йу р ӥ ду А л-'И н с ӓ ну Лийафҗу ра 'Амӓмаһ у \r",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5557,
      "text": "Йас'алу 'Айй ӓ на Йа ұ му А л- Қ ийӓмати \r",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5558,
      "text": "Фа'иҙӓ Ба р и қ а А л-Ба ҫ а ру \r",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5559,
      "text": "Ұа Ҳ асафа А л- Қ ама ру \r",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5560,
      "text": "Ұа Җуми`а А ш-Ша м су Ұа А л- Қ ама ру \r",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5561,
      "text": "Йа қ ӱ лу А л-'И н с ӓ ну Йаұма'иҙин 'А й на А л-Мафар ру \r",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5562,
      "text": "Каллӓ Лӓ Ұаза ра \r",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5563,
      "text": "'Илá Ра ббика Йаұма'иҙин А л-Муста қ ар ру \r",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5564,
      "text": "Йунабба'у А л-'И н с ӓ ну Йаұма'иҙи н Бимӓ Қ аддама Ұа 'А ҳ ҳ а ра \r",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5565,
      "text": "Бали А л-'И н с ӓ ну `Алá Нафсиһ и Ба ҫ ӥ ра ту н \r",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5566,
      "text": "Ұа Лаұ 'Ал қ á Ма`ӓҙӥ ра һ у \r",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5567,
      "text": "Лӓ Тухар р ик Биһ и Лисӓнака Лита`җала Биһ и \r",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5568,
      "text": "'И нн а `Алайнӓ Җа м `аһ у Ұа Қ ур' ӓ наһ у \r",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5569,
      "text": "Фа'иҙӓ Қ а ра 'н ӓ һу Фа А ттаби` Қ ур' ӓ наһ у \r",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5570,
      "text": "Ćу мм а 'И нн а `Алайнӓ Байӓнаһ у \r",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5571,
      "text": "Каллӓ Бал Тухибб ӱ на А л-`Ӓҗилата \r",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5572,
      "text": "Ұа Таҙар ӱ на А л-'Ӓ ҳ и ра та \r",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5573,
      "text": "Ұуҗ ӱ һу н Йаұма'иҙи н Нӓđи ра ту н \r",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5574,
      "text": "'Илá Ра ббиһӓ Нӓžи ра ту н \r",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5575,
      "text": "Ұа Ұуҗ ӱ һу н Йаұма'иҙи н Бӓси ра ту н \r",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5576,
      "text": "Таžу нн у 'А н Йуф`ала Биһӓ Фӓ қ и ра ту н \r",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5577,
      "text": "Калл ӓ 'Иҙӓ Бала ғ ати А т-Та рӓ қ ӥ \r",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5578,
      "text": "Ұа Қ ӥ ла Ма н ۜ Р ӓ қ и н \r",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5579,
      "text": "Ұа Ž а нн а 'А нн аһу А л-Фи р ӓ қ у \r",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5580,
      "text": "Ұа А т-Таффати А с-С ӓ қ у Бис-С ӓ қ и \r",
      "numberInSurah": 29,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5581,
      "text": "'Илá Ра ббика Йаұма'иҙин А л-Мас ӓ қ у \r",
      "numberInSurah": 30,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5582,
      "text": "Фалӓ Ҫ адда қ а Ұа Лӓ Ҫ аллá \r",
      "numberInSurah": 31,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5583,
      "text": "Ұа Лаки н Каҙҙаба Ұа Таұаллá \r",
      "numberInSurah": 32,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5584,
      "text": "Ćу мм а Ҙаһаба 'Ил á 'Аһлиһ и Йатама ҭ ҭ á \r",
      "numberInSurah": 33,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5585,
      "text": "'Аұлá Лака Фа'аұлá \r",
      "numberInSurah": 34,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5586,
      "text": "Ćу мм а 'Аұлá Лака Фа'аұлá \r",
      "numberInSurah": 35,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5587,
      "text": "'Айахсабу А л-'И н с ӓ ну 'А н Йут ра ка Су д а н \r",
      "numberInSurah": 36,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5588,
      "text": "'Ала м Йаку Ну ҭ фата н Ми н Ман ӥ йи н Йу м нá \r",
      "numberInSurah": 37,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5589,
      "text": "Ćу мм а К ӓ на `Ала қ ата н Фа ҳ ала қ а Фасаұұá \r",
      "numberInSurah": 38,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5590,
      "text": "Фаҗа`ала Минһу А з-Заұҗа й ни А ҙ-Ҙака ра Ұа А л-'У н ćá \r",
      "numberInSurah": 39,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5591,
      "text": "'Ала й са Ҙӓлика Би қ ӓди р ин `Ал á 'А н Йухйийа А л-Маұтá \r",
      "numberInSurah": 40,
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    }
  ],
