{
  "number": 20,
  "name": "سُورَةُ طه",
  "englishName": "Taa-Haa",
  "englishNameTranslation": "Taa-Haa",
  "revelationType": "Meccan",
  "numberOfAyahs": 135,
  "ayahs": [
    {
      "number": 2349,
      "text": "ṭâ-hâ.",
      "numberInSurah": 1,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2350,
      "text": "mâ enzelnâ `aleyke-lḳur'âne liteşḳâ.",
      "numberInSurah": 2,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2351,
      "text": "illâ teẕkiratel limey yaḫşâ.",
      "numberInSurah": 3,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2352,
      "text": "tenzîlem mimmen ḫaleḳa-l'arḍa vessemâvâti-l`ulâ.",
      "numberInSurah": 4,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2353,
      "text": "erraḥmânü `ale-l`arşi-stevâ.",
      "numberInSurah": 5,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2354,
      "text": "lehû mâ fi-ssemâvâti vemâ fi-l'arḍi vemâ beynehümâ vemâ taḥte-ŝŝerâ.",
      "numberInSurah": 6,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2355,
      "text": "vein techer bilḳavli feinnehû ya`lemü-ssirra veaḫfâ.",
      "numberInSurah": 7,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2356,
      "text": "allâhü lâ ilâhe illâ hû. lehü-l'esmâü-lḥusnâ.",
      "numberInSurah": 8,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2357,
      "text": "vehel etâke ḥadîŝü mûsâ.",
      "numberInSurah": 9,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2358,
      "text": "iẕ raâ nâran feḳâle liehlihi-mküŝû innî ânestü nâral le`allî âtîküm minhâ biḳabesin ev ecidü `ale-nnâri hüdâ.",
      "numberInSurah": 10,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2359,
      "text": "felemmâ etâhâ nûdiye yâ mûsâ.",
      "numberInSurah": 11,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2360,
      "text": "innî ene rabbüke faḫla` na`leyk. inneke bilvâdi-lmüḳaddesi ṭuvâ.",
      "numberInSurah": 12,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2361,
      "text": "veene-ḫtertüke festemi` limâ yûḥâ.",
      "numberInSurah": 13,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2362,
      "text": "innenî ene-llâhü lâ ilâhe illâ ene fa`büdnî veeḳimi-ṣṣalâte liẕikrî.",
      "numberInSurah": 14,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2363,
      "text": "inne-ssâ`ate âtiyetün ekâdü uḫfîhâ litüczâ küllü nefsim bimâ tes`â.",
      "numberInSurah": 15,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2364,
      "text": "felâ yeṣuddenneke `anhâ mel lâ yü'minü bihâ vettebe`a hevâhü feterdâ.",
      "numberInSurah": 16,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2365,
      "text": "vemâ tilke biyemînike yâ mûsâ.",
      "numberInSurah": 17,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2366,
      "text": "ḳâle hiye `aṣây. etevekkeü `aleyhâ veehüşşü bihâ `alâ ganemî veliye fîhâ meâribü uḫrâ.",
      "numberInSurah": 18,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2367,
      "text": "ḳâle elḳihâ yâ mûsâ.",
      "numberInSurah": 19,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2368,
      "text": "feelḳâhâ feiẕâ hiye ḥayyetün tes`â.",
      "numberInSurah": 20,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2369,
      "text": "ḳâle ḫuẕhâ velâ teḫaf. senü`îdühâ sîratehe-l'ûlâ.",
      "numberInSurah": 21,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2370,
      "text": "vaḍmüm yedeke ilâ cenâḥike taḫruc beyḍâe min gayri sûin âyeten uḫrâ.",
      "numberInSurah": 22,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2371,
      "text": "linüriyeke min âyâtine-lkübrâ.",
      "numberInSurah": 23,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2372,
      "text": "iẕheb ilâ fir`avne innehû ṭagâ.",
      "numberInSurah": 24,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2373,
      "text": "ḳâle rabbi-şraḥ lî ṣadrî.",
      "numberInSurah": 25,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2374,
      "text": "veyessir lî emrî.",
      "numberInSurah": 26,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2375,
      "text": "vaḥlül `uḳdetem mil lisânî.",
      "numberInSurah": 27,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2376,
      "text": "yefḳahû ḳavlî.",
      "numberInSurah": 28,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2377,
      "text": "vec`al lî vezîram min ehlî.",
      "numberInSurah": 29,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2378,
      "text": "hârûne eḫî.",
      "numberInSurah": 30,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2379,
      "text": "üşdüd bihî ezrî.",
      "numberInSurah": 31,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2380,
      "text": "veeşrikhü fî emrî.",
      "numberInSurah": 32,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2381,
      "text": "key nüsebbiḥake keŝîrâ.",
      "numberInSurah": 33,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2382,
      "text": "veneẕkürake keŝîrâ.",
      "numberInSurah": 34,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2383,
      "text": "inneke künte binâ beṣîrâ.",
      "numberInSurah": 35,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2384,
      "text": "ḳâle ḳad ûtîte sü'leke yâ mûsâ.",
      "numberInSurah": 36,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2385,
      "text": "veleḳad menennâ `aleyke merraten uḫrâ.",
      "numberInSurah": 37,
      "juz": 16,
      "manzil": 4,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2386,
      "text": "iẕ evḥaynâ ilâ ümmike mâ yûḥâ.",
      "numberInSurah": 38,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2387,
      "text": "eni-ḳẕifîhi fi-ttâbûti faḳẕifîhi fi-lyemmi felyülḳihi-lyemmü bissâḥili ye'ḫuẕhü `adüvvül lî ve`adüvvul leh. veelḳaytü `aleyke meḥabbetem minnî. velituṣne`a `alâ `aynî.",
      "numberInSurah": 39,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2388,
      "text": "iẕ temşî uḫtüke feteḳûlü hel edüllüküm `alâ mey yekfülüh. feraca`nâke ilâ ümmike key teḳarra `aynühâ velâ taḥzen. veḳatelte nefsen fenecceynâke mine-lgammi vefetennâke fütûnâ. felebiŝte sinîne fî ehli medyene ŝümme ci'te `alâ ḳaderiy yâ mûsâ.",
      "numberInSurah": 40,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2389,
      "text": "vaṣṭana`tüke linefsî.",
      "numberInSurah": 41,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2390,
      "text": "iẕheb ente veeḫûke biâyâtî velâ teniyâ fî ẕikrî.",
      "numberInSurah": 42,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2391,
      "text": "iẕhebâ ilâ fir`avne innehû ṭagâ.",
      "numberInSurah": 43,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2392,
      "text": "feḳûlâ lehû ḳavlel leyyinel le`allehû yeteẕekkeru ev yaḫşâ.",
      "numberInSurah": 44,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2393,
      "text": "ḳâlâ rabbenâ innenâ neḫâfü ey yefruṭa `aleynâ ev ey yaṭgâ.",
      "numberInSurah": 45,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2394,
      "text": "ḳâle lâ teḫâfâ innenî me`akümâ esme`u veerâ.",
      "numberInSurah": 46,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2395,
      "text": "fe'tiyâhü feḳûlâ innâ rasûlâ rabbike feersil me`anâ benî isrâîle velâ tü`aẕẕibhüm. ḳad ci'nâke biâyetim mir rabbik. vesselâmü `alâ meni-ttebe`a-lhüdâ.",
      "numberInSurah": 47,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2396,
      "text": "innâ ḳad ûḥiye ileynâ enne-l`aẕâbe `alâ men keẕẕebe vetevellâ.",
      "numberInSurah": 48,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2397,
      "text": "ḳâle femer rabbükümâ yâ mûsâ.",
      "numberInSurah": 49,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2398,
      "text": "ḳâle rabbüne-lleẕî a`ṭâ külle şey'in ḫalḳahû ŝümme hedâ.",
      "numberInSurah": 50,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2399,
      "text": "ḳâle femâ bâlü-lḳurûni-l'ûlâ.",
      "numberInSurah": 51,
      "juz": 16,
      "manzil": 4,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2400,
      "text": "ḳâle `ilmühâ `inde rabbî fî kitâb. lâ yeḍillü rabbî velâ yensâ.",
      "numberInSurah": 52,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2401,
      "text": "elleẕî ce`ale lekümü-l'arḍa mehdev veseleke leküm fîhâ sübülev veenzele mine-ssemâi mââ. feaḫracnâ bihî ezvâcem min nebetin şettâ.",
      "numberInSurah": 53,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2402,
      "text": "külû ver`av en`âmeküm. inne fî ẕâlike leâyâtil liüli-nnühâ.",
      "numberInSurah": 54,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2403,
      "text": "minhâ ḫalaḳnâküm vefîhâ nü`îdüküm veminhâ nuḫricüküm târaten uḫrâ.",
      "numberInSurah": 55,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2404,
      "text": "veleḳad eraynâhü âyâtinâ küllehâ fekeẕẕebe veebâ.",
      "numberInSurah": 56,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2405,
      "text": "ḳâle eci'tenâ lituḫricenâ min arḍinâ bisiḥrike yâ mûsâ.",
      "numberInSurah": 57,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2406,
      "text": "felene'tiyenneke bisiḥrim miŝlihî fec`al beynenâ vebeyneke mev`idel lâ nuḫlifühû naḥnü velâ ente mekânen süvâ.",
      "numberInSurah": 58,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2407,
      "text": "ḳâle mev`idüküm yevmü-zzîneti veey yuḥşera-nnâsü ḍuḥâ.",
      "numberInSurah": 59,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2408,
      "text": "fetevellâ fir`avnü feceme`a keydehû ŝümme etâ.",
      "numberInSurah": 60,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2409,
      "text": "ḳâle lehüm mûsâ veyleküm lâ tefterû `ale-llâhi keẕiben feyüsḥiteküm bi`aẕâb. veḳad ḫâbe meni-fterâ.",
      "numberInSurah": 61,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2410,
      "text": "fetenâza`û emrahüm beynehüm veeserrü-nnecvâ.",
      "numberInSurah": 62,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2411,
      "text": "ḳâlû in hâẕâni lesâḥirâni yürîdâni ey yuḫricâküm min arḍiküm bisiḥrihimâ veyeẕhebâ biṭarîḳatikümü-lmüŝlâ.",
      "numberInSurah": 63,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2412,
      "text": "feecmi`û keydeküm ŝümme-'tû ṣaffâ. veḳad efleḥa-lyevme meni-sta`lâ.",
      "numberInSurah": 64,
      "juz": 16,
      "manzil": 4,
      "page": 315,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2413,
      "text": "ḳâlû yâ mûsâ immâ en tülḳiye veimmâ en nekûne evvele men elḳâ.",
      "numberInSurah": 65,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2414,
      "text": "ḳâle bel elḳû. feiẕâ ḥibâlühüm ve`iṣiyyühüm yüḫayyelü ileyhi min siḥrihim ennehâ tes`â.",
      "numberInSurah": 66,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2415,
      "text": "feevcese fî nefsihî ḫîfetem mûsâ.",
      "numberInSurah": 67,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2416,
      "text": "ḳulnâ lâ teḫaf inneke ente-l'a`lâ.",
      "numberInSurah": 68,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2417,
      "text": "veelḳi mâ fî yemînike telḳaf mâ ṣane`û. innemâ ṣane`û keydü sâḥir. velâ yüfliḥu-ssâḥiru ḥayŝü etâ.",
      "numberInSurah": 69,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2418,
      "text": "feülḳiye-sseḥaratü sücceden ḳâlû âmennâ birabbi hârûne vemûsâ.",
      "numberInSurah": 70,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2419,
      "text": "ḳâle âmentüm lehû ḳable en âẕene leküm. innehû lekebîrukümü-lleẕî `allemekümü-ssiḥr. feleüḳaṭṭi`anne eydiyeküm veercüleküm min ḫilâfiv veleüṣallibenneküm fî cüẕû`i-nnaḫl. veleta`lemünne eyyünâ eşeddü `aẕâbev veebḳâ.",
      "numberInSurah": 71,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2420,
      "text": "ḳâlû len nü'ŝirake `alâ mâ câenâ mine-lbeyyinâti velleẕî feṭaranâ faḳḍi mâ ente ḳâḍ. innemâ taḳḍî hâẕihi-lḥayâte-ddünyâ.",
      "numberInSurah": 72,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2421,
      "text": "innâ âmennâ birabbinâ liyagfira lenâ ḫaṭâyânâ vemâ ekrahtenâ `aleyhi mine-ssiḥr. vellâhü ḫayruv veebḳâ.",
      "numberInSurah": 73,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2422,
      "text": "innehû mey ye'ti rabbehû mücrimen feinne lehû cehennem. lâ yemûtü fîhâ velâ yaḥyâ.",
      "numberInSurah": 74,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2423,
      "text": "vemey ye'tihî mü'minen ḳad `amile-ṣṣâliḥâti feülâike lehümü-dderacâtü-l`ulâ.",
      "numberInSurah": 75,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2424,
      "text": "cennâtü `adnin tecrî min taḥtihe-l'enhâru ḫâlidîne fîhâ. veẕâlike cezâü men tezekkâ.",
      "numberInSurah": 76,
      "juz": 16,
      "manzil": 4,
      "page": 316,
      "ruku": 272,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2425,
      "text": "veleḳad evḥaynâ ilâ mûsâ en esri bi`ibâdî faḍrib lehüm ṭarîḳan fi-lbaḥri yebesâ. lâ teḫâfü derakev velâ taḫşâ.",
      "numberInSurah": 77,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2426,
      "text": "feetbe`ahüm fir`avnü bicünûdihî fegaşiyehüm mine-lyemmi mâ gaşiyehüm.",
      "numberInSurah": 78,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2427,
      "text": "veeḍalle fir`avnü ḳavmehû vemâ hedâ.",
      "numberInSurah": 79,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2428,
      "text": "yâ benî isrâîle ḳad enceynâküm min `adüvviküm veve`adnâküm cânibe-ṭṭûri-l'eymene venezzelnâ `aleykümü-lmenne vesselvâ.",
      "numberInSurah": 80,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2429,
      "text": "külû min ṭayyibâti mâ razaḳnâküm velâ taṭgav fîhi feyeḥille `aleyküm gaḍabî. vemey yaḥlil `aleyhi gaḍabî feḳad hevâ.",
      "numberInSurah": 81,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2430,
      "text": "veinnî legaffârul limen tâbe veâmene ve`amile ṣâliḥan ŝümme-htedâ.",
      "numberInSurah": 82,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 126,
      "sajda": false
    },
    {
      "number": 2431,
      "text": "vemâ a`celeke `an ḳavmike yâ mûsâ.",
      "numberInSurah": 83,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2432,
      "text": "ḳâle hüm ülâi `alâ eŝerî ve`aciltü ileyke rabbi literḍâ.",
      "numberInSurah": 84,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2433,
      "text": "ḳâle feinnâ ḳad fetennâ ḳavmeke mim ba`dike veeḍallehümü-ssâmiriyy.",
      "numberInSurah": 85,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2434,
      "text": "ferace`a mûsâ ilâ ḳavmihî gaḍbâne esifâ. ḳâle yâ ḳavmi elem ye`idküm rabbüküm va`den ḥasenâ. efeṭâle `aleykümü-l`ahdü em erattüm ey yeḥille `aleyküm gaḍabüm mir rabbiküm feaḫleftüm mev`idî.",
      "numberInSurah": 86,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2435,
      "text": "ḳâlû mâ aḫlefnâ mev`ideke bimelkinâ velâkinnâ ḥummilnâ evzâram min zîneti-lḳavmi feḳaẕefnâhâ fekeẕâlike elḳa-ssâmiriyy.",
      "numberInSurah": 87,
      "juz": 16,
      "manzil": 4,
      "page": 317,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2436,
      "text": "feaḫrace lehüm `iclen cesedel lehû ḫuvârun feḳâlû hâẕâ ilâhüküm veilâhü mûsâ fenesî.",
      "numberInSurah": 88,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2437,
      "text": "efelâ yeravne ellâ yerci`u ileyhim ḳavlev velâ yemlikü lehüm ḍarrav velâ nef`â.",
      "numberInSurah": 89,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 273,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2438,
      "text": "veleḳad ḳâle lehüm hârûnü min ḳablü yâ ḳavmi innemâ fütintüm bih. veinne rabbekümü-rraḥmânü fettebi`ûnî veeṭî`û emrî.",
      "numberInSurah": 90,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2439,
      "text": "ḳâlû len nebraḥa `aleyhi `âkifîne ḥattâ yerci`a ileynâ mûsâ.",
      "numberInSurah": 91,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2440,
      "text": "ḳâle yâ hârûnü mâ mene`ake iẕ raeytehüm ḍallû.",
      "numberInSurah": 92,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2441,
      "text": "ellâ tettebi`an. efe`aṣayte emrî.",
      "numberInSurah": 93,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2442,
      "text": "ḳâle yebneümme lâ te'ḫuẕ biliḥyetî velâ bira'sî. innî ḫaşîtü en teḳûle ferraḳte beyne benî isrâîle velem terḳub ḳavlî.",
      "numberInSurah": 94,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2443,
      "text": "ḳâle femâ ḫaṭbüke yâ sâmiriyy.",
      "numberInSurah": 95,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2444,
      "text": "ḳâle beṣurtü bimâ lem yebṣurû bihî feḳabaḍtü ḳabḍatem min eŝeri-rrasûli fenebeẕtühâ vekeẕâlike sevvelet lî nefsî.",
      "numberInSurah": 96,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2445,
      "text": "ḳâle feẕheb feinne leke fi-lḥayâti en teḳûle lâ misâs. veinne leke mev`idel len tuḫlefeh. venżur ilâ ilâhike-lleẕî żalte `aleyhi `âkifâ. lenüḥarriḳannehû ŝümme lenensifennehû fi-lyemmi nesfâ.",
      "numberInSurah": 97,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2446,
      "text": "innemâ ilâhükümü-llâhü-lleẕî lâ ilâhe illâ hû. vesi`a külle şey'in `ilmâ.",
      "numberInSurah": 98,
      "juz": 16,
      "manzil": 4,
      "page": 318,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2447,
      "text": "keẕâlike neḳuṣṣu `aleyke min embâi mâ ḳad sebeḳ. veḳad âteynâke mil ledünnâ ẕikrâ.",
      "numberInSurah": 99,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2448,
      "text": "men a`raḍa `anhü feinnehû yaḥmilü yevme-lḳiyâmeti vizrâ.",
      "numberInSurah": 100,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2449,
      "text": "ḫâlidîne fîh. vesâe lehüm yevme-lḳiyâmeti ḥimlâ.",
      "numberInSurah": 101,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2450,
      "text": "yevme yünfeḫu fi-ṣṣûri venaḥşüru-lmücrimîne yevmeiẕin zürḳâ.",
      "numberInSurah": 102,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2451,
      "text": "yeteḫâfetûne beynehüm il lebiŝtüm illâ `aşrâ.",
      "numberInSurah": 103,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2452,
      "text": "naḥnü a`lemü bimâ yeḳûlûne iẕ yeḳûlü emŝelühüm ṭarîḳaten il lebiŝtüm illâ yevmâ.",
      "numberInSurah": 104,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 274,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2453,
      "text": "veyes'elûneke `ani-lcibâli feḳul yensifühâ rabbî nesfâ.",
      "numberInSurah": 105,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2454,
      "text": "feyeẕeruhâ ḳâ`an ṣafṣafâ.",
      "numberInSurah": 106,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2455,
      "text": "lâ terâ fîhâ `ivecev velâ emtâ.",
      "numberInSurah": 107,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2456,
      "text": "yevmeiẕiy yettebi`ûne-ddâ`iye lâ `ivece leh. veḫaşe`ati-l'aṣvâtü lirraḥmâni felâ tesme`u illâ hemsâ.",
      "numberInSurah": 108,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2457,
      "text": "yevmeiẕil lâ tenfe`u-şşefâ`atü illâ men eẕine lehü-rraḥmânü veraḍiye lehû ḳavlâ.",
      "numberInSurah": 109,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2458,
      "text": "ya`lemü mâ beyne eydîhim vemâ ḫalfehüm velâ yüḥîṭûne bihî `ilmâ.",
      "numberInSurah": 110,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 127,
      "sajda": false
    },
    {
      "number": 2459,
      "text": "ve`aneti-lvucûhü lilḥayyi-lḳayyûm. veḳad ḫâbe men ḥamele żulmâ.",
      "numberInSurah": 111,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2460,
      "text": "vemey ya`mel mine-ṣṣâliḥâti vehüve mü'minün felâ yeḫâfü żulmev velâ haḍmâ.",
      "numberInSurah": 112,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2461,
      "text": "vekeẕâlike enzelnâhü ḳur'ânen `arabiyyev veṣarrafnâ fîhi mine-lve`îdi le`allehüm yetteḳûne ev yuḥdiŝü lehüm ẕikrâ.",
      "numberInSurah": 113,
      "juz": 16,
      "manzil": 4,
      "page": 319,
      "ruku": 275,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2462,
      "text": "fete`âle-llâhü-lmelikü-lḥaḳḳ. velâ ta`cel bilḳur'âni min ḳabli ey yuḳḍâ ileyke vaḥyüh. veḳur rabbi zidnî `ilmâ.",
      "numberInSurah": 114,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 275,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2463,
      "text": "veleḳad `ahidnâ ilâ âdeme min ḳablü fenesiye velem necid lehû `azmâ.",
      "numberInSurah": 115,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 275,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2464,
      "text": "veiẕ ḳulnâ lilmelâiketi-scüdû liâdeme fesecedû illâ iblîs. ebâ.",
      "numberInSurah": 116,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2465,
      "text": "feḳulnâ yâ âdemü inne hâẕâ `adüvvül leke velizevcike felâ yuḫricennekümâ mine-lcenneti feteşḳâ.",
      "numberInSurah": 117,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2466,
      "text": "inne leke ellâ tecû`a fîhâ velâ ta`râ.",
      "numberInSurah": 118,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2467,
      "text": "veenneke lâ tażmeü fîhâ velâ taḍḥâ.",
      "numberInSurah": 119,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2468,
      "text": "fevesvese ileyhi-şşeyṭânü ḳâle yâ âdemü hel edüllüke `alâ şecerati-lḫuldi vemülkil lâ yeblâ.",
      "numberInSurah": 120,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2469,
      "text": "feekelâ minhâ febedet lehümâ sev'âtühümâ veṭafiḳâ yaḫṣifâni `aleyhimâ miv veraḳi-lcenneh. ve`aṣâ âdemü rabbehû fegavâ.",
      "numberInSurah": 121,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2470,
      "text": "ŝümme-ctebâhü rabbühû fetâbe `aleyhi vehedâ.",
      "numberInSurah": 122,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2471,
      "text": "ḳâle-hbiṭâ minhâ cemî`am ba`ḍuküm liba`ḍin `adüvv. feimmâ ye'tiyenneküm minnî hüden femeni-ttebe`a hüdâye felâ yeḍillü velâ yeşḳâ.",
      "numberInSurah": 123,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2472,
      "text": "vemen a`raḍa `an ẕikrî feinne lehû me`îşeten ḍankev venaḥşüruhû yevme-lḳiyâmeti a`mâ.",
      "numberInSurah": 124,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2473,
      "text": "ḳâle rabbi lime ḥaşertenî a`mâ veḳad küntü beṣîrâ.",
      "numberInSurah": 125,
      "juz": 16,
      "manzil": 4,
      "page": 320,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2474,
      "text": "ḳâle keẕâlike etetke âyâtünâ fenesîtehâ. vekeẕâlike-lyevme tünsâ.",
      "numberInSurah": 126,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2475,
      "text": "vekeẕâlike neczî men esrafe velem yü'mim biâyâti rabbih. vele`aẕâbü-l'âḫirati eşeddü veebḳâ.",
      "numberInSurah": 127,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2476,
      "text": "efelem yehdi lehüm kem ehleknâ ḳablehüm mine-lḳurûni yemşûne fî mesâkinihim. inne fî ẕâlike leâyâtil liüli-nnühâ.",
      "numberInSurah": 128,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 276,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2477,
      "text": "velevlâ kelimetün sebeḳat mir rabbike lekâne lizâmev veecelüm müsemmâ.",
      "numberInSurah": 129,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2478,
      "text": "faṣbir `alâ mâ yeḳûlûne vesebbiḥ biḥamdi rabbike ḳable ṭulû`i-şşemsi veḳable gurûbihâ. vemin ânâi-lleyli fesebbiḥ veaṭrâfe-nnehâri le`alleke terḍâ.",
      "numberInSurah": 130,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2479,
      "text": "velâ temüddenne `ayneyke ilâ mâ metta`nâ bihî ezvâcem minhüm zehrate-lḥayâti-ddünyâ lineftinehüm fîh. verizḳu rabbike ḫayruv veebḳâ.",
      "numberInSurah": 131,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2480,
      "text": "ve'mür ehleke biṣṣalâti vaṣṭabir `aleyhâ. lâ nes'elüke rizḳâ. naḥnü nerzüḳuk. vel`âḳibetü littaḳvâ.",
      "numberInSurah": 132,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2481,
      "text": "veḳâlû levlâ ye'tînâ biâyetim mir rabbih. evelem te'tihim beyyinetü mâ fi-ṣṣuḥufi-l'ûlâ.",
      "numberInSurah": 133,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2482,
      "text": "velev ennâ ehleknâhüm bi`aẕâbim min ḳablihî leḳâlû rabbenâ levlâ erselte ileynâ rasûlen fenettebi`a âyâtike min ḳabli en neẕille venaḫzâ.",
      "numberInSurah": 134,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    },
    {
      "number": 2483,
      "text": "ḳul küllüm müterabbiṣun feterabbeṣû. feseta`lemûne men aṣḥâbu-ṣṣirâṭi-sseviyyi vemeni-htedâ.",
      "numberInSurah": 135,
      "juz": 16,
      "manzil": 4,
      "page": 321,
      "ruku": 277,
      "hizbQuarter": 128,
      "sajda": false
    }
