{
  "number": 52,
  "name": "سُورَةُ الطُّورِ",
  "englishName": "At-Tur",
  "englishNameTranslation": "The Mount",
  "revelationType": "Meccan",
  "numberOfAyahs": 49,
  "ayahs": [
    {
      "number": 4736,
      "text": "veṭṭûr.",
      "numberInSurah": 1,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4737,
      "text": "vekitâbim mesṭûr.",
      "numberInSurah": 2,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4738,
      "text": "fî raḳḳim menşûr.",
      "numberInSurah": 3,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4739,
      "text": "velbeyti-lma`mûr.",
      "numberInSurah": 4,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4740,
      "text": "vessaḳfi-lmerfû`.",
      "numberInSurah": 5,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4741,
      "text": "velbaḥri-lmescûr.",
      "numberInSurah": 6,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4742,
      "text": "inne `aẕâbe rabbike levâḳi`.",
      "numberInSurah": 7,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4743,
      "text": "mâ lehû min dâfi`.",
      "numberInSurah": 8,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4744,
      "text": "yevme temûru-ssemâü mevrâ.",
      "numberInSurah": 9,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4745,
      "text": "vetesîru-lcibâlü seyrâ.",
      "numberInSurah": 10,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4746,
      "text": "feveylüy yevmeiẕil lilmükeẕẕibîn.",
      "numberInSurah": 11,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4747,
      "text": "elleẕîne hüm fî ḫavḍiy yel`abûn.",
      "numberInSurah": 12,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4748,
      "text": "yevme yüde``ûne ilâ nâri cehenneme da``â.",
      "numberInSurah": 13,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4749,
      "text": "hâẕihi-nnâru-lletî küntüm bihâ tükeẕẕibûn.",
      "numberInSurah": 14,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4750,
      "text": "efesiḥrun hâẕâ em entüm lâ tübṣirûn.",
      "numberInSurah": 15,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4751,
      "text": "iṣlevhâ faṣbirû ev lâ taṣbirû. sevâün `aleyküm. innemâ tüczevne mâ küntüm ta`melûn.",
      "numberInSurah": 16,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4752,
      "text": "inne-lmütteḳîne fî cennâtiv vene`îm.",
      "numberInSurah": 17,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4753,
      "text": "fâkihîne bimâ âtâhüm rabbühüm. veveḳâhüm rabbühüm `aẕâbe-lceḥîm.",
      "numberInSurah": 18,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4754,
      "text": "külû veşrabû henîem bimâ küntüm ta`melûn.",
      "numberInSurah": 19,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4755,
      "text": "müttekiîne `alâ sürurim maṣfûfeh. vezevvecnâhüm biḥûrin `în.",
      "numberInSurah": 20,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4756,
      "text": "velleẕîne âmenû vettebe`athüm ẕürriyyetühüm biîmânin elḥaḳnâ bihim ẕürriyyetehüm vemâ eletnâhüm min `amelihim min şey'. küllü-mriim bimâ kesebe rahîn.",
      "numberInSurah": 21,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4757,
      "text": "veemdednâhüm bifâkihetiv velaḥmim mimmâ yeştehûn.",
      "numberInSurah": 22,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4758,
      "text": "yetenâza`ûne fîhâ ke'sel lâ lagvun fîhâ velâ te'ŝîm.",
      "numberInSurah": 23,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4759,
      "text": "veyeṭûfü `aleyhim gilmânül lehüm keennehüm lü'lüüm meknûn.",
      "numberInSurah": 24,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4760,
      "text": "veaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn.",
      "numberInSurah": 25,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4761,
      "text": "ḳâlû innâ künnâ ḳablü fî ehlinâ müşfiḳîn.",
      "numberInSurah": 26,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4762,
      "text": "femenne-llâhü `aleynâ veveḳânâ `aẕâbe-ssemûm.",
      "numberInSurah": 27,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4763,
      "text": "innâ künnâ min ḳablü ned`ûh. innehû hüve-lberru-rraḥîm.",
      "numberInSurah": 28,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4764,
      "text": "feẕekkir femâ ente bini`meti rabbike bikâhiniv velâ mecnûn.",
      "numberInSurah": 29,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4765,
      "text": "em yeḳûlûne şâ`irun neterabbeṣu bihî raybe-lmenûn.",
      "numberInSurah": 30,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4766,
      "text": "ḳul terabbeṣû feinnî me`aküm mine-lmüterabbiṣîn.",
      "numberInSurah": 31,
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4767,
      "text": "em te'müruhüm aḥlâmühüm bihâẕâ em hüm ḳavmün ṭâgûn.",
      "numberInSurah": 32,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4768,
      "text": "em yeḳûlûne teḳavveleh. bel lâ yü'minûn.",
      "numberInSurah": 33,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4769,
      "text": "felye'tû biḥadîŝim miŝlihî in kânû ṣâdiḳîn.",
      "numberInSurah": 34,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4770,
      "text": "em ḫuliḳû min gayri şey'in em hümü-lḫâliḳûn.",
      "numberInSurah": 35,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4771,
      "text": "em ḫaleḳu-ssemâvâti vel'arḍ. bel lâ yûḳinûn.",
      "numberInSurah": 36,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4772,
      "text": "em `indehüm ḫazâinü rabbike em hümü-lmüṣayṭirûn.",
      "numberInSurah": 37,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4773,
      "text": "em lehüm süllemüy yestemi`ûne fîh. felye'ti müstemi`uhüm bisülṭânim mübîn.",
      "numberInSurah": 38,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4774,
      "text": "em lehü-lbenâtü velekümü-lbenûn.",
      "numberInSurah": 39,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4775,
      "text": "em tes'elühüm ecran fehüm mim magramim müŝḳalûn.",
      "numberInSurah": 40,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4776,
      "text": "em `indehümü-lgaybü fehüm yektübûn.",
      "numberInSurah": 41,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4777,
      "text": "em yürîdûne keydâ. felleẕîne keferû hümü-lmekîdûn.",
      "numberInSurah": 42,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4778,
      "text": "em lehüm ilâhün gayru-llâh. sübḥâne-llâhi `ammâ yüşrikûn.",
      "numberInSurah": 43,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4779,
      "text": "veiy yerav kisfem mine-ssemâi sâḳiṭay yeḳûlû seḥâbüm merkûm.",
      "numberInSurah": 44,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4780,
      "text": "feẕerhüm ḥattâ yülâḳû yevmehümü-lleẕî fîhi yuṣ`aḳûn.",
      "numberInSurah": 45,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4781,
      "text": "yevme lâ yugnî `anhüm keydühüm şey'ev velâ hüm yünṣarûn.",
      "numberInSurah": 46,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4782,
      "text": "veinne lilleẕîne żalemû `aẕâben dûne ẕâlike velâkinne ekŝerahüm lâ ya`lemûn.",
      "numberInSurah": 47,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4783,
      "text": "vaṣbir liḥukmi rabbike feinneke bia`yüninâ vesebbiḥ biḥamdi rabbike ḥîne teḳûm.",
      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4784,
      "text": "vemine-lleyli fesebbiḥhü veidbâra-nnücûm.",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    }
  ],
