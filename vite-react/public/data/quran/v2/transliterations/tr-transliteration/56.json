  "englishName": "Al-Waaqia",
  "englishNameTranslation": "The Inevitable",
  "revelationType": "Meccan",
  "numberOfAyahs": 96,
  "ayahs": [
    {
      "number": 4980,
      "text": "iẕâ veḳa`ati-lvâḳi`ah.",
      "numberInSurah": 1,
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
