{
  "number": 51,
  "name": "سُورَةُ الذَّارِيَاتِ",
  "englishName": "Adh-Dhaariyat",
  "englishNameTranslation": "The Winnowing Winds",
  "revelationType": "Meccan",
  "numberOfAyahs": 60,
  "ayahs": [
    {
      "number": 4676,
      "text": "veẕẕâriyâti ẕervâ.",
      "numberInSurah": 1,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4677,
      "text": "felḥâmilâti viḳrâ.",
      "numberInSurah": 2,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4678,
      "text": "felcâriyâti yüsrâ.",
      "numberInSurah": 3,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4679,
      "text": "felmüḳassimâti emrâ.",
      "numberInSurah": 4,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4680,
      "text": "innemâ tû`adûne leṣâdiḳ.",
      "numberInSurah": 5,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4681,
      "text": "veinne-ddîne levâḳi`.",
      "numberInSurah": 6,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4682,
      "text": "vessemâi ẕâti-lḥubük.",
      "numberInSurah": 7,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4683,
      "text": "inneküm lefî ḳavlim muḫtelif.",
      "numberInSurah": 8,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4684,
      "text": "yü'fekü `anhü men üfik.",
      "numberInSurah": 9,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4685,
      "text": "ḳutile-lḫarrâṣûn.",
      "numberInSurah": 10,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4686,
      "text": "elleẕîne hüm fî gamratin sâhûn.",
      "numberInSurah": 11,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4687,
      "text": "yes'elûne eyyâne yevmü-ddîn.",
      "numberInSurah": 12,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4688,
      "text": "yevme hüm `ale-nnâri yüftenûn.",
      "numberInSurah": 13,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4689,
      "text": "ẕûḳû fitneteküm. hâẕe-lleẕî küntüm bihî testa`cilûn.",
      "numberInSurah": 14,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4690,
      "text": "inne-lmütteḳîne fî cennâtiv ve`uyûn.",
      "numberInSurah": 15,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4691,
      "text": "âḫiẕîne mâ âtâhüm rabbühüm. innehüm kânû ḳable ẕâlike muḥsinîn.",
      "numberInSurah": 16,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4692,
      "text": "kânû ḳalîlem mine-lleyli mâ yehce`ûn.",
      "numberInSurah": 17,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4693,
      "text": "vebil'esḥâri hüm yestagfirûn.",
      "numberInSurah": 18,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4694,
      "text": "vefî emvâlihim ḥaḳḳul lissâili velmaḥrûm.",
      "numberInSurah": 19,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4695,
      "text": "vefi-l'arḍi âyâtül lilmûḳinîn.",
      "numberInSurah": 20,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4696,
      "text": "vefî enfüsiküm. efelâ tübṣirûn.",
      "numberInSurah": 21,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4697,
      "text": "vefi-ssemâi rizḳuküm vemâ tû`adûn.",
      "numberInSurah": 22,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4698,
      "text": "feverabbi-ssemâi vel'arḍi innehû leḥaḳḳum miŝle mâ enneküm tenṭiḳûn.",
      "numberInSurah": 23,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4699,
      "text": "hel etâke ḥadîŝü ḍayfi ibrâhîme-lmükramîn.",
      "numberInSurah": 24,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4700,
      "text": "iẕ deḫalû `aleyhi feḳâlû selâmâ. ḳâle selâm. ḳavmüm münkerûn.",
      "numberInSurah": 25,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4701,
      "text": "ferâga ilâ ehlihî fecâe bi`iclin semîn.",
      "numberInSurah": 26,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4702,
      "text": "feḳarrabehû ileyhim ḳâle elâ te'külûn.",
      "numberInSurah": 27,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4703,
      "text": "feevcese minhüm ḫîfeh. ḳâlû lâ teḫaf. vebeşşerûhü bigulâmin `alîm.",
      "numberInSurah": 28,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4704,
      "text": "feaḳbeleti-mraetühû fî ṣarratin feṣakket vechehâ veḳâlet `acûzün `aḳîm.",
      "numberInSurah": 29,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4705,
      "text": "ḳâlû keẕâliki ḳâle rabbük. innehû hüve-lḥakîmü-l`alîm.",
      "numberInSurah": 30,
      "juz": 26,
      "manzil": 7,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4706,
      "text": "ḳâle femâ ḫaṭbüküm eyyühe-lmürselûn.",
      "numberInSurah": 31,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4707,
      "text": "ḳâlû innâ ürsilnâ ilâ ḳavmim mücrimîn.",
      "numberInSurah": 32,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4708,
      "text": "linürsile `aleyhim ḥicâratem min ṭîn.",
      "numberInSurah": 33,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4709,
      "text": "müsevvemeten `inde rabbike lilmüsrifîn.",
      "numberInSurah": 34,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4710,
      "text": "feaḫracnâ men kâne fîhâ mine-lmü'minîn.",
      "numberInSurah": 35,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4711,
      "text": "femâ vecednâ fîhâ gayra beytim mine-lmüslimîn.",
      "numberInSurah": 36,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4712,
      "text": "veteraknâ fîhâ âyetel lilleẕîne yeḫâfûne-l`aẕâbe-l'elîm.",
      "numberInSurah": 37,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4713,
      "text": "vefî mûsâ iẕ erselnâhü ilâ fir`avne bisülṭânim mübîn.",
      "numberInSurah": 38,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4714,
      "text": "fetevellâ biruknihî veḳâle sâḥirun ev mecnûn.",
      "numberInSurah": 39,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4715,
      "text": "feeḫaẕnâhü vecünûdehû fenebeẕnâhüm fi-lyemmi vehüve mülîm.",
      "numberInSurah": 40,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4716,
      "text": "vefî `âdin iẕ erselnâ `aleyhimü-rrîḥa-l`aḳîm.",
      "numberInSurah": 41,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4717,
      "text": "mâ teẕeru min şey'in etet `aleyhi illâ ce`alethü kelramîm.",
      "numberInSurah": 42,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4718,
      "text": "vefî ŝemûde iẕ ḳîle lehüm temette`û ḥattâ ḥîn.",
      "numberInSurah": 43,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4719,
      "text": "fe`atev `an emri rabbihim feeḫaẕethümu-ṣṣâ`iḳatü vehüm yenżurûn.",
      "numberInSurah": 44,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4720,
      "text": "feme-steṭâ`û min ḳiyâmiv vemâ kânû münteṣirîn.",
      "numberInSurah": 45,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4721,
      "text": "veḳavme nûḥim min ḳabl. innehüm kânû ḳavmen fâsiḳîn.",
      "numberInSurah": 46,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4722,
      "text": "vessemâe beneynâhâ bieydiv veinnâ lemûsi`ûn.",
      "numberInSurah": 47,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4723,
      "text": "vel'arḍa feraşnâhâ feni`me-lmâhidûn.",
      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4724,
      "text": "vemin külli şey'in ḫalaḳnâ zevceyni le`alleküm teẕekkerûn.",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4725,
      "text": "fefirrû ile-llâh. innî leküm minhü neẕîrum mübîn.",
      "numberInSurah": 50,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4726,
      "text": "velâ tec`alû me`a-llâhi ilâhen âḫar. innî leküm minhü neẕîrum mübîn.",
      "numberInSurah": 51,
      "juz": 27,
      "manzil": 7,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4727,
      "text": "keẕâlike mâ ete-lleẕîne min ḳablihim mir rasûlin illâ ḳâlû sâḥirun ev mecnûn.",
      "numberInSurah": 52,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4728,
      "text": "etevâṣav bih. bel hüm ḳavmün ṭâgûn.",
      "numberInSurah": 53,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4729,
      "text": "fetevelle `anhüm femâ ente bimelûm.",
      "numberInSurah": 54,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4730,
      "text": "veẕekkir feinne-ẕẕikrâ tenfe`u-lmü'minîn.",
      "numberInSurah": 55,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4731,
      "text": "vemâ ḫalaḳtü-lcinne vel'inse illâ liya`büdûn.",
      "numberInSurah": 56,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4732,
      "text": "mâ ürîdü minhüm mir rizḳiv vemâ ürîdü ey yuṭ`imûn.",
      "numberInSurah": 57,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4733,
      "text": "inne-llâhe hüve-rrazzâḳu ẕü-lḳuvveti-lmetîn.",
      "numberInSurah": 58,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4734,
      "text": "feinne lilleẕîne żalemû ẕenûbem miŝle ẕenûbi aṣḥâbihim felâ yesta`cilûn.",
      "numberInSurah": 59,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4735,
      "text": "feveylül lilleẕîne keferû miy yevmihimü-lleẕî yû`adûn.",
      "numberInSurah": 60,
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    }
  ],
