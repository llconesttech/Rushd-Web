{
  "number": 21,
  "name": "سُورَةُ الأَنبِيَاءِ",
  "englishName": "Al-Anbiyaa",
  "englishNameTranslation": "The Prophets",
  "revelationType": "Meccan",
  "numberOfAyahs": 112,
  "ayahs": [
    {
      "number": 2484,
      "text": "iḳterabe linnâsi ḥisâbühüm vehüm fî gafletim mü`riḍûn.",
      "numberInSurah": 1,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2485,
      "text": "mâ ye'tîhim min ẕikrim mir rabbihim muḥdeŝin ille-steme`ûhü vehüm yel`abûn.",
      "numberInSurah": 2,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2486,
      "text": "lâhiyeten ḳulûbühüm. veeserrü-nnecvâ. elleẕîne żalemû. hel hâẕâ illâ beşer miŝlüküm. efete'tûne-ssiḥra veentüm tübṣirûn.",
      "numberInSurah": 3,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2487,
      "text": "ḳâle rabbî ya`lemü-lḳavle fi-ssemâi vel'arḍ. vehüve-ssemî`u-l`alîm.",
      "numberInSurah": 4,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2488,
      "text": "bel ḳâlû aḍgâŝü aḥlâmim beli-fterâhü bel hüve şâ`ir. felye'tinâ biâyetin kemâ ürsile-l'evvelûn.",
      "numberInSurah": 5,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2489,
      "text": "mâ âmenet ḳablehüm min ḳaryetin ehleknâhâ. efehüm yü'minûn.",
      "numberInSurah": 6,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2490,
      "text": "vemâ erselnâ ḳableke illâ ricâlen nûḥî ileyhim fes'elû ehle-ẕẕikri in küntüm lâ ta`lemûn.",
      "numberInSurah": 7,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2491,
      "text": "vemâ ce`alnâhüm cesedel lâ ye'külûne-ṭṭa`âme vemâ kânû ḫâlidîn.",
      "numberInSurah": 8,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2492,
      "text": "ŝümme ṣadaḳnâhümü-lva`de feenceynâhüm vemen neşâü veehlekne-lmüsrifîn.",
      "numberInSurah": 9,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2493,
      "text": "leḳad enzelnâ ileyküm kitâben fîhi ẕikruküm. efelâ ta`ḳilûn.",
      "numberInSurah": 10,
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2494,
      "text": "vekem ḳaṣamnâ min ḳaryetin kânet żâlimetev veenşe'nâ ba`dehâ ḳavmen âḫarîn.",
      "numberInSurah": 11,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2495,
      "text": "felemmâ eḥassû be'senâ iẕâ hüm minhâ yerküḍûn.",
      "numberInSurah": 12,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2496,
      "text": "lâ terküḍû verci`û ilâ mâ ütriftüm fîhi vemesâkiniküm le`alleküm tüs'elûn.",
      "numberInSurah": 13,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2497,
      "text": "ḳâlû yâ veylenâ innâ künnâ żâlimîn.",
      "numberInSurah": 14,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2498,
      "text": "femâ zâlet tilke da`vâhüm ḥattâ ce`alnâhüm ḥaṣîden ḫâmidîn.",
      "numberInSurah": 15,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2499,
      "text": "vemâ ḫalaḳne-ssemâe vel'arḍa vemâ beynehümâ lâ`ibîn.",
      "numberInSurah": 16,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2500,
      "text": "lev eradnâ en netteḫiẕe lehvel letteḫaẕnâhü mil ledünnâ. in künnâ fâ`ilîn.",
      "numberInSurah": 17,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2501,
      "text": "bel naḳẕifü bilḥaḳḳi `ale-lbâṭili feyedmeguhû feiẕâ hüve zâhiḳ. velekümü-lveylü mimmâ teṣifûn.",
      "numberInSurah": 18,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2502,
      "text": "velehû men fi-ssemâvâti vel'arḍ. vemen `indehû lâ yestekbirûne `an `ibâdetihî velâ yestaḥsirûn.",
      "numberInSurah": 19,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2503,
      "text": "yüsebbiḥûne-lleyle vennehâra lâ yeftürûn.",
      "numberInSurah": 20,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2504,
      "text": "emi-tteḫaẕû âlihetem mine-l'arḍi hüm yünşirûn.",
      "numberInSurah": 21,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2505,
      "text": "lev kâne fîhimâ âlihetün ille-llâhü lefesedetâ. fesübḥâne-llâhi rabbi-l`arşi `ammâ yeṣifûn.",
      "numberInSurah": 22,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2506,
      "text": "lâ yüs'elü `ammâ yef`alü vehüm yüs'elûn.",
      "numberInSurah": 23,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2507,
      "text": "emi-tteḫaẕû min dûnihî âliheh. ḳul hâtû bürhâneküm. hâẕâ ẕikru mem me`iye veẕikru men ḳablî. bel ekŝeruhüm lâ ya`lemûne-lḥaḳḳa fehüm mü`riḍûn.",
      "numberInSurah": 24,
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2508,
      "text": "vemâ erselnâ min ḳablike mir rasûlin illâ nûḥî ileyhi ennehû lâ ilâhe illâ ene fa`büdûn.",
      "numberInSurah": 25,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2509,
      "text": "veḳâlü-tteḫaẕe-rraḥmânü veleden sübḥâneh. bel `ibâdüm mükramûn.",
      "numberInSurah": 26,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2510,
      "text": "lâ yesbiḳûnehû bilḳavli vehüm biemrihî ya`melûn.",
      "numberInSurah": 27,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2511,
      "text": "ya`lemü mâ beyne eydîhim vemâ ḫalfehüm velâ yeşfe`ûne illâ limeni-rteḍâ vehüm min ḫaşyetihî müşfiḳûn.",
      "numberInSurah": 28,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2512,
      "text": "vemey yeḳul minhüm innî ilâhüm min dûnihî feẕâlike neczîhi cehennem. keẕâlike neczi-żżâlimîn.",
      "numberInSurah": 29,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2513,
      "text": "evelem yera-lleẕîne keferû enne-ssemâvâti vel'arḍa kânetâ ratḳan fefetaḳnâhümâ. vece`alnâ mine-lmâi külle şey'in ḥayy. efelâ yü'minûn.",
      "numberInSurah": 30,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2514,
      "text": "vece`alnâ fi-l'arḍi ravâsiye en temîde bihim vece`alnâ fîhâ ficâcen sübülel le`allehüm yehtedûn.",
      "numberInSurah": 31,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2515,
      "text": "vece`alne-ssemâe saḳfem maḥfûżâ. vehüm `an âyâtihâ mü`riḍûn.",
      "numberInSurah": 32,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2516,
      "text": "vehüve-lleẕî ḫaleḳa-lleyle vennehâra veşşemse velḳamer. küllün fî felekiy yesbeḥûn.",
      "numberInSurah": 33,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2517,
      "text": "vemâ ce`alnâ libeşerim min ḳablike-lḫuld. efeim mitte fehümü-lḫâlidûn.",
      "numberInSurah": 34,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2518,
      "text": "küllü nefsin ẕâiḳatü-lmevt. veneblûküm bişşerri velḫayri fitneh. veileynâ türce`ûn.",
      "numberInSurah": 35,
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2519,
      "text": "veiẕâ raâke-lleẕîne keferû iy yetteḫiẕûneke illâ hüzüvâ. ehâẕe-lleẕî yeẕküru âliheteküm. vehüm biẕikri-rraḥmâni hüm kâfirûn.",
      "numberInSurah": 36,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2520,
      "text": "ḫuliḳa-l'insânü min `acel. seürîküm âyâtî felâ testa`cilûn.",
      "numberInSurah": 37,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2521,
      "text": "veyeḳûlûne metâ hâẕe-lva`dü in küntüm ṣâdiḳîn.",
      "numberInSurah": 38,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2522,
      "text": "lev ya`lemü-lleẕîne keferû ḥîne lâ yeküffûne `av vucûhihimü-nnâra velâ `an żuhûrihim velâ hüm yünṣarûn.",
      "numberInSurah": 39,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2523,
      "text": "bel te'tîhim bagteten fetebhetühüm felâ yesteṭî`ûne raddehâ velâ hüm yünżarûn.",
      "numberInSurah": 40,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2524,
      "text": "veleḳadi-stühzie birusülim min ḳablike feḥâḳa billeẕîne seḫirû minhüm mâ kânû bihî yestehziûn.",
      "numberInSurah": 41,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2525,
      "text": "ḳul mey yekleüküm billeyli vennehâri mine-rraḥmân. bel hüm `an ẕikri rabbihim mü`riḍûn.",
      "numberInSurah": 42,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2526,
      "text": "em lehüm âlihetün temne`uhüm min dûninâ. lâ yesteṭî`ûne naṣra enfüsihim velâ hüm minnâ yuṣḥabûn.",
      "numberInSurah": 43,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2527,
      "text": "bel metta`nâ hâülâi veâbâehüm ḥattâ ṭâle `aleyhimü-l`umür. efelâ yeravne ennâ ne'ti-l'arḍa nenḳuṣuhâ min aṭrâfihâ. efehümü-lgâlibûn.",
      "numberInSurah": 44,
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2528,
      "text": "ḳul innemâ ünẕiruküm bilvaḥy. velâ yesme`u-ṣṣummü-ddü`âe iẕâ mâ yünẕerûn.",
      "numberInSurah": 45,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2529,
      "text": "veleim messethüm nefḥatüm min `aẕâbi rabbike leyeḳûlünne yâ veylenâ innâ künnâ żâlimîn.",
      "numberInSurah": 46,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2530,
      "text": "veneḍa`u-lmevâzîne-lḳiṣṭa liyevmi-lḳiyâmeti felâ tużlemü nefsün şey'â. vein kâne miŝḳâle ḥabbetim min ḫardelin eteynâ bihâ. vekefâ binâ ḥâsibîn.",
      "numberInSurah": 47,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2531,
      "text": "veleḳad âteynâ mûsâ vehârûne-lfürḳâne veḍiyâev veẕikral lilmütteḳîn.",
      "numberInSurah": 48,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2532,
      "text": "elleẕîne yaḫşevne rabbehüm bilgaybi vehüm mine-ssâ`ati müşfiḳûn.",
      "numberInSurah": 49,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2533,
      "text": "vehâẕâ ẕikrum mübârakün enzelnâh. efeentüm lehû münkirûn.",
      "numberInSurah": 50,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 2534,
      "text": "veleḳad âteynâ ibrâhîme ruşdehû min ḳablü vekünnâ bihî `âlimîn.",
      "numberInSurah": 51,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2535,
      "text": "iẕ ḳâle liebîhi veḳavmihî mâ hâẕihi-ttemâŝîlü-lletî entüm lehâ `âkifûn.",
      "numberInSurah": 52,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2536,
      "text": "ḳâlû vecednâ âbâenâ lehâ `âbidîn.",
      "numberInSurah": 53,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2537,
      "text": "ḳâle leḳad küntüm entüm veâbâüküm fî ḍalâlim mübîn.",
      "numberInSurah": 54,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2538,
      "text": "ḳâlû eci'tenâ bilḥaḳḳi em ente mine-llâ`ibîn.",
      "numberInSurah": 55,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2539,
      "text": "ḳâle ber rabbüküm rabbü-ssemâvâti vel'arḍi-lleẕî feṭarahünn. veenâ `alâ ẕâliküm mine-şşâhidîn.",
      "numberInSurah": 56,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2540,
      "text": "vetellâhi leekîdenne aṣnâmeküm ba`de en tüvellû müdbirîn.",
      "numberInSurah": 57,
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2541,
      "text": "fece`alehüm cüŝeŝen illâ kebîral lehüm le`allehüm ileyhi yerci`ûn.",
      "numberInSurah": 58,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2542,
      "text": "ḳâlû men fe`ale hâẕâ biâlihetinâ innehû lemine-żżâlimîn.",
      "numberInSurah": 59,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2543,
      "text": "ḳâlû semi`nâ fetey yeẕküruhüm yüḳâlü lehû ibrâhîm.",
      "numberInSurah": 60,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2544,
      "text": "ḳâlû fe'tû bihî `alâ a`yüni-nnâsi le`allehüm yeşhedûn.",
      "numberInSurah": 61,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2545,
      "text": "ḳâlû eente fe`alte hâẕâ biâlihetinâ yâ ibrâhîm.",
      "numberInSurah": 62,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2546,
      "text": "ḳâle bel fe`aleh. kebîruhüm hâẕâ fes'elûhüm in kânû yenṭiḳûn.",
      "numberInSurah": 63,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2547,
      "text": "ferace`û ilâ enfüsihim feḳâlû inneküm entümu-żżâlimûn.",
      "numberInSurah": 64,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2548,
      "text": "ŝümme nükisû `alâ ruûsihim. leḳad `alimte mâ hâülâi yenṭiḳûn.",
      "numberInSurah": 65,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2549,
      "text": "ḳâle efeta`büdûne min dûni-llâhi mâ lâ yenfe`uküm şey'ev velâ yeḍurruküm.",
      "numberInSurah": 66,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2550,
      "text": "üffil leküm velimâ ta`büdûne min dûni-llâh. efelâ ta`ḳilûn.",
      "numberInSurah": 67,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2551,
      "text": "ḳâlû ḥarriḳûhü venṣurû âliheteküm in küntüm fâ`ilîn.",
      "numberInSurah": 68,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2552,
      "text": "ḳulnâ yâ nâru kûnî berdev veselâmen `alâ ibrâhîm.",
      "numberInSurah": 69,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2553,
      "text": "veerâdû bihî keyden fece`alnâhümü-l'aḫserîn.",
      "numberInSurah": 70,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2554,
      "text": "venecceynâhü velûṭan ile-l'arḍi-lletî bâraknâ fîhâ lil`âlemîn.",
      "numberInSurah": 71,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2555,
      "text": "vevehebnâ lehû isḥâḳ. veya`ḳûbe nâfileten. veküllen ce`alnâ ṣâliḥîn.",
      "numberInSurah": 72,
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2556,
      "text": "vece`alnâhüm eimmetey yehdûne biemrinâ veevḥaynâ ileyhim fi`le-lḫayrâti veiḳâme-ṣṣalâti veîtâe-zzekâh. vekânû lenâ `âbidîn.",
      "numberInSurah": 73,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2557,
      "text": "velûṭan âteynâhü ḥukmev ve`ilmev venecceynâhü mine-lḳaryeti-lletî kânet ta`melü-lḫabâiŝ. innehüm kânû ḳavme sev'in fâsiḳîn.",
      "numberInSurah": 74,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2558,
      "text": "veedḫalnâhü fî raḥmetinâ. innehû mine-ṣṣâliḥîn.",
      "numberInSurah": 75,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2559,
      "text": "venûḥan iẕ nâdâ min ḳablü festecebnâ lehû fenecceynâhü veehlehû mine-lkerbi-l`ażîm.",
      "numberInSurah": 76,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2560,
      "text": "veneṣarnâhü mine-lḳavmi-lleẕîne keẕẕebû biâyâtinâ. innehüm kânû ḳavme sev'in feagraḳnâhüm ecme`în.",
      "numberInSurah": 77,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2561,
      "text": "vedâvûde vesüleymâne iẕ yaḥkümâni fi-lḥarŝi iẕ nefeşet fîhi ganemü-lḳavm. vekünnâ liḥukmihim şâhidîn.",
      "numberInSurah": 78,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2562,
      "text": "fefehhemnâhâ süleymân. veküllen âteynâ ḥukmev ve`ilmâ. veseḫḫarnâ me`a dâvûde-lcibâle yüsebbiḥne veṭṭayr. vekünnâ fâ`ilîn.",
      "numberInSurah": 79,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2563,
      "text": "ve`allemnâhü ṣan`ate lebûsil leküm lituḥṣineküm mim be'siküm. fehel entüm şâkirûn.",
      "numberInSurah": 80,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2564,
      "text": "velisüleymâne-rrîḥa `âṣifeten tecrî biemrih ile-l'arḍi-lletî bâraknâ fîhâ. vekünnâ bikülli şey'in `âlimîn.",
      "numberInSurah": 81,
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2565,
      "text": "vemine-şşeyâṭîni mey yegûṣûne lehû veya`melûne `amelen dûne ẕâlik. vekünnâ lehüm ḥâfiżîn.",
      "numberInSurah": 82,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 2566,
      "text": "veeyyûbe iẕ nâdâ rabbehû ennî messeniye-ḍḍurru veente erḥamü-rrâḥimîn.",
      "numberInSurah": 83,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2567,
      "text": "festecebnâ lehû fekeşefnâ mâ bihî min ḍurriv veâteynâhü ehlehû vemiŝlehüm me`ahüm raḥmetem min `indinâ veẕikrâ lil`âbidîn.",
      "numberInSurah": 84,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2568,
      "text": "veismâ`île veidrîse veẕe-lkifl. küllüm mine-ṣṣâbirîn.",
      "numberInSurah": 85,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2569,
      "text": "veedḫalnâhüm fî raḥmetinâ. innehüm mine-ṣṣâliḥîn.",
      "numberInSurah": 86,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2570,
      "text": "veẕe-nnûni iẕ ẕehebe mügâḍiben feżanne el len naḳdira `aleyhi fenâdâ fi-żżulümâti el lâ ilâhe illâ ente sübḥânek. innî küntü mine-żżâlimîn.",
      "numberInSurah": 87,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2571,
      "text": "festecebnâ lehû venecceynâhü mine-lgamm. vekeẕâlike nünci-lmü'minîn.",
      "numberInSurah": 88,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2572,
      "text": "vezekeriyyâ iẕ nâdâ rabbehû rabbi lâ teẕernî ferdev veente ḫayru-lvâriŝîn.",
      "numberInSurah": 89,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2573,
      "text": "festecebnâ leh. vevehebnâ lehû yaḥyâ veaṣlaḥnâ lehû zevceh. innehüm kânû yüsâri`ûne fi-lḫayrâti veyed`ûnenâ ragabev verahebâ. vekânû lenâ ḫâşi`în.",
      "numberInSurah": 90,
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2574,
      "text": "velletî aḥṣanet fercehâ fenefaḫnâ fîhâ mir rûḥinâ vece`alnâhâ vebnehâ âyetel lil`âlemîn.",
      "numberInSurah": 91,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2575,
      "text": "inne hâẕihî ümmetüküm ümmetev vâḥideh. veenâ rabbüküm fa`büdûn.",
      "numberInSurah": 92,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2576,
      "text": "veteḳaṭṭa`û emrahüm beynehüm. küllün ileynâ râci`ûn.",
      "numberInSurah": 93,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2577,
      "text": "femey ya`mel mine-ṣṣâliḥâti vehüve mü'minün felâ küfrâne lisa`yih. veinnâ lehû kâtibûn.",
      "numberInSurah": 94,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2578,
      "text": "veḥarâmün `alâ ḳaryetin ehleknâhâ ennehüm lâ yerci`ûn.",
      "numberInSurah": 95,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2579,
      "text": "ḥattâ iẕâ fütiḥat ye'cûcü veme'cûcü vehüm min külli ḥadebiy yensilûn.",
      "numberInSurah": 96,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2580,
      "text": "vaḳterabe-lva`dü-lḥaḳḳu feiẕâ hiye şâḫiṣatün ebṣâru-lleẕîne keferû. yâ veylenâ ḳad künnâ fî gafletim min hâẕâ bel künnâ żâlimîn.",
      "numberInSurah": 97,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2581,
      "text": "inneküm vemâ ta`büdûne min dûni-llâhi ḥaṣabü cehennem. entüm lehâ vâridûn.",
      "numberInSurah": 98,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2582,
      "text": "lev kâne hâülâi âlihetem mâ veradûhâ. veküllün fîhâ ḫâlidûn.",
      "numberInSurah": 99,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2583,
      "text": "lehüm fîhâ zefîruv vehüm fîhâ lâ yesme`ûn.",
      "numberInSurah": 100,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2584,
      "text": "inne-lleẕîne sebeḳat lehüm minne-lḥusnâ ülâike `anhâ müb`adûn.",
      "numberInSurah": 101,
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2585,
      "text": "lâ yesme`ûne ḥasîsehâ. vehüm fî me-ştehet enfüsühüm ḫâlidûn.",
      "numberInSurah": 102,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2586,
      "text": "lâ yaḥzünühümü-lfeza`u-l'ekberu veteteleḳḳâhümü-lmelâikeh. hâẕâ yevmükümü-lleẕî küntüm tû`adûn.",
      "numberInSurah": 103,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2587,
      "text": "yevme naṭvi-ssemâe keṭayyi-ssicilli lilkütüb. kemâ bede'nâ evvele ḫalḳin nü`îdüh. va`den `aleynâ. innâ künnâ fâ`ilîn.",
      "numberInSurah": 104,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2588,
      "text": "veleḳad ketebnâ fi-zzebûri mim ba`di-ẕẕikri enne-l'arḍa yeriŝühâ `ibâdiye-ṣṣâliḥûn.",
      "numberInSurah": 105,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2589,
      "text": "inne fî hâẕâ lebelâgal liḳavmin `âbidîn.",
      "numberInSurah": 106,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2590,
      "text": "vemâ erselnâke illâ raḥmetel lil`âlemîn.",
      "numberInSurah": 107,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2591,
      "text": "ḳul innemâ yûḥâ ileyye ennemâ ilâhüküm ilâhüv vâḥid. fehel entüm müslimûn.",
      "numberInSurah": 108,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2592,
      "text": "fein tevellev feḳul ehentüküm `alâ sevâ'. vein edrî eḳarîbün em be`îdüm mâ tû`adûn.",
      "numberInSurah": 109,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2593,
      "text": "innehû ya`lemü-lcehra mine-lḳavli veya`lemü mâ tektümûn.",
      "numberInSurah": 110,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2594,
      "text": "vein edrî le`allehû fitnetül leküm vemetâ`un ilâ ḥîn.",
      "numberInSurah": 111,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 2595,
      "text": "ḳâle rabbi-ḥküm bilḥaḳḳ. verabbüne-rraḥmânü-lmüste`ânü `alâ mâ teṣifûn.",
      "numberInSurah": 112,
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    }
  ],
