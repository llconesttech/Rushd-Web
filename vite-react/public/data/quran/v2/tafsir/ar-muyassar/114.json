{
  "number": 114,
  "name": "سُورَةُ النَّاسِ",
  "englishName": "An-Naas",
  "ayahs": [
    {
      "number": 1,
      "text": "قل -أيها الرسول-: أعوذ وأعتصم برب الناس، القادر وحده على ردِّ شر الوسواس.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ملك الناس المتصرف في كل شؤونهم، الغنيِّ عنهم.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إله الناس الذي لا معبود بحق سواه.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "من أذى الشيطان الذي يوسوس عند الغفلة، ويختفي عند ذكر الله.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "الذي يبثُّ الشر والشكوك في صدور الناس.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "من شياطين الجن والإنس.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}