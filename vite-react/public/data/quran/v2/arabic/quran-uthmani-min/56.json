{
  "number": 56,
  "name": "سُورَةُ الوَاقِعَةِ",
  "englishName": "Al-Waaqia",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ إِذا وَقَعَتِ الواقِعَةُ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 2,
      "text": "لَيسَ لِوَقعَتِها كاذِبَةٌ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 3,
      "text": "خافِضَةٌ رافِعَةٌ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِذا رُجَّتِ الأَرضُ رَجًّا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَبُسَّتِ الجِبالُ بَسًّا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَكانَت هَباءً مُنبَثًّا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَكُنتُم أَزوٰجًا ثَلٰثَةً",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَأَصحٰبُ المَيمَنَةِ ما أَصحٰبُ المَيمَنَةِ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَأَصحٰبُ المَشـَٔمَةِ ما أَصحٰبُ المَشـَٔمَةِ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَالسّٰبِقونَ السّٰبِقونَ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 11,
      "text": "أُولٰئِكَ المُقَرَّبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فى جَنّٰتِ النَّعيمِ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُلَّةٌ مِنَ الأَوَّلينَ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَقَليلٌ مِنَ الءاخِرينَ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 15,
      "text": "عَلىٰ سُرُرٍ مَوضونَةٍ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 16,
      "text": "مُتَّكِـٔينَ عَلَيها مُتَقٰبِلينَ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 17,
      "text": "يَطوفُ عَلَيهِم وِلدٰنٌ مُخَلَّدونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 18,
      "text": "بِأَكوابٍ وَأَباريقَ وَكَأسٍ مِن مَعينٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 19,
      "text": "لا يُصَدَّعونَ عَنها وَلا يُنزِفونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَفٰكِهَةٍ مِمّا يَتَخَيَّرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَلَحمِ طَيرٍ مِمّا يَشتَهونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَحورٌ عينٌ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 23,
      "text": "كَأَمثٰلِ اللُّؤلُؤِ المَكنونِ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 24,
      "text": "جَزاءً بِما كانوا يَعمَلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 25,
      "text": "لا يَسمَعونَ فيها لَغوًا وَلا تَأثيمًا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 26,
      "text": "إِلّا قيلًا سَلٰمًا سَلٰمًا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَأَصحٰبُ اليَمينِ ما أَصحٰبُ اليَمينِ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 28,
      "text": "فى سِدرٍ مَخضودٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَطَلحٍ مَنضودٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَظِلٍّ مَمدودٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وَماءٍ مَسكوبٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَفٰكِهَةٍ كَثيرَةٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 33,
      "text": "لا مَقطوعَةٍ وَلا مَمنوعَةٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَفُرُشٍ مَرفوعَةٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 35,
      "text": "إِنّا أَنشَأنٰهُنَّ إِنشاءً",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَجَعَلنٰهُنَّ أَبكارًا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 37,
      "text": "عُرُبًا أَترابًا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 38,
      "text": "لِأَصحٰبِ اليَمينِ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ثُلَّةٌ مِنَ الأَوَّلينَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَثُلَّةٌ مِنَ الءاخِرينَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَأَصحٰبُ الشِّمالِ ما أَصحٰبُ الشِّمالِ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فى سَمومٍ وَحَميمٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وَظِلٍّ مِن يَحمومٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 44,
      "text": "لا بارِدٍ وَلا كَريمٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 45,
      "text": "إِنَّهُم كانوا قَبلَ ذٰلِكَ مُترَفينَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 46,
      "text": "وَكانوا يُصِرّونَ عَلَى الحِنثِ العَظيمِ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَكانوا يَقولونَ أَئِذا مِتنا وَكُنّا تُرابًا وَعِظٰمًا أَءِنّا لَمَبعوثونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 48,
      "text": "أَوَءاباؤُنَا الأَوَّلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 49,
      "text": "قُل إِنَّ الأَوَّلينَ وَالءاخِرينَ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 50,
      "text": "لَمَجموعونَ إِلىٰ ميقٰتِ يَومٍ مَعلومٍ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ثُمَّ إِنَّكُم أَيُّهَا الضّالّونَ المُكَذِّبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 52,
      "text": "لَءاكِلونَ مِن شَجَرٍ مِن زَقّومٍ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فَمالِـٔونَ مِنهَا البُطونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فَشٰرِبونَ عَلَيهِ مِنَ الحَميمِ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فَشٰرِبونَ شُربَ الهيمِ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 56,
      "text": "هٰذا نُزُلُهُم يَومَ الدّينِ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 57,
      "text": "نَحنُ خَلَقنٰكُم فَلَولا تُصَدِّقونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 58,
      "text": "أَفَرَءَيتُم ما تُمنونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 59,
      "text": "ءَأَنتُم تَخلُقونَهُ أَم نَحنُ الخٰلِقونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 60,
      "text": "نَحنُ قَدَّرنا بَينَكُمُ المَوتَ وَما نَحنُ بِمَسبوقينَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 61,
      "text": "عَلىٰ أَن نُبَدِّلَ أَمثٰلَكُم وَنُنشِئَكُم فى ما لا تَعلَمونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 62,
      "text": "وَلَقَد عَلِمتُمُ النَّشأَةَ الأولىٰ فَلَولا تَذَكَّرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 63,
      "text": "أَفَرَءَيتُم ما تَحرُثونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 64,
      "text": "ءَأَنتُم تَزرَعونَهُ أَم نَحنُ الزّٰرِعونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 65,
      "text": "لَو نَشاءُ لَجَعَلنٰهُ حُطٰمًا فَظَلتُم تَفَكَّهونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 66,
      "text": "إِنّا لَمُغرَمونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 67,
      "text": "بَل نَحنُ مَحرومونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 68,
      "text": "أَفَرَءَيتُمُ الماءَ الَّذى تَشرَبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 69,
      "text": "ءَأَنتُم أَنزَلتُموهُ مِنَ المُزنِ أَم نَحنُ المُنزِلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 70,
      "text": "لَو نَشاءُ جَعَلنٰهُ أُجاجًا فَلَولا تَشكُرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 71,
      "text": "أَفَرَءَيتُمُ النّارَ الَّتى تورونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 72,
      "text": "ءَأَنتُم أَنشَأتُم شَجَرَتَها أَم نَحنُ المُنشِـٔونَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 73,
      "text": "نَحنُ جَعَلنٰها تَذكِرَةً وَمَتٰعًا لِلمُقوينَ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 74,
      "text": "فَسَبِّح بِاسمِ رَبِّكَ العَظيمِ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 75,
      "text": "۞ فَلا أُقسِمُ بِمَوٰقِعِ النُّجومِ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وَإِنَّهُ لَقَسَمٌ لَو تَعلَمونَ عَظيمٌ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 77,
      "text": "إِنَّهُ لَقُرءانٌ كَريمٌ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 78,
      "text": "فى كِتٰبٍ مَكنونٍ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 79,
      "text": "لا يَمَسُّهُ إِلَّا المُطَهَّرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 80,
      "text": "تَنزيلٌ مِن رَبِّ العٰلَمينَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 81,
      "text": "أَفَبِهٰذَا الحَديثِ أَنتُم مُدهِنونَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 82,
      "text": "وَتَجعَلونَ رِزقَكُم أَنَّكُم تُكَذِّبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 83,
      "text": "فَلَولا إِذا بَلَغَتِ الحُلقومَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 84,
      "text": "وَأَنتُم حينَئِذٍ تَنظُرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وَنَحنُ أَقرَبُ إِلَيهِ مِنكُم وَلٰكِن لا تُبصِرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 86,
      "text": "فَلَولا إِن كُنتُم غَيرَ مَدينينَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 87,
      "text": "تَرجِعونَها إِن كُنتُم صٰدِقينَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 88,
      "text": "فَأَمّا إِن كانَ مِنَ المُقَرَّبينَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 89,
      "text": "فَرَوحٌ وَرَيحانٌ وَجَنَّتُ نَعيمٍ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 90,
      "text": "وَأَمّا إِن كانَ مِن أَصحٰبِ اليَمينِ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 91,
      "text": "فَسَلٰمٌ لَكَ مِن أَصحٰبِ اليَمينِ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 92,
      "text": "وَأَمّا إِن كانَ مِنَ المُكَذِّبينَ الضّالّينَ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 93,
      "text": "فَنُزُلٌ مِن حَميمٍ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 94,
      "text": "وَتَصلِيَةُ جَحيمٍ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 95,
      "text": "إِنَّ هٰذا لَهُوَ حَقُّ اليَقينِ",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 96,
      "text": "فَسَبِّح بِاسمِ رَبِّكَ العَظيمِ",
