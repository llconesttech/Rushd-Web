{
  "number": 81,
  "name": "سُورَةُ التَّكۡوِيرِ",
  "englishName": "At-Takwir",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ إِذَا الشَّمسُ كُوِّرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَإِذَا النُّجومُ انكَدَرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَإِذَا الجِبالُ سُيِّرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَإِذَا العِشارُ عُطِّلَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَإِذَا الوُحوشُ حُشِرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَإِذَا البِحارُ سُجِّرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَإِذَا النُّفوسُ زُوِّجَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَإِذَا المَوءۥدَةُ سُئِلَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 9,
      "text": "بِأَىِّ ذَنبٍ قُتِلَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَإِذَا الصُّحُفُ نُشِرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَإِذَا السَّماءُ كُشِطَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَإِذَا الجَحيمُ سُعِّرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَإِذَا الجَنَّةُ أُزلِفَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 14,
      "text": "عَلِمَت نَفسٌ ما أَحضَرَت",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَلا أُقسِمُ بِالخُنَّسِ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 16,
      "text": "الجَوارِ الكُنَّسِ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَالَّيلِ إِذا عَسعَسَ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَالصُّبحِ إِذا تَنَفَّسَ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 19,
      "text": "إِنَّهُ لَقَولُ رَسولٍ كَريمٍ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 20,
      "text": "ذى قُوَّةٍ عِندَ ذِى العَرشِ مَكينٍ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 21,
      "text": "مُطاعٍ ثَمَّ أَمينٍ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَما صاحِبُكُم بِمَجنونٍ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَلَقَد رَءاهُ بِالأُفُقِ المُبينِ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَما هُوَ عَلَى الغَيبِ بِضَنينٍ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَما هُوَ بِقَولِ شَيطٰنٍ رَجيمٍ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَأَينَ تَذهَبونَ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إِن هُوَ إِلّا ذِكرٌ لِلعٰلَمينَ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 28,
      "text": "لِمَن شاءَ مِنكُم أَن يَستَقيمَ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَما تَشاءونَ إِلّا أَن يَشاءَ اللَّهُ رَبُّ العٰلَمينَ",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    }
  ]
}