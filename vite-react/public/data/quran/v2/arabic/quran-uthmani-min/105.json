{
  "number": 105,
  "name": "سُورَةُ الفِيلِ",
  "englishName": "Al-Fil",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ أَلَم تَرَ كَيفَ فَعَلَ رَبُّكَ بِأَصحٰبِ الفيلِ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "أَلَم يَجعَل كَيدَهُم فى تَضليلٍ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَأَرسَلَ عَلَيهِم طَيرًا أَبابيلَ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَرميهِم بِحِجارَةٍ مِن سِجّيلٍ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَجَعَلَهُم كَعَصفٍ مَأكولٍ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}