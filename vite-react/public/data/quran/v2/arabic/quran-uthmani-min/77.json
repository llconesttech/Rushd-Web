{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالمُرسَلٰتِ عُرفًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالعٰصِفٰتِ عَصفًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالنّٰشِرٰتِ نَشرًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَالفٰرِقٰتِ فَرقًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَالمُلقِيٰتِ ذِكرًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 6,
      "text": "عُذرًا أَو نُذرًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِنَّما توعَدونَ لَوٰقِعٌ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَإِذَا النُّجومُ طُمِسَت",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَإِذَا السَّماءُ فُرِجَت",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَإِذَا الجِبالُ نُسِفَت",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَإِذَا الرُّسُلُ أُقِّتَت",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 12,
      "text": "لِأَىِّ يَومٍ أُجِّلَت",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 13,
      "text": "لِيَومِ الفَصلِ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَما أَدرىٰكَ ما يَومُ الفَصلِ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 16,
      "text": "أَلَم نُهلِكِ الأَوَّلينَ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ثُمَّ نُتبِعُهُمُ الءاخِرينَ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 18,
      "text": "كَذٰلِكَ نَفعَلُ بِالمُجرِمينَ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 20,
      "text": "أَلَم نَخلُقكُم مِن ماءٍ مَهينٍ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَجَعَلنٰهُ فى قَرارٍ مَكينٍ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 22,
      "text": "إِلىٰ قَدَرٍ مَعلومٍ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَقَدَرنا فَنِعمَ القٰدِرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 25,
      "text": "أَلَم نَجعَلِ الأَرضَ كِفاتًا",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 26,
      "text": "أَحياءً وَأَموٰتًا",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَجَعَلنا فيها رَوٰسِىَ شٰمِخٰتٍ وَأَسقَينٰكُم ماءً فُراتًا",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 29,
      "text": "انطَلِقوا إِلىٰ ما كُنتُم بِهِ تُكَذِّبونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 30,
      "text": "انطَلِقوا إِلىٰ ظِلٍّ ذى ثَلٰثِ شُعَبٍ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 31,
      "text": "لا ظَليلٍ وَلا يُغنى مِنَ اللَّهَبِ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 32,
      "text": "إِنَّها تَرمى بِشَرَرٍ كَالقَصرِ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَأَنَّهُ جِمٰلَتٌ صُفرٌ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 35,
      "text": "هٰذا يَومُ لا يَنطِقونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَلا يُؤذَنُ لَهُم فَيَعتَذِرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 38,
      "text": "هٰذا يَومُ الفَصلِ ۖ جَمَعنٰكُم وَالأَوَّلينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَإِن كانَ لَكُم كَيدٌ فَكيدونِ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 41,
      "text": "إِنَّ المُتَّقينَ فى ظِلٰلٍ وَعُيونٍ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وَفَوٰكِهَ مِمّا يَشتَهونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 43,
      "text": "كُلوا وَاشرَبوا هَنيـًٔا بِما كُنتُم تَعمَلونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 44,
      "text": "إِنّا كَذٰلِكَ نَجزِى المُحسِنينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كُلوا وَتَمَتَّعوا قَليلًا إِنَّكُم مُجرِمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَإِذا قيلَ لَهُمُ اركَعوا لا يَركَعونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَبِأَىِّ حَديثٍ بَعدَهُ يُؤمِنونَ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    }
  ]
}