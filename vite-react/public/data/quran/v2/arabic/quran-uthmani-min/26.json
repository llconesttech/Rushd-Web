      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 145,
      "text": "وَما أَسـَٔلُكُم عَلَيهِ مِن أَجرٍ ۖ إِن أَجرِىَ إِلّا عَلىٰ رَبِّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 146,
      "text": "أَتُترَكونَ فى ما هٰهُنا ءامِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 147,
      "text": "فى جَنّٰتٍ وَعُيونٍ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 148,
      "text": "وَزُروعٍ وَنَخلٍ طَلعُها هَضيمٌ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 149,
      "text": "وَتَنحِتونَ مِنَ الجِبالِ بُيوتًا فٰرِهينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 150,
      "text": "فَاتَّقُوا اللَّهَ وَأَطيعونِ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 151,
      "text": "وَلا تُطيعوا أَمرَ المُسرِفينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 152,
      "text": "الَّذينَ يُفسِدونَ فِى الأَرضِ وَلا يُصلِحونَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 153,
      "text": "قالوا إِنَّما أَنتَ مِنَ المُسَحَّرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 154,
      "text": "ما أَنتَ إِلّا بَشَرٌ مِثلُنا فَأتِ بِـٔايَةٍ إِن كُنتَ مِنَ الصّٰدِقينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 155,
      "text": "قالَ هٰذِهِ ناقَةٌ لَها شِربٌ وَلَكُم شِربُ يَومٍ مَعلومٍ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 156,
