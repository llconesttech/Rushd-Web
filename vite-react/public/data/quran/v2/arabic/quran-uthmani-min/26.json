    {
      "number": 196,
      "text": "وَإِنَّهُ لَفى زُبُرِ الأَوَّلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 197,
      "text": "أَوَلَم يَكُن لَهُم ءايَةً أَن يَعلَمَهُ عُلَمٰؤُا۟ بَنى إِسرٰءيلَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 198,
      "text": "وَلَو نَزَّلنٰهُ عَلىٰ بَعضِ الأَعجَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 199,
      "text": "فَقَرَأَهُ عَلَيهِم ما كانوا بِهِ مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 200,
      "text": "كَذٰلِكَ سَلَكنٰهُ فى قُلوبِ المُجرِمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 201,
      "text": "لا يُؤمِنونَ بِهِ حَتّىٰ يَرَوُا العَذابَ الأَليمَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 202,
      "text": "فَيَأتِيَهُم بَغتَةً وَهُم لا يَشعُرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 203,
      "text": "فَيَقولوا هَل نَحنُ مُنظَرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 204,
      "text": "أَفَبِعَذابِنا يَستَعجِلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 205,
      "text": "أَفَرَءَيتَ إِن مَتَّعنٰهُم سِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 206,
      "text": "ثُمَّ جاءَهُم ما كانوا يوعَدونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 207,
      "text": "ما أَغنىٰ عَنهُم ما كانوا يُمَتَّعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 208,
      "text": "وَما أَهلَكنا مِن قَريَةٍ إِلّا لَها مُنذِرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 209,
      "text": "ذِكرىٰ وَما كُنّا ظٰلِمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 210,
      "text": "وَما تَنَزَّلَت بِهِ الشَّيٰطينُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 211,
      "text": "وَما يَنبَغى لَهُم وَما يَستَطيعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 212,
      "text": "إِنَّهُم عَنِ السَّمعِ لَمَعزولونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 213,
      "text": "فَلا تَدعُ مَعَ اللَّهِ إِلٰهًا ءاخَرَ فَتَكونَ مِنَ المُعَذَّبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 214,
      "text": "وَأَنذِر عَشيرَتَكَ الأَقرَبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 215,
      "text": "وَاخفِض جَناحَكَ لِمَنِ اتَّبَعَكَ مِنَ المُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 216,
      "text": "فَإِن عَصَوكَ فَقُل إِنّى بَريءٌ مِمّا تَعمَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 217,
      "text": "وَتَوَكَّل عَلَى العَزيزِ الرَّحيمِ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 218,
      "text": "الَّذى يَرىٰكَ حينَ تَقومُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 219,
      "text": "وَتَقَلُّبَكَ فِى السّٰجِدينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 220,
      "text": "إِنَّهُ هُوَ السَّميعُ العَليمُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 221,
      "text": "هَل أُنَبِّئُكُم عَلىٰ مَن تَنَزَّلُ الشَّيٰطينُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 222,
      "text": "تَنَزَّلُ عَلىٰ كُلِّ أَفّاكٍ أَثيمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 223,
      "text": "يُلقونَ السَّمعَ وَأَكثَرُهُم كٰذِبونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 224,
      "text": "وَالشُّعَراءُ يَتَّبِعُهُمُ الغاوۥنَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 225,
      "text": "أَلَم تَرَ أَنَّهُم فى كُلِّ وادٍ يَهيمونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 226,
      "text": "وَأَنَّهُم يَقولونَ ما لا يَفعَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 227,
      "text": "إِلَّا الَّذينَ ءامَنوا وَعَمِلُوا الصّٰلِحٰتِ وَذَكَرُوا اللَّهَ كَثيرًا وَانتَصَروا مِن بَعدِ ما ظُلِموا ۗ وَسَيَعلَمُ الَّذينَ ظَلَموا أَىَّ مُنقَلَبٍ يَنقَلِبونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    }
  ]
}