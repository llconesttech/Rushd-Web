{
  "number": 26,
  "name": "سُورَةُ الشُّعَرَاءِ",
  "englishName": "Ash-Shu'araa",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ طسم",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 2,
      "text": "تِلكَ ءايٰتُ الكِتٰبِ المُبينِ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لَعَلَّكَ بٰخِعٌ نَفسَكَ أَلّا يَكونوا مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِن نَشَأ نُنَزِّل عَلَيهِم مِنَ السَّماءِ ءايَةً فَظَلَّت أَعنٰقُهُم لَها خٰضِعينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَما يَأتيهِم مِن ذِكرٍ مِنَ الرَّحمٰنِ مُحدَثٍ إِلّا كانوا عَنهُ مُعرِضينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَقَد كَذَّبوا فَسَيَأتيهِم أَنبٰؤُا۟ ما كانوا بِهِ يَستَهزِءونَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 7,
      "text": "أَوَلَم يَرَوا إِلَى الأَرضِ كَم أَنبَتنا فيها مِن كُلِّ زَوجٍ كَريمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّ فى ذٰلِكَ لَءايَةً ۖ وَما كانَ أَكثَرُهُم مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ العَزيزُ الرَّحيمُ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَإِذ نادىٰ رَبُّكَ موسىٰ أَنِ ائتِ القَومَ الظّٰلِمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 11,
      "text": "قَومَ فِرعَونَ ۚ أَلا يَتَّقونَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 12,
      "text": "قالَ رَبِّ إِنّى أَخافُ أَن يُكَذِّبونِ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَيَضيقُ صَدرى وَلا يَنطَلِقُ لِسانى فَأَرسِل إِلىٰ هٰرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَلَهُم عَلَىَّ ذَنبٌ فَأَخافُ أَن يَقتُلونِ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 15,
      "text": "قالَ كَلّا ۖ فَاذهَبا بِـٔايٰتِنا ۖ إِنّا مَعَكُم مُستَمِعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَأتِيا فِرعَونَ فَقولا إِنّا رَسولُ رَبِّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 17,
      "text": "أَن أَرسِل مَعَنا بَنى إِسرٰءيلَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قالَ أَلَم نُرَبِّكَ فينا وَليدًا وَلَبِثتَ فينا مِن عُمُرِكَ سِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَفَعَلتَ فَعلَتَكَ الَّتى فَعَلتَ وَأَنتَ مِنَ الكٰفِرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 20,
      "text": "قالَ فَعَلتُها إِذًا وَأَنا۠ مِنَ الضّالّينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَفَرَرتُ مِنكُم لَمّا خِفتُكُم فَوَهَبَ لى رَبّى حُكمًا وَجَعَلَنى مِنَ المُرسَلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَتِلكَ نِعمَةٌ تَمُنُّها عَلَىَّ أَن عَبَّدتَ بَنى إِسرٰءيلَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 23,
      "text": "قالَ فِرعَونُ وَما رَبُّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 24,
      "text": "قالَ رَبُّ السَّمٰوٰتِ وَالأَرضِ وَما بَينَهُما ۖ إِن كُنتُم موقِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 25,
      "text": "قالَ لِمَن حَولَهُ أَلا تَستَمِعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قالَ رَبُّكُم وَرَبُّ ءابائِكُمُ الأَوَّلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 27,
      "text": "قالَ إِنَّ رَسولَكُمُ الَّذى أُرسِلَ إِلَيكُم لَمَجنونٌ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قالَ رَبُّ المَشرِقِ وَالمَغرِبِ وَما بَينَهُما ۖ إِن كُنتُم تَعقِلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قالَ لَئِنِ اتَّخَذتَ إِلٰهًا غَيرى لَأَجعَلَنَّكَ مِنَ المَسجونينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 30,
      "text": "قالَ أَوَلَو جِئتُكَ بِشَيءٍ مُبينٍ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قالَ فَأتِ بِهِ إِن كُنتَ مِنَ الصّٰدِقينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فَأَلقىٰ عَصاهُ فَإِذا هِىَ ثُعبانٌ مُبينٌ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَنَزَعَ يَدَهُ فَإِذا هِىَ بَيضاءُ لِلنّٰظِرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 34,
      "text": "قالَ لِلمَلَإِ حَولَهُ إِنَّ هٰذا لَسٰحِرٌ عَليمٌ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 35,
      "text": "يُريدُ أَن يُخرِجَكُم مِن أَرضِكُم بِسِحرِهِ فَماذا تَأمُرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 36,
      "text": "قالوا أَرجِه وَأَخاهُ وَابعَث فِى المَدائِنِ حٰشِرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 37,
      "text": "يَأتوكَ بِكُلِّ سَحّارٍ عَليمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 38,
      "text": "فَجُمِعَ السَّحَرَةُ لِميقٰتِ يَومٍ مَعلومٍ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَقيلَ لِلنّاسِ هَل أَنتُم مُجتَمِعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 40,
      "text": "لَعَلَّنا نَتَّبِعُ السَّحَرَةَ إِن كانوا هُمُ الغٰلِبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فَلَمّا جاءَ السَّحَرَةُ قالوا لِفِرعَونَ أَئِنَّ لَنا لَأَجرًا إِن كُنّا نَحنُ الغٰلِبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 42,
      "text": "قالَ نَعَم وَإِنَّكُم إِذًا لَمِنَ المُقَرَّبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 43,
      "text": "قالَ لَهُم موسىٰ أَلقوا ما أَنتُم مُلقونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَأَلقَوا حِبالَهُم وَعِصِيَّهُم وَقالوا بِعِزَّةِ فِرعَونَ إِنّا لَنَحنُ الغٰلِبونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 45,
      "text": "فَأَلقىٰ موسىٰ عَصاهُ فَإِذا هِىَ تَلقَفُ ما يَأفِكونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 46,
      "text": "فَأُلقِىَ السَّحَرَةُ سٰجِدينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 47,
      "text": "قالوا ءامَنّا بِرَبِّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 48,
      "text": "رَبِّ موسىٰ وَهٰرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 49,
      "text": "قالَ ءامَنتُم لَهُ قَبلَ أَن ءاذَنَ لَكُم ۖ إِنَّهُ لَكَبيرُكُمُ الَّذى عَلَّمَكُمُ السِّحرَ فَلَسَوفَ تَعلَمونَ ۚ لَأُقَطِّعَنَّ أَيدِيَكُم وَأَرجُلَكُم مِن خِلٰفٍ وَلَأُصَلِّبَنَّكُم أَجمَعينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 50,
      "text": "قالوا لا ضَيرَ ۖ إِنّا إِلىٰ رَبِّنا مُنقَلِبونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 51,
      "text": "إِنّا نَطمَعُ أَن يَغفِرَ لَنا رَبُّنا خَطٰيٰنا أَن كُنّا أَوَّلَ المُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 52,
      "text": "۞ وَأَوحَينا إِلىٰ موسىٰ أَن أَسرِ بِعِبادى إِنَّكُم مُتَّبَعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فَأَرسَلَ فِرعَونُ فِى المَدائِنِ حٰشِرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 54,
      "text": "إِنَّ هٰؤُلاءِ لَشِرذِمَةٌ قَليلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 55,
      "text": "وَإِنَّهُم لَنا لَغائِظونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 56,
      "text": "وَإِنّا لَجَميعٌ حٰذِرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 57,
      "text": "فَأَخرَجنٰهُم مِن جَنّٰتٍ وَعُيونٍ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 58,
      "text": "وَكُنوزٍ وَمَقامٍ كَريمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 59,
      "text": "كَذٰلِكَ وَأَورَثنٰها بَنى إِسرٰءيلَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 60,
      "text": "فَأَتبَعوهُم مُشرِقينَ",
      "juz": 19,
      "manzil": 5,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 61,
      "text": "فَلَمّا تَرٰءَا الجَمعانِ قالَ أَصحٰبُ موسىٰ إِنّا لَمُدرَكونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 62,
      "text": "قالَ كَلّا ۖ إِنَّ مَعِىَ رَبّى سَيَهدينِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 63,
      "text": "فَأَوحَينا إِلىٰ موسىٰ أَنِ اضرِب بِعَصاكَ البَحرَ ۖ فَانفَلَقَ فَكانَ كُلُّ فِرقٍ كَالطَّودِ العَظيمِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 64,
      "text": "وَأَزلَفنا ثَمَّ الءاخَرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 65,
      "text": "وَأَنجَينا موسىٰ وَمَن مَعَهُ أَجمَعينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 66,
      "text": "ثُمَّ أَغرَقنَا الءاخَرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 67,
      "text": "إِنَّ فى ذٰلِكَ لَءايَةً ۖ وَما كانَ أَكثَرُهُم مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 68,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ العَزيزُ الرَّحيمُ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 69,
      "text": "وَاتلُ عَلَيهِم نَبَأَ إِبرٰهيمَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 70,
      "text": "إِذ قالَ لِأَبيهِ وَقَومِهِ ما تَعبُدونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 71,
      "text": "قالوا نَعبُدُ أَصنامًا فَنَظَلُّ لَها عٰكِفينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 72,
      "text": "قالَ هَل يَسمَعونَكُم إِذ تَدعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 73,
      "text": "أَو يَنفَعونَكُم أَو يَضُرّونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 74,
      "text": "قالوا بَل وَجَدنا ءاباءَنا كَذٰلِكَ يَفعَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 75,
      "text": "قالَ أَفَرَءَيتُم ما كُنتُم تَعبُدونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 76,
      "text": "أَنتُم وَءاباؤُكُمُ الأَقدَمونَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 77,
      "text": "فَإِنَّهُم عَدُوٌّ لى إِلّا رَبَّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 78,
      "text": "الَّذى خَلَقَنى فَهُوَ يَهدينِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 79,
      "text": "وَالَّذى هُوَ يُطعِمُنى وَيَسقينِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 80,
      "text": "وَإِذا مَرِضتُ فَهُوَ يَشفينِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 81,
      "text": "وَالَّذى يُميتُنى ثُمَّ يُحيينِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 82,
      "text": "وَالَّذى أَطمَعُ أَن يَغفِرَ لى خَطيـَٔتى يَومَ الدّينِ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 83,
      "text": "رَبِّ هَب لى حُكمًا وَأَلحِقنى بِالصّٰلِحينَ",
      "juz": 19,
      "manzil": 5,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 84,
      "text": "وَاجعَل لى لِسانَ صِدقٍ فِى الءاخِرينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وَاجعَلنى مِن وَرَثَةِ جَنَّةِ النَّعيمِ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 86,
      "text": "وَاغفِر لِأَبى إِنَّهُ كانَ مِنَ الضّالّينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 87,
      "text": "وَلا تُخزِنى يَومَ يُبعَثونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 88,
      "text": "يَومَ لا يَنفَعُ مالٌ وَلا بَنونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 89,
      "text": "إِلّا مَن أَتَى اللَّهَ بِقَلبٍ سَليمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 90,
      "text": "وَأُزلِفَتِ الجَنَّةُ لِلمُتَّقينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 91,
      "text": "وَبُرِّزَتِ الجَحيمُ لِلغاوينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 92,
      "text": "وَقيلَ لَهُم أَينَ ما كُنتُم تَعبُدونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 93,
      "text": "مِن دونِ اللَّهِ هَل يَنصُرونَكُم أَو يَنتَصِرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 94,
      "text": "فَكُبكِبوا فيها هُم وَالغاوۥنَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 95,
      "text": "وَجُنودُ إِبليسَ أَجمَعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 96,
      "text": "قالوا وَهُم فيها يَختَصِمونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 97,
      "text": "تَاللَّهِ إِن كُنّا لَفى ضَلٰلٍ مُبينٍ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 98,
      "text": "إِذ نُسَوّيكُم بِرَبِّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 99,
      "text": "وَما أَضَلَّنا إِلَّا المُجرِمونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 100,
      "text": "فَما لَنا مِن شٰفِعينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 101,
      "text": "وَلا صَديقٍ حَميمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 102,
      "text": "فَلَو أَنَّ لَنا كَرَّةً فَنَكونَ مِنَ المُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 103,
      "text": "إِنَّ فى ذٰلِكَ لَءايَةً ۖ وَما كانَ أَكثَرُهُم مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 104,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ العَزيزُ الرَّحيمُ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 105,
      "text": "كَذَّبَت قَومُ نوحٍ المُرسَلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 106,
      "text": "إِذ قالَ لَهُم أَخوهُم نوحٌ أَلا تَتَّقونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 107,
      "text": "إِنّى لَكُم رَسولٌ أَمينٌ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 108,
      "text": "فَاتَّقُوا اللَّهَ وَأَطيعونِ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 109,
      "text": "وَما أَسـَٔلُكُم عَلَيهِ مِن أَجرٍ ۖ إِن أَجرِىَ إِلّا عَلىٰ رَبِّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 110,
      "text": "فَاتَّقُوا اللَّهَ وَأَطيعونِ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 111,
      "text": "۞ قالوا أَنُؤمِنُ لَكَ وَاتَّبَعَكَ الأَرذَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 112,
      "text": "قالَ وَما عِلمى بِما كانوا يَعمَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 113,
      "text": "إِن حِسابُهُم إِلّا عَلىٰ رَبّى ۖ لَو تَشعُرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 114,
      "text": "وَما أَنا۠ بِطارِدِ المُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 115,
      "text": "إِن أَنا۠ إِلّا نَذيرٌ مُبينٌ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 116,
      "text": "قالوا لَئِن لَم تَنتَهِ يٰنوحُ لَتَكونَنَّ مِنَ المَرجومينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 117,
      "text": "قالَ رَبِّ إِنَّ قَومى كَذَّبونِ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 118,
      "text": "فَافتَح بَينى وَبَينَهُم فَتحًا وَنَجِّنى وَمَن مَعِىَ مِنَ المُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 119,
      "text": "فَأَنجَينٰهُ وَمَن مَعَهُ فِى الفُلكِ المَشحونِ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 120,
      "text": "ثُمَّ أَغرَقنا بَعدُ الباقينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 121,
      "text": "إِنَّ فى ذٰلِكَ لَءايَةً ۖ وَما كانَ أَكثَرُهُم مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 122,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ العَزيزُ الرَّحيمُ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 123,
      "text": "كَذَّبَت عادٌ المُرسَلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 124,
      "text": "إِذ قالَ لَهُم أَخوهُم هودٌ أَلا تَتَّقونَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 125,
      "text": "إِنّى لَكُم رَسولٌ أَمينٌ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 126,
      "text": "فَاتَّقُوا اللَّهَ وَأَطيعونِ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 127,
      "text": "وَما أَسـَٔلُكُم عَلَيهِ مِن أَجرٍ ۖ إِن أَجرِىَ إِلّا عَلىٰ رَبِّ العٰلَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 128,
      "text": "أَتَبنونَ بِكُلِّ ريعٍ ءايَةً تَعبَثونَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 129,
      "text": "وَتَتَّخِذونَ مَصانِعَ لَعَلَّكُم تَخلُدونَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 130,
      "text": "وَإِذا بَطَشتُم بَطَشتُم جَبّارينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 131,
      "text": "فَاتَّقُوا اللَّهَ وَأَطيعونِ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 132,
      "text": "وَاتَّقُوا الَّذى أَمَدَّكُم بِما تَعلَمونَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 133,
      "text": "أَمَدَّكُم بِأَنعٰمٍ وَبَنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 134,
      "text": "وَجَنّٰتٍ وَعُيونٍ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 135,
      "text": "إِنّى أَخافُ عَلَيكُم عَذابَ يَومٍ عَظيمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 136,
      "text": "قالوا سَواءٌ عَلَينا أَوَعَظتَ أَم لَم تَكُن مِنَ الوٰعِظينَ",
      "juz": 19,
      "manzil": 5,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 137,
      "text": "إِن هٰذا إِلّا خُلُقُ الأَوَّلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 138,
      "text": "وَما نَحنُ بِمُعَذَّبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 139,
      "text": "فَكَذَّبوهُ فَأَهلَكنٰهُم ۗ إِنَّ فى ذٰلِكَ لَءايَةً ۖ وَما كانَ أَكثَرُهُم مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 140,
      "text": "وَإِنَّ رَبَّكَ لَهُوَ العَزيزُ الرَّحيمُ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 141,
      "text": "كَذَّبَت ثَمودُ المُرسَلينَ",
      "juz": 19,
      "manzil": 5,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 142,
      "text": "إِذ قالَ لَهُم أَخوهُم صٰلِحٌ أَلا تَتَّقونَ",
