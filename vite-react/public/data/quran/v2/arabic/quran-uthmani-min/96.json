{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ اقرَأ بِاسمِ رَبِّكَ الَّذى خَلَقَ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "خَلَقَ الإِنسٰنَ مِن عَلَقٍ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اقرَأ وَرَبُّكَ الأَكرَمُ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "الَّذى عَلَّمَ بِالقَلَمِ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلَّمَ الإِنسٰنَ ما لَم يَعلَم",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "كَلّا إِنَّ الإِنسٰنَ لَيَطغىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "أَن رَءاهُ استَغنىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّ إِلىٰ رَبِّكَ الرُّجعىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "أَرَءَيتَ الَّذى يَنهىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 10,
      "text": "عَبدًا إِذا صَلّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 11,
      "text": "أَرَءَيتَ إِن كانَ عَلَى الهُدىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 12,
      "text": "أَو أَمَرَ بِالتَّقوىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 13,
      "text": "أَرَءَيتَ إِن كَذَّبَ وَتَوَلّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 14,
      "text": "أَلَم يَعلَم بِأَنَّ اللَّهَ يَرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلّا لَئِن لَم يَنتَهِ لَنَسفَعًا بِالنّاصِيَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ناصِيَةٍ كٰذِبَةٍ خاطِئَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَليَدعُ نادِيَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 18,
      "text": "سَنَدعُ الزَّبانِيَةَ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 19,
      "text": "كَلّا لا تُطِعهُ وَاسجُد وَاقتَرِب ۩",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
        "id": 15,
        "recommended": false,
        "obligatory": true
      }
    }
  ]
}