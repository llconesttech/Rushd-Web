{
  "number": 78,
  "name": "سُورَةُ النَّبَإِ",
  "englishName": "An-Naba",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم عم يتساءلون",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "عن النبإ العظيم",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "الذي هم فيه مختلفون",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كلا سيعلمون",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ثم كلا سيعلمون",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ألم نجعل الأرض مهادا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "والجبال أوتادا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وخلقناكم أزواجا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وجعلنا نومكم سباتا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وجعلنا الليل لباسا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وجعلنا النهار معاشا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وبنينا فوقكم سبعا شدادا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وجعلنا سراجا وهاجا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وأنزلنا من المعصرات ماء ثجاجا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "لنخرج به حبا ونباتا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وجنات ألفافا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إن يوم الفصل كان ميقاتا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "يوم ينفخ في الصور فتأتون أفواجا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وفتحت السماء فكانت أبوابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وسيرت الجبال فكانت سرابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "إن جهنم كانت مرصادا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "للطاغين مآبا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "لابثين فيها أحقابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "لا يذوقون فيها بردا ولا شرابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "إلا حميما وغساقا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "جزاء وفاقا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إنهم كانوا لا يرجون حسابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وكذبوا بآياتنا كذابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وكل شيء أحصيناه كتابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فذوقوا فلن نزيدكم إلا عذابا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "إن للمتقين مفازا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "حدائق وأعنابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وكواعب أترابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وكأسا دهاقا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "لا يسمعون فيها لغوا ولا كذابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "جزاء من ربك عطاء حسابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "رب السماوات والأرض وما بينهما الرحمن ۖ لا يملكون منه خطابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "يوم يقوم الروح والملائكة صفا ۖ لا يتكلمون إلا من أذن له الرحمن وقال صوابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ذلك اليوم الحق ۖ فمن شاء اتخذ إلى ربه مآبا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إنا أنذرناكم عذابا قريبا يوم ينظر المرء ما قدمت يداه ويقول الكافر يا ليتني كنت ترابا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}