{
  "number": 67,
  "name": "سُورَةُ المُلۡكِ",
  "englishName": "Al-Mulk",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم تبارك الذي بيده الملك وهو على كل شيء قدير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الذي خلق الموت والحياة ليبلوكم أيكم أحسن عملا ۚ وهو العزيز الغفور",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 3,
      "text": "الذي خلق سبع سماوات طباقا ۖ ما ترى في خلق الرحمن من تفاوت ۖ فارجع البصر هل ترى من فطور",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ثم ارجع البصر كرتين ينقلب إليك البصر خاسئا وهو حسير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ولقد زينا السماء الدنيا بمصابيح وجعلناها رجوما للشياطين ۖ وأعتدنا لهم عذاب السعير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وللذين كفروا بربهم عذاب جهنم ۖ وبئس المصير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إذا ألقوا فيها سمعوا لها شهيقا وهي تفور",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 8,
      "text": "تكاد تميز من الغيظ ۖ كلما ألقي فيها فوج سألهم خزنتها ألم يأتكم نذير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 9,
      "text": "قالوا بلى قد جاءنا نذير فكذبنا وقلنا ما نزل الله من شيء إن أنتم إلا في ضلال كبير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وقالوا لو كنا نسمع أو نعقل ما كنا في أصحاب السعير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فاعترفوا بذنبهم فسحقا لأصحاب السعير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إن الذين يخشون ربهم بالغيب لهم مغفرة وأجر كبير",
      "juz": 29,
      "manzil": 7,
      "page": 562,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وأسروا قولكم أو اجهروا به ۖ إنه عليم بذات الصدور",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ألا يعلم من خلق وهو اللطيف الخبير",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 496,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 15,
      "text": "هو الذي جعل لكم الأرض ذلولا فامشوا في مناكبها وكلوا من رزقه ۖ وإليه النشور",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 16,
      "text": "أأمنتم من في السماء أن يخسف بكم الأرض فإذا هي تمور",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 17,
      "text": "أم أمنتم من في السماء أن يرسل عليكم حاصبا ۖ فستعلمون كيف نذير",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ولقد كذب الذين من قبلهم فكيف كان نكير",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 19,
      "text": "أولم يروا إلى الطير فوقهم صافات ويقبضن ۚ ما يمسكهن إلا الرحمن ۚ إنه بكل شيء بصير",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 20,
      "text": "أمن هذا الذي هو جند لكم ينصركم من دون الرحمن ۚ إن الكافرون إلا في غرور",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 21,
      "text": "أمن هذا الذي يرزقكم إن أمسك رزقه ۚ بل لجوا في عتو ونفور",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 22,
      "text": "أفمن يمشي مكبا على وجهه أهدى أمن يمشي سويا على صراط مستقيم",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 23,
      "text": "قل هو الذي أنشأكم وجعل لكم السمع والأبصار والأفئدة ۖ قليلا ما تشكرون",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 24,
      "text": "قل هو الذي ذرأكم في الأرض وإليه تحشرون",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 25,
      "text": "ويقولون متى هذا الوعد إن كنتم صادقين",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قل إنما العلم عند الله وإنما أنا نذير مبين",
      "juz": 29,
      "manzil": 7,
      "page": 563,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فلما رأوه زلفة سيئت وجوه الذين كفروا وقيل هذا الذي كنتم به تدعون",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قل أرأيتم إن أهلكني الله ومن معي أو رحمنا فمن يجير الكافرين من عذاب أليم",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قل هو الرحمن آمنا به وعليه توكلنا ۖ فستعلمون من هو في ضلال مبين",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    },
    {
      "number": 30,
      "text": "قل أرأيتم إن أصبح ماؤكم غورا فمن يأتيكم بماء معين",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 497,
      "hizbQuarter": 225,
      "sajda": false
    }
  ]
}