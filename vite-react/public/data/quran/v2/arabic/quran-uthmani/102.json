      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "حَتَّىٰ زُرْتُمُ ٱلْمَقَابِرَ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "كَلَّا سَوْفَ تَعْلَمُونَ",
