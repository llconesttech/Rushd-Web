{
  "number": 89,
  "name": "سُورَةُ الفَجۡرِ",
  "englishName": "Al-Fajr",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلْفَجْرِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلَيَالٍ عَشْرٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَٱلشَّفْعِ وَٱلْوَتْرِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَٱلَّيْلِ إِذَا يَسْرِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "هَلْ فِى ذَٰلِكَ قَسَمٌۭ لِّذِى حِجْرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِرَمَ ذَاتِ ٱلْعِمَادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَٰدِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَٰدِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَأَمَّا ٱلْإِنسَٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَٰنَنِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًۭا لَّمًّۭا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَتُحِبُّونَ ٱلْمَالَ حُبًّۭا جَمًّۭا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّۭا دَكًّۭا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّۭا صَفًّۭا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍۢ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَيَوْمَئِذٍۢ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 27,
      "text": "يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 28,
      "text": "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةًۭ مَّرْضِيَّةًۭ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَٱدْخُلِى فِى عِبَٰدِى",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَٱدْخُلِى جَنَّتِى",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}