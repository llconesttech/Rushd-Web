{
  "number": 84,
  "name": "سُورَةُ الانشِقَاقِ",
  "englishName": "Al-Inshiqaaq",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِذَا ٱلسَّمَآءُ ٱنشَقَّتْ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَإِذَا ٱلْأَرْضُ مُدَّتْ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًۭا فَمُلَٰقِيهِ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِيَمِينِهِۦ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَسَوْفَ يُحَاسَبُ حِسَابًۭا يَسِيرًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَيَنقَلِبُ إِلَىٰٓ أَهْلِهِۦ مَسْرُورًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ وَرَآءَ ظَهْرِهِۦ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَسَوْفَ يَدْعُوا۟ ثُبُورًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَيَصْلَىٰ سَعِيرًا",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إِنَّهُۥ كَانَ فِىٓ أَهْلِهِۦ مَسْرُورًا",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "بَلَىٰٓ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَلَآ أُقْسِمُ بِٱلشَّفَقِ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَٱلَّيْلِ وَمَا وَسَقَ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَٱلْقَمَرِ إِذَا ٱتَّسَقَ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 19,
      "text": "لَتَرْكَبُنَّ طَبَقًا عَن طَبَقٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَمَا لَهُمْ لَا يُؤْمِنُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَإِذَا قُرِئَ عَلَيْهِمُ ٱلْقُرْءَانُ لَا يَسْجُدُونَ ۩",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": {
        "id": 14,
        "recommended": true,
        "obligatory": false
      }
    },
    {
      "number": 22,
      "text": "بَلِ ٱلَّذِينَ كَفَرُوا۟ يُكَذِّبُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَٱللَّهُ أَعْلَمُ بِمَا يُوعُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَبَشِّرْهُم بِعَذَابٍ أَلِيمٍ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 25,
      "text": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۭ",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}