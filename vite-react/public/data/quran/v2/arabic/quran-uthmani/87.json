{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَٱلْءَاخِرَةُ خَيْرٌۭ وَأَبْقَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}