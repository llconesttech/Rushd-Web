{
  "number": 101,
  "name": "سُورَةُ القَارِعَةِ",
  "englishName": "Al-Qaari'a",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱلْقَارِعَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَا ٱلْقَارِعَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلْقَارِعَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "يَوْمَ يَكُونُ ٱلنَّاسُ كَٱلْفَرَاشِ ٱلْمَبْثُوثِ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ ٱلْمَنفُوشِ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَهُوَ فِى عِيشَةٍۢ رَّاضِيَةٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَأُمُّهُۥ هَاوِيَةٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَمَآ أَدْرَىٰكَ مَا هِيَهْ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "نَارٌ حَامِيَةٌۢ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}