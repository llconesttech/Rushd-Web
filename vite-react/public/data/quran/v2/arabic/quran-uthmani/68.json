{
  "number": 68,
  "name": "سُورَةُ القَلَمِ",
  "englishName": "Al-Qalam",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ نٓ ۚ وَٱلْقَلَمِ وَمَا يَسْطُرُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَسَتُبْصِرُ وَيُبْصِرُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بِأَييِّكُمُ ٱلْمَفْتُونُ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِٱلْمُهْتَدِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَلَا تُطِعِ ٱلْمُكَذِّبِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَدُّوا۟ لَوْ تُدْهِنُ فَيُدْهِنُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَا تُطِعْ كُلَّ حَلَّافٍۢ مَّهِينٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 11,
      "text": "هَمَّازٍۢ مَّشَّآءٍۭ بِنَمِيمٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 12,
      "text": "مَّنَّاعٍۢ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 13,
      "text": "عُتُلٍّۭ بَعْدَ ذَٰلِكَ زَنِيمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 14,
      "text": "أَن كَانَ ذَا مَالٍۢ وَبَنِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 15,
      "text": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلْأَوَّلِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 16,
      "text": "سَنَسِمُهُۥ عَلَى ٱلْخُرْطُومِ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إِنَّا بَلَوْنَٰهُمْ كَمَا بَلَوْنَآ أَصْحَٰبَ ٱلْجَنَّةِ إِذْ أَقْسَمُوا۟ لَيَصْرِمُنَّهَا مُصْبِحِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلَا يَسْتَثْنُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَطَافَ عَلَيْهَا طَآئِفٌۭ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَأَصْبَحَتْ كَٱلصَّرِيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَتَنَادَوْا۟ مُصْبِحِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 22,
      "text": "أَنِ ٱغْدُوا۟ عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَٰرِمِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَٱنطَلَقُوا۟ وَهُمْ يَتَخَٰفَتُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 24,
      "text": "أَن لَّا يَدْخُلَنَّهَا ٱلْيَوْمَ عَلَيْكُم مِّسْكِينٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَغَدَوْا۟ عَلَىٰ حَرْدٍۢ قَٰدِرِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَلَمَّا رَأَوْهَا قَالُوٓا۟ إِنَّا لَضَآلُّونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 27,
      "text": "بَلْ نَحْنُ مَحْرُومُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قَالُوا۟ سُبْحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍۢ يَتَلَٰوَمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قَالُوا۟ يَٰوَيْلَنَآ إِنَّا كُنَّا طَٰغِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 32,
      "text": "عَسَىٰ رَبُّنَآ أَن يُبْدِلَنَا خَيْرًۭا مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَذَٰلِكَ ٱلْعَذَابُ ۖ وَلَعَذَابُ ٱلْءَاخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا۟ يَعْلَمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 34,
      "text": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّٰتِ ٱلنَّعِيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 35,
      "text": "أَفَنَجْعَلُ ٱلْمُسْلِمِينَ كَٱلْمُجْرِمِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 36,
      "text": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 37,
      "text": "أَمْ لَكُمْ كِتَٰبٌۭ فِيهِ تَدْرُسُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 38,
      "text": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 39,
      "text": "أَمْ لَكُمْ أَيْمَٰنٌ عَلَيْنَا بَٰلِغَةٌ إِلَىٰ يَوْمِ ٱلْقِيَٰمَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 40,
      "text": "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 41,
      "text": "أَمْ لَهُمْ شُرَكَآءُ فَلْيَأْتُوا۟ بِشُرَكَآئِهِمْ إِن كَانُوا۟ صَٰدِقِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 42,
      "text": "يَوْمَ يُكْشَفُ عَن سَاقٍۢ وَيُدْعَوْنَ إِلَى ٱلسُّجُودِ فَلَا يَسْتَطِيعُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 43,
      "text": "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌۭ ۖ وَقَدْ كَانُوا۟ يُدْعَوْنَ إِلَى ٱلسُّجُودِ وَهُمْ سَٰلِمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَذَرْنِى وَمَن يُكَذِّبُ بِهَٰذَا ٱلْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَأُمْلِى لَهُمْ ۚ إِنَّ كَيْدِى مَتِينٌ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 46,
      "text": "أَمْ تَسْـَٔلُهُمْ أَجْرًۭا فَهُم مِّن مَّغْرَمٍۢ مُّثْقَلُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 47,
      "text": "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 48,
      "text": "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 49,
      "text": "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٌۭ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلْعَرَآءِ وَهُوَ مَذْمُومٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَٱجْتَبَٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّٰلِحِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُوا۟ لَيُزْلِقُونَكَ بِأَبْصَٰرِهِمْ لَمَّا سَمِعُوا۟ ٱلذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَمَا هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    }
  ]
}