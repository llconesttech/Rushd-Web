    },
    {
      "number": 2,
      "text": "تِلْكَ اٰیٰتُ [h:5932[ا]لْكِتٰبِ [h:6541[ا]لْمُبِ[p[ی]ْنِ ۟",
      "juz": 20,
      "page": 385,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 3,
      "text": "نَتْلُوْ[s[ا] عَلَیْكَ مِ[a:10061[ن]ْ [a:10061[ن]َّبَاِ مُوْسٰی وَفِرْعَوْنَ بِ[h:370[ا]لْحَقِّ لِقَوْ[a:4294[م]ٍ [a:4294[ی]ُّؤْمِنُ[p[و]ْنَ ۟",
      "juz": 20,
      "page": 385,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِ[g[ن]َّ فِرْعَوْنَ عَلَا فِی [h:44[ا]لْاَرْضِ وَجَعَلَ اَهْلَهَا شِیَ[a:10062[ع]ً[a:10062[ا] [a:10062[ی]َّسْتَضْعِفُ طَ[o[ا]ٓىِٕفَ[a:10063[ة]ً [a:10063[م]ِّنْهُمْ یُذَبِّحُ اَ[q:767[ب]ْنَ[o[ا]ٓءَهُمْ وَیَسْتَحْیٖ نِسَ[o[ا]ٓءَهُمْ ؕ اِ[g[ن]َّهٗ كَانَ مِنَ [h:6188[ا]لْمُفْسِدِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 385,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَنُرِیْدُ اَ[a:10064[ن]ْ [a:10064[ن]َّمُ[g[ن]َّ عَلَی [h:289[ا]لَّذِیْنَ [h:10065[ا]سْتُضْعِفُوْ[s[ا] فِی [h:44[ا]لْاَرْضِ وَنَ[q:10066[ج]ْعَلَهُمْ اَىِٕ[g[م]َّ[a:10067[ة]ً [a:10067[و]َّنَ[q:10068[ج]ْعَلَهُمُ [h:10069[ا]لْوٰرِثِ[p[ی]ْنَ ۟ۙ",
      "juz": 20,
      "page": 385,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَنُمَكِّنَ لَهُمْ فِی [h:44[ا]لْاَرْضِ وَنُرِیَ فِرْعَوْنَ وَهَامٰنَ وَجُنُوْدَهُمَا مِنْهُ[w:3670[م]ْ [w:3670[م]َّا كَانُوْ[s[ا] یَحْذَرُ[p[و]ْنَ ۟",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَاَوْحَیْنَ[o[ا]ۤ اِلٰۤ[o[ی] اُ[g[م]ِّ مُوْسٰۤ[o[ی] اَنْ اَرْضِعِیْهِ ۚ فَاِذَا خِفْتِ عَلَیْهِ فَاَلْقِیْهِ فِی [h:4971[ا]لْیَ[g[م]ِّ وَلَا تَخَافِیْ وَلَا تَحْزَنِ[o[ی]ْ ۚ اِ[g[ن]َّا رَ[m[ا]ٓدُّوْهُ اِلَیْكِ وَجَاعِلُوْهُ مِنَ [h:4822[ا]لْمُرْسَلِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَ[h:9999[ا]لْتَقَطَهٗۤ اٰلُ فِرْعَوْنَ لِیَكُوْنَ لَهُمْ عَدُوًّ[a:10070[ا] [a:10070[و]َّحَزَنًا ؕ اِ[g[ن]َّ فِرْعَوْنَ وَهَامٰنَ وَجُنُوْدَهُمَا كَانُوْ[s[ا] خٰطِـِٕ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَقَالَتِ [h:10071[ا]مْرَاَتُ فِرْعَوْنَ قُرَّتُ عَیْ[u:10072[ن]ٍ [u:10072[ل]ِّیْ وَلَكَ ؕ لَا تَ[q:10073[ق]ْتُلُوْهُ ۖۗ عَسٰۤ[o[ی] اَ[a:6597[ن]ْ [a:6597[ی]َّ[f:6598[ن]ْ[f:6598[ف]َعَنَ[o[ا]ۤ اَوْ نَتَّخِذَهٗ وَلَ[a:10074[د]ً[a:10074[ا] [a:10074[و]َّهُمْ لَا یَشْعُرُ[p[و]ْنَ ۟",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاَصْبَحَ فُؤَادُ اُ[g[م]ِّ مُوْسٰی فٰرِغًا ؕ اِ[f:10075[ن]ْ [f:10075[ك]َادَتْ لَتُ[q:10076[ب]ْدِیْ بِهٖ لَوْلَ[o[ا]ۤ اَ[u:10077[ن]ْ [u:10077[ر]َّبَ[q:10078[ط]ْنَا عَلٰی قَلْبِهَا لِتَكُوْنَ مِنَ [h:3047[ا]لْمُؤْمِنِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَقَالَتْ لِاُخْتِهٖ قُصِّیْهِ ؗ فَبَصُرَتْ بِهٖ عَ[f:10079[ن]ْ [f:10079[ج]ُنُ[a:10080[ب]ٍ [a:10080[و]َّهُمْ لَا یَشْعُرُ[p[و]ْنَ ۟ۙ",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 154,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَحَرَّمْنَا عَلَیْهِ [h:10081[ا]لْمَرَاضِعَ مِ[f:128[ن]ْ [f:128[ق]َ[q:129[ب]ْلُ فَقَالَتْ هَلْ اَدُلُّكُمْ عَلٰۤ[o[ی] اَهْلِ بَیْ[a:10082[ت]ٍ [a:10082[ی]َّكْفُلُوْنَهٗ لَكُمْ وَهُمْ لَهٗ نٰصِحُ[p[و]ْنَ ۟",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَرَدَ[q:10083[د]ْنٰهُ اِلٰۤ[o[ی] اُ[g[م]ِّهٖ كَیْ تَقَرَّ عَیْنُهَا وَلَا تَحْزَنَ وَلِتَعْلَمَ اَ[g[ن]َّ وَعْدَ [h:3149[ا]للّٰهِ حَ[a:6092[ق]ٌّ [a:6092[و]َّلٰكِ[g[ن]َّ اَكْثَرَهُمْ لَا یَعْلَمُ[p[و]ْنَ ۟۠",
      "juz": 20,
      "page": 386,
      "ruku": 334,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَلَ[g[م]َّا بَلَغَ اَشُدَّهٗ وَ[h:10084[ا]سْتَوٰۤ[o[ی] اٰتَیْنٰهُ حُكْ[a:6600[م]ً[a:6600[ا] [a:6600[و]َّعِلْ[a:6601[م]ً[a:6601[ا] ؕ [a:6601[و]َكَذٰلِكَ نَ[q:4214[ج]ْزِی [h:4215[ا]لْمُحْسِنِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَدَخَلَ [h:10085[ا]لْمَدِیْنَةَ عَلٰی حِیْنِ غَفْلَ[a:8863[ة]ٍ [a:8863[م]ِّنْ اَهْلِهَا فَوَجَدَ فِیْهَا رَجُلَیْنِ یَ[q:10086[ق]ْتَتِلٰنِ ؗۗ هٰذَا مِ[f:10087[ن]ْ [f:10087[ش]ِیْعَتِهٖ وَهٰذَا مِنْ عَدُوِّهٖ ۚ فَ[h:10088[ا]سْتَغَ[h:10089[ا]ثَهُ [h:4534[ا]لَّذِیْ مِنْ شِیْعَتِهٖ عَلَی الَّذِیْ مِنْ عَدُوِّهٖ ۙ فَوَكَزَهٗ [a:10090[م]ُوْسٰ[p[ی] فَقَضٰی عَلَیْهِ ؗۗ قَالَ هٰذَا مِنْ عَمَلِ الشَّیْطٰنِ ؕ اِنَّهٗ عَدُوٌّ مُّضِلٌّ مُّبِیْنٌ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 16,
      "text": "قَالَ رَبِّ اِ[g[ن]ِّیْ ظَلَمْتُ نَفْسِیْ فَ[h:1791[ا]غْفِرْ لِیْ فَغَفَرَ لَهٗ ؕ اِ[g[ن]َّهٗ هُوَ [h:6822[ا]لْغَفُوْرُ [h:6228[ا][l[ل]رَّحِ[p[ی]ْمُ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 17,
      "text": "قَالَ رَبِّ بِمَ[o[ا]ۤ اَنْعَمْتَ عَلَیَّ فَلَنْ اَكُوْنَ ظَهِیْ[u:10092[ر]ً[u:10092[ا] [u:10092[ل]ِّلْمُ[q:9609[ج]ْرِمِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَاَصْبَحَ فِی [h:4932[ا]لْمَدِیْنَةِ خَ[o[ا]ٓىِٕ[a:10093[ف]ً[a:10093[ا] [a:10093[ی]َّتَرَقَّبُ فَاِذَا [h:10094[ا]لَّذِی [h:10095[ا]سْتَ[f:10096[ن]ْ[f:10096[ص]َرَهٗ بِ[h:6017[ا]لْاَمْسِ یَسْتَصْرِخُهٗ ؕ قَالَ لَهٗ مُوْسٰۤ[o[ی] اِ[g[ن]َّكَ لَغَوِ[a:10097[ی]ٌّ [a:10097[م]ُّبِ[p[ی]ْنٌ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَلَ[g[م]َّ[o[ا]ۤ اَنْ اَرَادَ اَ[a:10098[ن]ْ [a:10098[ی]َّ[q:10099[ب]ْطِشَ بِ[h:315[ا]لَّذِیْ هُوَ عَدُ[u:10100[و]ٌّ [u:10100[ل]َّهُمَا ۙ قَالَ یٰمُوْسٰۤ[o[ی] اَتُرِیْدُ اَ[f:10101[ن]ْ [f:10101[ت]َ[q:10102[ق]ْتُلَنِیْ كَمَا قَتَلْتَ نَفْسً[i:10103[ا] [i:10103[ب]ِ[h:6017[ا]لْاَمْسِ ۖۗ اِ[f:10104[ن]ْ [f:10104[ت]ُرِیْدُ اِلَّ[o[ا]ۤ اَ[f:1566[ن]ْ [f:1566[ت]َكُوْنَ جَبَّا[f:10105[ر]ً[f:10105[ا] [f:10105[ف]ِی [h:44[ا]لْاَرْضِ وَمَا تُرِیْدُ اَ[f:1566[ن]ْ [f:1566[ت]َكُوْنَ مِنَ [h:10106[ا]لْمُصْلِحِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَجَ[o[ا]ٓءَ رَجُ[a:10107[ل]ٌ [a:10107[م]ِّنْ اَ[q:10108[ق]ْصَا [h:10109[ا]لْمَدِیْنَةِ یَسْعٰی ؗ قَالَ یٰمُوْسٰۤ[o[ی] اِ[g[ن]َّ [h:10110[ا]لْمَلَاَ یَاْتَمِرُوْنَ بِكَ لِیَ[q:10111[ق]ْتُلُوْكَ فَ[h:4603[ا]خْرُ[q:4603[ج]ْ اِ[g[ن]ِّیْ لَكَ مِنَ [h:10112[ا][l[ل][g[ن]ّٰصِحِ[p[ی]ْنَ ۟",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَخَرَجَ مِنْهَا خَ[o[ا]ٓىِٕ[a:10093[ف]ً[a:10093[ا] [a:10093[ی]َّتَرَقَّبُ ؗ قَالَ رَبِّ نَجِّنِیْ مِنَ [h:4196[ا]لْقَوْمِ [h:4155[ا][l[ل]ظّٰلِمِ[p[ی]ْنَ ۟۠",
      "juz": 20,
      "page": 387,
      "ruku": 335,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَلَ[g[م]َّا تَوَجَّهَ تِلْقَ[o[ا]ٓءَ مَ[q:4842[د]ْیَنَ قَالَ عَسٰی رَبِّ[o[ی]ْۤ اَ[a:10113[ن]ْ [a:10113[ی]َّهْدِیَنِیْ سَوَ[o[ا]ٓءَ [h:564[ا][l[ل]سَّبِ[p[ی]ْلِ ۟",
      "juz": 20,
      "page": 388,
      "ruku": 336,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَلَ[g[م]َّا وَرَدَ مَ[o[ا]ٓءَ مَ[q:4842[د]ْیَنَ وَجَدَ عَلَیْهِ اُ[g[م]َّ[a:10114[ة]ً [a:10114[م]ِّنَ [h:721[ا][l[ل][g[ن]َّاسِ یَسْقُوْنَ ؗ۬ وَوَجَدَ مِ[f:10115[ن]ْ [f:10115[د]ُوْنِهِمُ [h:10116[ا]مْرَاَتَیْنِ تَذُوْدٰنِ ۚ قَالَ مَا خَ[q:10117[ط]ْبُكُمَا ؕ قَالَتَا لَا نَسْقِیْ حَتّٰی یُصْدِرَ [h:10118[ا][l[ل]رِّعَ[o[ا]ٓءُ ٚ وَاَبُوْنَا شَیْ[f:10119[خ]ٌ [f:10119[ك]َبِ[p[ی]ْرٌ ۟",
      "juz": 20,
      "page": 388,
      "ruku": 336,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَسَقٰی لَهُمَا ثُ[g[م]َّ تَوَلّٰۤ[o[ی] اِلَی [h:10120[ا][l[ل]ظِّلِّ فَقَالَ رَبِّ اِ[g[ن]ِّیْ لِمَ[o[ا]ۤ اَ[f:1954[ن]ْ[f:1954[ز]َلْتَ اِلَیَّ مِنْ خَیْ[f:10121[ر]ٍ [f:10121[ف]َقِ[p[ی]ْرٌ ۟",
      "juz": 20,
      "page": 388,
      "ruku": 336,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَجَ[o[ا]ٓءَتْهُ اِحْدٰىهُمَ[h:10122[ا] تَمْشِیْ عَلَی اسْتِحْیَآءٍ ؗ قَالَتْ اِنَّ [o[ا]َبِیْ یَدْعُوْكَ لِیَجْزِیَكَ [h:4196[ا]َجْرَ مَ[h:4155[ا] سَقَ[p[ی]ْتَ لَنَا ؕ فَلَمَّا جَآءَهٗ وَقَصَّ عَلَیْهِ الْقَصَصَ ۙ قَالَ لَا تَخَفْ ۫ۥ نَجَوْتَ مِنَ الْقَوْمِ الظّٰلِمِیْنَ ۟",
      "juz": 20,
      "page": 388,
      "ruku": 336,
      "hizbQuarter": 155,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قَالَتْ اِحْدٰىهُمَ[h:10128[ا] یٰۤ[h:10129[ا]َبَتِ [h:10130[ا]سْتَاْجِرْهُ ؗ اِنَّ خَیْرَ مَنِ اسْتَاْجَرْتَ الْقَوِیُّ الْاَمِیْنُ ۟",
      "juz": 20,
      "page": 388,
      "ruku": 336,
      "hizbQuarter": 155,
