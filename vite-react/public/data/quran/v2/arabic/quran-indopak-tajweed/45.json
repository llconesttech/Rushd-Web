    },
    {
      "number": 2,
      "text": "تَ[f:10623[ن]ْ[f:10623[ز]ِیْلُ [h:10624[ا]لْكِتٰبِ مِنَ [h:321[ا]للّٰهِ [h:11613[ا]لْعَزِیْزِ [h:2229[ا]لْحَكِ[p[ی]ْمِ ۟",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اِ[g[ن]َّ فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ لَاٰیٰ[u:12387[ت]ٍ [u:12387[ل]ِّلْمُؤْمِنِ[p[ی]ْنَ ۟ؕ",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَفِیْ خَلْقِكُمْ وَمَا یَبُثُّ مِ[f:4026[ن]ْ [f:4026[د]َ[m[ا]ٓبَّةٍ اٰیٰ[u:12388[ت]ٌ [u:12388[ل]ِّقَوْ[a:621[م]ٍ [a:621[ی]ُّوْقِنُ[p[و]ْنَ ۟ۙ",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَ[h:9999[ا]خْتِلَ[h:833[ا]فِ [h:834[ا][l[ل]َّیْلِ وَ[o[ا]لنَّهَ[h:64[ا][u:6099[ر]ِ وَمَاۤ اَنْزَلَ اللّٰهُ مِنَ ا[l[ل]سَّمَآءِ [a:850[م]ِنْ رِّزْقٍ فَاَحْیَا بِهِ الْاَرْضَ بَعْدَ مَوْتِهَا وَتَصْرِیْفِ الرِّیٰحِ اٰیٰتٌ لِّقَوْمٍ یَّعْقِلُوْنَ ۟",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تِلْكَ اٰیٰتُ [h:1442[ا]للّٰهِ نَتْلُوْهَا عَلَیْكَ بِ[h:370[ا]لْحَقِّ ۚ فَبِاَیِّ حَدِیْ[i:12390[ث]ٍ [i:12390[ب]َعْدَ [h:12391[ا]للّٰهِ وَاٰیٰتِهٖ یُؤْمِنُ[p[و]ْنَ ۟",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَیْ[u:12392[ل]ٌ [u:12392[ل]ِّكُلِّ اَفَّاكٍ اَثِ[p[ی]ْمٍ ۟ۙ",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یَّسْمَعُ اٰیٰتِ [h:1298[ا]للّٰهِ تُتْلٰی عَلَیْهِ ثُ[g[م]َّ یُصِرُّ مُسْتَكْبِ[f:10551[ر]ً[f:10551[ا] [f:10551[ك]َاَ[u:2914[ن]ْ [u:2914[ل]َّمْ یَسْمَعْهَا ۚ فَبَشِّرْهُ بِعَذَابٍ اَلِ[p[ی]ْمٍ ۟",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَاِذَا عَلِمَ مِنْ اٰیٰتِنَا شَیْـَٔا تَّخَذَهَا هُزُوًا ؕ اُ[s[و]لٰٓىِٕكَ لَهُمْ عَذَا[a:478[ب]ٌ [a:478[م]ُّهِ[p[ی]ْنٌ ۟ؕ",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 10,
      "text": "مِ[a:12394[ن]ْ [a:12394[و]َّرَ[o[ا]ٓىِٕهِمْ جَهَ[g[ن]َّمُ ۚ وَلَا یُغْنِیْ عَنْهُ[w:3985[م]ْ [w:3985[م]َّا كَسَبُوْ[s[ا] شَیْـًٔ[a:232[ا] [a:232[و]َّلَا مَا [h:12395[ا]تَّخَذُوْ[s[ا] مِ[f:107[ن]ْ [f:107[د]ُوْنِ [h:108[ا]للّٰهِ اَوْلِیَ[o[ا]ٓءَ ۚ وَلَهُمْ عَذَابٌ عَظِ[p[ی]ْمٌ ۟ؕ",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 11,
      "text": "هٰذَا هُ[a:12396[د]ً[a:12396[ی] ۚ [a:12396[و]َ[h:37[ا]لَّذِیْنَ كَفَرُوْ[s[ا] بِاٰیٰتِ رَبِّهِمْ لَهُمْ عَذَا[a:10923[ب]ٌ [a:10923[م]ِّ[u:10924[ن]ْ [u:10924[ر]ِّ[q:10925[ج]ْزٍ اَلِ[p[ی]ْمٌ ۟۠",
      "juz": 25,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اَللّٰهُ [h:4738[ا]لَّذِیْ سَخَّرَ لَكُمُ [h:12397[ا]لْبَحْرَ لِتَ[q:7132[ج]ْرِیَ [h:12398[ا]لْفُلْكُ فِیْهِ بِاَمْرِهٖ وَلِتَ[q:7353[ب]ْتَغُوْ[s[ا] مِ[f:475[ن]ْ [f:475[ف]َضْلِهٖ وَلَعَلَّكُمْ تَشْكُرُ[p[و]ْنَ ۟ۚ",
      "juz": 25,
      "page": 499,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَسَخَّرَ لَكُ[w:154[م]ْ [w:154[م]َّا فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَمَا فِی [h:44[ا]لْاَرْضِ جَمِیْ[a:12399[ع]ً[a:12399[ا] [a:12399[م]ِّنْهُ ؕ اِ[g[ن]َّ فِیْ ذٰلِكَ لَاٰیٰ[u:849[ت]ٍ [u:849[ل]ِّقَوْ[a:6018[م]ٍ [a:6018[ی]َّتَفَكَّرُ[p[و]ْنَ ۟",
      "juz": 25,
      "page": 499,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قُلْ لِّلَّذِیْنَ اٰمَنُوْ[s[ا] یَغْفِرُوْ[s[ا] لِلَّذِیْنَ لَا یَرْجُوْنَ اَیَّامَ [h:12400[ا]للّٰهِ لِیَ[q:5945[ج]ْزِیَ قَوْمً[i:12401[ا] [i:12401[ب]ِمَا كَانُوْ[s[ا] یَكْسِبُ[p[و]ْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 15,
      "text": "مَنْ عَمِلَ صَ[f:12053[ا]لِحًا فَلِنَفْسِهٖ ۚ وَ[g[م]َنْ اَسَآءَ فَعَلَیْهَا ؗ ثُمَّ اِلٰی رَبِّكُمْ تُرْجَعُوْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَلَقَ[q:341[د]ْ اٰتَیْنَا بَنِ[o[ی]ْۤ اِسْرَ[o[ا]ٓءِیْلَ [h:11906[ا]لْكِتٰبَ وَ[h:2046[ا]لْحُكْمَ وَ[h:2047[ا][l[ل][g[ن]ُّبُوَّةَ وَرَزَ[q:6191[ق]ْنٰهُ[w:6192[م]ْ [w:6192[م]ِّنَ [h:5307[ا][l[ل]طَّیِّبٰتِ وَفَضَّلْنٰهُمْ عَلَی [h:227[ا]لْعٰلَمِ[p[ی]ْنَ ۟ۚ",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَاٰتَیْنٰهُ[c:12402[م]ْ [c:12402[ب]َیِّنٰ[a:12403[ت]ٍ [a:12403[م]ِّنَ [h:2233[ا]لْاَمْرِ ۚ فَمَا [h:6193[ا]خْتَلَفُ[o[و]ْۤ[s[ا] اِلَّا مِ[i:146[ن]ْ [i:146[ب]َعْدِ مَا جَ[o[ا]ٓءَهُمُ [h:1804[ا]لْعِلْمُ ۙ بَغْیً[i:1169[ا] [i:1169[ب]َیْنَهُمْ ؕ اِ[g[ن]َّ رَبَّكَ یَ[q:6194[ق]ْضِیْ بَیْنَهُمْ یَوْمَ [h:592[ا]لْقِیٰمَةِ فِیْمَا كَانُوْ[s[ا] فِیْهِ یَخْتَلِفُ[p[و]ْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ثُ[g[م]َّ جَعَلْنٰكَ عَلٰی شَرِیْعَ[a:12404[ة]ٍ [a:12404[م]ِّنَ [h:2233[ا]لْاَمْرِ فَ[h:12405[ا]تَّبِعْهَا وَلَا تَتَّبِعْ اَهْوَ[o[ا]ٓءَ [h:4516[ا]لَّذِیْنَ لَا یَعْلَمُ[p[و]ْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اِ[g[ن]َّهُمْ لَ[a:12406[ن]ْ [a:12406[ی]ُّغْنُوْ[s[ا] عَ[f:626[ن]ْ[f:626[ك]َ مِنَ [h:321[ا]للّٰهِ شَیْـًٔ[a:12407[ا] ؕ [a:12407[و]َاِ[g[ن]َّ [h:9074[ا][l[ل]ظّٰلِمِیْنَ بَعْضُهُمْ اَوْلِیَ[o[ا]ٓءُ بَعْ[a:1863[ض]ٍ ۚ [a:1863[و]َ[h:72[ا]للّٰهُ وَلِیُّ [h:12408[ا]لْمُتَّقِ[p[ی]ْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 20,
      "text": "هٰذَا بَصَآ[a:4547[ى]ِٕرُ [u:5218[ل]ِلنَّاسِ [p[و]َهُدًی وَّرَحْمَةٌ لِّقَوْمٍ یُّوْقِنُوْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اَمْ حَسِبَ [h:10282[ا]لَّذِیْنَ [h:12409[ا][q:12410[ج]ْتَرَحُو[s[ا] [h:12411[ا][l[ل]سَّیِّاٰتِ اَ[a:12412[ن]ْ [a:12412[ن]َّ[q:12413[ج]ْعَلَهُمْ كَ[h:2146[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] وَعَمِلُو[s[ا] [h:118[ا][l[ل]صّٰلِحٰتِ ۙ سَوَ[o[ا]ٓ[a:12414[ء]ً [a:12414[م]َّحْیَاهُمْ وَمَمَاتُهُمْ ؕ سَ[o[ا]ٓءَ مَا یَحْكُمُ[p[و]ْنَ ۟۠",
      "juz": 25,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَخَلَقَ [h:12415[ا]للّٰهُ [h:10377[ا][l[ل]سَّمٰوٰتِ وَ[h:1474[ا]لْاَرْضَ بِ[h:370[ا]لْحَقِّ وَلِتُ[q:12416[ج]ْزٰی كُلُّ نَفْ[i:6995[س]ٍ [i:6995[ب]ِمَا كَسَبَتْ وَهُمْ لَا یُظْلَمُ[p[و]ْنَ ۟",
      "juz": 25,
      "page": 500,
      "ruku": 436,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اَفَرَءَیْتَ مَنِ [h:8418[ا]تَّخَذَ اِلٰهَهٗ هَوٰىهُ وَاَضَلَّهُ [h:12417[ا]للّٰهُ عَلٰی عِلْ[a:12418[م]ٍ [a:12418[و]َّخَتَمَ عَلٰی سَمْعِهٖ وَقَلْبِهٖ وَجَعَلَ عَلٰی بَصَرِهٖ غِشٰوَ[f:12419[ة]ً ؕ [f:12419[ف]َمَ[a:12420[ن]ْ [a:12420[ی]َّهْدِیْهِ مِ[i:146[ن]ْ [i:146[ب]َعْدِ [h:12421[ا]للّٰهِ ؕ اَفَلَا تَذَكَّرُ[p[و]ْنَ ۟",
