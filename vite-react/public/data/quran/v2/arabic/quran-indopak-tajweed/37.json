      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَ[h:9999[ا][l[ل]زّٰجِرٰتِ زَ[q:11312[ج]ْرًا ۟ۙ",
      "juz": 23,
      "page": 446,
