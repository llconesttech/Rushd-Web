      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِ[g[ن]َّ اِلٰهَكُمْ لَوَ[n[ا]حِ[q[د]ٌ ۟ؕ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 5,
      "text": "رَبُّ [h:6926[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ وَمَا بَیْنَهُمَا وَرَبُّ [h:11313[ا]لْمَشَارِ[q[ق]ِ ۟ؕ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِ[g[ن]َّا زَیَّ[g[ن]َّا [h:11314[ا][l[ل]سَّمَ[o[ا]ٓءَ [h:11315[ا][l[ل]دُّنْیَا بِزِیْنَةِ لْكَوَاكِ[q[ب]ِ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَحِفْ[a:11317[ظ]ً[a:11317[ا] [a:11317[م]ِّ[f:843[ن]ْ [f:843[ك]ُلِّ شَیْطٰ[a:11318[ن]ٍ [a:11318[م]َّارِ[q[د]ٍ ۟ۚ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 8,
      "text": "لَا یَسَّ[g[م]َّعُوْنَ اِلَی [h:1384[ا]لْمَلَاِ [h:11319[ا]لْاَعْلٰی وَیُ[q:11320[ق]ْذَفُوْنَ مِ[f:843[ن]ْ [f:843[ك]ُلِّ جَانِ[q[ب]ٍ ۟ۗۖ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 9,
      "text": "دُحُوْ[a:11321[ر]ً[a:11321[ا] [a:11321[و]َّلَهُمْ عَذَا[a:11322[ب]ٌ [a:11322[و]َّاصِ[q[ب]ٌ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اِلَّا مَنْ خَطِفَ [h:11323[ا]لْخَ[q:11324[ط]ْفَةَ فَاَتْبَعَهٗ شِهَا[f:11325[ب]ٌ [f:11325[ث]َاقِ[q[ب]ٌ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَ[h:9999[ا]سْتَفْتِهِمْ اَهُمْ اَشَدُّ خَلْقًا اَ[w:3110[م]ْ [w:3110[م]َّنْ خَلَ[q:5159[ق]ْنَ[o[ا] ؕ اِ[g[ن]َّا خَلَ[q:11326[ق]ْنٰهُ[w:11327[م]ْ [w:11327[م]ِّ[f:3917[ن]ْ [f:3917[ط]ِیْ[u:11328[ن]ٍ [u:11328[ل]َّازِ[q[ب]ٍ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 12,
      "text": "بَلْ عَجِ[q:11329[ب]ْتَ وَیَسْخَرُ[p[و]ْنَ ۪۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاِذَا ذُكِّرُوْ[s[ا] لَا یَذْكُرُ[p[و]ْنَ ۪۟",
