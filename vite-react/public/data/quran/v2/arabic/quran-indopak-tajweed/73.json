{
  "number": 73,
  "name": "سُورَةُ المُزَّمِّلِ",
  "englishName": "Al-Muzzammil",
  "ayahs": [
    {
      "number": 1,
      "text": "یٰۤاَیُّهَا [h:14020[ا]لْمُزَّ[g[م]ِّلُ ۟ۙ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 2,
      "text": "قُمِ [h:14021[ا]لَّیْلَ اِلَّا قَلِیْلًا ۟ۙ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 3,
      "text": "نِّصْفَهٗۤ اَوِ [h:14022[ا][f:14023[ن]ْ[f:14023[ق]ُصْ مِنْهُ قَلِیْلًا ۟ۙ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اَوْ زِ[q:14024[د]ْ عَلَیْهِ وَرَتِّلِ [h:14025[ا]لْقُرْاٰنَ تَرْتِیْلًا ۟ؕ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اِ[g[ن]َّا سَنُلْقِیْ عَلَیْكَ قَوْلً[f:14026[ا] [f:14026[ث]َقِیْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِ[g[ن]َّ نَاشِئَةَ [h:14027[ا]لَّیْلِ هِیَ اَشَدُّ وَ[q:14028[ط]ْ[a:14029[ا]ً [a:14029[و]َّاَ[q:1684[ق]ْوَمُ قِیْلًا ۟ؕ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِ[g[ن]َّ لَكَ فِی [h:1829[ا][l[ل][g[ن]َّهَارِ سَ[q:14030[ب]ْ[f:14031[ح]ً[f:14031[ا] [f:14031[ط]َوِیْلًا ۟ؕ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَ[h:9999[ا]ذْكُرِ [h:14032[ا]سْمَ رَبِّكَ وَتَبَتَّلْ اِلَیْهِ تَ[q:14033[ب]ْتِیْلًا ۟ؕ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 9,
      "text": "رَبُّ [h:9739[ا]لْمَشْرِقِ وَ[h:904[ا]لْمَغْرِبِ لَ[o[ا]ۤ اِلٰهَ اِلَّا هُوَ فَ[h:14034[ا]تَّخِذْهُ وَكِیْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَ[h:9999[ا]صْبِرْ عَلٰی مَا یَقُوْلُوْنَ وَ[h:14035[ا]هْجُرْهُمْ هَ[q:14036[ج]ْ[f:14037[ر]ً[f:14037[ا] [f:14037[ج]َمِیْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَذَرْنِیْ وَ[h:14038[ا]لْمُكَذِّبِیْنَ اُ[s[و]لِی [h:14039[ا][l[ل][g[ن]َّعْمَةِ وَمَهِّلْهُمْ قَلِیْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِ[g[ن]َّ لَدَیْنَ[o[ا]ۤ اَ[f:14040[ن]ْ[f:14040[ك]َالً[a:14041[ا] [a:14041[و]َّجَحِیْمًا ۟ۙ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَّطَعَا[f:14042[م]ً[f:14042[ا] [f:14042[ذ]َا غُصَّ[a:14043[ة]ٍ [a:14043[و]َّعَذَابًا اَلِیْمًا ۟ۗ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 14,
      "text": "یَوْمَ تَرْجُفُ [h:14044[ا]لْاَرْضُ وَ[h:8957[ا]لْجِبَالُ وَكَانَتِ [h:14045[ا]لْجِبَالُ كَثِیْ[a:14046[ب]ً[a:14046[ا] [a:14046[م]َّهِیْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِ[g[ن]َّ[o[ا]ۤ اَرْسَلْنَ[o[ا]ۤ اِلَیْكُمْ رَسُوْلً[f:14047[ا] ۙ۬ [f:14047[ش]َاهِدً[o[ا] عَلَیْكُمْ كَمَاۤ اَرْسَلْنَاۤ اِلٰی فِرْعَوْنَ رَسُوْلًا ۟ؕ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَعَصٰی فِرْعَوْنُ [h:14048[ا][l[ل]رَّسُوْلَ فَاَخَذْنٰهُ اَخْ[a:14049[ذ]ً[a:14049[ا] [a:14049[و]َّبِیْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَكَیْفَ تَتَّقُوْنَ اِ[f:14050[ن]ْ [f:14050[ك]َفَرْتُمْ یَوْ[a:14051[م]ً[a:14051[ا] [a:14051[ی]َّ[q:4388[ج]ْعَلُ [h:14052[ا]لْوِلْدَ[n[ا]نَ شِیْبَا ۟ۗۖ",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 18,
      "text": "[l[ل]سَّمَ[o[ا]ٓءُ مُ[f:14053[ن]ْ[f:14053[ف]َطِ[i:14054[ر]ٌ [i:14054[ب]ِهٖ ؕ كَانَ وَعْدُهٗ مَفْعُوْلًا ۟",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اِ[g[ن]َّ هٰذِهٖ تَذْكِرَ[f:14055[ة]ٌ ۚ [f:14055[ف]َمَ[f:8041[ن]ْ [f:8041[ش]َ[o[ا]ٓءَ [h:14056[ا]تَّخَذَ اِلٰی رَبِّهٖ سَبِیْلًا ۟۠",
      "juz": 29,
      "page": 574,
      "ruku": 508,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اِ[g[ن]َّ رَبَّكَ یَعْلَمُ اَ[g[ن]َّكَ تَقُوْمُ اَ[q:314[د]ْنٰی مِ[f:14057[ن]ْ  [h:14058[ا]لَّیْلِ وَنِصْفَهٗ وَثُلُثَهٗ وَطَ[o[ا]ٓىِٕفَ[a:14059[ة]ٌ [a:14059[م]ِّنَ [h:523[ا]لَّذِیْنَ مَعَكَ ؕ وَ[h:72[ا]للّٰهُ یُقَدِّرُ [h:14060[ا]لَّیْلَ وَ[h:6117[ا][l[ل][g[ن]َّهَارَ ؕ عَلِمَ اَ[u:8842[ن]ْ [u:8842[ل]َّ[f:14061[ن]ْ [f:14061[ت]ُحْصُوْهُ فَتَابَ عَلَیْكُمْ فَ[h:14062[ا][q:14062[ق]ْرَءُوْ[s[ا] مَا تَیَسَّرَ مِنَ [h:7870[ا]لْقُرْاٰنِ ؕ عَلِمَ اَ[f:14063[ن]ْ [f:14063[س]َیَكُوْنُ مِ[f:344[ن]ْ[f:344[ك]ُ[w:14064[م]ْ [w:14064[م]َّرْضٰی ۙ وَاٰخَرُوْنَ یَضْرِبُوْنَ فِی [h:44[ا]لْاَرْضِ یَ[q:3357[ب]ْتَغُوْنَ مِ[f:6655[ن]ْ [f:6655[ف]َضْلِ [h:6656[ا]للّٰهِ ۙ وَاٰخَرُوْنَ یُقَاتِلُوْنَ فِیْ سَبِیْلِ [h:788[ا]للّٰهِ ۖؗ فَ[h:14062[ا][q:14062[ق]ْرَءُوْ[s[ا] مَا تَیَسَّرَ مِنْهُ ۙ وَاَقِیْمُو[s[ا] [h:214[ا][l[ل]صَّلٰ[s[و]ةَ وَاٰتُو[s[ا] [h:215[ا][l[ل]زَّكٰ[s[و]ةَ وَاَ[q:14065[ق]ْرِضُو[s[ا] [h:14066[ا]للّٰهَ قَرْضًا حَسَ[a:6460[ن]ً[a:6460[ا] ؕ [a:6460[و]َمَا تُقَدِّمُوْ[s[ا] لِاَ[f:574[ن]ْ[f:574[ف]ُسِكُ[w:575[م]ْ [w:575[م]ِّنْ خَیْ[f:576[ر]ٍ [f:576[ت]َجِدُوْهُ عِ[f:264[ن]ْ[f:264[د]َ [h:408[ا]للّٰهِ هُوَ خَیْ[a:14067[ر]ً[a:14067[ا] [a:14067[و]َّاَعْظَمَ اَ[q:2795[ج]ْ[a:14068[ر]ً[a:14068[ا] ؕ [a:14068[و]َ[h:1101[ا]سْتَغْفِرُو[s[ا] [h:1102[ا]للّٰهَ ؕ اِ[g[ن]َّ [h:80[ا]للّٰهَ غَفُوْ[u:890[ر]ٌ [u:890[ر]َّحِ[p[ی]ْمٌ ۟۠",
      "juz": 29,
      "page": 575,
      "ruku": 509,
      "hizbQuarter": 230,
      "sajda": false
    }
  ]
}