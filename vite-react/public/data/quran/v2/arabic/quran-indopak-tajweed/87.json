      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَ[h:9999[ا]لَّذِیْ قَدَّرَ فَهَدٰی ۟",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَ[h:9999[ا]لَّذِ[o[ی]ْۤ اَخْرَجَ [h:14439[ا]لْمَرْعٰی ۟",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَجَعَلَهٗ غُثَ[o[ا]ٓءً اَحْوٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "سَنُ[q:14440[ق]ْرِئُكَ فَلَا تَ[f:14441[ن]ْ[f:14441[س]ٰۤی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِلَّا مَا شَ[o[ا]ٓءَ [h:78[ا]للّٰهُ ؕ اِ[g[ن]َّهٗ یَعْلَمُ [h:8891[ا]لْجَهْرَ وَمَا یَخْفٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَنُیَسِّرُكَ لِلْیُسْرٰی ۟ۚۖ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَذَكِّرْ اِ[a:14442[ن]ْ [a:14442[ن]َّفَعَتِ [h:14443[ا][l[ل]ذِّكْرٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "سَیَذَّكَّرُ مَ[a:14444[ن]ْ [a:14444[ی]َّخْشٰی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَیَتَجَ[g[ن]َّبُهَا [h:14445[ا]لْاَشْقَی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "الَّذِیْ یَصْلَی [h:14446[ا][l[ل][g[ن]َّارَ [h:14447[ا]لْكُ[q:8463[ب]ْرٰی ۟ۚ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُ[g[م]َّ لَا یَمُوْتُ فِیْهَا وَلَا یَحْیٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قَ[q:298[د]ْ اَفْلَحَ مَ[f:8555[ن]ْ [f:8555[ت]َزَكّٰی ۟ۙ",
      "juz": 30,
