{
  "number": 15,
  "name": "سُورَةُ الحِجۡرِ",
  "englishName": "Al-Hijr",
  "ayahs": [
    {
      "number": 1,
      "text": "ا[m[ل]ٓرٰ ۫ تِلْكَ اٰیٰتُ [h:5932[ا]لْكِتٰبِ وَقُرْاٰ[a:7193[ن]ٍ [a:7193[م]ُّبِ[p[ی]ْنٍ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 2,
      "text": "رُبَمَا یَوَدُّ [h:544[ا]لَّذِیْنَ كَفَرُوْ[s[ا] لَوْ كَانُوْ[s[ا] مُسْلِمِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ذَرْهُمْ یَاْكُلُوْ[s[ا] وَیَتَمَتَّعُوْ[s[ا] وَیُلْهِهِمُ [h:7194[ا]لْاَمَلُ فَسَوْفَ یَعْلَمُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَمَ[o[ا]ۤ اَهْلَكْنَا مِ[f:4585[ن]ْ [f:4585[ق]َرْیَةٍ اِلَّا وَلَهَا كِتَا[a:7195[ب]ٌ [a:7195[م]َّعْلُ[p[و]ْمٌ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 5,
      "text": "مَا تَسْبِقُ مِنْ اُ[g[م]َّةٍ اَجَلَهَا وَمَا یَسْتَاْخِرُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَقَالُوْ[s[ا] یٰۤاَیُّهَا [h:7196[ا]لَّذِیْ نُزِّلَ عَلَیْهِ [h:7197[ا][l[ل]ذِّكْرُ اِ[g[ن]َّكَ لَمَ[q:7198[ج]ْنُ[p[و]ْنٌ ۟ؕ",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لَوْ مَا تَاْتِیْنَا بِ[h:7199[ا]لْمَلٰٓىِٕكَةِ اِ[f:4792[ن]ْ [f:4792[ك]ُ[f:735[ن]ْ[f:735[ت]َ مِنَ [h:4793[ا][l[ل]صّٰدِقِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 8,
      "text": "مَا نُنَزِّلُ [h:7200[ا]لْمَلٰٓىِٕكَةَ اِلَّا بِ[h:370[ا]لْحَقِّ وَمَا كَانُ[o[و]ْۤ[s[ا] اِ[a:7201[ذ]ً[a:7201[ا] [a:7201[م]ُّ[f:7202[ن]ْ[f:7202[ظ]َرِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اِ[g[ن]َّا نَحْنُ نَزَّلْنَا [h:7203[ا][l[ل]ذِّكْرَ وَاِ[g[ن]َّا لَهٗ لَحٰفِظُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَقَ[q:341[د]ْ اَرْسَلْنَا مِ[f:18[ن]ْ [f:18[ق]َ[q:19[ب]ْلِكَ فِیْ شِیَعِ [h:7204[ا]لْاَوَّلِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَمَا یَاْتِیْهِ[w:7205[م]ْ [w:7205[م]ِّ[u:2882[ن]ْ [u:2882[ر]َّسُوْلٍ اِلَّا كَانُوْ[s[ا] بِهٖ یَسْتَهْزِءُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 12,
      "text": "كَذٰلِكَ نَسْلُكُهٗ فِیْ قُلُوْبِ [h:7206[ا]لْمُ[q:4093[ج]ْرِمِ[p[ی]ْنَ ۟ۙ",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 13,
      "text": "لَا یُؤْمِنُوْنَ بِهٖ وَقَ[q:387[د]ْ خَلَتْ سُ[g[ن]َّةُ [h:7207[ا]لْاَوَّلِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَلَوْ فَتَحْنَا عَلَیْهِ[c:7208[م]ْ [c:7208[ب]َا[a:7209[ب]ً[a:7209[ا] [a:7209[م]ِّنَ [h:64[ا][l[ل]سَّمَ[o[ا]ٓءِ فَظَلُّوْ[s[ا] فِیْهِ یَعْرُجُ[p[و]ْنَ ۟ۙ",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 15,
      "text": "لَقَالُ[o[و]ْۤ[s[ا] اِ[g[ن]َّمَا سُكِّرَتْ اَ[q:7210[ب]ْصَارُنَا بَلْ نَحْنُ قَوْمٌ مَّسْحُوْرُوْنَ ۟۠",
      "juz": 14,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَلَقَ[q:341[د]ْ جَعَلْنَا فِی [h:746[ا][l[ل]سَّمَ[o[ا]ٓءِ بُرُوْ[a:7212[ج]ً[a:7212[ا] [a:7212[و]َّزَیَّ[g[ن]ّٰهَا لِل[g[ن]ّٰظِرِ[p[ی]ْنَ ۟ۙ",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَحَفِظْنٰهَا مِ[f:843[ن]ْ [f:843[ك]ُلِّ شَیْطٰ[u:7213[ن]ٍ [u:7213[ر]َّجِ[p[ی]ْمٍ ۟ۙ",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اِلَّا مَنِ [h:7214[ا]سْتَرَقَ [h:7215[ا][l[ل]سَّمْعَ فَاَتْبَعَهٗ شِهَا[a:7216[ب]ٌ [a:7216[م]ُّبِ[p[ی]ْنٌ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَ[h:9999[ا]لْاَرْضَ مَدَ[q:7217[د]ْنٰهَا وَاَلْقَیْنَا فِیْهَا رَوَ[n[ا]سِیَ وَاَ[i:7218[ن]ْۢ[i:7218[ب]َتْنَا فِیْهَا مِ[f:843[ن]ْ [f:843[ك]ُلِّ شَ[a:7219[ی]ْ[a:7219[ء]ٍ [a:7219[م]َّوْزُ[p[و]ْنٍ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَجَعَلْنَا لَكُمْ فِیْهَا مَعَایِشَ وَمَ[u:7220[ن]ْ [u:7220[ل]َّسْتُمْ لَهٗ بِرٰزِقِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَاِ[a:3281[ن]ْ [a:3281[م]ِّ[f:2103[ن]ْ [f:2103[ش]َیْءٍ اِلَّا عِ[f:2359[ن]ْ[f:2359[د]َنَا خَزَ[o[ا]ٓىِٕنُهٗ ؗ وَمَا نُنَزِّلُهٗۤ اِلَّا بِقَدَ[a:7221[ر]ٍ [a:7221[م]َّعْلُ[p[و]ْمٍ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَاَرْسَلْنَا [h:7222[ا][l[ل]رِّیٰحَ لَوَ[n[ا]قِحَ فَاَ[f:288[ن]ْ[f:288[ز]َلْنَا مِنَ [h:64[ا][l[ل]سَّمَ[o[ا]ٓءِ مَ[o[ا]ٓ[f:7223[ء]ً [f:7223[ف]َاَسْقَیْنٰكُمُوْهُ ۚ وَمَ[o[ا]ۤ اَ[f:428[ن]ْ[f:428[ت]ُمْ لَهٗ بِخٰزِنِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَاِ[g[ن]َّا لَنَحْنُ نُحْیٖ وَنُمِیْتُ وَنَحْنُ [h:7224[ا]لْوٰرِثُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَلَقَ[q:341[د]ْ عَلِمْنَا [h:7225[ا]لْمُسْتَ[q:7226[ق]ْدِمِیْنَ مِ[f:344[ن]ْ[f:344[ك]ُمْ وَلَقَ[q:341[د]ْ عَلِمْنَا [h:7227[ا]لْمُسْتَاْخِرِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَاِ[g[ن]َّ رَبَّكَ هُوَ یَحْشُرُهُمْ ؕ اِ[g[ن]َّهٗ حَكِیْمٌ عَلِ[p[ی]ْمٌ ۟۠",
      "juz": 14,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلَقَ[q:341[د]ْ خَلَ[q:5159[ق]ْنَا [h:7228[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ مِ[f:7229[ن]ْ [f:7229[ص]َلْصَ[a:7231[ا]لٍ [a:7231[م]ِّنْ حَمَاٍ مَّسْنُوْنٍ ۟ۚ",
      "juz": 14,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَ[h:9999[ا]لْجَ[m[ا]ٓ[g[ن]َّ خَلَ[q:7232[ق]ْنٰهُ مِ[f:128[ن]ْ [f:128[ق]َ[q:129[ب]ْلُ مِ[a:7233[ن]ْ [a:7233[ن]َّارِ [h:7234[ا][l[ل]سَّمُ[p[و]ْمِ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَاِذْ قَالَ رَبُّكَ لِلْمَلٰٓىِٕكَةِ اِ[g[ن]ِّیْ خَ[a:7236[ا]لِقٌۢ بَشَرً[a:7231[ا] [a:7231[م]ِّنْ صَلْصَالٍ مِّنْ حَمَاٍ مَّسْنُوْنٍ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَاِذَا سَوَّیْتُهٗ وَنَفَخْتُ فِیْهِ مِ[u:7237[ن]ْ [u:7237[ر]ُّوْحِیْ فَقَعُوْ[s[ا] لَهٗ سٰجِدِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَسَجَدَ [h:7238[ا]لْمَلٰٓىِٕكَةُ كُلُّهُمْ اَ[q:7239[ج]ْمَعُ[p[و]ْنَ ۟ۙ",
      "juz": 14,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اِلَّ[o[ا]ۤ اِ[q:180[ب]ْلِیْسَ ؕ اَبٰۤ[o[ی] اَ[a:3312[ن]ْ [a:3312[ی]َّكُوْنَ مَعَ [h:7240[ا][l[ل]سّٰجِدِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 32,
      "text": "قَالَ یٰۤاِ[q:7241[ب]ْلِیْسُ مَا لَكَ اَلَّا تَكُوْنَ مَعَ [h:7240[ا][l[ل]سّٰجِدِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 33,
      "text": "قَالَ لَمْ اَكُ[u:7242[ن]ْ [u:7242[ل]ِّاَسْجُدَ لِبَشَرٍ خَلَ[q:7243[ق]ْتَهٗ مِ[f:7229[ن]ْ [f:7229[ص]َلْصَ[a:7231[ا]لٍ [a:7231[م]ِّنْ حَمَاٍ مَّسْنُوْنٍ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 34,
      "text": "قَالَ فَ[h:4603[ا]خْرُ[q:4603[ج]ْ مِنْهَا فَاِ[g[ن]َّكَ رَجِ[p[ی]ْمٌ ۟ۙ",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 35,
      "text": "وَّاِ[g[ن]َّ عَلَیْكَ [h:7244[ا][l[ل]لَّعْنَةَ اِلٰی یَوْمِ [h:5[ا][l[ل]دِّ[p[ی]ْنِ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 36,
      "text": "قَالَ رَبِّ فَاَ[f:7245[ن]ْ[f:7245[ظ]ِرْنِ[o[ی]ْۤ اِلٰی یَوْمِ یُ[q:4606[ب]ْعَثُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 37,
      "text": "قَالَ فَاِ[g[ن]َّكَ مِنَ [h:4607[ا]لْمُ[f:4608[ن]ْ[f:4608[ظ]َرِ[p[ی]ْنَ ۟ۙ",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اِلٰی یَوْمِ [h:7246[ا]لْوَ[q:7247[ق]ْتِ [h:7248[ا]لْمَعْلُ[p[و]ْمِ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 39,
      "text": "قَالَ رَبِّ بِمَ[o[ا]ۤ اَغْوَیْتَنِیْ لَاُزَیِّنَ[g[ن]َّ لَهُمْ فِی [h:44[ا]لْاَرْضِ وَلَاُغْوِیَ[g[ن]َّهُمْ اَ[q:825[ج]ْمَعِ[p[ی]ْنَ ۟ۙ",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اِلَّا عِبَادَكَ مِنْهُمُ [h:7249[ا]لْمُخْلَصِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 41,
      "text": "قَالَ هٰذَا صِرَ[n[ا]طٌ عَلَیَّ مُسْتَقِ[p[ی]ْمٌ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 42,
      "text": "اِ[g[ن]َّ عِبَادِیْ لَیْسَ لَكَ عَلَیْهِمْ سُلْطٰنٌ اِلَّا مَنِ [h:7250[ا]تَّبَعَكَ مِنَ [h:5135[ا]لْغٰوِ[p[ی]ْنَ ۟",
      "juz": 14,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
