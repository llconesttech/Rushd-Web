{
  "number": 91,
  "name": "سُورَةُ الشَّمۡسِ",
  "englishName": "Ash-Shams",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالشَّمْسِ وَضُحٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالْقَمَرِ اِذَا تَلٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالنَّهَارِ اِذَا جَلّٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّیْلِ اِذَا یَغْشٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَالسَّمَآءِ وَمَا بَنٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَالْاَرْضِ وَمَا طَحٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَنَفْسٍ وَّمَا سَوّٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَاَلْهَمَهَا فُجُوْرَهَا وَتَقْوٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "قَدْ اَفْلَحَ مَنْ زَكّٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَقَدْ خَابَ مَنْ دَسّٰىهَا ۟ؕ",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَذَّبَتْ ثَمُوْدُ بِطَغْوٰىهَاۤ ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِذِ انْۢبَعَثَ اَشْقٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَقَالَ لَهُمْ رَسُوْلُ اللّٰهِ نَاقَةَ اللّٰهِ وَسُقْیٰهَا ۟ؕ",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَكَذَّبُوْهُ فَعَقَرُوْهَا فَدَمْدَمَ عَلَیْهِمْ رَبُّهُمْ بِذَنْۢبِهِمْ فَسَوّٰىهَا ۟",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَا یَخَافُ عُقْبٰهَا ۟۠",
      "juz": 30,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}