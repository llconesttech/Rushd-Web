{
  "number": 84,
  "name": "سُورَةُ الانشِقَاقِ",
  "englishName": "Al-Inshiqaaq",
  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا السَّمَآءُ انْشَقَّتْ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاَذِنَتْ لِرَبِّهَا وَحُقَّتْ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِذَا الْاَرْضُ مُدَّتْ ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاَلْقَتْ مَا فِیْهَا وَتَخَلَّتْ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَاَذِنَتْ لِرَبِّهَا وَحُقَّتْ ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یٰۤاَیُّهَا الْاِنْسَانُ اِنَّكَ كَادِحٌ اِلٰی رَبِّكَ كَدْحًا فَمُلٰقِیْهِ ۟ۚ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَاَمَّا مَنْ اُوْتِیَ كِتٰبَهٗ بِیَمِیْنِهٖ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَسَوْفَ یُحَاسَبُ حِسَابًا یَّسِیْرًا ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَّیَنْقَلِبُ اِلٰۤی اَهْلِهٖ مَسْرُوْرًا ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاَمَّا مَنْ اُوْتِیَ كِتٰبَهٗ وَرَآءَ ظَهْرِهٖ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَسَوْفَ یَدْعُوْا ثُبُوْرًا ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَّیَصْلٰی سَعِیْرًا ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِنَّهٗ كَانَ فِیْۤ اَهْلِهٖ مَسْرُوْرًا ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اِنَّهٗ ظَنَّ اَنْ لَّنْ یَّحُوْرَ ۟ۚۛ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "بَلٰۤی ۛۚ اِنَّ رَبَّهٗ كَانَ بِهٖ بَصِیْرًا ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَلَاۤ اُقْسِمُ بِالشَّفَقِ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَالَّیْلِ وَمَا وَسَقَ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَالْقَمَرِ اِذَا اتَّسَقَ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 19,
      "text": "لَتَرْكَبُنَّ طَبَقًا عَنْ طَبَقٍ ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَمَا لَهُمْ لَا یُؤْمِنُوْنَ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَاِذَا قُرِئَ عَلَیْهِمُ الْقُرْاٰنُ لَا یَسْجُدُوْنَ ۟",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": {
        "id": 14,
        "recommended": true,
        "obligatory": false
      }
    },
    {
      "number": 22,
      "text": "بَلِ الَّذِیْنَ كَفَرُوْا یُكَذِّبُوْنَ ۟ؗۖ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَاللّٰهُ اَعْلَمُ بِمَا یُوْعُوْنَ ۟ؗۖ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَبَشِّرْهُمْ بِعَذَابٍ اَلِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اِلَّا الَّذِیْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ لَهُمْ اَجْرٌ غَیْرُ مَمْنُوْنٍ ۟۠",
      "juz": 30,
      "page": 589,
      "ruku": 526,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}