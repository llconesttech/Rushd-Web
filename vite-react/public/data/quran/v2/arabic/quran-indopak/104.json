{
  "number": 104,
  "name": "سُورَةُ الهُمَزَةِ",
  "englishName": "Al-Humaza",
  "ayahs": [
    {
      "number": 1,
      "text": "وَیْلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةِ ۟ۙ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "لَّذِیْ جَمَعَ مَالًا وَّعَدَّدَهٗ ۟ۙ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "یَحْسَبُ اَنَّ مَالَهٗۤ اَخْلَدَهٗ ۟ۚ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَلَّا لَیُنْۢبَذَنَّ فِی الْحُطَمَةِ ۟ؗۖ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَمَاۤ اَدْرٰىكَ مَا الْحُطَمَةُ ۟ؕ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "نَارُ اللّٰهِ الْمُوْقَدَةُ ۟ۙ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "الَّتِیْ تَطَّلِعُ عَلَی الْاَفْـِٕدَةِ ۟ؕ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِنَّهَا عَلَیْهِمْ مُّؤْصَدَةٌ ۟ۙ",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فِیْ عَمَدٍ مُّمَدَّدَةٍ ۟۠",
      "juz": 30,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
