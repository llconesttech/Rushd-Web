{
  "number": 99,
  "name": "سُورَةُ الزَّلۡزَلَةِ",
  "englishName": "Az-Zalzala",
  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا زُلْزِلَتِ الْاَرْضُ زِلْزَالَهَا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاَخْرَجَتِ الْاَرْضُ اَثْقَالَهَا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَقَالَ الْاِنْسَانُ مَا لَهَا ۟ۚ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "یَوْمَىِٕذٍ تُحَدِّثُ اَخْبَارَهَا ۟ؕ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بِاَنَّ رَبَّكَ اَوْحٰی لَهَا ۟ؕ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَوْمَىِٕذٍ یَّصْدُرُ النَّاسُ اَشْتَاتًا ۙ۬ لِّیُرَوْا اَعْمَالَهُمْ ۟ؕ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَمَنْ یَّعْمَلْ مِثْقَالَ ذَرَّةٍ خَیْرًا یَّرَهٗ ۟ؕ",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَمَنْ یَّعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا یَّرَهٗ ۟۠",
      "juz": 30,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}