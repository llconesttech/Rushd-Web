{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "ayahs": [
    {
      "number": 1,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ الْغَاشِیَةِ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وُجُوْهٌ یَّوْمَىِٕذٍ خَاشِعَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "عَامِلَةٌ نَّاصِبَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَصْلٰی نَارًا حَامِیَةً ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تُسْقٰی مِنْ عَیْنٍ اٰنِیَةٍ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "لَیْسَ لَهُمْ طَعَامٌ اِلَّا مِنْ ضَرِیْعٍ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لَّا یُسْمِنُ وَلَا یُغْنِیْ مِنْ جُوْعٍ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وُجُوْهٌ یَّوْمَىِٕذٍ نَّاعِمَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "لِّسَعْیِهَا رَاضِیَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فِیْ جَنَّةٍ عَالِیَةٍ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "لَّا تَسْمَعُ فِیْهَا لَاغِیَةً ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فِیْهَا عَیْنٌ جَارِیَةٌ ۟ۘ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فِیْهَا سُرُرٌ مَّرْفُوْعَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّاَكْوَابٌ مَّوْضُوْعَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَّنَمَارِقُ مَصْفُوْفَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَّزَرَابِیُّ مَبْثُوْثَةٌ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اَفَلَا یَنْظُرُوْنَ اِلَی الْاِبِلِ كَیْفَ خُلِقَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَاِلَی السَّمَآءِ كَیْفَ رُفِعَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَاِلَی الْجِبَالِ كَیْفَ نُصِبَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَاِلَی الْاَرْضِ كَیْفَ سُطِحَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَذَكِّرْ ؕ۫ اِنَّمَاۤ اَنْتَ مُذَكِّرٌ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لَسْتَ عَلَیْهِمْ بِمُصَۜیْطِرٍ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِلَّا مَنْ تَوَلّٰی وَكَفَرَ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَیُعَذِّبُهُ اللّٰهُ الْعَذَابَ الْاَكْبَرَ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اِنَّ اِلَیْنَاۤ اِیَابَهُمْ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ثُمَّ اِنَّ عَلَیْنَا حِسَابَهُمْ ۟۠",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}