{
  "number": 95,
  "name": "سُورَةُ التِّينِ",
  "englishName": "At-Tin",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالتِّیْنِ وَالزَّیْتُوْنِ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَطُوْرِ سِیْنِیْنَ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَهٰذَا الْبَلَدِ الْاَمِیْنِ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "لَقَدْ خَلَقْنَا الْاِنْسَانَ فِیْۤ اَحْسَنِ تَقْوِیْمٍ ۟ؗ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ثُمَّ رَدَدْنٰهُ اَسْفَلَ سٰفِلِیْنَ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِلَّا الَّذِیْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ فَلَهُمْ اَجْرٌ غَیْرُ مَمْنُوْنٍ ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَمَا یُكَذِّبُكَ بَعْدُ بِالدِّیْنِ ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اَلَیْسَ اللّٰهُ بِاَحْكَمِ الْحٰكِمِیْنَ ۟۠",
      "juz": 30,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}