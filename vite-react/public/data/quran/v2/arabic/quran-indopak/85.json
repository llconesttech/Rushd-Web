{
  "number": 85,
  "name": "سُورَةُ البُرُوجِ",
  "englishName": "Al-Burooj",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالسَّمَآءِ ذَاتِ الْبُرُوْجِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالْیَوْمِ الْمَوْعُوْدِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَشَاهِدٍ وَّمَشْهُوْدٍ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "قُتِلَ اَصْحٰبُ الْاُخْدُوْدِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "النَّارِ ذَاتِ الْوَقُوْدِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِذْ هُمْ عَلَیْهَا قُعُوْدٌ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّهُمْ عَلٰی مَا یَفْعَلُوْنَ بِالْمُؤْمِنِیْنَ شُهُوْدٌ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَمَا نَقَمُوْا مِنْهُمْ اِلَّاۤ اَنْ یُّؤْمِنُوْا بِاللّٰهِ الْعَزِیْزِ الْحَمِیْدِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "الَّذِیْ لَهٗ مُلْكُ السَّمٰوٰتِ وَالْاَرْضِ ؕ وَاللّٰهُ عَلٰی كُلِّ شَیْءٍ شَهِیْدٌ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اِنَّ الَّذِیْنَ فَتَنُوا الْمُؤْمِنِیْنَ وَالْمُؤْمِنٰتِ ثُمَّ لَمْ یَتُوْبُوْا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِیْقِ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اِنَّ الَّذِیْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ لَهُمْ جَنّٰتٌ تَجْرِیْ مِنْ تَحْتِهَا الْاَنْهٰرُ ؔؕ۬ ذٰلِكَ الْفَوْزُ الْكَبِیْرُ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِنَّ بَطْشَ رَبِّكَ لَشَدِیْدٌ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِنَّهٗ هُوَ یُبْدِئُ وَیُعِیْدُ ۟ۚ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَهُوَ الْغَفُوْرُ الْوَدُوْدُ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "ذُو الْعَرْشِ الْمَجِیْدُ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَعَّالٌ لِّمَا یُرِیْدُ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ الْجُنُوْدِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فِرْعَوْنَ وَثَمُوْدَ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 19,
      "text": "بَلِ الَّذِیْنَ كَفَرُوْا فِیْ تَكْذِیْبٍ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَّاللّٰهُ مِنْ وَّرَآىِٕهِمْ مُّحِیْطٌ ۟ۚ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 21,
      "text": "بَلْ هُوَ قُرْاٰنٌ مَّجِیْدٌ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 22,
      "text": "فِیْ لَوْحٍ مَّحْفُوْظٍ ۟۠",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}