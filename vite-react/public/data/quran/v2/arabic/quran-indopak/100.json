  "ayahs": [
    {
      "number": 1,
      "text": "وَالْعٰدِیٰتِ ضَبْحًا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالْمُوْرِیٰتِ قَدْحًا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فَالْمُغِیْرٰتِ صُبْحًا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَاَثَرْنَ بِهٖ نَقْعًا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَوَسَطْنَ بِهٖ جَمْعًا ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِنَّ الْاِنْسَانَ لِرَبِّهٖ لَكَنُوْدٌ ۟ۚ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَاِنَّهٗ عَلٰی ذٰلِكَ لَشَهِیْدٌ ۟ۚ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَاِنَّهٗ لِحُبِّ الْخَیْرِ لَشَدِیْدٌ ۟ؕ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اَفَلَا یَعْلَمُ اِذَا بُعْثِرَ مَا فِی الْقُبُوْرِ ۟ۙ",
      "juz": 30,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَحُصِّلَ مَا فِی الصُّدُوْرِ ۟ۙ",
      "juz": 30,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اِنَّ رَبَّهُمْ بِهِمْ یَوْمَىِٕذٍ لَّخَبِیْرٌ ۟۠",
      "juz": 30,
      "page": 600,
      "ruku": 542,
