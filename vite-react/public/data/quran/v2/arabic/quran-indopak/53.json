{
  "number": 53,
  "name": "سُورَةُ النَّجۡمِ",
  "englishName": "An-Najm",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالنَّجْمِ اِذَا هَوٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَا ضَلَّ صَاحِبُكُمْ وَمَا غَوٰی ۟ۚ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَا یَنْطِقُ عَنِ الْهَوٰی ۟ؕ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِنْ هُوَ اِلَّا وَحْیٌ یُّوْحٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلَّمَهٗ شَدِیْدُ الْقُوٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ذُوْ مِرَّةٍ ؕ فَاسْتَوٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَهُوَ بِالْاُفُقِ الْاَعْلٰی ۟ؕ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ثُمَّ دَنَا فَتَدَلّٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَكَانَ قَابَ قَوْسَیْنِ اَوْ اَدْنٰی ۟ۚ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَاَوْحٰۤی اِلٰی عَبْدِهٖ مَاۤ اَوْحٰی ۟ؕ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 11,
      "text": "مَا كَذَبَ الْفُؤَادُ مَا رَاٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اَفَتُمٰرُوْنَهٗ عَلٰی مَا یَرٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَلَقَدْ رَاٰهُ نَزْلَةً اُخْرٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 14,
      "text": "عِنْدَ سِدْرَةِ الْمُنْتَهٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 15,
      "text": "عِنْدَهَا جَنَّةُ الْمَاْوٰی ۟ؕ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اِذْ یَغْشَی السِّدْرَةَ مَا یَغْشٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 17,
      "text": "مَا زَاغَ الْبَصَرُ وَمَا طَغٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 18,
      "text": "لَقَدْ رَاٰی مِنْ اٰیٰتِ رَبِّهِ الْكُبْرٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اَفَرَءَیْتُمُ اللّٰتَ وَالْعُزّٰی ۟ۙ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَمَنٰوةَ الثَّالِثَةَ الْاُخْرٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اَلَكُمُ الذَّكَرُ وَلَهُ الْاُ ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 22,
      "text": "تِلْكَ اِذًا قِسْمَةٌ ضِیْزٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِنْ هِیَ اِلَّاۤ اَسْمَآءٌ سَمَّیْتُمُوْهَاۤ اَنْتُمْ وَاٰبَآؤُكُمْ مَّاۤ اَنْزَلَ اللّٰهُ بِهَا مِنْ سُلْطٰنٍ ؕ اِنْ یَّتَّبِعُوْنَ اِلَّا الظَّنَّ وَمَا تَهْوَی الْاَنْفُسُ ۚ وَلَقَدْ جَآءَهُمْ مِّنْ رَّبِّهِمُ الْهُدٰی ۟ؕ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اَمْ لِلْاِنْسَانِ مَا تَمَنّٰی ۟ؗۖ",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَلِلّٰهِ الْاٰخِرَةُ وَالْاُوْلٰی ۟۠",
      "juz": 27,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَكَمْ مِّنْ مَّلَكٍ فِی السَّمٰوٰتِ لَا تُغْنِیْ شَفَاعَتُهُمْ شَیْـًٔا اِلَّا مِنْ بَعْدِ اَنْ یَّاْذَنَ اللّٰهُ لِمَنْ یَّشَآءُ وَیَرْضٰی ۟",
      "juz": 27,
      "page": 526,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اِنَّ الَّذِیْنَ لَا یُؤْمِنُوْنَ بِالْاٰخِرَةِ لَیُسَمُّوْنَ الْمَلٰٓىِٕكَةَ تَسْمِیَةَ الْاُ ۟",
      "juz": 27,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَمَا لَهُمْ بِهٖ مِنْ عِلْمٍ ؕ اِنْ یَّتَّبِعُوْنَ اِلَّا الظَّنَّ ۚ وَاِنَّ الظَّنَّ لَا یُغْنِیْ مِنَ الْحَقِّ شَیْـًٔا ۟ۚ",
      "juz": 27,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَاَعْرِضْ عَنْ مَّنْ تَوَلّٰی ۙ۬ عَنْ ذِكْرِنَا وَلَمْ یُرِدْ اِلَّا الْحَیٰوةَ الدُّنْیَا ۟ؕ",
      "juz": 27,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 30,
      "text": "ذٰلِكَ مَبْلَغُهُمْ مِّنَ الْعِلْمِ ؕ اِنَّ رَبَّكَ هُوَ اَعْلَمُ بِمَنْ ضَلَّ عَنْ سَبِیْلِهٖ ۙ وَهُوَ اَعْلَمُ بِمَنِ اهْتَدٰی ۟",
      "juz": 27,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وَلِلّٰهِ مَا فِی السَّمٰوٰتِ وَمَا فِی الْاَرْضِ ۙ لِیَجْزِیَ الَّذِیْنَ اَسَآءُوْا بِمَا عَمِلُوْا وَیَجْزِیَ الَّذِیْنَ اَحْسَنُوْا بِالْحُسْنٰی ۟ۚ",
      "juz": 27,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اَلَّذِیْنَ یَجْتَنِبُوْنَ كَبٰٓىِٕرَ الْاِثْمِ وَالْفَوَاحِشَ اِلَّا اللَّمَمَ ؕ اِنَّ رَبَّكَ وَاسِعُ الْمَغْفِرَةِ ؕ هُوَ اَعْلَمُ بِكُمْ اِذْ اَنْشَاَكُمْ مِّنَ الْاَرْضِ وَاِذْ اَنْتُمْ اَجِنَّةٌ فِیْ بُطُوْنِ اُمَّهٰتِكُمْ ۚ فَلَا تُزَكُّوْۤا اَنْفُسَكُمْ ؕ هُوَ اَعْلَمُ بِمَنِ اتَّقٰی ۟۠",
      "juz": 27,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اَفَرَءَیْتَ الَّذِیْ تَوَلّٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَاَعْطٰی قَلِیْلًا وَّاَكْدٰی ۟",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اَعِنْدَهٗ عِلْمُ الْغَیْبِ فَهُوَ یَرٰی ۟",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اَمْ لَمْ یُنَبَّاْ بِمَا فِیْ صُحُفِ مُوْسٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَاِبْرٰهِیْمَ الَّذِیْ وَ ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اَلَّا تَزِرُ وَازِرَةٌ وِّزْرَ اُخْرٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَاَنْ لَّیْسَ لِلْاِنْسَانِ اِلَّا مَا سَعٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَاَنَّ سَعْیَهٗ سَوْفَ یُرٰی ۪۟",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 41,
      "text": "ثُمَّ یُجْزٰىهُ الْجَزَآءَ الْاَوْفٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وَاَنَّ اِلٰی رَبِّكَ الْمُنْتَهٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وَاَنَّهٗ هُوَ اَضْحَكَ وَاَبْكٰی ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وَاَنَّهٗ هُوَ اَمَاتَ وَاَحْیَا ۟ۙ",
      "juz": 27,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَاَنَّهٗ خَلَقَ الزَّوْجَیْنِ الذَّكَرَ وَالْاُ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 46,
      "text": "مِنْ نُّطْفَةٍ اِذَا تُمْنٰی ۪۟",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَاَنَّ عَلَیْهِ النَّشْاَةَ الْاُخْرٰی ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَاَنَّهٗ هُوَ اَغْنٰی وَاَقْنٰی ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَاَنَّهٗ هُوَ رَبُّ الشِّعْرٰی ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَاَنَّهٗۤ اَهْلَكَ عَادَا لْاُوْلٰی ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَثَمُوْدَاۡ فَمَاۤ اَبْقٰی ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَقَوْمَ نُوْحٍ مِّنْ قَبْلُ ؕ اِنَّهُمْ كَانُوْا هُمْ اَظْلَمَ وَاَطْغٰی ۟ؕ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 53,
      "text": "وَالْمُؤْتَفِكَةَ اَهْوٰی ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فَغَشّٰىهَا مَا غَشّٰی ۟ۚ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكَ تَتَمَارٰی ۟",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 56,
      "text": "هٰذَا نَذِیْرٌ مِّنَ النُّذُرِ الْاُوْلٰی ۟",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 57,
      "text": "اَزِفَتِ الْاٰزِفَةُ ۟ۚ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 58,
      "text": "لَیْسَ لَهَا مِنْ دُوْنِ اللّٰهِ كَاشِفَةٌ ۟ؕ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 59,
      "text": "اَفَمِنْ هٰذَا الْحَدِیْثِ تَعْجَبُوْنَ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 60,
