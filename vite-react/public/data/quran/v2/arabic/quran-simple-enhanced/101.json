      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَا أَدْرَاكَ مَا الْقَارِعَةُ",
