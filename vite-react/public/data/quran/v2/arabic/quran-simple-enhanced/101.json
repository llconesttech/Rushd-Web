      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنفُوشِ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَأَمَّا مَن ثَقُلَتْ مَوَازِينُهُ",
      "juz": 30,
