{
  "number": 15,
  "name": "سُورَةُ الحِجۡرِ",
  "englishName": "Al-Hijr",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ الر ۚ تِلكَ آياتُ الكِتابِ وَقُرآنٍ مُبينٍ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 2,
      "text": "رُبَما يَوَدُّ الَّذينَ كَفَروا لَو كانوا مُسلِمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ذَرهُم يَأكُلوا وَيَتَمَتَّعوا وَيُلهِهِمُ الأَمَلُ ۖ فَسَوفَ يَعلَمونَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَما أَهلَكنا مِن قَريَةٍ إِلّا وَلَها كِتابٌ مَعلومٌ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ما تَسبِقُ مِن أُمَّةٍ أَجَلَها وَما يَستَأخِرونَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَقالوا يا أَيُّهَا الَّذي نُزِّلَ عَلَيهِ الذِّكرُ إِنَّكَ لَمَجنونٌ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لَو ما تَأتينا بِالمَلائِكَةِ إِن كُنتَ مِنَ الصّادِقينَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ما نُنَزِّلُ المَلائِكَةَ إِلّا بِالحَقِّ وَما كانوا إِذًا مُنظَرينَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 9,
      "text": "إِنّا نَحنُ نَزَّلنَا الذِّكرَ وَإِنّا لَهُ لَحافِظونَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَقَد أَرسَلنا مِن قَبلِكَ في شِيَعِ الأَوَّلينَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَما يَأتيهِم مِن رَسولٍ إِلّا كانوا بِهِ يَستَهزِئونَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 12,
      "text": "كَذٰلِكَ نَسلُكُهُ في قُلوبِ المُجرِمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 13,
      "text": "لا يُؤمِنونَ بِهِ ۖ وَقَد خَلَت سُنَّةُ الأَوَّلينَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَلَو فَتَحنا عَلَيهِم بابًا مِنَ السَّماءِ فَظَلّوا فيهِ يَعرُجونَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 15,
      "text": "لَقالوا إِنَّما سُكِّرَت أَبصارُنا بَل نَحنُ قَومٌ مَسحورونَ",
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَلَقَد جَعَلنا فِي السَّماءِ بُروجًا وَزَيَّنّاها لِلنّاظِرينَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَحَفِظناها مِن كُلِّ شَيطانٍ رَجيمٍ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 18,
      "text": "إِلّا مَنِ استَرَقَ السَّمعَ فَأَتبَعَهُ شِهابٌ مُبينٌ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَالأَرضَ مَدَدناها وَأَلقَينا فيها رَواسِيَ وَأَنبَتنا فيها مِن كُلِّ شَيءٍ مَوزونٍ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَجَعَلنا لَكُم فيها مَعايِشَ وَمَن لَستُم لَهُ بِرازِقينَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَإِن مِن شَيءٍ إِلّا عِندَنا خَزائِنُهُ وَما نُنَزِّلُهُ إِلّا بِقَدَرٍ مَعلومٍ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَأَرسَلنَا الرِّياحَ لَواقِحَ فَأَنزَلنا مِنَ السَّماءِ ماءً فَأَسقَيناكُموهُ وَما أَنتُم لَهُ بِخازِنينَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَإِنّا لَنَحنُ نُحيي وَنُميتُ وَنَحنُ الوارِثونَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَلَقَد عَلِمنَا المُستَقدِمينَ مِنكُم وَلَقَد عَلِمنَا المُستَأخِرينَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَإِنَّ رَبَّكَ هُوَ يَحشُرُهُم ۚ إِنَّهُ حَكيمٌ عَليمٌ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلَقَد خَلَقنَا الإِنسانَ مِن صَلصالٍ مِن حَمَإٍ مَسنونٍ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَالجانَّ خَلَقناهُ مِن قَبلُ مِن نارِ السَّمومِ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَإِذ قالَ رَبُّكَ لِلمَلائِكَةِ إِنّي خالِقٌ بَشَرًا مِن صَلصالٍ مِن حَمَإٍ مَسنونٍ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَإِذا سَوَّيتُهُ وَنَفَختُ فيهِ مِن روحي فَقَعوا لَهُ ساجِدينَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَسَجَدَ المَلائِكَةُ كُلُّهُم أَجمَعونَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 31,
      "text": "إِلّا إِبليسَ أَبىٰ أَن يَكونَ مَعَ السّاجِدينَ",
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 32,
      "text": "قالَ يا إِبليسُ ما لَكَ أَلّا تَكونَ مَعَ السّاجِدينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 33,
      "text": "قالَ لَم أَكُن لِأَسجُدَ لِبَشَرٍ خَلَقتَهُ مِن صَلصالٍ مِن حَمَإٍ مَسنونٍ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 34,
      "text": "قالَ فَاخرُج مِنها فَإِنَّكَ رَجيمٌ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 35,
      "text": "وَإِنَّ عَلَيكَ اللَّعنَةَ إِلىٰ يَومِ الدّينِ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 36,
      "text": "قالَ رَبِّ فَأَنظِرني إِلىٰ يَومِ يُبعَثونَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 37,
      "text": "قالَ فَإِنَّكَ مِنَ المُنظَرينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 38,
      "text": "إِلىٰ يَومِ الوَقتِ المَعلومِ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 39,
      "text": "قالَ رَبِّ بِما أَغوَيتَني لَأُزَيِّنَنَّ لَهُم فِي الأَرضِ وَلَأُغوِيَنَّهُم أَجمَعينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إِلّا عِبادَكَ مِنهُمُ المُخلَصينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 41,
      "text": "قالَ هٰذا صِراطٌ عَلَيَّ مُستَقيمٌ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 42,
      "text": "إِنَّ عِبادي لَيسَ لَكَ عَلَيهِم سُلطانٌ إِلّا مَنِ اتَّبَعَكَ مِنَ الغاوينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وَإِنَّ جَهَنَّمَ لَمَوعِدُهُم أَجمَعينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 44,
      "text": "لَها سَبعَةُ أَبوابٍ لِكُلِّ بابٍ مِنهُم جُزءٌ مَقسومٌ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 45,
      "text": "إِنَّ المُتَّقينَ في جَنّاتٍ وَعُيونٍ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 46,
      "text": "ادخُلوها بِسَلامٍ آمِنينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَنَزَعنا ما في صُدورِهِم مِن غِلٍّ إِخوانًا عَلىٰ سُرُرٍ مُتَقابِلينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 48,
      "text": "لا يَمَسُّهُم فيها نَصَبٌ وَما هُم مِنها بِمُخرَجينَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 49,
      "text": "۞ نَبِّئ عِبادي أَنّي أَنَا الغَفورُ الرَّحيمُ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَأَنَّ عَذابي هُوَ العَذابُ الأَليمُ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَنَبِّئهُم عَن ضَيفِ إِبراهيمَ",
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 52,
      "text": "إِذ دَخَلوا عَلَيهِ فَقالوا سَلامًا قالَ إِنّا مِنكُم وَجِلونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 53,
      "text": "قالوا لا تَوجَل إِنّا نُبَشِّرُكَ بِغُلامٍ عَليمٍ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 54,
      "text": "قالَ أَبَشَّرتُموني عَلىٰ أَن مَسَّنِيَ الكِبَرُ فَبِمَ تُبَشِّرونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 55,
      "text": "قالوا بَشَّرناكَ بِالحَقِّ فَلا تَكُن مِنَ القانِطينَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 56,
      "text": "قالَ وَمَن يَقنَطُ مِن رَحمَةِ رَبِّهِ إِلَّا الضّالّونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 57,
      "text": "قالَ فَما خَطبُكُم أَيُّهَا المُرسَلونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 58,
      "text": "قالوا إِنّا أُرسِلنا إِلىٰ قَومٍ مُجرِمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 59,
      "text": "إِلّا آلَ لوطٍ إِنّا لَمُنَجّوهُم أَجمَعينَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 60,
      "text": "إِلَّا امرَأَتَهُ قَدَّرنا ۙ إِنَّها لَمِنَ الغابِرينَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 61,
      "text": "فَلَمّا جاءَ آلَ لوطٍ المُرسَلونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 62,
      "text": "قالَ إِنَّكُم قَومٌ مُنكَرونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 63,
      "text": "قالوا بَل جِئناكَ بِما كانوا فيهِ يَمتَرونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 64,
      "text": "وَأَتَيناكَ بِالحَقِّ وَإِنّا لَصادِقونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 65,
      "text": "فَأَسرِ بِأَهلِكَ بِقِطعٍ مِنَ اللَّيلِ وَاتَّبِع أَدبارَهُم وَلا يَلتَفِت مِنكُم أَحَدٌ وَامضوا حَيثُ تُؤمَرونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 66,
      "text": "وَقَضَينا إِلَيهِ ذٰلِكَ الأَمرَ أَنَّ دابِرَ هٰؤُلاءِ مَقطوعٌ مُصبِحينَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 67,
      "text": "وَجاءَ أَهلُ المَدينَةِ يَستَبشِرونَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 68,
      "text": "قالَ إِنَّ هٰؤُلاءِ ضَيفي فَلا تَفضَحونِ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 69,
      "text": "وَاتَّقُوا اللَّهَ وَلا تُخزونِ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 70,
      "text": "قالوا أَوَلَم نَنهَكَ عَنِ العالَمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 71,
      "text": "قالَ هٰؤُلاءِ بَناتي إِن كُنتُم فاعِلينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 72,
      "text": "لَعَمرُكَ إِنَّهُم لَفي سَكرَتِهِم يَعمَهونَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 73,
      "text": "فَأَخَذَتهُمُ الصَّيحَةُ مُشرِقينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 74,
      "text": "فَجَعَلنا عالِيَها سافِلَها وَأَمطَرنا عَلَيهِم حِجارَةً مِن سِجّيلٍ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 75,
      "text": "إِنَّ في ذٰلِكَ لَآياتٍ لِلمُتَوَسِّمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وَإِنَّها لَبِسَبيلٍ مُقيمٍ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 77,
      "text": "إِنَّ في ذٰلِكَ لَآيَةً لِلمُؤمِنينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 78,
      "text": "وَإِن كانَ أَصحابُ الأَيكَةِ لَظالِمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 79,
      "text": "فَانتَقَمنا مِنهُم وَإِنَّهُما لَبِإِمامٍ مُبينٍ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 80,
      "text": "وَلَقَد كَذَّبَ أَصحابُ الحِجرِ المُرسَلينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 81,
      "text": "وَآتَيناهُم آياتِنا فَكانوا عَنها مُعرِضينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 82,
      "text": "وَكانوا يَنحِتونَ مِنَ الجِبالِ بُيوتًا آمِنينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 83,
      "text": "فَأَخَذَتهُمُ الصَّيحَةُ مُصبِحينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 84,
      "text": "فَما أَغنىٰ عَنهُم ما كانوا يَكسِبونَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وَما خَلَقنَا السَّماواتِ وَالأَرضَ وَما بَينَهُما إِلّا بِالحَقِّ ۗ وَإِنَّ السّاعَةَ لَآتِيَةٌ ۖ فَاصفَحِ الصَّفحَ الجَميلَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 86,
      "text": "إِنَّ رَبَّكَ هُوَ الخَلّاقُ العَليمُ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 87,
      "text": "وَلَقَد آتَيناكَ سَبعًا مِنَ المَثاني وَالقُرآنَ العَظيمَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 88,
      "text": "لا تَمُدَّنَّ عَينَيكَ إِلىٰ ما مَتَّعنا بِهِ أَزواجًا مِنهُم وَلا تَحزَن عَلَيهِم وَاخفِض جَناحَكَ لِلمُؤمِنينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 89,
      "text": "وَقُل إِنّي أَنَا النَّذيرُ المُبينُ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 90,
      "text": "كَما أَنزَلنا عَلَى المُقتَسِمينَ",
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 91,
      "text": "الَّذينَ جَعَلُوا القُرآنَ عِضينَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 92,
      "text": "فَوَرَبِّكَ لَنَسأَلَنَّهُم أَجمَعينَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 93,
      "text": "عَمّا كانوا يَعمَلونَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 94,
      "text": "فَاصدَع بِما تُؤمَرُ وَأَعرِض عَنِ المُشرِكينَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 95,
      "text": "إِنّا كَفَيناكَ المُستَهزِئينَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 96,
      "text": "الَّذينَ يَجعَلونَ مَعَ اللَّهِ إِلٰهًا آخَرَ ۚ فَسَوفَ يَعلَمونَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 97,
      "text": "وَلَقَد نَعلَمُ أَنَّكَ يَضيقُ صَدرُكَ بِما يَقولونَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 98,
      "text": "فَسَبِّح بِحَمدِ رَبِّكَ وَكُن مِنَ السّاجِدينَ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 99,
      "text": "وَاعبُد رَبَّكَ حَتّىٰ يَأتِيَكَ اليَقينُ",
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    }
  ]
}