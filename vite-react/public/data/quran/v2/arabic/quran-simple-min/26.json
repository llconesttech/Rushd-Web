    },
    {
      "number": 197,
      "text": "أَوَلَم يَكُن لَهُم آيَةً أَن يَعلَمَهُ عُلَماءُ بَني إِسرائيلَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 198,
      "text": "وَلَو نَزَّلناهُ عَلىٰ بَعضِ الأَعجَمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 199,
      "text": "فَقَرَأَهُ عَلَيهِم ما كانوا بِهِ مُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 200,
      "text": "كَذٰلِكَ سَلَكناهُ في قُلوبِ المُجرِمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 201,
      "text": "لا يُؤمِنونَ بِهِ حَتّىٰ يَرَوُا العَذابَ الأَليمَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 202,
      "text": "فَيَأتِيَهُم بَغتَةً وَهُم لا يَشعُرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 203,
      "text": "فَيَقولوا هَل نَحنُ مُنظَرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 204,
      "text": "أَفَبِعَذابِنا يَستَعجِلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 205,
      "text": "أَفَرَأَيتَ إِن مَتَّعناهُم سِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 206,
      "text": "ثُمَّ جاءَهُم ما كانوا يوعَدونَ",
      "juz": 19,
      "manzil": 5,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 207,
      "text": "ما أَغنىٰ عَنهُم ما كانوا يُمَتَّعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 208,
      "text": "وَما أَهلَكنا مِن قَريَةٍ إِلّا لَها مُنذِرونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 209,
      "text": "ذِكرىٰ وَما كُنّا ظالِمينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 210,
      "text": "وَما تَنَزَّلَت بِهِ الشَّياطينُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 211,
      "text": "وَما يَنبَغي لَهُم وَما يَستَطيعونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 212,
      "text": "إِنَّهُم عَنِ السَّمعِ لَمَعزولونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 213,
      "text": "فَلا تَدعُ مَعَ اللَّهِ إِلٰهًا آخَرَ فَتَكونَ مِنَ المُعَذَّبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 214,
      "text": "وَأَنذِر عَشيرَتَكَ الأَقرَبينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 215,
      "text": "وَاخفِض جَناحَكَ لِمَنِ اتَّبَعَكَ مِنَ المُؤمِنينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 216,
      "text": "فَإِن عَصَوكَ فَقُل إِنّي بَريءٌ مِمّا تَعمَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 217,
      "text": "وَتَوَكَّل عَلَى العَزيزِ الرَّحيمِ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 218,
      "text": "الَّذي يَراكَ حينَ تَقومُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 219,
      "text": "وَتَقَلُّبَكَ فِي السّاجِدينَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 220,
      "text": "إِنَّهُ هُوَ السَّميعُ العَليمُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 221,
      "text": "هَل أُنَبِّئُكُم عَلىٰ مَن تَنَزَّلُ الشَّياطينُ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 222,
      "text": "تَنَزَّلُ عَلىٰ كُلِّ أَفّاكٍ أَثيمٍ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 223,
      "text": "يُلقونَ السَّمعَ وَأَكثَرُهُم كاذِبونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 224,
      "text": "وَالشُّعَراءُ يَتَّبِعُهُمُ الغاوونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 225,
      "text": "أَلَم تَرَ أَنَّهُم في كُلِّ وادٍ يَهيمونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 226,
      "text": "وَأَنَّهُم يَقولونَ ما لا يَفعَلونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 227,
      "text": "إِلَّا الَّذينَ آمَنوا وَعَمِلُوا الصّالِحاتِ وَذَكَرُوا اللَّهَ كَثيرًا وَانتَصَروا مِن بَعدِ ما ظُلِموا ۗ وَسَيَعلَمُ الَّذينَ ظَلَموا أَيَّ مُنقَلَبٍ يَنقَلِبونَ",
      "juz": 19,
      "manzil": 5,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    }
  ]
}