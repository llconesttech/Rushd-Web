{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ أَرَأَيتَ الَّذي يُكَذِّبُ بِالدّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَذٰلِكَ الَّذي يَدُعُّ اليَتيمَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَلا يَحُضُّ عَلىٰ طَعامِ المِسكينِ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَوَيلٌ لِلمُصَلّينَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "الَّذينَ هُم عَن صَلاتِهِم ساهونَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "الَّذينَ هُم يُراءونَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَيَمنَعونَ الماعونَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
