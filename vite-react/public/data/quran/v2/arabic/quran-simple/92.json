{
  "number": 92,
  "name": "سُورَةُ اللَّيۡلِ",
  "englishName": "Al-Lail",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَاللَّيْلِ إِذَا يَغْشَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالنَّهَارِ إِذَا تَجَلَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنْثَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَصَدَّقَ بِالْحُسْنَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَأَمَّا مَنْ بَخِلَ وَاسْتَغْنَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَكَذَّبَ بِالْحُسْنَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِنَّ عَلَيْنَا لَلْهُدَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَأَنْذَرْتُكُمْ نَارًا تَلَظَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 16,
      "text": "الَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَسَيُجَنَّبُهَا الْأَتْقَى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 18,
      "text": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَمَا لِأَحَدٍ عِنْدَهُ مِنْ نِعْمَةٍ تُجْزَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 20,
      "text": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَلَسَوْفَ يَرْضَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}