{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالنَّازِعَاتِ غَرْقًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالنَّاشِطَاتِ نَشْطًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالسَّابِحَاتِ سَبْحًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَالسَّابِقَاتِ سَبْقًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَالْمُدَبِّرَاتِ أَمْرًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يَوْمَ تَرْجُفُ الرَّاجِفَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تَتْبَعُهَا الرَّادِفَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "أَبْصَارُهَا خَاشِعَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "أَإِذَا كُنَّا عِظَامًا نَخِرَةً",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَإِذَا هُمْ بِالسَّاهِرَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَقُلْ هَلْ لَكَ إِلَىٰ أَنْ تَزَكَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَكَذَّبَ وَعَصَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَحَشَرَ فَنَادَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِمَنْ يَخْشَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "رَفَعَ سَمْكَهَا فَسَوَّاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَالْجِبَالَ أَرْسَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "فَأَمَّا مَنْ طَغَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَآثَرَ الْحَيَاةَ الدُّنْيَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 42,
      "text": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 43,
      "text": "فِيمَ أَنْتَ مِنْ ذِكْرَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 44,
      "text": "إِلَىٰ رَبِّكَ مُنْتَهَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 45,
      "text": "إِنَّمَا أَنْتَ مُنْذِرُ مَنْ يَخْشَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}