{
  "number": 95,
  "name": "سُورَةُ التِّينِ",
  "englishName": "At-Tin",
  "ayahs": [
    {
      "number": 1,
      "text": "By the fig and the olive,",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "By Mount Sinai,",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "And by this land made safe;",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "Surely We created man of the best stature",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "Then we reduced him to the lowest of the low,",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "Save those who believe and do good works, and theirs is a reward unfailing.",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "So who henceforth will give the lie to thee about the judgment?",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "Is not Allah the most conclusive of all judges?",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}