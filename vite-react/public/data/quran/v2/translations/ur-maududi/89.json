{
  "number": 89,
  "name": "سُورَةُ الفَجۡرِ",
  "englishName": "Al-Fajr",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے فجر کی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور دس راتوں کی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور جفت اور طاق کی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور رات کی جبکہ وہ رخصت ہو رہی ہو",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "کیا اِس میں کسی صاحب عقل کے لیے کوئی قسم ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تم نے دیکھا نہیں کہ تمہارے رب نے کیا برتاؤ کیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اونچے ستونوں والے عاد ارم کے ساتھ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "جن کے مانند کوئی قوم دنیا کے ملکوں میں پیدا نہیں کی گئی تھی؟",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور ثمود کے ساتھ جنہوں نے وادی میں چٹانیں تراشی تھیں؟",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور میخوں والے فرعون کے ساتھ؟",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یہ وہ لوگ تھے جنہوں نے دنیا کے ملکوں میں بڑی سرکشی کی تھی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور ان میں بہت فساد پھیلایا تھا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "آخرکار تمہارے رب نے ان پر عذاب کا کوڑا برسا دیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "حقیقت یہ ہے کہ تمہارا رب گھات لگائے ہوئے ہے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "مگر انسان کا حال یہ ہے کہ اس کا رب جب اُس کو آزمائش میں ڈالتا ہے اور اُسے عزت اور نعمت دیتا ہے تو وہ کہتا ہے کہ میرے رب نے مجھے عزت دار بنا دیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور جب وہ اُس کو آزمائش میں ڈالتا ہے اور اُس کا رزق اُس پر تنگ کر دیتا ہے تو وہ کہتا ہے کہ میرے رب نے مجھے ذلیل کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ہرگز نہیں، بلکہ تم یتیم سے عزت کا سلوک نہیں کرتے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اور مسکین کو کھانا کھلانے پر ایک دوسرے کو نہیں اکساتے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور میراث کا سارا مال سمیٹ کر کھا جاتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور مال کی محبت میں بری طرح گرفتار ہو",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "ہرگز نہیں، جب زمین پے در پے کوٹ کوٹ کر ریگ زار بنا دی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اور تمہارا رب جلوہ فرما ہوگا اِس حال میں کہ فرشتے صف در صف کھڑے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اور جہنم اُس روز سامنے لے آئی جائے گی، اُس دن انسان کو سمجھ آئے گی اور اس وقت اُس کے سمجھنے کا کیا حاصل؟",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وہ کہے گا کہ کاش میں نے اپنی اِس زندگی کے لیے کچھ پیشگی سامان کیا ہوتا!",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "پھر اُس دن اللہ جو عذاب دے گا ویسا عذاب دینے والا کوئی نہیں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "اور اللہ جیسا باندھے گا ویسا باندھنے والا کوئی نہیں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 27,
      "text": "(دوسری طرف ارشاد ہوگا) اے نفس مطمئن!",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 28,
      "text": "چل اپنے رب کی طرف، اِس حال میں کہ تو (اپنے انجام نیک سے) خوش (اور اپنے رب کے نزدیک) پسندیدہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 29,
      "text": "شامل ہو جا میرے (نیک) بندوں میں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اور داخل ہو جا میری جنت میں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}