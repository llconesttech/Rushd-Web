{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے روز روشن کی",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور رات کی جبکہ وہ سکون کے ساتھ طاری ہو جائے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "(اے نبیؐ) تمہارے رب نے تم کو ہرگز نہیں چھوڑا اور نہ وہ ناراض ہوا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور یقیناً تمہارے لیے بعد کا دور پہلے دور سے بہتر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور عنقریب تمہارا رب تم کو اتنا دے گا کہ تم خوش ہو جاؤ گے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "کیا اس نے تم کو یتیم نہیں پایا اور پھر ٹھکانا فراہم کیا؟",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور تمہیں ناواقف راہ پایا اور پھر ہدایت بخشی",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور تمہیں نادار پایا اور پھر مالدار کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "لہٰذا یتیم پر سختی نہ کرو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور سائل کو نہ جھڑکو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور اپنے رب کی نعمت کا اظہار کرو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}