{
  "number": 111,
  "name": "سُورَةُ المَسَدِ",
  "englishName": "Al-Masad",
  "ayahs": [
    {
      "number": 1,
      "text": "ٹوٹ گئے ابولہب کے ہاتھ اور نامراد ہو گیا وہ",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اُس کا مال اور جو کچھ اس نے کمایا وہ اُس کے کسی کام نہ آیا",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ضرور وہ شعلہ زن آگ میں ڈالا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور (اُس کے ساتھ) اُس کی جورو بھی، لگائی بجھائی کرنے والی",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اُس کی گردن میں مونجھ کی رسی ہوگی",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}