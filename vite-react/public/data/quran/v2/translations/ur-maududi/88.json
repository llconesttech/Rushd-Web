{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "ayahs": [
    {
      "number": 1,
      "text": "کیا تمہیں اُس چھا جانے والی آفت کی خبر پہنچی ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "کچھ چہرے اُس روز خوف زدہ ہونگے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "سخت مشقت کر رہے ہونگے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تھکے جاتے ہونگے، شدید آگ میں جھلس رہے ہونگے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "کھولتے ہوئے چشمے کا پانی انہیں پینے کو دیا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "خار دار سوکھی گھاس کے سوا کوئی کھانا اُن کے لیے نہ ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "جو نہ موٹا کرے نہ بھوک مٹائے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کچھ چہرے اُس روز با رونق ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اپنی کار گزاری پر خوش ہونگے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "عالی مقام جنت میں ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "کوئی بیہودہ بات وہاں نہ سنیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اُس میں چشمے رواں ہونگے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اُس کے اندر اونچی مسندیں ہوں گی",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ساغر رکھے ہوئے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "گاؤ تکیوں کی قطاریں لگی ہوں گی",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور نفیس فرش بچھے ہوئے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "(یہ لوگ نہیں مانتے) تو کیا یہ اونٹوں کو نہیں دیکھتے کہ کیسے بنائے گئے؟",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "آسمان کو نہیں دیکھتے کہ کیسے اٹھایا گیا؟",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "پہاڑوں کو نہیں دیکھتے کہ کیسے جمائے گئے؟",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور زمین کو نہیں دیکھتے کہ کیسے بچھائی گئی؟",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اچھا تو (اے نبیؐ) نصیحت کیے جاؤ، تم بس نصیحت ہی کرنے والے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "کچھ ان پر جبر کرنے والے نہیں ہو",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "البتہ جو شخص منہ موڑے گا اور انکار کرے گا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "تو اللہ اس کو بھاری سزا دے گا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اِن لوگوں کو پلٹنا ہماری طرف ہی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "پھر اِن کا حساب لینا ہمارے ہی ذمہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}