{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "ayahs": [
    {
      "number": 1,
      "text": "قطار در قطار صف باندھنے والوں کی قسم",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 2,
      "text": "پھر اُن کی قسم جو ڈانٹنے پھٹکارنے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3,
      "text": "پھر اُن کی قسم جو کلام نصیحت سنانے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تمہارا معبود حقیقی بس ایک ہی ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وہ جو زمین اور آسمانوں کا اور تمام اُن چیزوں کا مالک ہے جو زمین و آسمان میں ہیں، اور سارے مشرقوں کا مالک",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ہم نے آسمان دنیا کو تاروں کی زینت سے آراستہ کیا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور ہر شیطان سرکش سے اس کو محفوظ کر دیا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یہ شیاطین ملاء اعلیٰ کی باتیں نہیں سن سکتے، ہر طرف سے مارے اور ہانکے جاتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور ان کے لیے پیہم عذاب ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 10,
      "text": "تاہم اگر کوئی ان میں سے کچھ لے اڑے تو ایک تیز شعلہ اس کا پیچھا کرتا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اب اِن سے پوچھو، اِن کی پیدائش زیادہ مشکل ہے یا اُن چیزوں کی جو ہم نے پیدا کر رکھی ہیں؟ اِن کو تو ہم نے لیس دار گارے سے پیدا کیا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 12,
      "text": "تم (اللہ کی قدرت کے کرشموں پر) حیران ہو اور یہ اس کا مذاق اڑا رہے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 13,
      "text": "سمجھایا جاتا ہے تو سمجھ کر نہیں دیتے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 14,
      "text": "کوئی نشانی دیکھتے ہیں تو اسے ٹھٹھوں میں اڑاتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اور کہتے ہیں \"یہ تو صریح جادو ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 16,
      "text": "بھلا کہیں ایسا ہو سکتا ہے کہ جب ہم مر چکے ہوں اور مٹی بن جائیں اور ہڈیوں کا پنجر رہ جائیں اُس وقت ہم پھر زندہ کر کے اٹھا کھڑے کیے جائیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اور کیا ہمارے اگلے وقتوں کے آبا و اجداد بھی اٹھائے جائیں گے؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اِن سے کہو ہاں، اور تم (خدا کے مقابلے میں) بے بس ہو",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 19,
      "text": "بس ایک ہی جھڑکی ہو گی اور یکایک یہ اپنی آنکھوں سے (وہ سب کچھ جس کی خبر دی جا رہی ہے) دیکھ رہے ہوں گے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اُس وقت یہ کہیں گے \"ہائے ہماری کم بختی، یہ تو یوم الجزا ہے\"",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 21,
      "text": "\"یہ وہی فیصلے کا دن ہے جسے تم جھٹلایا کرتے تھے\"",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 22,
      "text": "(حکم ہو گا) گھیر لاؤ سب ظالموں اور ان کے ساتھیوں اور اُن معبودوں کو جن کی وہ خدا کو چھوڑ کر بندگی کیا کرتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 23,
      "text": "پھر ان سب کو جہنم کا راستہ دکھاؤ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اور ذرا اِنہیں ٹھیراؤ، اِن سے کچھ پوچھنا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 25,
      "text": "\"کیا ہو گیا تمہیں، اب کیوں ایک دوسرے کی مدد نہیں کرتے؟",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ارے، آج تو یہ اپنے آپ کو (اور ایک دوسرے کو) حوالے کیے دے رہے ہیں!\"",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اس کے بعد یہ ایک دوسرے کی طرف مڑیں گے اور باہم تکرار شروع کر دیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 28,
      "text": "(پیروی کرنے والے اپنے پیشواؤں سے) کہیں گے، \"تم ہمارے پاس سیدھے رخ سے آتے تھے\"",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وہ جواب دیں گے، \"نہیں بلکہ تم خود ایمان لانے والے نہ تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 30,
      "text": "ہمارا تم پر کوئی زور نہ تھا، تم خود ہی سرکش لوگ تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 31,
      "text": "آخرکار ہم اپنے رب کے اِس فرمان کے مستحق ہو گئے کہ ہم عذاب کا مزا چکھنے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 32,
      "text": "سو ہم نے تم کو بہکا یا، ہم خود بہکے ہوئے تھے\"",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اِس طرح وہ سب اُس روز عذاب میں مشترک ہوں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 34,
      "text": "ہم مجرموں کے ساتھ یہی کچھ کیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 35,
      "text": "یہ وہ لوگ تھے کہ جب ان سے کہا جاتا \"اللہ کے سوا کوئی معبود بر حق نہیں ہے\" تو یہ گھمنڈ میں آ جاتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اور کہتے تھے \"کیا ہم ایک شاعر مجنون کی خاطر اپنے معبودوں کو چھوڑ دیں؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 37,
      "text": "حالانکہ وہ حق لے کر آیا تھا اور اس نے رسولوں کی تصدیق کی تھی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 38,
      "text": "(اب ان سے کہا جائے گا کہ) تم لازماً دردناک سزا کا مزا چکھنے والے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اور تمہیں جو بدلہ بھی دیا جا رہا ہے اُنہی اعمال کا دیا جا رہا ہے جو تم کرتے رہے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 40,
      "text": "مگر اللہ کے چیدہ بندے (اس انجام بد سے) محفوظ ہوں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 41,
      "text": "ان کے لیے جانا بوجھا رزق ہے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 42,
      "text": "ہر طرح کی لذیذ چیزیں،",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اور نعمت بھری جنتیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 44,
      "text": "جن میں وہ عزت کے ساتھ رکھے جائیں گے تختوں پر آمنے سامنے بیٹھیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 45,
      "text": "شراب کے چشموں سے ساغر بھر بھر کر ان کے درمیان پھرائے جائیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 46,
      "text": "چمکتی ہوئی شراب، جو پینے والوں کے لیے لذّت ہو گی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 47,
      "text": "نہ ان کے جسم کو اس سے کوئی ضرر ہوگا اور نہ ان کی عقل اس سے خراب ہو گی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 48,
      "text": "اور ان کے پاس نگاہیں بچانے والی، خوبصورت آنکھوں والی عورتیں ہوں گی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 49,
      "text": "ایسی نازک جیسے انڈے کے چھلکے کے نیچے چھپی ہوئی جھلی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 50,
      "text": "پھر وہ ایک دوسرے کی طرف متوجہ ہو کر حالات پوچھیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ان میں سے ایک کہے گا، \"دنیا میں میرا ایک ہم نشین تھا",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 52,
      "text": "جو مجھ سے کہا کرتا تھا، کیا تم بھی تصدیق کرنے والوں میں سے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 53,
      "text": "کیا واقعی جب ہم مر چکے ہوں گے اور مٹی ہو جائیں گے اور ہڈیوں کا پنجر بن کر رہ جائیں گے تو ہمیں جزا و سزا دی جائے گی؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 54,
      "text": "اب کیا آپ لوگ دیکھنا چاہتے ہیں کہ وہ صاحب اب کہاں ہیں؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 55,
      "text": "یہ کہہ کر جونہی وہ جھکے گا تو جہنم کی گہرائی میں اس کو دیکھ لے گا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 56,
      "text": "اور اس سے خطاب کر کے کہے گا \"خدا کی قسم، تُو تو مجھے تباہ ہی کر دینے والا تھا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 57,
      "text": "میرے رب کا فضل شامل حال نہ ہوتا تو آج میں بھی اُن لوگوں میں سے ہوتا جو پکڑے ہوئے آئے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 58,
      "text": "اچھا تو کیا اب ہم مرنے والے نہیں ہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 59,
      "text": "موت جو ہمیں آنی تھی وہ بس پہلے آ چکی؟ اب ہمیں کوئی عذاب نہیں ہونا؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 60,
      "text": "یقیناً یہی عظیم الشان کامیابی ہے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 61,
      "text": "ایسی ہی کامیابی کے لیے عمل کرنے والوں کو عمل کرنا چاہیے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 62,
      "text": "بولو، یہ ضیافت اچھی ہے یا زقوم کا درخت؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 63,
      "text": "ہم نے اُس درخت کو ظالموں کے لیے فتنہ بنا دیا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 64,
      "text": "وہ ایک درخت ہے جو جہنم کی تہ سے نکلتا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 65,
      "text": "اُس کے شگوفے ایسے ہیں جیسے شیطانوں کے سر",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 66,
      "text": "جہنم کے لوگ اُسے کھائیں گے اور اسی سے پیٹ بھریں گے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 67,
      "text": "پھر اس پر پینے کے لیے کھولتا ہوا پانی ملے گا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 68,
      "text": "اور اس کے بعد ان کی واپسی اُسی آتش دوزخ کی طرف ہو گی",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 69,
      "text": "یہ وہ لوگ ہیں جنہوں نے اپنے باپ دادا کو گمراہ پایا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 70,
      "text": "اور انہی کے نقش قدم پر دوڑ چلے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 71,
      "text": "حالانکہ ان سے پہلے بہت سے لوگ گمراہ ہو چکے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 72,
      "text": "اور اُن میں ہم نے تنبیہ کرنے والے رسول بھیجے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 73,
      "text": "اب دیکھ لو کہ اُن تنبیہ کیے جانے والوں کا کیا انجام ہوا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 74,
      "text": "اس بد انجامی سے بس اللہ کے وہی بندے بچے ہیں جنہیں اس نے اپنے لیے خالص کر لیا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 75,
      "text": "ہم کو (اِس سے پہلے) نوحؑ نے پکارا تھا، تو دیکھو کہ ہم کیسے اچھے جواب دینے والے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 76,
      "text": "ہم نے اُس کو اور اس کے گھر والوں کو کرب عظیم سے بچا لیا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 77,
      "text": "اور اسی کی نسل کو باقی رکھا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 78,
      "text": "اور بعد کی نسلوں میں اس کی تعریف و توصیف چھوڑ دی",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 79,
      "text": "سلام ہے نوحؑ پر تمام دنیا والوں میں",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 80,
      "text": "ہم نیکی کرنے والوں کو ایسی ہی جزا دیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 81,
      "text": "در حقیقت وہ ہمارے مومن بندوں میں سے تھا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 82,
      "text": "پھر دوسرے گروہ کو ہم نے غرق کر دیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 83,
      "text": "اور نوحؑ ہی کے طریقے پر چلنے والا ابراہیمؑ تھا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 84,
      "text": "جب وہ اپنے رب کے حضور قلب سلیم لے کر آیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 85,
      "text": "جب اُس نے اپنے باپ اور اپنی قوم سے کہا \"یہ کیا چیزیں ہیں جن کی تم عبادت کر رہے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 86,
      "text": "کیا اللہ کو چھوڑ کر جھوٹ گھڑے ہوئے معبود چاہتے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 87,
      "text": "آخر اللہ ربّ العالمین کے بارے میں تمہارا کیا گمان ہے؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 88,
      "text": "پھر اس نے تاروں پر ایک نگاہ ڈالی",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 89,
      "text": "اور کہا میری طبیعت خراب ہے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 90,
      "text": "چنانچہ وہ لوگ اسے چھوڑ کر چلے گئے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 91,
      "text": "اُن کے پیچھے وہ چپکے سے اُن کے معبودوں کے مندر میں گھس گیا اور بولا \" آپ لوگ کھاتے کیوں نہیں ہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 92,
      "text": "کیا ہو گیا، آپ لوگ بولتے بھی نہیں؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 93,
      "text": "اس کے بعد وہ اُن پر پل پڑا اور سیدھے ہاتھ سے خوب ضربیں لگائیں",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 94,
      "text": "(واپس آ کر) وہ لوگ بھاگے بھاگے اس کے پاس آئے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 95,
      "text": "اس نے کہا \"کیا تم اپنی ہی تراشی ہوئی چیزوں کو پوجتے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 96,
      "text": "حالانکہ اللہ ہی نے تم کو بھی پیدا کیا ہے اور اُن چیزوں کو بھی جنہیں تم بناتے ہو\"",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 97,
      "text": "انہوں نے آپس میں کہا \"اس کے لیے ایک الاؤ تیار کرو اور اسے دہکتی ہوئی آگ کے ڈھیر میں پھینک دو\"",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 98,
      "text": "انہوں نے اس کے خلاف ایک کاروائی کرنی چاہی تھی، مگر ہم نے انہی کو نیچا دکھا دیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 99,
      "text": "ابراہیمؑ نے کہا \"میں اپنے رب کی طرف جاتا ہوں، وہی میری رہنمائی کرے گا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 100,
      "text": "اے پروردگار، مجھے ایک بیٹا عطا کر جو صالحوں میں سے ہو\"",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 101,
      "text": "(اس دعا کے جواب میں) ہم نے اس کو ایک حلیم (بردبار) لڑکے کی بشارت دی",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 102,
      "text": "وہ لڑکا جب اس کے ساتھ دوڑ دھوپ کرنے کی عمر کو پہنچ گیا تو (ایک روز) ابراہیمؑ نے اس سے کہا، \"بیٹا، میں خواب میں دیکھتا ہوں کہ میں تجھے ذبح کر رہا ہوں، اب تو بتا، تیرا کیا خیال ہے؟\" اُس نے کہا، \"ابا جان، جو کچھ آپ کو حکم دیا جا رہا ہے اسے کر ڈالیے، آپ انشاءاللہ مجھے صابروں میں سے پائیں گے\"",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 103,
      "text": "آخر کو جب اِن دونوں نے سر تسلیم خم کر دیا اور ابراہیمؑ نے بیٹے کو ماتھے کے بل گرا دیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 104,
      "text": "اور ہم نے ندا دی کہ \"اے ابراہیمؑ",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 105,
      "text": "تو نے خواب سچ کر دکھایا ہم نیکی کرنے والوں کو ایسی ہی جزا دیتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 106,
      "text": "یقیناً یہ ایک کھلی آزمائش تھی\"",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 107,
      "text": "اور ہم نے ایک بڑی قربانی فدیے میں دے کر اس بچے کو چھڑا لیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 108,
      "text": "اور اس کی تعریف و توصیف ہمیشہ کے لیے بعد کی نسلوں میں چھوڑ دی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 109,
      "text": "سلام ہے ابراہیمؑ پر",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 110,
      "text": "ہم نیکی کرنے والوں کو ایسی ہی جزا دیتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 111,
      "text": "یقیناً وہ ہمارے مومن بندوں میں سے تھا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 112,
      "text": "اور ہم نے اسے اسحاقؑ کی بشارت دی، ایک نبی صالحین میں سے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 113,
      "text": "اور اسے اور اسحاقؑ کو برکت دی اب ان دونوں کی ذریّت میں سے کوئی محسن ہے اور کوئی اپنے نفس پر صریح ظلم کرنے والا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 114,
      "text": "اور ہم نے موسیٰؑ و ہارونؑ پر احسان کیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 115,
      "text": "اُن کو اور ان کی قوم کو کرب عظیم سے نجات دی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 116,
      "text": "اُنہیں نصرت بخشی جس کی وجہ سے وہی غالب رہے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 117,
      "text": "ان کو نہایت واضح کتاب عطا کی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 118,
      "text": "انہیں راہ راست دکھائی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 119,
      "text": "اور بعد کی نسلوں میں ان کا ذکر خیر باقی رکھا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 120,
      "text": "سلام ہے موسیٰؑ اور ہارونؑ پر",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 121,
      "text": "ہم نیکی کرنے والوں کو ایسی ہی جزا دیتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 122,
      "text": "در حقیقت وہ ہمارے مومن بندوں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 123,
      "text": "اور الیاسؑ بھی یقیناً مرسلین میں سے تھا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 124,
      "text": "یاد کرو جب اس نے اپنی قوم سے کہا تھا کہ \"تم لوگ ڈرتے نہیں ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 125,
      "text": "کیا تم بعل کو پکارتے ہو اور احسن الخالقین کو چھوڑ دیتے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 126,
      "text": "اُس اللہ کو جو تمہارا اور تمہارے اگلے پچھلے آبا و اجداد کا رب ہے؟\"",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 127,
      "text": "مگر انہوں نے اسے جھٹلا دیا، سو اب یقیناً وہ سزا کے لیے پیش کیے جانے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 128,
      "text": "بجز اُن بندگان خدا کے جن کو خالص کر لیا گیا تھا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 129,
      "text": "اور الیاسؑ کا ذکر خیر ہم نے بعد کی نسلوں میں باقی رکھا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 130,
      "text": "سلام ہے الیاسؑ پر",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 131,
      "text": "ہم نیکی کرنے والوں کو ایسی ہی جزا دیتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 132,
      "text": "واقعی وہ ہمارے مومن بندوں میں سے تھا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 133,
      "text": "اور لوطؑ بھی انہی لوگوں میں سے تھا جو رسول بنا کر بھیجے گئے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 134,
      "text": "یاد کرو جب ہم نے اس کو اور اس کے سب گھر والوں کو نجات دی",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 135,
      "text": "سوائے ایک بڑھیا کے جو پیچھے رہ جانے والوں میں سے تھی",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 136,
      "text": "پھر باقی سب کو تہس نہس کر دیا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 137,
      "text": "آج تم شب و روز اُن کے اجڑے دیار پر سے گزرتے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 138,
      "text": "کیا تم کو عقل نہیں آتی؟",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 139,
      "text": "اور یقیناً یونسؑ بھی رسولوں میں سے تھا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 140,
      "text": "یاد کرو جب وہ ایک بھری کشتی کی طرف بھاگ نکلا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 141,
      "text": "پھر قرعہ اندازی میں شریک ہوا اور اس میں مات کھائی",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 142,
      "text": "آخرکار مچھلی نے اسے نگل لیا اور وہ ملامت زدہ تھا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 143,
      "text": "اب اگر وہ تسبیح کرنے والوں میں سے نہ ہوتا تو",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 144,
      "text": "روز قیامت تک اسی مچھلی کے پیٹ میں رہتا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 145,
      "text": "آخرکار ہم نے اسے بڑی سقیم حالت میں ایک چٹیل زمین پر پھینک دیا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 146,
      "text": "اور اُس پر ایک بیل دار درخت اگا دیا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 147,
      "text": "اس کے بعد ہم نے اُسے ایک لاکھ، یا اس سے زائد لوگوں کی طرف بھیجا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 148,
      "text": "وہ ایمان لائے اور ہم نے ایک وقت خاص تک انہیں باقی رکھا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 149,
      "text": "پھر ذرا اِن لوگوں سے پوچھو، کیا (اِن کے دل کو یہ بات لگتی ہے کہ) تمہارے رب کے لیے تو ہوں بیٹیاں اور ان کے لیے ہوں بیٹے!",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 150,
      "text": "کیا واقعی ہم نے ملائکہ کو عورتیں ہی بنا یا ہے اور یہ آنکھوں دیکھی بات کہہ رہے ہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 151,
      "text": "خوب سن رکھو، دراصل یہ لوگ اپنی من گھڑت سے یہ بات کہتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 152,
      "text": "کہ اللہ اولاد رکھتا ہے، اور فی الواقع یہ جھوٹے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 153,
      "text": "کیا اللہ نے بیٹوں کے بجائے بیٹیاں اپنے لیے پسند کر لیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 154,
      "text": "تمہیں کیا ہو گیا ہے، کیسے حکم لگا رہے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 155,
      "text": "کیا تمہیں ہوش نہیں آتا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 156,
      "text": "یا پھر تمہارے پاس اپنی ان باتوں کے لیے کوئی صاف سند ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 157,
      "text": "تو لاؤ اپنی وہ کتاب اگر تم سچے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 158,
      "text": "اِنہوں نے اللہ اور ملائکہ کے درمیان نسب کا رشتہ بنا رکھا ہے، حالانکہ ملائکہ خوب جانتے ہیں کہ یہ لوگ مجرم کی حیثیت سے پیش ہونے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 159,
      "text": "(اور وہ کہتے ہیں کہ) \"اللہ اُن صفات سے پاک ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 160,
      "text": "جو اُس کے خالص بندوں کے سوا دوسرے لوگ اس کی طرف منسوب کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 161,
      "text": "پس تم اور تمہارے یہ معبود",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 162,
      "text": "اللہ سے کسی کو پھیر نہیں سکتے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 163,
      "text": "مگر صرف اُس کو جو دوزخ کی بھڑکتی ہوئی آگ میں جھلسنے والا ہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 164,
      "text": "اور ہمارا حال تو یہ ہے کہ ہم میں سے ہر ایک کا ایک مقام مقرر ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 165,
      "text": "اور ہم صف بستہ خدمت گار ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 166,
      "text": "اور تسبیح کرنے والے ہیں\"",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 167,
      "text": "یہ لوگ پہلے تو کہا کرتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 168,
      "text": "کہ کاش ہمارے پاس وہ \"ذکر\" ہوتا جو پچھلی قوموں کو ملا تھا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 169,
      "text": "تو ہم اللہ کے چیدہ بندے ہوتے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 170,
      "text": "مگر (جب وہ آ گیا) تو انہوں نے اس کا انکار کر دیا اب عنقریب اِنہیں (اِس روش کا نتیجہ) معلوم ہو جائے گا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 171,
      "text": "اپنے بھیجے ہوئے بندوں سے ہم پہلے ہی وعدہ کر چکے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 172,
      "text": "کہ یقیناً ان کی مدد کی جائے گی",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 173,
      "text": "اور ہمارا لشکر ہی غالب ہو کر رہے گا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 174,
      "text": "پس اے نبیؐ، ذرا کچھ مدّت تک انہیں اِن کے حال پر چھوڑ دو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 175,
      "text": "اور دیکھتے رہو، عنقریب یہ خود بھی دیکھ لیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 176,
      "text": "کیا یہ ہمارے عذاب کے لیے جلدی مچا رہے ہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 177,
      "text": "جب وہ اِن کے صحن میں آ اترے گا تو وہ دن اُن لوگوں کے لیے بہت برا ہو گا جنہیں متنبہ کیا جا چکا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 178,
      "text": "بس ذرا اِنہیں کچھ مدت کے لیے چھوڑ دو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 179,
      "text": "اور دیکھتے رہو، عنقریب یہ خود دیکھ لیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 180,
      "text": "پاک ہے تیرا رب، عزت کا مالک، اُن تمام باتوں سے جو یہ لوگ بنا رہے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 181,
      "text": "اور سلام ہے مرسلین پر",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 182,
      "text": "اور ساری تعریف اللہ ربّ العالمین ہی کے لیے ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    }
  ]
}