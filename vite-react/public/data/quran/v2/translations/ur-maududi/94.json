{
  "number": 94,
  "name": "سُورَةُ الشَّرۡحِ",
  "englishName": "Ash-Sharh",
  "ayahs": [
    {
      "number": 1,
      "text": "(اے نبیؐ) کیا ہم نے تمہارا سینہ تمہارے لیے کھول نہیں دیا؟",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تم پر سے وہ بھاری بوجھ اتار دیا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "جو تمہاری کمر توڑے ڈال رہا تھا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور تمہاری خاطر تمہارے ذکر کا آوازہ بلند کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پس حقیقت یہ ہے کہ تنگی کے ساتھ فراخی بھی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بے شک تنگی کے ساتھ فراخی بھی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لہٰذا جب تم فارغ ہو تو عبادت کی مشقت میں لگ جاؤ",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور اپنے رب ہی کی طرف راغب ہو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}