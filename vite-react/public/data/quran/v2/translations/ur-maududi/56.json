{
  "number": 56,
  "name": "سُورَةُ الوَاقِعَةِ",
  "englishName": "Al-Waaqia",
  "ayahs": [
    {
      "number": 1,
      "text": "جب وہ ہونے والا واقعہ پیش آ جائے گا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 2,
      "text": "تو کوئی اس کے وقوع کو جھٹلانے والا نہ ہوگا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وہ تہ و بالا کر دینے والی آفت ہوگی",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 4,
      "text": "زمین اس وقت یکبارگی ہلا ڈالی جائے گی",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور پہاڑ اس طرح ریزہ ریزہ کر دیے جائیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 6,
      "text": "کہ پراگندہ غبار بن کر رہ جائیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تم لوگ اُس وقت تین گروہوں میں تقسیم ہو جاؤ گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 8,
      "text": "دائیں بازو والے، سو دائیں بازو والوں (کی خوش نصیبی) کا کیا کہنا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور بائیں بازو والے، تو بائیں بازو والوں (کی بد نصیبی کا) کا کیا ٹھکانا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور آگے والے تو پھر آگے وا لے ہی ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وہی تو مقرب لوگ ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 12,
      "text": "نعمت بھری جنتوں میں رہیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اگلوں میں سے بہت ہوں گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور پچھلوں میں سے کم",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 15,
      "text": "مرصع تختوں پر",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 16,
      "text": "تکیے لگا ئے آمنے سامنے بیٹھیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اُن کی مجلسوں میں ابدی لڑکے شراب چشمہ جاری سے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 18,
      "text": "لبریز پیالے کنٹر اور ساغر لیے دوڑتے پھرتے ہونگے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 19,
      "text": "جسے پی کر نہ اُن کا سر چکرائے گا نہ ان کی عقل میں فتور آئے گا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور وہ اُن کے سامنے طرح طرح کے لذیذ پھل پیش کریں گے جسے چاہیں چن لیں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اور پرندوں کے گوشت پیش کریں گے کہ جس پرندے کا چاہیں استعمال کریں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اور ان کے لیے خوبصورت آنکھوں والی حوریں ہونگی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 23,
      "text": "ایسی حسین جیسے چھپا کر رکھے ہوئے موتی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 24,
      "text": "یہ سب کچھ اُن اعمال کی جزا کے طور پر انہیں ملے گا جو وہ دنیا میں کرتے رہے تھے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وہاں وہ کوئی بیہودہ کلام یا گناہ کی بات نہ سنیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 26,
      "text": "جو بات بھی ہوگی ٹھیک ٹھیک ہوگی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اور دائیں بازو والے، دائیں بازو والوں کی خوش نصیبی کا کیا کہنا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وہ بے خار بیریوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اور تہ بر تہ چڑھے ہوئے کیلوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اور دور تک پھیلی ہوئی چھاؤں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اور ہر دم رواں پانی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اور کبھی ختم نہ ہونے والے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اور بے روک ٹوک ملنے والے بکثرت پھلوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اور اونچی نشست گاہوں میں ہوں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ان کی بیویوں کو ہم خاص طور پر نئے سرے سے پیدا کریں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اور انہیں با کرہ بنا دیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اپنے شوہروں کی عاشق اور عمر میں ہم سن",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 38,
      "text": "یہ کچھ دائیں بازو والوں کے لیے ہے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وہ اگلوں میں سے بہت ہوں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اور پچھلوں میں سے بھی بہت",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اور بائیں بازو والے، بائیں بازو والوں کی بد نصیبی کا کیا پوچھنا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وہ لو کی لپٹ اور کھولتے ہوئے پانی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اور کالے دھوئیں کے سائے میں ہوں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 44,
      "text": "جو نہ ٹھنڈا ہوگا نہ آرام دہ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 45,
      "text": "یہ وہ لوگ ہوں گے جو اِس انجام کو پہنچنے سے پہلے خوشحال تھے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 46,
      "text": "اور گناہ عظیم پر اصرار کرتے تھے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 47,
      "text": "کہتے تھے \"کیا جب ہم مر کر خاک ہو جائیں گے اور ہڈیوں کا پنجر رہ جائیں گے تو پھر اٹھا کھڑے کیے جائیں گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 48,
      "text": "اور کیا ہمارے وہ باپ دادا بھی اٹھائے جائیں گے جو پہلے گزر چکے ہیں؟\"",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 49,
      "text": "اے نبیؐ، اِن لوگوں سے کہو، یقیناً اگلے اور پچھلے سب",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 50,
      "text": "ایک دن ضرور جمع کیے جانے والے ہیں جس کا وقت مقرر کیا جا چکا ہے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 51,
      "text": "پھر اے گمراہو اور جھٹلانے والو!",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 52,
      "text": "تم شجر زقوم کی غذا کھانے والے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 53,
      "text": "اُسی سے تم پیٹ بھرو گے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 54,
      "text": "اور اوپر سے کھولتا ہوا پانی",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 55,
      "text": "تونس لگے ہوئے اونٹ کی طرح پیو گے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 56,
      "text": "یہ ہے بائیں والوں کی ضیافت کا سامان روز جزا میں",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 57,
      "text": "ہم نے تمہیں پیدا کیا ہے پھر کیوں تصدیق نہیں کرتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 58,
      "text": "کبھی تم نے غور کیا، یہ نطفہ جو تم ڈالتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 59,
      "text": "اس سے بچہ تم بناتے ہو یا اس کے بنانے والے ہم ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 60,
      "text": "ہم نے تمہارے درمیان موت کو تقسیم کیا ہے، اور ہم اِس سے عاجز نہیں ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 61,
      "text": "کہ تمہاری شکلیں بدل دیں اور کسی ایسی شکل میں تمہیں پیدا کر دیں جس کو تم نہیں جانتے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 62,
      "text": "اپنی پہلی پیدائش کو تو تم جانتے ہو، پھر کیوں سبق نہیں لیتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 63,
      "text": "کبھی تم نے سوچا، یہ بیج جو تم بوتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 64,
      "text": "اِن سے کھیتیاں تم اگاتے ہو یا اُن کے اگانے والے ہم ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 65,
      "text": "ہم چاہیں تو ان کھیتیوں کو بھس بنا کر رکھ دیں اور تم طرح طرح کی باتیں بناتے رہ جاؤ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 66,
      "text": "کہ ہم پر تو الٹی چٹی پڑ گئی",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 67,
      "text": "بلکہ ہمارے تو نصیب ہی پھوٹے ہوئے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 68,
      "text": "کبھی تم نے آنکھیں کھول کر دیکھا، یہ پانی جو تم پیتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 69,
      "text": "اِسے تم نے بادل سے برسایا ہے یا اِس کے برسانے والے ہم ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 70,
      "text": "ہم چاہیں تو اسے سخت کھاری بنا کر رکھ دیں، پھر کیوں تم شکر گزار نہیں ہوتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 71,
      "text": "کبھی تم نے خیال کیا، یہ آگ جو تم سلگاتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 72,
      "text": "اِس کا درخت تم نے پیدا کیا ہے، یا اس کے پیدا کرنے والے ہم ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 73,
      "text": "ہم نے اُس کو یاد دہانی کا ذریعہ اور حاجت مندوں کے لیے سامان زیست بنایا ہے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 74,
      "text": "پس اے نبیؐ، اپنے رب عظیم کے نام کی تسبیح کرو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 75,
      "text": "پس نہیں، میں قسم کھاتا ہوں تاروں کے مواقع کی",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 76,
      "text": "اور اگر تم سمجھو تو یہ بہت بڑی قسم ہے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 77,
      "text": "کہ یہ ایک بلند پایہ قرآن ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 78,
      "text": "ایک محفوظ کتاب میں ثبت",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 79,
      "text": "جسے مطہرین کے سوا کوئی چھو نہیں سکتا",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 80,
      "text": "یہ رب العالمین کا نازل کردہ ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 81,
      "text": "پھر کیا اس کلام کے ساتھ تم بے اعتنائی برتتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 82,
      "text": "اور اِس نعمت میں اپنا حصہ تم نے یہ رکھا ہے کہ اِسے جھٹلاتے ہو؟",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 83,
      "text": "تو جب مرنے والے کی جان حلق تک پہنچ چکی ہوتی ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 84,
      "text": "اور تم آنکھوں سے دیکھ رہے ہوتے ہو کہ وہ مر رہا ہے،",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 85,
      "text": "اُس وقت تمہاری بہ نسبت ہم اُس کے زیادہ قریب ہوتے ہیں مگر تم کو نظر نہیں آتے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 86,
      "text": "اب اگر تم کسی کے محکوم نہیں ہو اور اپنے اِس خیال میں سچے ہو،",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 87,
      "text": "اُس وقت اُس کی نکلتی ہوئی جان کو واپس کیوں نہیں لے آتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 88,
      "text": "پھر وہ مرنے والا اگر مقربین میں سے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 89,
      "text": "تو اس کے لیے راحت اور عمدہ رزق اور نعمت بھری جنت ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 90,
      "text": "اور اگر وہ اصحاب یمین میں سے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 91,
      "text": "تو اس کا استقبال یوں ہوتا ہے کہ سلام ہے تجھے، تو اصحاب الیمین میں سے ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 92,
      "text": "اور اگر وہ جھٹلانے والے گمراہ لوگوں میں سے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 93,
      "text": "تو اس کی تواضع کے لیے کھولتا ہوا پانی ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 94,
      "text": "اور جہنم میں جھونکا جانا",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 95,
      "text": "یہ سب کچھ قطعی حق ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 96,
      "text": "پس اے نبیؐ، اپنے رب عظیم کے نام کی تسبیح کرو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    }
  ]
}