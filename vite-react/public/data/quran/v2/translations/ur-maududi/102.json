{
  "number": 102,
  "name": "سُورَةُ التَّكَاثُرِ",
  "englishName": "At-Takaathur",
  "ayahs": [
    {
      "number": 1,
      "text": "تم لوگوں کو زیادہ سے زیادہ اور ایک دوسرے سے بڑھ کر دنیا حاصل کرنے کی دھن نے غفلت میں ڈال رکھا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "یہاں تک کہ (اسی فکر میں) تم لب گور تک پہنچ جاتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ہرگز نہیں، عنقریب تم کو معلوم ہو جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر (سن لو کہ) ہرگز نہیں، عنقریب تم کو معلوم ہو جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ہرگز نہیں، اگر تم یقینی علم کی حیثیت سے (اِس روش کے انجام کو) جانتے ہوتے (تو تمہارا یہ طرز عمل نہ ہوتا)",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تم دوزخ دیکھ کر رہو گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "پھر (سن لو کہ) تم بالکل یقین کے ساتھ اُسے دیکھ لو گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "پھر ضرور اُس روز تم سے اِن نعمتوں کے بارے میں جواب طلبی کی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}