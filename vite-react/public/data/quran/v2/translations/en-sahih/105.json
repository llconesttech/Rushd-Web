{
  "number": 105,
  "name": "سُورَةُ الفِيلِ",
  "englishName": "Al-Fil",
  "ayahs": [
    {
      "number": 1,
      "text": "Have you not considered, [O Muhammad], how your Lord dealt with the companions of the elephant?",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "Did He not make their plan into misguidance?",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "And He sent against them birds in flocks,",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "Striking them with stones of hard clay,",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "And He made them like eaten straw.",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}