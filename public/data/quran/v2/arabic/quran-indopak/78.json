{
  "number": 78,
  "name": "سُورَةُ النَّبَإِ",
  "englishName": "An-Naba",
  "ayahs": [
    {
      "number": 1,
      "text": "عَمَّ یَتَسَآءَلُوْنَ ۟ۚ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "عَنِ النَّبَاِ الْعَظِیْمِ ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "الَّذِیْ هُمْ فِیْهِ مُخْتَلِفُوْنَ ۟ؕ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَلَّا سَیَعْلَمُوْنَ ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ثُمَّ كَلَّا سَیَعْلَمُوْنَ ۟",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اَلَمْ نَجْعَلِ الْاَرْضَ مِهٰدًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّالْجِبَالَ اَوْتَادًا ۟",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَّخَلَقْنٰكُمْ اَزْوَاجًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَّجَعَلْنَا نَوْمَكُمْ سُبَاتًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَّجَعَلْنَا الَّیْلَ لِبَاسًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَّجَعَلْنَا النَّهَارَ مَعَاشًا ۪۟",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَّبَنَیْنَا فَوْقَكُمْ سَبْعًا شِدَادًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَّجَعَلْنَا سِرَاجًا وَّهَّاجًا ۟",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّاَنْزَلْنَا مِنَ الْمُعْصِرٰتِ مَآءً ثَجَّاجًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "لِّنُخْرِجَ بِهٖ حَبًّا وَّنَبَاتًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَّجَنّٰتٍ اَلْفَافًا ۟ؕ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِنَّ یَوْمَ الْفَصْلِ كَانَ مِیْقَاتًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "یَّوْمَ یُنْفَخُ فِی الصُّوْرِ فَتَاْتُوْنَ اَفْوَاجًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَّفُتِحَتِ السَّمَآءُ فَكَانَتْ اَبْوَابًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَّسُیِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا ۟ؕ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا ۟",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لِّلطَّاغِیْنَ مَاٰبًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "لّٰبِثِیْنَ فِیْهَاۤ اَحْقَابًا ۟ۚ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "لَا یَذُوْقُوْنَ فِیْهَا بَرْدًا وَّلَا شَرَابًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اِلَّا حَمِیْمًا وَّغَسَّاقًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "جَزَآءً وِّفَاقًا ۟ؕ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اِنَّهُمْ كَانُوْا لَا یَرْجُوْنَ حِسَابًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَّكَذَّبُوْا بِاٰیٰتِنَا كِذَّابًا ۟ؕ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَكُلَّ شَیْءٍ اَحْصَیْنٰهُ كِتٰبًا ۟ۙ",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَذُوْقُوْا فَلَنْ نَّزِیْدَكُمْ اِلَّا عَذَابًا ۟۠",
      "juz": 30,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اِنَّ لِلْمُتَّقِیْنَ مَفَازًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "حَدَآىِٕقَ وَاَعْنَابًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَّكَوَاعِبَ اَتْرَابًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَّكَاْسًا دِهَاقًا ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "لَا یَسْمَعُوْنَ فِیْهَا لَغْوًا وَّلَا كِذّٰبًا ۟ۚ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "جَزَآءً مِّنْ رَّبِّكَ عَطَآءً حِسَابًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "رَّبِّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَیْنَهُمَا الرَّحْمٰنِ لَا یَمْلِكُوْنَ مِنْهُ خِطَابًا ۟ۚ",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "یَوْمَ یَقُوْمُ الرُّوْحُ وَالْمَلٰٓىِٕكَةُ صَفًّا ۙۗؕ لَّا یَتَكَلَّمُوْنَ اِلَّا مَنْ اَذِنَ لَهُ الرَّحْمٰنُ وَقَالَ صَوَابًا ۟",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ذٰلِكَ الْیَوْمُ الْحَقُّ ۚ فَمَنْ شَآءَ اتَّخَذَ اِلٰی رَبِّهٖ مَاٰبًا ۟",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اِنَّاۤ اَنْذَرْنٰكُمْ عَذَابًا قَرِیْبًا ۖۚ۬ یَّوْمَ یَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ یَدٰهُ وَیَقُوْلُ الْكٰفِرُ یٰلَیْتَنِیْ كُنْتُ تُرٰبًا ۟۠",
      "juz": 30,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}