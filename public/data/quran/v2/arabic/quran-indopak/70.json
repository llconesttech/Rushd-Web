{
  "number": 70,
  "name": "سُورَةُ المَعَارِجِ",
  "englishName": "Al-Ma'aarij",
  "ayahs": [
    {
      "number": 1,
      "text": "سَاَلَ سَآىِٕلٌۢ بِعَذَابٍ وَّاقِعٍ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 2,
      "text": "لِّلْكٰفِرِیْنَ لَیْسَ لَهٗ دَافِعٌ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 3,
      "text": "مِّنَ اللّٰهِ ذِی الْمَعَارِجِ ۟ؕ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَعْرُجُ الْمَلٰٓىِٕكَةُ وَالرُّوْحُ اِلَیْهِ فِیْ یَوْمٍ كَانَ مِقْدَارُهٗ خَمْسِیْنَ اَلْفَ سَنَةٍ ۟ۚ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَاصْبِرْ صَبْرًا جَمِیْلًا ۟",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِنَّهُمْ یَرَوْنَهٗ بَعِیْدًا ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّنَرٰىهُ قَرِیْبًا ۟ؕ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یَوْمَ تَكُوْنُ السَّمَآءُ كَالْمُهْلِ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَتَكُوْنُ الْجِبَالُ كَالْعِهْنِ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَا یَسْـَٔلُ حَمِیْمٌ حَمِیْمًا ۟ۚۖ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یُّبَصَّرُوْنَهُمْ ؕ یَوَدُّ الْمُجْرِمُ لَوْ یَفْتَدِیْ مِنْ عَذَابِ یَوْمِىِٕذٍ بِبَنِیْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَصَاحِبَتِهٖ وَاَخِیْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَفَصِیْلَتِهِ الَّتِیْ تُـْٔوِیْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَمَنْ فِی الْاَرْضِ جَمِیْعًا ۙ ثُمَّ یُنْجِیْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلَّا ؕ اِنَّهَا لَظٰی ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 16,
      "text": "نَزَّاعَةً لِّلشَّوٰی ۟ۚۖ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 17,
      "text": "تَدْعُوْا مَنْ اَدْبَرَ وَتَوَلّٰی ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَجَمَعَ فَاَوْعٰی ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اِنَّ الْاِنْسَانَ خُلِقَ هَلُوْعًا ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اِذَا مَسَّهُ الشَّرُّ جَزُوْعًا ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَّاِذَا مَسَّهُ الْخَیْرُ مَنُوْعًا ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اِلَّا الْمُصَلِّیْنَ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 23,
      "text": "الَّذِیْنَ هُمْ عَلٰی صَلَاتِهِمْ دَآىِٕمُوْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَالَّذِیْنَ فِیْۤ اَمْوَالِهِمْ حَقٌّ مَّعْلُوْمٌ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 25,
      "text": "لِّلسَّآىِٕلِ وَالْمَحْرُوْمِ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَالَّذِیْنَ یُصَدِّقُوْنَ بِیَوْمِ الدِّیْنِ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَالَّذِیْنَ هُمْ مِّنْ عَذَابِ رَبِّهِمْ مُّشْفِقُوْنَ ۟ۚ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اِنَّ عَذَابَ رَبِّهِمْ غَیْرُ مَاْمُوْنٍ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَالَّذِیْنَ هُمْ لِفُرُوْجِهِمْ حٰفِظُوْنَ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اِلَّا عَلٰۤی اَزْوَاجِهِمْ اَوْ مَا مَلَكَتْ اَیْمَانُهُمْ فَاِنَّهُمْ غَیْرُ مَلُوْمِیْنَ ۟ۚ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَمَنِ ابْتَغٰی وَرَآءَ ذٰلِكَ فَاُولٰٓىِٕكَ هُمُ الْعٰدُوْنَ ۟ۚ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَالَّذِیْنَ هُمْ لِاَمٰنٰتِهِمْ وَعَهْدِهِمْ رٰعُوْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَالَّذِیْنَ هُمْ بِشَهٰدٰتِهِمْ قَآىِٕمُوْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَالَّذِیْنَ هُمْ عَلٰی صَلَاتِهِمْ یُحَافِظُوْنَ ۟ؕ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اُولٰٓىِٕكَ فِیْ جَنّٰتٍ مُّكْرَمُوْنَ ۟ؕ۠",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَمَالِ الَّذِیْنَ كَفَرُوْا قِبَلَكَ مُهْطِعِیْنَ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 37,
      "text": "عَنِ الْیَمِیْنِ وَعَنِ الشِّمَالِ عِزِیْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اَیَطْمَعُ كُلُّ امْرِئٍ مِّنْهُمْ اَنْ یُّدْخَلَ جَنَّةَ نَعِیْمٍ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 39,
      "text": "كَلَّا ؕ اِنَّا خَلَقْنٰهُمْ مِّمَّا یَعْلَمُوْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 40,
      "text": "فَلَاۤ اُقْسِمُ بِرَبِّ الْمَشٰرِقِ وَالْمَغٰرِبِ اِنَّا لَقٰدِرُوْنَ ۟ۙ",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 41,
      "text": "عَلٰۤی اَنْ نُّبَدِّلَ خَیْرًا مِّنْهُمْ ۙ وَمَا نَحْنُ بِمَسْبُوْقِیْنَ ۟",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فَذَرْهُمْ یَخُوْضُوْا وَیَلْعَبُوْا حَتّٰی یُلٰقُوْا یَوْمَهُمُ الَّذِیْ یُوْعَدُوْنَ ۟ۙ",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 43,
      "text": "یَوْمَ یَخْرُجُوْنَ مِنَ الْاَجْدَاثِ سِرَاعًا كَاَنَّهُمْ اِلٰی نُصُبٍ یُّوْفِضُوْنَ ۟ۙ",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 44,
      "text": "خَاشِعَةً اَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ؕ ذٰلِكَ الْیَوْمُ الَّذِیْ كَانُوْا یُوْعَدُوْنَ ۟۠",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    }
  ]
}