{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالضُّحٰی ۟ۙ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالَّیْلِ اِذَا سَجٰی ۟ۙ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلٰی ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَلَلْاٰخِرَةُ خَیْرٌ لَّكَ مِنَ الْاُوْلٰی ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَلَسَوْفَ یُعْطِیْكَ رَبُّكَ فَتَرْضٰی ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اَلَمْ یَجِدْكَ یَتِیْمًا فَاٰوٰی ۪۟",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَوَجَدَكَ ضَآلًّا فَهَدٰی ۪۟",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَوَجَدَكَ عَآىِٕلًا فَاَغْنٰی ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَاَمَّا الْیَتِیْمَ فَلَا تَقْهَرْ ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاَمَّا السَّآىِٕلَ فَلَا تَنْهَرْ ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَاَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ ۟۠",
      "juz": 30,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}