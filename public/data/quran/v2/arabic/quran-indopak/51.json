{
  "number": 51,
  "name": "سُورَةُ الذَّارِيَاتِ",
  "englishName": "Adh-Dhaariyat",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالذّٰرِیٰتِ ذَرْوًا ۟ۙ",
      "juz": 26,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالْحٰمِلٰتِ وِقْرًا ۟ۙ",
      "juz": 26,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فَالْجٰرِیٰتِ یُسْرًا ۟ۙ",
      "juz": 26,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَالْمُقَسِّمٰتِ اَمْرًا ۟ۙ",
      "juz": 26,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اِنَّمَا تُوْعَدُوْنَ لَصَادِقٌ ۟ۙ",
      "juz": 26,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَّاِنَّ الدِّیْنَ لَوَاقِعٌ ۟ؕ",
      "juz": 26,
      "page": 520,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَالسَّمَآءِ ذَاتِ الْحُبُكِ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِنَّكُمْ لَفِیْ قَوْلٍ مُّخْتَلِفٍ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یُّؤْفَكُ عَنْهُ مَنْ اُفِكَ ۟ؕ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 10,
      "text": "قُتِلَ الْخَرّٰصُوْنَ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الَّذِیْنَ هُمْ فِیْ غَمْرَةٍ سَاهُوْنَ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 12,
      "text": "یَسْـَٔلُوْنَ اَیَّانَ یَوْمُ الدِّیْنِ ۟ؕ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 13,
      "text": "یَوْمَ هُمْ عَلَی النَّارِ یُفْتَنُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ذُوْقُوْا فِتْنَتَكُمْ ؕ هٰذَا الَّذِیْ كُنْتُمْ بِهٖ تَسْتَعْجِلُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِنَّ الْمُتَّقِیْنَ فِیْ جَنّٰتٍ وَّعُیُوْنٍ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اٰخِذِیْنَ مَاۤ اٰتٰىهُمْ رَبُّهُمْ ؕ اِنَّهُمْ كَانُوْا قَبْلَ ذٰلِكَ مُحْسِنِیْنَ ۟ؕ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 17,
      "text": "كَانُوْا قَلِیْلًا مِّنَ الَّیْلِ مَا یَهْجَعُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَبِالْاَسْحَارِ هُمْ یَسْتَغْفِرُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَفِیْۤ اَمْوَالِهِمْ حَقٌّ لِّلسَّآىِٕلِ وَالْمَحْرُوْمِ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَفِی الْاَرْضِ اٰیٰتٌ لِّلْمُوْقِنِیْنَ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَفِیْۤ اَنْفُسِكُمْ ؕ اَفَلَا تُبْصِرُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَفِی السَّمَآءِ رِزْقُكُمْ وَمَا تُوْعَدُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَوَرَبِّ السَّمَآءِ وَالْاَرْضِ اِنَّهٗ لَحَقٌّ مِّثْلَ مَاۤ اَنَّكُمْ تَنْطِقُوْنَ ۟۠",
      "juz": 26,
      "page": 521,
      "ruku": 455,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 24,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ ضَیْفِ اِبْرٰهِیْمَ الْمُكْرَمِیْنَ ۟ۘ",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اِذْ دَخَلُوْا عَلَیْهِ فَقَالُوْا سَلٰمًا ؕ قَالَ سَلٰمٌ ۚ قَوْمٌ مُّنْكَرُوْنَ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَرَاغَ اِلٰۤی اَهْلِهٖ فَجَآءَ بِعِجْلٍ سَمِیْنٍ ۟ۙ",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فَقَرَّبَهٗۤ اِلَیْهِمْ قَالَ اَلَا تَاْكُلُوْنَ ۟ؗ",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 28,
      "text": "فَاَوْجَسَ مِنْهُمْ خِیْفَةً ؕ قَالُوْا لَا تَخَفْ ؕ وَبَشَّرُوْهُ بِغُلٰمٍ عَلِیْمٍ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَاَقْبَلَتِ امْرَاَتُهٗ فِیْ صَرَّةٍ فَصَكَّتْ وَجْهَهَا وَقَالَتْ عَجُوْزٌ عَقِیْمٌ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 30,
      "text": "قَالُوْا كَذٰلِكِ ۙ قَالَ رَبُّكِ ؕ اِنَّهٗ هُوَ الْحَكِیْمُ الْعَلِیْمُ ۟",
      "juz": 26,
      "page": 521,
      "ruku": 456,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قَالَ فَمَا خَطْبُكُمْ اَیُّهَا الْمُرْسَلُوْنَ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 32,
      "text": "قَالُوْۤا اِنَّاۤ اُرْسِلْنَاۤ اِلٰی قَوْمٍ مُّجْرِمِیْنَ ۟ۙ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 33,
      "text": "لِنُرْسِلَ عَلَیْهِمْ حِجَارَةً مِّنْ طِیْنٍ ۟ۙ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 34,
      "text": "مُّسَوَّمَةً عِنْدَ رَبِّكَ لِلْمُسْرِفِیْنَ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 35,
      "text": "فَاَخْرَجْنَا مَنْ كَانَ فِیْهَا مِنَ الْمُؤْمِنِیْنَ ۟ۚ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَمَا وَجَدْنَا فِیْهَا غَیْرَ بَیْتٍ مِّنَ الْمُسْلِمِیْنَ ۟ۚ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَتَرَكْنَا فِیْهَاۤ اٰیَةً لِّلَّذِیْنَ یَخَافُوْنَ الْعَذَابَ الْاَلِیْمَ ۟ؕ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَفِیْ مُوْسٰۤی اِذْ اَرْسَلْنٰهُ اِلٰی فِرْعَوْنَ بِسُلْطٰنٍ مُّبِیْنٍ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَتَوَلّٰی بِرُكْنِهٖ وَقَالَ سٰحِرٌ اَوْ مَجْنُوْنٌ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 40,
      "text": "فَاَخَذْنٰهُ وَجُنُوْدَهٗ فَنَبَذْنٰهُمْ فِی الْیَمِّ وَهُوَ مُلِیْمٌ ۟ؕ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَفِیْ عَادٍ اِذْ اَرْسَلْنَا عَلَیْهِمُ الرِّیْحَ الْعَقِیْمَ ۟ۚ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 42,
      "text": "مَا تَذَرُ مِنْ شَیْءٍ اَتَتْ عَلَیْهِ اِلَّا جَعَلَتْهُ كَالرَّمِیْمِ ۟ؕ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وَفِیْ ثَمُوْدَ اِذْ قِیْلَ لَهُمْ تَمَتَّعُوْا حَتّٰی حِیْنٍ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَعَتَوْا عَنْ اَمْرِ رَبِّهِمْ فَاَخَذَتْهُمُ الصّٰعِقَةُ وَهُمْ یَنْظُرُوْنَ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 45,
      "text": "فَمَا اسْتَطَاعُوْا مِنْ قِیَامٍ وَّمَا كَانُوْا مُنْتَصِرِیْنَ ۟ۙ",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 46,
      "text": "وَقَوْمَ نُوْحٍ مِّنْ قَبْلُ ؕ اِنَّهُمْ كَانُوْا قَوْمًا فٰسِقِیْنَ ۟۠",
      "juz": 27,
      "page": 522,
      "ruku": 456,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَالسَّمَآءَ بَنَیْنٰهَا بِاَیْىدٍ وَّاِنَّا لَمُوْسِعُوْنَ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَالْاَرْضَ فَرَشْنٰهَا فَنِعْمَ الْمٰهِدُوْنَ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَمِنْ كُلِّ شَیْءٍ خَلَقْنَا زَوْجَیْنِ لَعَلَّكُمْ تَذَكَّرُوْنَ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَفِرُّوْۤا اِلَی اللّٰهِ ؕ اِنِّیْ لَكُمْ مِّنْهُ نَذِیْرٌ مُّبِیْنٌ ۟ۚ",
      "juz": 27,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَلَا تَجْعَلُوْا مَعَ اللّٰهِ اِلٰهًا اٰخَرَ ؕ اِنِّیْ لَكُمْ مِّنْهُ نَذِیْرٌ مُّبِیْنٌ ۟",
      "juz": 27,
      "page": 522,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 52,
      "text": "كَذٰلِكَ مَاۤ اَتَی الَّذِیْنَ مِنْ قَبْلِهِمْ مِّنْ رَّسُوْلٍ اِلَّا قَالُوْا سَاحِرٌ اَوْ مَجْنُوْنٌ ۟ۚ",
      "juz": 27,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 53,
      "text": "اَتَوَاصَوْا بِهٖ ۚ بَلْ هُمْ قَوْمٌ طَاغُوْنَ ۟ۚ",
      "juz": 27,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فَتَوَلَّ عَنْهُمْ فَمَاۤ اَنْتَ بِمَلُوْمٍ ۟",
      "juz": 27,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 55,
      "text": "وَّذَكِّرْ فَاِنَّ الذِّكْرٰی تَنْفَعُ الْمُؤْمِنِیْنَ ۟",
      "juz": 27,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 56,
      "text": "وَمَا خَلَقْتُ الْجِنَّ وَالْاِنْسَ اِلَّا لِیَعْبُدُوْنِ ۟",
      "juz": 27,
      "page": 523,
      "ruku": 457,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 57,
      "text": "مَاۤ اُرِیْدُ مِنْهُمْ مِّنْ رِّزْقٍ وَّمَاۤ اُرِیْدُ اَنْ یُّطْعِمُوْنِ ۟",
      "juz": 27,
