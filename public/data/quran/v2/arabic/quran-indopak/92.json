{
  "number": 92,
  "name": "سُورَةُ اللَّيۡلِ",
  "englishName": "Al-Lail",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالَّیْلِ اِذَا یَغْشٰی ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالنَّهَارِ اِذَا تَجَلّٰی ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَا خَلَقَ الذَّكَرَ وَالْاُ ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِنَّ سَعْیَكُمْ لَشَتّٰی ۟ؕ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَاَمَّا مَنْ اَعْطٰی وَاتَّقٰی ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَصَدَّقَ بِالْحُسْنٰی ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَسَنُیَسِّرُهٗ لِلْیُسْرٰی ۟ؕ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَاَمَّا مَنْ بَخِلَ وَاسْتَغْنٰی ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَكَذَّبَ بِالْحُسْنٰی ۟ۙ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَسَنُیَسِّرُهٗ لِلْعُسْرٰی ۟ؕ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَمَا یُغْنِیْ عَنْهُ مَالُهٗۤ اِذَا تَرَدّٰی ۟ؕ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِنَّ عَلَیْنَا لَلْهُدٰی ۟ؗۖ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاِنَّ لَنَا لَلْاٰخِرَةَ وَالْاُوْلٰی ۟",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَاَنْذَرْتُكُمْ نَارًا تَلَظّٰی ۟ۚ",
      "juz": 30,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "لَا یَصْلٰىهَاۤ اِلَّا الْاَشْقَی ۟ۙ",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 16,
      "text": "الَّذِیْ كَذَّبَ وَتَوَلّٰی ۟ؕ",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَسَیُجَنَّبُهَا الْاَتْقَی ۟ۙ",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 18,
      "text": "الَّذِیْ یُؤْتِیْ مَالَهٗ یَتَزَكّٰی ۟ۚ",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَمَا لِاَحَدٍ عِنْدَهٗ مِنْ نِّعْمَةٍ تُجْزٰۤی ۟ۙ",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اِلَّا ابْتِغَآءَ وَجْهِ رَبِّهِ الْاَعْلٰی ۟ۚ",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَلَسَوْفَ یَرْضٰی ۟۠",
      "juz": 30,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}