{
  "number": 26,
  "name": "سُورَةُ الشُّعَرَاءِ",
  "englishName": "Ash-Shu'araa",
  "ayahs": [
    {
      "number": 1,
      "text": "طٰسٓمّٓ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 2,
      "text": "تِلْكَ اٰیٰتُ الْكِتٰبِ الْمُبِیْنِ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لَعَلَّكَ بَاخِعٌ نَّفْسَكَ اَلَّا یَكُوْنُوْا مُؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِنْ نَّشَاْ نُنَزِّلْ عَلَیْهِمْ مِّنَ السَّمَآءِ اٰیَةً فَظَلَّتْ اَعْنَاقُهُمْ لَهَا خٰضِعِیْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَمَا یَاْتِیْهِمْ مِّنْ ذِكْرٍ مِّنَ الرَّحْمٰنِ مُحْدَثٍ اِلَّا كَانُوْا عَنْهُ مُعْرِضِیْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَقَدْ كَذَّبُوْا فَسَیَاْتِیْهِمْ اَنْۢبٰٓؤُا مَا كَانُوْا بِهٖ یَسْتَهْزِءُوْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اَوَلَمْ یَرَوْا اِلَی الْاَرْضِ كَمْ اَنْۢبَتْنَا فِیْهَا مِنْ كُلِّ زَوْجٍ كَرِیْمٍ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 367,
      "ruku": 316,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاِذْ نَادٰی رَبُّكَ مُوْسٰۤی اَنِ ائْتِ الْقَوْمَ الظّٰلِمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 11,
      "text": "قَوْمَ فِرْعَوْنَ ؕ اَلَا یَتَّقُوْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 12,
      "text": "قَالَ رَبِّ اِنِّیْۤ اَخَافُ اَنْ یُّكَذِّبُوْنِ ۟ؕ",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَیَضِیْقُ صَدْرِیْ وَلَا یَنْطَلِقُ لِسَانِیْ فَاَرْسِلْ اِلٰی هٰرُوْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَلَهُمْ عَلَیَّ ذَنْۢبٌ فَاَخَافُ اَنْ یَّقْتُلُوْنِ ۟ۚۖ",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 15,
      "text": "قَالَ كَلَّا ۚ فَاذْهَبَا بِاٰیٰتِنَاۤ اِنَّا مَعَكُمْ مُّسْتَمِعُوْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَاْتِیَا فِرْعَوْنَ فَقُوْلَاۤ اِنَّا رَسُوْلُ رَبِّ الْعٰلَمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اَنْ اَرْسِلْ مَعَنَا بَنِیْۤ اِسْرَآءِیْلَ ۟ؕ",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قَالَ اَلَمْ نُرَبِّكَ فِیْنَا وَلِیْدًا وَّلَبِثْتَ فِیْنَا مِنْ عُمُرِكَ سِنِیْنَ ۟ۙ",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَفَعَلْتَ فَعْلَتَكَ الَّتِیْ فَعَلْتَ وَاَنْتَ مِنَ الْكٰفِرِیْنَ ۟",
      "juz": 19,
      "page": 367,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 20,
      "text": "قَالَ فَعَلْتُهَاۤ اِذًا وَّاَنَا مِنَ الضَّآلِّیْنَ ۟ؕ",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَفَرَرْتُ مِنْكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِیْ رَبِّیْ حُكْمًا وَّجَعَلَنِیْ مِنَ الْمُرْسَلِیْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَیَّ اَنْ عَبَّدْتَّ بَنِیْۤ اِسْرَآءِیْلَ ۟ؕ",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 23,
      "text": "قَالَ فِرْعَوْنُ وَمَا رَبُّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 24,
      "text": "قَالَ رَبُّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَیْنَهُمَا ؕ اِنْ كُنْتُمْ مُّوْقِنِیْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 25,
      "text": "قَالَ لِمَنْ حَوْلَهٗۤ اَلَا تَسْتَمِعُوْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قَالَ رَبُّكُمْ وَرَبُّ اٰبَآىِٕكُمُ الْاَوَّلِیْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 27,
      "text": "قَالَ اِنَّ رَسُوْلَكُمُ الَّذِیْۤ اُرْسِلَ اِلَیْكُمْ لَمَجْنُوْنٌ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قَالَ رَبُّ الْمَشْرِقِ وَالْمَغْرِبِ وَمَا بَیْنَهُمَا ؕ اِنْ كُنْتُمْ تَعْقِلُوْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قَالَ لَىِٕنِ اتَّخَذْتَ اِلٰهًا غَیْرِیْ لَاَجْعَلَنَّكَ مِنَ الْمَسْجُوْنِیْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 30,
      "text": "قَالَ اَوَلَوْ جِئْتُكَ بِشَیْءٍ مُّبِیْنٍ ۟ۚ",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قَالَ فَاْتِ بِهٖۤ اِنْ كُنْتَ مِنَ الصّٰدِقِیْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فَاَلْقٰی عَصَاهُ فَاِذَا هِیَ ثُعْبَانٌ مُّبِیْنٌ ۟ۚۖ",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَّنَزَعَ یَدَهٗ فَاِذَا هِیَ بَیْضَآءُ لِلنّٰظِرِیْنَ ۟۠",
      "juz": 19,
      "page": 368,
      "ruku": 317,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 34,
      "text": "قَالَ لِلْمَلَاِ حَوْلَهٗۤ اِنَّ هٰذَا لَسٰحِرٌ عَلِیْمٌ ۟ۙ",
      "juz": 19,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 35,
      "text": "یُّرِیْدُ اَنْ یُّخْرِجَكُمْ مِّنْ اَرْضِكُمْ بِسِحْرِهٖ ۖۗ فَمَاذَا تَاْمُرُوْنَ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 36,
      "text": "قَالُوْۤا اَرْجِهْ وَاَخَاهُ وَابْعَثْ فِی الْمَدَآىِٕنِ حٰشِرِیْنَ ۟ۙ",
      "juz": 19,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 37,
      "text": "یَاْتُوْكَ بِكُلِّ سَحَّارٍ عَلِیْمٍ ۟",
      "juz": 19,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 38,
      "text": "فَجُمِعَ السَّحَرَةُ لِمِیْقَاتِ یَوْمٍ مَّعْلُوْمٍ ۟ۙ",
      "juz": 19,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَّقِیْلَ لِلنَّاسِ هَلْ اَنْتُمْ مُّجْتَمِعُوْنَ ۟ۙ",
      "juz": 19,
      "page": 368,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 40,
      "text": "لَعَلَّنَا نَتَّبِعُ السَّحَرَةَ اِنْ كَانُوْا هُمُ الْغٰلِبِیْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فَلَمَّا جَآءَ السَّحَرَةُ قَالُوْا لِفِرْعَوْنَ اَىِٕنَّ لَنَا لَاَجْرًا اِنْ كُنَّا نَحْنُ الْغٰلِبِیْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 42,
      "text": "قَالَ نَعَمْ وَاِنَّكُمْ اِذًا لَّمِنَ الْمُقَرَّبِیْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 43,
      "text": "قَالَ لَهُمْ مُّوْسٰۤی اَلْقُوْا مَاۤ اَنْتُمْ مُّلْقُوْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَاَلْقَوْا حِبَالَهُمْ وَعِصِیَّهُمْ وَقَالُوْا بِعِزَّةِ فِرْعَوْنَ اِنَّا لَنَحْنُ الْغٰلِبُوْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 45,
      "text": "فَاَلْقٰی مُوْسٰی عَصَاهُ فَاِذَا هِیَ تَلْقَفُ مَا یَاْفِكُوْنَ ۟ۚۖ",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 46,
      "text": "فَاُلْقِیَ السَّحَرَةُ سٰجِدِیْنَ ۟ۙ",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 47,
      "text": "قَالُوْۤا اٰمَنَّا بِرَبِّ الْعٰلَمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 48,
      "text": "رَبِّ مُوْسٰی وَهٰرُوْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 49,
      "text": "قَالَ اٰمَنْتُمْ لَهٗ قَبْلَ اَنْ اٰذَنَ لَكُمْ ۚ اِنَّهٗ لَكَبِیْرُكُمُ الَّذِیْ عَلَّمَكُمُ السِّحْرَ ۚ فَلَسَوْفَ تَعْلَمُوْنَ ؕ۬ لَاُقَطِّعَنَّ اَیْدِیَكُمْ وَاَرْجُلَكُمْ مِّنْ خِلَافٍ وَّلَاُوصَلِّبَنَّكُمْ اَجْمَعِیْنَ ۟ۚ",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 50,
      "text": "قَالُوْا لَا ضَیْرَ ؗ اِنَّاۤ اِلٰی رَبِّنَا مُنْقَلِبُوْنَ ۟ۚ",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 51,
      "text": "اِنَّا نَطْمَعُ اَنْ یَّغْفِرَ لَنَا رَبُّنَا خَطٰیٰنَاۤ اَنْ كُنَّاۤ اَوَّلَ الْمُؤْمِنِیْنَ ۟ؕ۠",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 147,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَاَوْحَیْنَاۤ اِلٰی مُوْسٰۤی اَنْ اَسْرِ بِعِبَادِیْۤ اِنَّكُمْ مُّتَّبَعُوْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 318,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فَاَرْسَلَ فِرْعَوْنُ فِی الْمَدَآىِٕنِ حٰشِرِیْنَ ۟ۚ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 54,
      "text": "اِنَّ هٰۤؤُلَآءِ لَشِرْذِمَةٌ قَلِیْلُوْنَ ۟ۙ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 55,
      "text": "وَاِنَّهُمْ لَنَا لَغَآىِٕظُوْنَ ۟ۙ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 56,
      "text": "وَاِنَّا لَجَمِیْعٌ حٰذِرُوْنَ ۟ؕ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 57,
      "text": "فَاَخْرَجْنٰهُمْ مِّنْ جَنّٰتٍ وَّعُیُوْنٍ ۟ۙ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 58,
      "text": "وَّكُنُوْزٍ وَّمَقَامٍ كَرِیْمٍ ۟ۙ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 59,
      "text": "كَذٰلِكَ ؕ وَاَوْرَثْنٰهَا بَنِیْۤ اِسْرَآءِیْلَ ۟ؕ",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 60,
      "text": "فَاَتْبَعُوْهُمْ مُّشْرِقِیْنَ ۟",
      "juz": 19,
      "page": 369,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 61,
      "text": "فَلَمَّا تَرَآءَ الْجَمْعٰنِ قَالَ اَصْحٰبُ مُوْسٰۤی اِنَّا لَمُدْرَكُوْنَ ۟ۚ",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 62,
      "text": "قَالَ كَلَّا ۚ اِنَّ مَعِیَ رَبِّیْ سَیَهْدِیْنِ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 63,
      "text": "فَاَوْحَیْنَاۤ اِلٰی مُوْسٰۤی اَنِ اضْرِبْ بِّعَصَاكَ الْبَحْرَ ؕ فَانْفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَالطَّوْدِ الْعَظِیْمِ ۟ۚ",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 64,
      "text": "وَاَزْلَفْنَا ثَمَّ الْاٰخَرِیْنَ ۟ۚ",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 65,
      "text": "وَاَنْجَیْنَا مُوْسٰی وَمَنْ مَّعَهٗۤ اَجْمَعِیْنَ ۟ۚ",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 66,
      "text": "ثُمَّ اَغْرَقْنَا الْاٰخَرِیْنَ ۟ؕ",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 67,
      "text": "اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 68,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 69,
      "text": "وَاتْلُ عَلَیْهِمْ نَبَاَ اِبْرٰهِیْمَ ۟ۘ",
      "juz": 19,
      "page": 370,
      "ruku": 319,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 70,
      "text": "اِذْ قَالَ لِاَبِیْهِ وَقَوْمِهٖ مَا تَعْبُدُوْنَ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 71,
      "text": "قَالُوْا نَعْبُدُ اَصْنَامًا فَنَظَلُّ لَهَا عٰكِفِیْنَ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 72,
      "text": "قَالَ هَلْ یَسْمَعُوْنَكُمْ اِذْ تَدْعُوْنَ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 73,
      "text": "اَوْ یَنْفَعُوْنَكُمْ اَوْ یَضُرُّوْنَ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 74,
      "text": "قَالُوْا بَلْ وَجَدْنَاۤ اٰبَآءَنَا كَذٰلِكَ یَفْعَلُوْنَ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 75,
      "text": "قَالَ اَفَرَءَیْتُمْ مَّا كُنْتُمْ تَعْبُدُوْنَ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 76,
      "text": "اَنْتُمْ وَاٰبَآؤُكُمُ الْاَقْدَمُوْنَ ۟ؗۖ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 77,
      "text": "فَاِنَّهُمْ عَدُوٌّ لِّیْۤ اِلَّا رَبَّ الْعٰلَمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 78,
      "text": "الَّذِیْ خَلَقَنِیْ فَهُوَ یَهْدِیْنِ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 79,
      "text": "وَالَّذِیْ هُوَ یُطْعِمُنِیْ وَیَسْقِیْنِ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 80,
      "text": "وَاِذَا مَرِضْتُ فَهُوَ یَشْفِیْنِ ۟",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 81,
      "text": "وَالَّذِیْ یُمِیْتُنِیْ ثُمَّ یُحْیِیْنِ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 82,
      "text": "وَالَّذِیْۤ اَطْمَعُ اَنْ یَّغْفِرَ لِیْ خَطِیْٓـَٔتِیْ یَوْمَ الدِّیْنِ ۟ؕ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 83,
      "text": "رَبِّ هَبْ لِیْ حُكْمًا وَّاَلْحِقْنِیْ بِالصّٰلِحِیْنَ ۟ۙ",
      "juz": 19,
      "page": 370,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 84,
      "text": "وَاجْعَلْ لِّیْ لِسَانَ صِدْقٍ فِی الْاٰخِرِیْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وَاجْعَلْنِیْ مِنْ وَّرَثَةِ جَنَّةِ النَّعِیْمِ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 86,
      "text": "وَاغْفِرْ لِاَبِیْۤ اِنَّهٗ كَانَ مِنَ الضَّآلِّیْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 87,
      "text": "وَلَا تُخْزِنِیْ یَوْمَ یُبْعَثُوْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 88,
      "text": "یَوْمَ لَا یَنْفَعُ مَالٌ وَّلَا بَنُوْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 89,
      "text": "اِلَّا مَنْ اَتَی اللّٰهَ بِقَلْبٍ سَلِیْمٍ ۟ؕ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 90,
      "text": "وَاُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِیْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 91,
      "text": "وَبُرِّزَتِ الْجَحِیْمُ لِلْغٰوِیْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 92,
      "text": "وَقِیْلَ لَهُمْ اَیْنَ مَا كُنْتُمْ تَعْبُدُوْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 93,
      "text": "مِنْ دُوْنِ اللّٰهِ ؕ هَلْ یَنْصُرُوْنَكُمْ اَوْ یَنْتَصِرُوْنَ ۟ؕ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 94,
      "text": "فَكُبْكِبُوْا فِیْهَا هُمْ وَالْغَاوٗنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 95,
      "text": "وَجُنُوْدُ اِبْلِیْسَ اَجْمَعُوْنَ ۟ؕ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 96,
      "text": "قَالُوْا وَهُمْ فِیْهَا یَخْتَصِمُوْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 97,
      "text": "تَاللّٰهِ اِنْ كُنَّا لَفِیْ ضَلٰلٍ مُّبِیْنٍ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 98,
      "text": "اِذْ نُسَوِّیْكُمْ بِرَبِّ الْعٰلَمِیْنَ ۟",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 99,
      "text": "وَمَاۤ اَضَلَّنَاۤ اِلَّا الْمُجْرِمُوْنَ ۟",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 100,
      "text": "فَمَا لَنَا مِنْ شَافِعِیْنَ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 101,
      "text": "وَلَا صَدِیْقٍ حَمِیْمٍ ۟",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 102,
      "text": "فَلَوْ اَنَّ لَنَا كَرَّةً فَنَكُوْنَ مِنَ الْمُؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 103,
      "text": "اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 104,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 371,
      "ruku": 320,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 105,
      "text": "كَذَّبَتْ قَوْمُ نُوْحِ لْمُرْسَلِیْنَ ۟ۚۖ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 106,
      "text": "اِذْ قَالَ لَهُمْ اَخُوْهُمْ نُوْحٌ اَلَا تَتَّقُوْنَ ۟ۚ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 107,
      "text": "اِنِّیْ لَكُمْ رَسُوْلٌ اَمِیْنٌ ۟ۙ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 108,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 109,
      "text": "وَمَاۤ اَسْـَٔلُكُمْ عَلَیْهِ مِنْ اَجْرٍ ۚ اِنْ اَجْرِیَ اِلَّا عَلٰی رَبِّ الْعٰلَمِیْنَ ۟ۚ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 110,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ؕ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 148,
      "sajda": false
    },
    {
      "number": 111,
      "text": "قَالُوْۤا اَنُؤْمِنُ لَكَ وَاتَّبَعَكَ الْاَرْذَلُوْنَ ۟ؕ",
      "juz": 19,
      "page": 371,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 112,
      "text": "قَالَ وَمَا عِلْمِیْ بِمَا كَانُوْا یَعْمَلُوْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 113,
      "text": "اِنْ حِسَابُهُمْ اِلَّا عَلٰی رَبِّیْ لَوْ تَشْعُرُوْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 114,
      "text": "وَمَاۤ اَنَا بِطَارِدِ الْمُؤْمِنِیْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 115,
      "text": "اِنْ اَنَا اِلَّا نَذِیْرٌ مُّبِیْنٌ ۟ؕ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 116,
      "text": "قَالُوْا لَىِٕنْ لَّمْ تَنْتَهِ یٰنُوْحُ لَتَكُوْنَنَّ مِنَ الْمَرْجُوْمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 117,
      "text": "قَالَ رَبِّ اِنَّ قَوْمِیْ كَذَّبُوْنِ ۟ۚۖ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 118,
      "text": "فَافْتَحْ بَیْنِیْ وَبَیْنَهُمْ فَتْحًا وَّنَجِّنِیْ وَمَنْ مَّعِیَ مِنَ الْمُؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 119,
      "text": "فَاَنْجَیْنٰهُ وَمَنْ مَّعَهٗ فِی الْفُلْكِ الْمَشْحُوْنِ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 120,
      "text": "ثُمَّ اَغْرَقْنَا بَعْدُ الْبٰقِیْنَ ۟ؕ",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 121,
      "text": "اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 122,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 372,
      "ruku": 321,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 123,
      "text": "كَذَّبَتْ عَادُ لْمُرْسَلِیْنَ ۟ۚۖ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 124,
      "text": "اِذْ قَالَ لَهُمْ اَخُوْهُمْ هُوْدٌ اَلَا تَتَّقُوْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 125,
      "text": "اِنِّیْ لَكُمْ رَسُوْلٌ اَمِیْنٌ ۟ۙ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 126,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 127,
      "text": "وَمَاۤ اَسْـَٔلُكُمْ عَلَیْهِ مِنْ اَجْرٍ ۚ اِنْ اَجْرِیَ اِلَّا عَلٰی رَبِّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 128,
      "text": "اَتَبْنُوْنَ بِكُلِّ رِیْعٍ اٰیَةً تَعْبَثُوْنَ ۟ۙ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 129,
      "text": "وَتَتَّخِذُوْنَ مَصَانِعَ لَعَلَّكُمْ تَخْلُدُوْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 130,
      "text": "وَاِذَا بَطَشْتُمْ بَطَشْتُمْ جَبَّارِیْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 131,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 132,
      "text": "وَاتَّقُوا الَّذِیْۤ اَمَدَّكُمْ بِمَا تَعْلَمُوْنَ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 133,
      "text": "اَمَدَّكُمْ بِاَنْعَامٍ وَّبَنِیْنَ ۟ۚۙ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 134,
      "text": "وَجَنّٰتٍ وَّعُیُوْنٍ ۟ۚ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 135,
      "text": "اِنِّیْۤ اَخَافُ عَلَیْكُمْ عَذَابَ یَوْمٍ عَظِیْمٍ ۟ؕ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 136,
      "text": "قَالُوْا سَوَآءٌ عَلَیْنَاۤ اَوَعَظْتَ اَمْ لَمْ تَكُنْ مِّنَ الْوٰعِظِیْنَ ۟ۙ",
      "juz": 19,
      "page": 372,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 137,
      "text": "اِنْ هٰذَاۤ اِلَّا خُلُقُ الْاَوَّلِیْنَ ۟ۙ",
      "juz": 19,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 138,
      "text": "وَمَا نَحْنُ بِمُعَذَّبِیْنَ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 139,
      "text": "فَكَذَّبُوْهُ فَاَهْلَكْنٰهُمْ ؕ اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 140,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 373,
      "ruku": 322,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 141,
      "text": "كَذَّبَتْ ثَمُوْدُ الْمُرْسَلِیْنَ ۟ۚۖ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 142,
      "text": "اِذْ قَالَ لَهُمْ اَخُوْهُمْ صٰلِحٌ اَلَا تَتَّقُوْنَ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 143,
      "text": "اِنِّیْ لَكُمْ رَسُوْلٌ اَمِیْنٌ ۟ۙ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 144,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 145,
      "text": "وَمَاۤ اَسْـَٔلُكُمْ عَلَیْهِ مِنْ اَجْرٍ ۚ اِنْ اَجْرِیَ اِلَّا عَلٰی رَبِّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 146,
      "text": "اَتُتْرَكُوْنَ فِیْ مَا هٰهُنَاۤ اٰمِنِیْنَ ۟ۙ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 147,
      "text": "فِیْ جَنّٰتٍ وَّعُیُوْنٍ ۟ۙ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 148,
      "text": "وَّزُرُوْعٍ وَّنَخْلٍ طَلْعُهَا هَضِیْمٌ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 149,
      "text": "وَتَنْحِتُوْنَ مِنَ الْجِبَالِ بُیُوْتًا فٰرِهِیْنَ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 150,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 151,
      "text": "وَلَا تُطِیْعُوْۤا اَمْرَ الْمُسْرِفِیْنَ ۟ۙ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 152,
      "text": "الَّذِیْنَ یُفْسِدُوْنَ فِی الْاَرْضِ وَلَا یُصْلِحُوْنَ ۟",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 153,
      "text": "قَالُوْۤا اِنَّمَاۤ اَنْتَ مِنَ الْمُسَحَّرِیْنَ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 154,
      "text": "مَاۤ اَنْتَ اِلَّا بَشَرٌ مِّثْلُنَا ۖۚ فَاْتِ بِاٰیَةٍ اِنْ كُنْتَ مِنَ الصّٰدِقِیْنَ ۟",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 155,
      "text": "قَالَ هٰذِهٖ نَاقَةٌ لَّهَا شِرْبٌ وَّلَكُمْ شِرْبُ یَوْمٍ مَّعْلُوْمٍ ۟ۚ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 156,
      "text": "وَلَا تَمَسُّوْهَا بِسُوْٓءٍ فَیَاْخُذَكُمْ عَذَابُ یَوْمٍ عَظِیْمٍ ۟",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 157,
      "text": "فَعَقَرُوْهَا فَاَصْبَحُوْا نٰدِمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 158,
      "text": "فَاَخَذَهُمُ الْعَذَابُ ؕ اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 159,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 373,
      "ruku": 323,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 160,
      "text": "كَذَّبَتْ قَوْمُ لُوْطِ لْمُرْسَلِیْنَ ۟ۚۖ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 161,
      "text": "اِذْ قَالَ لَهُمْ اَخُوْهُمْ لُوْطٌ اَلَا تَتَّقُوْنَ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 162,
      "text": "اِنِّیْ لَكُمْ رَسُوْلٌ اَمِیْنٌ ۟ۙ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 163,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 164,
      "text": "وَمَاۤ اَسْـَٔلُكُمْ عَلَیْهِ مِنْ اَجْرٍ ۚ اِنْ اَجْرِیَ اِلَّا عَلٰی رَبِّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 165,
      "text": "اَتَاْتُوْنَ الذُّكْرَانَ مِنَ الْعٰلَمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 166,
      "text": "وَتَذَرُوْنَ مَا خَلَقَ لَكُمْ رَبُّكُمْ مِّنْ اَزْوَاجِكُمْ ؕ بَلْ اَنْتُمْ قَوْمٌ عٰدُوْنَ ۟",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 167,
      "text": "قَالُوْا لَىِٕنْ لَّمْ تَنْتَهِ یٰلُوْطُ لَتَكُوْنَنَّ مِنَ الْمُخْرَجِیْنَ ۟",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 168,
      "text": "قَالَ اِنِّیْ لِعَمَلِكُمْ مِّنَ الْقَالِیْنَ ۟ؕ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 169,
      "text": "رَبِّ نَجِّنِیْ وَاَهْلِیْ مِمَّا یَعْمَلُوْنَ ۟",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 170,
      "text": "فَنَجَّیْنٰهُ وَاَهْلَهٗۤ اَجْمَعِیْنَ ۟ۙ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 171,
      "text": "اِلَّا عَجُوْزًا فِی الْغٰبِرِیْنَ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 172,
      "text": "ثُمَّ دَمَّرْنَا الْاٰخَرِیْنَ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 173,
      "text": "وَاَمْطَرْنَا عَلَیْهِمْ مَّطَرًا ۚ فَسَآءَ مَطَرُ الْمُنْذَرِیْنَ ۟",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 174,
      "text": "اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 175,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 374,
      "ruku": 324,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 176,
      "text": "كَذَّبَ اَصْحٰبُ لْـَٔیْكَةِ الْمُرْسَلِیْنَ ۟ۚۖ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 177,
      "text": "اِذْ قَالَ لَهُمْ شُعَیْبٌ اَلَا تَتَّقُوْنَ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 178,
      "text": "اِنِّیْ لَكُمْ رَسُوْلٌ اَمِیْنٌ ۟ۙ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 179,
      "text": "فَاتَّقُوا اللّٰهَ وَاَطِیْعُوْنِ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 180,
      "text": "وَمَاۤ اَسْـَٔلُكُمْ عَلَیْهِ مِنْ اَجْرٍ ۚ اِنْ اَجْرِیَ اِلَّا عَلٰی رَبِّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 149,
      "sajda": false
    },
    {
      "number": 181,
      "text": "اَوْفُوا الْكَیْلَ وَلَا تَكُوْنُوْا مِنَ الْمُخْسِرِیْنَ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 182,
      "text": "وَزِنُوْا بِالْقِسْطَاسِ الْمُسْتَقِیْمِ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 183,
      "text": "وَلَا تَبْخَسُوا النَّاسَ اَشْیَآءَهُمْ وَلَا تَعْثَوْا فِی الْاَرْضِ مُفْسِدِیْنَ ۟ۚ",
      "juz": 19,
      "page": 374,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 184,
      "text": "وَاتَّقُوا الَّذِیْ خَلَقَكُمْ وَالْجِبِلَّةَ الْاَوَّلِیْنَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 185,
      "text": "قَالُوْۤا اِنَّمَاۤ اَنْتَ مِنَ الْمُسَحَّرِیْنَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 186,
      "text": "وَمَاۤ اَنْتَ اِلَّا بَشَرٌ مِّثْلُنَا وَاِنْ نَّظُنُّكَ لَمِنَ الْكٰذِبِیْنَ ۟ۚ",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 187,
      "text": "فَاَسْقِطْ عَلَیْنَا كِسَفًا مِّنَ السَّمَآءِ اِنْ كُنْتَ مِنَ الصّٰدِقِیْنَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 188,
      "text": "قَالَ رَبِّیْۤ اَعْلَمُ بِمَا تَعْمَلُوْنَ ۟",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 189,
      "text": "فَكَذَّبُوْهُ فَاَخَذَهُمْ عَذَابُ یَوْمِ الظُّلَّةِ ؕ اِنَّهٗ كَانَ عَذَابَ یَوْمٍ عَظِیْمٍ ۟",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 190,
      "text": "اِنَّ فِیْ ذٰلِكَ لَاٰیَةً ؕ وَمَا كَانَ اَكْثَرُهُمْ مُّؤْمِنِیْنَ ۟",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 191,
      "text": "وَاِنَّ رَبَّكَ لَهُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 19,
      "page": 375,
      "ruku": 325,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 192,
      "text": "وَاِنَّهٗ لَتَنْزِیْلُ رَبِّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 193,
      "text": "نَزَلَ بِهِ الرُّوْحُ الْاَمِیْنُ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 194,
      "text": "عَلٰی قَلْبِكَ لِتَكُوْنَ مِنَ الْمُنْذِرِیْنَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 195,
      "text": "بِلِسَانٍ عَرَبِیٍّ مُّبِیْنٍ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 196,
      "text": "وَاِنَّهٗ لَفِیْ زُبُرِ الْاَوَّلِیْنَ ۟",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 197,
      "text": "اَوَلَمْ یَكُنْ لَّهُمْ اٰیَةً اَنْ یَّعْلَمَهٗ عُلَمٰٓؤُا بَنِیْۤ اِسْرَآءِیْلَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 198,
      "text": "وَلَوْ نَزَّلْنٰهُ عَلٰی بَعْضِ الْاَعْجَمِیْنَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 199,
      "text": "فَقَرَاَهٗ عَلَیْهِمْ مَّا كَانُوْا بِهٖ مُؤْمِنِیْنَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 200,
      "text": "كَذٰلِكَ سَلَكْنٰهُ فِیْ قُلُوْبِ الْمُجْرِمِیْنَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 201,
      "text": "لَا یُؤْمِنُوْنَ بِهٖ حَتّٰی یَرَوُا الْعَذَابَ الْاَلِیْمَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 202,
      "text": "فَیَاْتِیَهُمْ بَغْتَةً وَّهُمْ لَا یَشْعُرُوْنَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 203,
      "text": "فَیَقُوْلُوْا هَلْ نَحْنُ مُنْظَرُوْنَ ۟ؕ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 204,
      "text": "اَفَبِعَذَابِنَا یَسْتَعْجِلُوْنَ ۟",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 205,
      "text": "اَفَرَءَیْتَ اِنْ مَّتَّعْنٰهُمْ سِنِیْنَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 206,
      "text": "ثُمَّ جَآءَهُمْ مَّا كَانُوْا یُوْعَدُوْنَ ۟ۙ",
      "juz": 19,
      "page": 375,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 207,
      "text": "مَاۤ اَغْنٰی عَنْهُمْ مَّا كَانُوْا یُمَتَّعُوْنَ ۟ؕ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 208,
      "text": "وَمَاۤ اَهْلَكْنَا مِنْ قَرْیَةٍ اِلَّا لَهَا مُنْذِرُوْنَ ۟",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 209,
      "text": "ذِكْرٰی ۛ۫ وَمَا كُنَّا ظٰلِمِیْنَ ۟",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 210,
      "text": "وَمَا تَنَزَّلَتْ بِهِ الشَّیٰطِیْنُ ۟",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 211,
      "text": "وَمَا یَنْۢبَغِیْ لَهُمْ وَمَا یَسْتَطِیْعُوْنَ ۟ؕ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 212,
      "text": "اِنَّهُمْ عَنِ السَّمْعِ لَمَعْزُوْلُوْنَ ۟ؕ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 213,
      "text": "فَلَا تَدْعُ مَعَ اللّٰهِ اِلٰهًا اٰخَرَ فَتَكُوْنَ مِنَ الْمُعَذَّبِیْنَ ۟ۚ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 214,
      "text": "وَاَنْذِرْ عَشِیْرَتَكَ الْاَقْرَبِیْنَ ۟ۙ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 215,
      "text": "وَاخْفِضْ جَنَاحَكَ لِمَنِ اتَّبَعَكَ مِنَ الْمُؤْمِنِیْنَ ۟ۚ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 216,
      "text": "فَاِنْ عَصَوْكَ فَقُلْ اِنِّیْ بَرِیْٓءٌ مِّمَّا تَعْمَلُوْنَ ۟ۚ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 217,
      "text": "وَتَوَكَّلْ عَلَی الْعَزِیْزِ الرَّحِیْمِ ۟ۙ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 218,
      "text": "الَّذِیْ یَرٰىكَ حِیْنَ تَقُوْمُ ۟ۙ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 219,
      "text": "وَتَقَلُّبَكَ فِی السّٰجِدِیْنَ ۟",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 220,
      "text": "اِنَّهٗ هُوَ السَّمِیْعُ الْعَلِیْمُ ۟",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 221,
      "text": "هَلْ اُنَبِّئُكُمْ عَلٰی مَنْ تَنَزَّلُ الشَّیٰطِیْنُ ۟ؕ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 222,
      "text": "تَنَزَّلُ عَلٰی كُلِّ اَفَّاكٍ اَثِیْمٍ ۟ۙ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 223,
      "text": "یُّلْقُوْنَ السَّمْعَ وَاَكْثَرُهُمْ كٰذِبُوْنَ ۟ؕ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 224,
      "text": "وَالشُّعَرَآءُ یَتَّبِعُهُمُ الْغَاوٗنَ ۟ؕ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 225,
      "text": "اَلَمْ تَرَ اَنَّهُمْ فِیْ كُلِّ وَادٍ یَّهِیْمُوْنَ ۟ۙ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 226,
      "text": "وَاَنَّهُمْ یَقُوْلُوْنَ مَا لَا یَفْعَلُوْنَ ۟ۙ",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    },
    {
      "number": 227,
      "text": "اِلَّا الَّذِیْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ وَذَكَرُوا اللّٰهَ كَثِیْرًا وَّانْتَصَرُوْا مِنْ بَعْدِ مَا ظُلِمُوْا ؕ وَسَیَعْلَمُ الَّذِیْنَ ظَلَمُوْۤا اَیَّ مُنْقَلَبٍ یَّنْقَلِبُوْنَ ۟۠",
      "juz": 19,
      "page": 376,
      "ruku": 326,
      "hizbQuarter": 150,
      "sajda": false
    }
  ]
}