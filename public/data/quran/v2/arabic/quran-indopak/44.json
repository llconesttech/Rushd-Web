{
  "number": 44,
  "name": "سُورَةُ الدُّخَانِ",
  "englishName": "Ad-Dukhaan",
  "ayahs": [
    {
      "number": 1,
      "text": "حٰمٓ ۟ۚۛ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالْكِتٰبِ الْمُبِیْنِ ۟ۙۛ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اِنَّاۤ اَنْزَلْنٰهُ فِیْ لَیْلَةٍ مُّبٰرَكَةٍ اِنَّا كُنَّا مُنْذِرِیْنَ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فِیْهَا یُفْرَقُ كُلُّ اَمْرٍ حَكِیْمٍ ۟ۙ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اَمْرًا مِّنْ عِنْدِنَا ؕ اِنَّا كُنَّا مُرْسِلِیْنَ ۟ۚ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 6,
      "text": "رَحْمَةً مِّنْ رَّبِّكَ ؕ اِنَّهٗ هُوَ السَّمِیْعُ الْعَلِیْمُ ۟ۙ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 7,
      "text": "رَبِّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَیْنَهُمَا ۘ اِنْ كُنْتُمْ مُّوْقِنِیْنَ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 8,
      "text": "لَاۤ اِلٰهَ اِلَّا هُوَ یُحْیٖ وَیُمِیْتُ ؕ رَبُّكُمْ وَرَبُّ اٰبَآىِٕكُمُ الْاَوَّلِیْنَ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 9,
      "text": "بَلْ هُمْ فِیْ شَكٍّ یَّلْعَبُوْنَ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَارْتَقِبْ یَوْمَ تَاْتِی السَّمَآءُ بِدُخَانٍ مُّبِیْنٍ ۟ۙ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یَّغْشَی النَّاسَ ؕ هٰذَا عَذَابٌ اَلِیْمٌ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 12,
      "text": "رَبَّنَا اكْشِفْ عَنَّا الْعَذَابَ اِنَّا مُؤْمِنُوْنَ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اَنّٰی لَهُمُ الذِّكْرٰی وَقَدْ جَآءَهُمْ رَسُوْلٌ مُّبِیْنٌ ۟ۙ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ثُمَّ تَوَلَّوْا عَنْهُ وَقَالُوْا مُعَلَّمٌ مَّجْنُوْنٌ ۟ۘ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِنَّا كَاشِفُوا الْعَذَابِ قَلِیْلًا اِنَّكُمْ عَآىِٕدُوْنَ ۟ۘ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 16,
      "text": "یَوْمَ نَبْطِشُ الْبَطْشَةَ الْكُبْرٰی ۚ اِنَّا مُنْتَقِمُوْنَ ۟",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَلَقَدْ فَتَنَّا قَبْلَهُمْ قَوْمَ فِرْعَوْنَ وَجَآءَهُمْ رَسُوْلٌ كَرِیْمٌ ۟ۙ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اَنْ اَدُّوْۤا اِلَیَّ عِبَادَ اللّٰهِ ؕ اِنِّیْ لَكُمْ رَسُوْلٌ اَمِیْنٌ ۟ۙ",
      "juz": 25,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَّاَنْ لَّا تَعْلُوْا عَلَی اللّٰهِ ؕ اِنِّیْۤ اٰتِیْكُمْ بِسُلْطٰنٍ مُّبِیْنٍ ۟ۚ",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَاِنِّیْ عُذْتُ بِرَبِّیْ وَرَبِّكُمْ اَنْ تَرْجُمُوْنِ ۟ؗ",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَاِنْ لَّمْ تُؤْمِنُوْا لِیْ فَاعْتَزِلُوْنِ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 22,
      "text": "فَدَعَا رَبَّهٗۤ اَنَّ هٰۤؤُلَآءِ قَوْمٌ مُّجْرِمُوْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَاَسْرِ بِعِبَادِیْ لَیْلًا اِنَّكُمْ مُّتَّبَعُوْنَ ۟ۙ",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَاتْرُكِ الْبَحْرَ رَهْوًا ؕ اِنَّهُمْ جُنْدٌ مُّغْرَقُوْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 25,
      "text": "كَمْ تَرَكُوْا مِنْ جَنّٰتٍ وَّعُیُوْنٍ ۟ۙ",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَّزُرُوْعٍ وَّمَقَامٍ كَرِیْمٍ ۟ۙ",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَّنَعْمَةٍ كَانُوْا فِیْهَا فٰكِهِیْنَ ۟ۙ",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 28,
      "text": "كَذٰلِكَ ۫ وَاَوْرَثْنٰهَا قَوْمًا اٰخَرِیْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَمَا بَكَتْ عَلَیْهِمُ السَّمَآءُ وَالْاَرْضُ وَمَا كَانُوْا مُنْظَرِیْنَ ۟۠",
      "juz": 25,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَلَقَدْ نَجَّیْنَا بَنِیْۤ اِسْرَآءِیْلَ مِنَ الْعَذَابِ الْمُهِیْنِ ۟ۙ",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 31,
      "text": "مِنْ فِرْعَوْنَ ؕ اِنَّهٗ كَانَ عَالِیًا مِّنَ الْمُسْرِفِیْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلَقَدِ اخْتَرْنٰهُمْ عَلٰی عِلْمٍ عَلَی الْعٰلَمِیْنَ ۟ۚ",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَاٰتَیْنٰهُمْ مِّنَ الْاٰیٰتِ مَا فِیْهِ بَلٰٓؤٌا مُّبِیْنٌ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اِنَّ هٰۤؤُلَآءِ لَیَقُوْلُوْنَ ۟ۙ",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اِنْ هِیَ اِلَّا مَوْتَتُنَا الْاُوْلٰی وَمَا نَحْنُ بِمُنْشَرِیْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَاْتُوْا بِاٰبَآىِٕنَاۤ اِنْ كُنْتُمْ صٰدِقِیْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اَهُمْ خَیْرٌ اَمْ قَوْمُ تُبَّعٍ ۙ وَّالَّذِیْنَ مِنْ قَبْلِهِمْ ؕ اَهْلَكْنٰهُمْ ؗ اِنَّهُمْ كَانُوْا مُجْرِمِیْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَمَا خَلَقْنَا السَّمٰوٰتِ وَالْاَرْضَ وَمَا بَیْنَهُمَا لٰعِبِیْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 39,
      "text": "مَا خَلَقْنٰهُمَاۤ اِلَّا بِالْحَقِّ وَلٰكِنَّ اَكْثَرَهُمْ لَا یَعْلَمُوْنَ ۟",
      "juz": 25,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اِنَّ یَوْمَ الْفَصْلِ مِیْقَاتُهُمْ اَجْمَعِیْنَ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 41,
      "text": "یَوْمَ لَا یُغْنِیْ مَوْلًی عَنْ مَّوْلًی شَیْـًٔا وَّلَا هُمْ یُنْصَرُوْنَ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 42,
      "text": "اِلَّا مَنْ رَّحِمَ اللّٰهُ ؕ اِنَّهٗ هُوَ الْعَزِیْزُ الرَّحِیْمُ ۟۠",
      "juz": 25,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اِنَّ شَجَرَتَ الزَّقُّوْمِ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 44,
      "text": "طَعَامُ الْاَثِیْمِ ۟",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 45,
      "text": "كَالْمُهْلِ ۛۚ یَغْلِیْ فِی الْبُطُوْنِ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كَغَلْیِ الْحَمِیْمِ ۟",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 47,
      "text": "خُذُوْهُ فَاعْتِلُوْهُ اِلٰی سَوَآءِ الْجَحِیْمِ ۟ۗۖ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 48,
      "text": "ثُمَّ صُبُّوْا فَوْقَ رَاْسِهٖ مِنْ عَذَابِ الْحَمِیْمِ ۟ؕ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 49,
      "text": "ذُقْ ۙۚ اِنَّكَ اَنْتَ الْعَزِیْزُ الْكَرِیْمُ ۟",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 50,
      "text": "اِنَّ هٰذَا مَا كُنْتُمْ بِهٖ تَمْتَرُوْنَ ۟",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 51,
      "text": "اِنَّ الْمُتَّقِیْنَ فِیْ مَقَامٍ اَمِیْنٍ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 52,
      "text": "فِیْ جَنّٰتٍ وَّعُیُوْنٍ ۟ۚۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 53,
      "text": "یَّلْبَسُوْنَ مِنْ سُنْدُسٍ وَّاِسْتَبْرَقٍ مُّتَقٰبِلِیْنَ ۟ۚۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 54,
      "text": "كَذٰلِكَ ۫ وَزَوَّجْنٰهُمْ بِحُوْرٍ عِیْنٍ ۟ؕ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 55,
      "text": "یَدْعُوْنَ فِیْهَا بِكُلِّ فَاكِهَةٍ اٰمِنِیْنَ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 56,
      "text": "لَا یَذُوْقُوْنَ فِیْهَا الْمَوْتَ اِلَّا الْمَوْتَةَ الْاُوْلٰی ۚ وَوَقٰىهُمْ عَذَابَ الْجَحِیْمِ ۟ۙ",
      "juz": 25,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 57,
