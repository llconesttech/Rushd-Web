{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالنّٰزِعٰتِ غَرْقًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَّالنّٰشِطٰتِ نَشْطًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّالسّٰبِحٰتِ سَبْحًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَالسّٰبِقٰتِ سَبْقًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَالْمُدَبِّرٰتِ اَمْرًا ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَوْمَ تَرْجُفُ الرَّاجِفَةُ ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تَتْبَعُهَا الرَّادِفَةُ ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "قُلُوْبٌ یَّوْمَىِٕذٍ وَّاجِفَةٌ ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اَبْصَارُهَا خَاشِعَةٌ ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "یَقُوْلُوْنَ ءَاِنَّا لَمَرْدُوْدُوْنَ فِی الْحَافِرَةِ ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "قَالُوْا تِلْكَ اِذًا كَرَّةٌ خَاسِرَةٌ ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَاِنَّمَا هِیَ زَجْرَةٌ وَّاحِدَةٌ ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَاِذَا هُمْ بِالسَّاهِرَةِ ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ مُوْسٰی ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اِذْ نَادٰىهُ رَبُّهٗ بِالْوَادِ الْمُقَدَّسِ طُوًی ۟ۚ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِذْهَبْ اِلٰی فِرْعَوْنَ اِنَّهٗ طَغٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَقُلْ هَلْ لَّكَ اِلٰۤی اَنْ تَزَكّٰی ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَاَهْدِیَكَ اِلٰی رَبِّكَ فَتَخْشٰی ۟ۚ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَاَرٰىهُ الْاٰیَةَ الْكُبْرٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَكَذَّبَ وَعَصٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ثُمَّ اَدْبَرَ یَسْعٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَحَشَرَ فَنَادٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَقَالَ اَنَا رَبُّكُمُ الْاَعْلٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَاَخَذَهُ اللّٰهُ نَكَالَ الْاٰخِرَةِ وَالْاُوْلٰی ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "اِنَّ فِیْ ذٰلِكَ لَعِبْرَةً لِّمَنْ یَّخْشٰی ۟ؕ۠",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "ءَاَنْتُمْ اَشَدُّ خَلْقًا اَمِ السَّمَآءُ ؕ بَنٰىهَا ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "رَفَعَ سَمْكَهَا فَسَوّٰىهَا ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَاَغْطَشَ لَیْلَهَا وَاَخْرَجَ ضُحٰىهَا ۪۟",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَالْاَرْضَ بَعْدَ ذٰلِكَ دَحٰىهَا ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعٰىهَا ۪۟",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَالْجِبَالَ اَرْسٰىهَا ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "مَتَاعًا لَّكُمْ وَلِاَنْعَامِكُمْ ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَاِذَا جَآءَتِ الطَّآمَّةُ الْكُبْرٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "یَوْمَ یَتَذَكَّرُ الْاِنْسَانُ مَا سَعٰی ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَبُرِّزَتِ الْجَحِیْمُ لِمَنْ یَّرٰی ۟",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "فَاَمَّا مَنْ طَغٰی ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَاٰثَرَ الْحَیٰوةَ الدُّنْیَا ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَاِنَّ الْجَحِیْمَ هِیَ الْمَاْوٰی ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَاَمَّا مَنْ خَافَ مَقَامَ رَبِّهٖ وَنَهَی النَّفْسَ عَنِ الْهَوٰی ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فَاِنَّ الْجَنَّةَ هِیَ الْمَاْوٰی ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 42,
      "text": "یَسْـَٔلُوْنَكَ عَنِ السَّاعَةِ اَیَّانَ مُرْسٰىهَا ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 43,
      "text": "فِیْمَ اَنْتَ مِنْ ذِكْرٰىهَا ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 44,
      "text": "اِلٰی رَبِّكَ مُنْتَهٰىهَا ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 45,
      "text": "اِنَّمَاۤ اَنْتَ مُنْذِرُ مَنْ یَّخْشٰىهَا ۟ؕ",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كَاَنَّهُمْ یَوْمَ یَرَوْنَهَا لَمْ یَلْبَثُوْۤا اِلَّا عَشِیَّةً اَوْ ضُحٰىهَا ۟۠",
      "juz": 30,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}