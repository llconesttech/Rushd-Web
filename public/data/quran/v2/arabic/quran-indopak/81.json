{
  "number": 81,
  "name": "سُورَةُ التَّكۡوِيرِ",
  "englishName": "At-Takwir",
  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا الشَّمْسُ كُوِّرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاِذَا النُّجُوْمُ انْكَدَرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِذَا الْجِبَالُ سُیِّرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاِذَا الْعِشَارُ عُطِّلَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَاِذَا الْوُحُوْشُ حُشِرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَاِذَا الْبِحَارُ سُجِّرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَاِذَا النُّفُوْسُ زُوِّجَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَاِذَا الْمَوْءٗدَةُ سُىِٕلَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 9,
      "text": "بِاَیِّ ذَنْۢبٍ قُتِلَتْ ۟ۚ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاِذَا الصُّحُفُ نُشِرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَاِذَا السَّمَآءُ كُشِطَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَاِذَا الْجَحِیْمُ سُعِّرَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاِذَا الْجَنَّةُ اُزْلِفَتْ ۟",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 14,
      "text": "عَلِمَتْ نَفْسٌ مَّاۤ اَحْضَرَتْ ۟ؕ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَلَاۤ اُقْسِمُ بِالْخُنَّسِ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 16,
      "text": "الْجَوَارِ الْكُنَّسِ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَالَّیْلِ اِذَا عَسْعَسَ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَالصُّبْحِ اِذَا تَنَفَّسَ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اِنَّهٗ لَقَوْلُ رَسُوْلٍ كَرِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 20,
      "text": "ذِیْ قُوَّةٍ عِنْدَ ذِی الْعَرْشِ مَكِیْنٍ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 21,
      "text": "مُّطَاعٍ ثَمَّ اَمِیْنٍ ۟ؕ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَمَا صَاحِبُكُمْ بِمَجْنُوْنٍ ۟ۚ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَلَقَدْ رَاٰهُ بِالْاُفُقِ الْمُبِیْنِ ۟ۚ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَمَا هُوَ عَلَی الْغَیْبِ بِضَنِیْنٍ ۟ۚ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَمَا هُوَ بِقَوْلِ شَیْطٰنٍ رَّجِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَاَیْنَ تَذْهَبُوْنَ ۟ؕ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اِنْ هُوَ اِلَّا ذِكْرٌ لِّلْعٰلَمِیْنَ ۟ۙ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 28,
      "text": "لِمَنْ شَآءَ مِنْكُمْ اَنْ یَّسْتَقِیْمَ ۟ؕ",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَمَا تَشَآءُوْنَ اِلَّاۤ اَنْ یَّشَآءَ اللّٰهُ رَبُّ الْعٰلَمِیْنَ ۟۠",
      "juz": 30,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    }
  ]
}