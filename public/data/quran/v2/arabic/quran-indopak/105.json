{
  "number": 105,
  "name": "سُورَةُ الفِيلِ",
  "englishName": "Al-Fil",
  "ayahs": [
    {
      "number": 1,
      "text": "اَلَمْ تَرَ كَیْفَ فَعَلَ رَبُّكَ بِاَصْحٰبِ الْفِیْلِ ۟ؕ",
      "juz": 30,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اَلَمْ یَجْعَلْ كَیْدَهُمْ فِیْ تَضْلِیْلٍ ۟ۙ",
      "juz": 30,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّاَرْسَلَ عَلَیْهِمْ طَیْرًا اَبَابِیْلَ ۟ۙ",
      "juz": 30,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَرْمِیْهِمْ بِحِجَارَةٍ مِّنْ سِجِّیْلٍ ۟",
      "juz": 30,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَجَعَلَهُمْ كَعَصْفٍ مَّاْكُوْلٍ ۟۠",
      "juz": 30,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
