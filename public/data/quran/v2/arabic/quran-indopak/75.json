{
  "number": 75,
  "name": "سُورَةُ القِيَامَةِ",
  "englishName": "Al-Qiyaama",
  "ayahs": [
    {
      "number": 1,
      "text": "لَاۤ اُقْسِمُ بِیَوْمِ الْقِیٰمَةِ ۟ۙ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلَاۤ اُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اَیَحْسَبُ الْاِنْسَانُ اَلَّنْ نَّجْمَعَ عِظَامَهٗ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "بَلٰى قٰدِرِیْنَ عَلٰۤی اَنْ نُّسَوِّیَ بَنَانَهٗ ۟",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بَلْ یُرِیْدُ الْاِنْسَانُ لِیَفْجُرَ اَمَامَهٗ ۟ۚ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَسْـَٔلُ اَیَّانَ یَوْمُ الْقِیٰمَةِ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَاِذَا بَرِقَ الْبَصَرُ ۟ۙ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَخَسَفَ الْقَمَرُ ۟ۙ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ ۟ۙ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 10,
      "text": "یَقُوْلُ الْاِنْسَانُ یَوْمَىِٕذٍ اَیْنَ الْمَفَرُّ ۟ۚ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَلَّا لَا وَزَرَ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِلٰى رَبِّكَ یَوْمَىِٕذِ لْمُسْتَقَرُّ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 13,
      "text": "یُنَبَّؤُا الْاِنْسَانُ یَوْمَىِٕذٍۭ بِمَا قَدَّمَ وَاَخَّرَ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 14,
      "text": "بَلِ الْاِنْسَانُ عَلٰی نَفْسِهٖ بَصِیْرَةٌ ۟ۙ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَّلَوْ اَلْقٰى مَعَاذِیْرَهٗ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 16,
      "text": "لَا تُحَرِّكْ بِهٖ لِسَانَكَ لِتَعْجَلَ بِهٖ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِنَّ عَلَیْنَا جَمْعَهٗ وَقُرْاٰنَهٗ ۟ۚۖ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَاِذَا قَرَاْنٰهُ فَاتَّبِعْ قُرْاٰنَهٗ ۟ۚ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 19,
      "text": "ثُمَّ اِنَّ عَلَیْنَا بَیَانَهٗ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 20,
      "text": "كَلَّا بَلْ تُحِبُّوْنَ الْعَاجِلَةَ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَتَذَرُوْنَ الْاٰخِرَةَ ۟ؕ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وُجُوْهٌ یَّوْمَىِٕذٍ نَّاضِرَةٌ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِلٰى رَبِّهَا نَاظِرَةٌ ۟ۚ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَوُجُوْهٌ یَّوْمَىِٕذٍۭ بَاسِرَةٌ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تَظُنُّ اَنْ یُّفْعَلَ بِهَا فَاقِرَةٌ ۟ؕ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 26,
      "text": "كَلَّاۤ اِذَا بَلَغَتِ التَّرَاقِیَ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَقِیْلَ مَنْ ٚ رَاقٍ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَّظَنَّ اَنَّهُ الْفِرَاقُ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اِلٰى رَبِّكَ یَوْمَىِٕذِ لْمَسَاقُ ۟ؕ۠",
      "juz": 29,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَلَا صَدَّقَ وَلَا صَلّٰى ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلٰكِنْ كَذَّبَ وَتَوَلّٰى ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 33,
      "text": "ثُمَّ ذَهَبَ اِلٰۤی اَهْلِهٖ یَتَمَطّٰى ۟ؕ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اَوْلٰى لَكَ فَاَوْلٰى ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ثُمَّ اَوْلٰى لَكَ فَاَوْلٰى ۟ؕ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اَیَحْسَبُ الْاِنْسَانُ اَنْ یُّتْرَكَ سُدًی ۟ؕ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اَلَمْ یَكُ نُطْفَةً مِّنْ مَّنِیٍّ یُّمْنٰى ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 38,
      "text": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوّٰى ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَجَعَلَ مِنْهُ الزَّوْجَیْنِ الذَّكَرَ وَالْاُ ۟ؕ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اَلَیْسَ ذٰلِكَ بِقٰدِرٍ عَلٰۤی اَنْ یُّحْیِ الْمَوْتٰى ۟۠",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    }
  ]
}