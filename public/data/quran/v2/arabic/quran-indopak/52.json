{
  "number": 52,
  "name": "سُورَةُ الطُّورِ",
  "englishName": "At-Tur",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالطُّوْرِ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَكِتٰبٍ مَّسْطُوْرٍ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فِیْ رَقٍّ مَّنْشُوْرٍ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَّالْبَیْتِ الْمَعْمُوْرِ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَالسَّقْفِ الْمَرْفُوْعِ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَالْبَحْرِ الْمَسْجُوْرِ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِنَّ عَذَابَ رَبِّكَ لَوَاقِعٌ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 8,
      "text": "مَّا لَهٗ مِنْ دَافِعٍ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یَّوْمَ تَمُوْرُ السَّمَآءُ مَوْرًا ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَّتَسِیْرُ الْجِبَالُ سَیْرًا ۟ؕ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَوَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟ۙ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 12,
      "text": "الَّذِیْنَ هُمْ فِیْ خَوْضٍ یَّلْعَبُوْنَ ۟ۘ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 13,
      "text": "یَوْمَ یُدَعُّوْنَ اِلٰی نَارِ جَهَنَّمَ دَعًّا ۟ؕ",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 14,
      "text": "هٰذِهِ النَّارُ الَّتِیْ كُنْتُمْ بِهَا تُكَذِّبُوْنَ ۟",
      "juz": 27,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اَفَسِحْرٌ هٰذَاۤ اَمْ اَنْتُمْ لَا تُبْصِرُوْنَ ۟ۚ",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اِصْلَوْهَا فَاصْبِرُوْۤا اَوْ لَا تَصْبِرُوْا ۚ سَوَآءٌ عَلَیْكُمْ ؕ اِنَّمَا تُجْزَوْنَ مَا كُنْتُمْ تَعْمَلُوْنَ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِنَّ الْمُتَّقِیْنَ فِیْ جَنّٰتٍ وَّنَعِیْمٍ ۟ۙ",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فٰكِهِیْنَ بِمَاۤ اٰتٰىهُمْ رَبُّهُمْ ۚ وَوَقٰىهُمْ رَبُّهُمْ عَذَابَ الْجَحِیْمِ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 19,
      "text": "كُلُوْا وَاشْرَبُوْا هَنِیْٓـًٔا بِمَا كُنْتُمْ تَعْمَلُوْنَ ۟ۙ",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 20,
      "text": "مُتَّكِـِٕیْنَ عَلٰی سُرُرٍ مَّصْفُوْفَةٍ ۚ وَزَوَّجْنٰهُمْ بِحُوْرٍ عِیْنٍ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَالَّذِیْنَ اٰمَنُوْا وَاتَّبَعَتْهُمْ ذُرِّیَّتُهُمْ بِاِیْمَانٍ اَلْحَقْنَا بِهِمْ ذُرِّیَّتَهُمْ وَمَاۤ اَلَتْنٰهُمْ مِّنْ عَمَلِهِمْ مِّنْ شَیْءٍ ؕ كُلُّ امْرِىۢ بِمَا كَسَبَ رَهِیْنٌ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَاَمْدَدْنٰهُمْ بِفَاكِهَةٍ وَّلَحْمٍ مِّمَّا یَشْتَهُوْنَ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 23,
      "text": "یَتَنَازَعُوْنَ فِیْهَا كَاْسًا لَّا لَغْوٌ فِیْهَا وَلَا تَاْثِیْمٌ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَیَطُوْفُ عَلَیْهِمْ غِلْمَانٌ لَّهُمْ كَاَنَّهُمْ لُؤْلُؤٌ مَّكْنُوْنٌ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَاَقْبَلَ بَعْضُهُمْ عَلٰی بَعْضٍ یَّتَسَآءَلُوْنَ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قَالُوْۤا اِنَّا كُنَّا قَبْلُ فِیْۤ اَهْلِنَا مُشْفِقِیْنَ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فَمَنَّ اللّٰهُ عَلَیْنَا وَوَقٰىنَا عَذَابَ السَّمُوْمِ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اِنَّا كُنَّا مِنْ قَبْلُ نَدْعُوْهُ ؕ اِنَّهٗ هُوَ الْبَرُّ الرَّحِیْمُ ۟۠",
      "juz": 27,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَذَكِّرْ فَمَاۤ اَنْتَ بِنِعْمَتِ رَبِّكَ بِكَاهِنٍ وَّلَا مَجْنُوْنٍ ۟ؕ",
      "juz": 27,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اَمْ یَقُوْلُوْنَ شَاعِرٌ نَّتَرَبَّصُ بِهٖ رَیْبَ الْمَنُوْنِ ۟",
      "juz": 27,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قُلْ تَرَبَّصُوْا فَاِنِّیْ مَعَكُمْ مِّنَ الْمُتَرَبِّصِیْنَ ۟ؕ",
      "juz": 27,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اَمْ تَاْمُرُهُمْ اَحْلَامُهُمْ بِهٰذَاۤ اَمْ هُمْ قَوْمٌ طَاغُوْنَ ۟ۚ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اَمْ یَقُوْلُوْنَ تَقَوَّلَهٗ ۚ بَلْ لَّا یُؤْمِنُوْنَ ۟ۚ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَلْیَاْتُوْا بِحَدِیْثٍ مِّثْلِهٖۤ اِنْ كَانُوْا صٰدِقِیْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اَمْ خُلِقُوْا مِنْ غَیْرِ شَیْءٍ اَمْ هُمُ الْخٰلِقُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اَمْ خَلَقُوا السَّمٰوٰتِ وَالْاَرْضَ ۚ بَلْ لَّا یُوْقِنُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اَمْ عِنْدَهُمْ خَزَآىِٕنُ رَبِّكَ اَمْ هُمُ الْمُصَۜیْطِرُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اَمْ لَهُمْ سُلَّمٌ یَّسْتَمِعُوْنَ فِیْهِ ۚ فَلْیَاْتِ مُسْتَمِعُهُمْ بِسُلْطٰنٍ مُّبِیْنٍ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اَمْ لَهُ الْبَنٰتُ وَلَكُمُ الْبَنُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اَمْ تَسْـَٔلُهُمْ اَجْرًا فَهُمْ مِّنْ مَّغْرَمٍ مُّثْقَلُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اَمْ عِنْدَهُمُ الْغَیْبُ فَهُمْ یَكْتُبُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 42,
      "text": "اَمْ یُرِیْدُوْنَ كَیْدًا ؕ فَالَّذِیْنَ كَفَرُوْا هُمُ الْمَكِیْدُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اَمْ لَهُمْ اِلٰهٌ غَیْرُ اللّٰهِ ؕ سُبْحٰنَ اللّٰهِ عَمَّا یُشْرِكُوْنَ ۟",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وَاِنْ یَّرَوْا كِسْفًا مِّنَ السَّمَآءِ سَاقِطًا یَّقُوْلُوْا سَحَابٌ مَّرْكُوْمٌ ۟",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 45,
      "text": "فَذَرْهُمْ حَتّٰی یُلٰقُوْا یَوْمَهُمُ الَّذِیْ فِیْهِ یُصْعَقُوْنَ ۟ۙ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 46,
      "text": "یَوْمَ لَا یُغْنِیْ عَنْهُمْ كَیْدُهُمْ شَیْـًٔا وَّلَا هُمْ یُنْصَرُوْنَ ۟ؕ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَاِنَّ لِلَّذِیْنَ ظَلَمُوْا عَذَابًا دُوْنَ ذٰلِكَ وَلٰكِنَّ اَكْثَرَهُمْ لَا یَعْلَمُوْنَ ۟",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَاصْبِرْ لِحُكْمِ رَبِّكَ فَاِنَّكَ بِاَعْیُنِنَا وَسَبِّحْ بِحَمْدِ رَبِّكَ حِیْنَ تَقُوْمُ ۟ۙ",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَمِنَ الَّیْلِ فَسَبِّحْهُ وَاِدْبَارَ النُّجُوْمِ ۟۠",
      "juz": 27,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    }
  ]
}