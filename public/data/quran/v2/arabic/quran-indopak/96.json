{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "ayahs": [
    {
      "number": 1,
      "text": "اِقْرَاْ بِاسْمِ رَبِّكَ الَّذِیْ خَلَقَ ۟ۚ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "خَلَقَ الْاِنْسَانَ مِنْ عَلَقٍ ۟ۚ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اِقْرَاْ وَرَبُّكَ الْاَكْرَمُ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "الَّذِیْ عَلَّمَ بِالْقَلَمِ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلَّمَ الْاِنْسَانَ مَا لَمْ یَعْلَمْ ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "كَلَّاۤ اِنَّ الْاِنْسَانَ لَیَطْغٰۤی ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اَنْ رَّاٰهُ اسْتَغْنٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِنَّ اِلٰی رَبِّكَ الرُّجْعٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اَرَءَیْتَ الَّذِیْ یَنْهٰی ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 10,
      "text": "عَبْدًا اِذَا صَلّٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اَرَءَیْتَ اِنْ كَانَ عَلَی الْهُدٰۤی ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اَوْ اَمَرَ بِالتَّقْوٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اَرَءَیْتَ اِنْ كَذَّبَ وَتَوَلّٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اَلَمْ یَعْلَمْ بِاَنَّ اللّٰهَ یَرٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلَّا لَىِٕنْ لَّمْ یَنْتَهِ ۙ۬ لَنَسْفَعًا بِالنَّاصِیَةِ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 16,
      "text": "نَاصِیَةٍ كَاذِبَةٍ خَاطِئَةٍ ۟ۚ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَلْیَدْعُ نَادِیَهٗ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 18,
      "text": "سَنَدْعُ الزَّبَانِیَةَ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 19,
      "text": "كَلَّا ؕ لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ ۟",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
        "id": 15,
        "recommended": false,
        "obligatory": true
      }
    }
  ]
}