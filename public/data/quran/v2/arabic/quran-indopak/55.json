{
  "number": 55,
  "name": "سُورَةُ الرَّحۡمَٰن",
  "englishName": "Ar-Rahmaan",
  "ayahs": [
    {
      "number": 1,
      "text": "اَلرَّحْمٰنُ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 2,
      "text": "عَلَّمَ الْقُرْاٰنَ ۟ؕ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 3,
      "text": "خَلَقَ الْاِنْسَانَ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4,
      "text": "عَلَّمَهُ الْبَیَانَ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اَلشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَّالنَّجْمُ وَالشَّجَرُ یَسْجُدٰنِ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَالسَّمَآءَ رَفَعَهَا وَوَضَعَ الْمِیْزَانَ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اَلَّا تَطْغَوْا فِی الْمِیْزَانِ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَاَقِیْمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِیْزَانَ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَالْاَرْضَ وَضَعَهَا لِلْاَنَامِ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فِیْهَا فَاكِهَةٌ وَّالنَّخْلُ ذَاتُ الْاَكْمَامِ ۟ۖ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّیْحَانُ ۟ۚ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 14,
      "text": "خَلَقَ الْاِنْسَانَ مِنْ صَلْصَالٍ كَالْفَخَّارِ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَخَلَقَ الْجَآنَّ مِنْ مَّارِجٍ مِّنْ نَّارٍ ۟ۚ",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 17,
      "text": "رَبُّ الْمَشْرِقَیْنِ وَرَبُّ الْمَغْرِبَیْنِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 19,
      "text": "مَرَجَ الْبَحْرَیْنِ یَلْتَقِیٰنِ ۟ۙ",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 20,
      "text": "بَیْنَهُمَا بَرْزَخٌ لَّا یَبْغِیٰنِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 22,
      "text": "یَخْرُجُ مِنْهُمَا اللُّؤْلُؤُ وَالْمَرْجَانُ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَلَهُ الْجَوَارِ الْمُنْشَـٰٔتُ فِی الْبَحْرِ كَالْاَعْلَامِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟۠",
      "juz": 27,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 26,
      "text": "كُلُّ مَنْ عَلَیْهَا فَانٍ ۟ۚۖ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَّیَبْقٰی وَجْهُ رَبِّكَ ذُو الْجَلٰلِ وَالْاِكْرَامِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 28,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 29,
      "text": "یَسْـَٔلُهٗ مَنْ فِی السَّمٰوٰتِ وَالْاَرْضِ ؕ كُلَّ یَوْمٍ هُوَ فِیْ شَاْنٍ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 31,
      "text": "سَنَفْرُغُ لَكُمْ اَیُّهَ الثَّقَلٰنِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 33,
      "text": "یٰمَعْشَرَ الْجِنِّ وَالْاِنْسِ اِنِ اسْتَطَعْتُمْ اَنْ تَنْفُذُوْا مِنْ اَقْطَارِ السَّمٰوٰتِ وَالْاَرْضِ فَانْفُذُوْا ؕ لَا تَنْفُذُوْنَ اِلَّا بِسُلْطٰنٍ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 35,
      "text": "یُرْسَلُ عَلَیْكُمَا شُوَاظٌ مِّنْ نَّارٍ ۙ۬ وَّنُحَاسٌ فَلَا تَنْتَصِرٰنِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 37,
      "text": "فَاِذَا انْشَقَّتِ السَّمَآءُ فَكَانَتْ وَرْدَةً كَالدِّهَانِ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 38,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَیَوْمَىِٕذٍ لَّا یُسْـَٔلُ عَنْ ذَنْۢبِهٖۤ اِنْسٌ وَّلَا جَآنٌّ ۟ۚ",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 40,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 41,
      "text": "یُعْرَفُ الْمُجْرِمُوْنَ بِسِیْمٰهُمْ فَیُؤْخَذُ بِالنَّوَاصِیْ وَالْاَقْدَامِ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 43,
      "text": "هٰذِهٖ جَهَنَّمُ الَّتِیْ یُكَذِّبُ بِهَا الْمُجْرِمُوْنَ ۟ۘ",
      "juz": 27,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 44,
      "text": "یَطُوْفُوْنَ بَیْنَهَا وَبَیْنَ حَمِیْمٍ اٰنٍ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 45,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟۠",
      "juz": 27,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 46,
      "text": "وَلِمَنْ خَافَ مَقَامَ رَبِّهٖ جَنَّتٰنِ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 47,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟ۙ",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 48,
      "text": "ذَوَاتَاۤ اَفْنَانٍ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 49,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فِیْهِمَا عَیْنٰنِ تَجْرِیٰنِ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 51,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 52,
      "text": "فِیْهِمَا مِنْ كُلِّ فَاكِهَةٍ زَوْجٰنِ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 54,
      "text": "مُتَّكِـِٕیْنَ عَلٰی فُرُشٍ بَطَآىِٕنُهَا مِنْ اِسْتَبْرَقٍ ؕ وَجَنَا الْجَنَّتَیْنِ دَانٍ ۟ۚ",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فَبِاَیِّ اٰلَآءِ رَبِّكُمَا تُكَذِّبٰنِ ۟",
      "juz": 27,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 56,
      "text": "فِیْهِنَّ قٰصِرٰتُ الطَّرْفِ ۙ لَمْ یَطْمِثْهُنَّ اِنْسٌ قَبْلَهُمْ وَلَا جَآنٌّ ۟ۚ",
      "juz": 27,
