{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالصّٰٓفّٰتِ صَفًّا ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالزّٰجِرٰتِ زَجْرًا ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فَالتّٰلِیٰتِ ذِكْرًا ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِنَّ اِلٰهَكُمْ لَوَاحِدٌ ۟ؕ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 5,
      "text": "رَبُّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَیْنَهُمَا وَرَبُّ الْمَشَارِقِ ۟ؕ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِنَّا زَیَّنَّا السَّمَآءَ الدُّنْیَا بِزِیْنَةِ لْكَوَاكِبِ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَحِفْظًا مِّنْ كُلِّ شَیْطٰنٍ مَّارِدٍ ۟ۚ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 8,
      "text": "لَا یَسَّمَّعُوْنَ اِلَی الْمَلَاِ الْاَعْلٰی وَیُقْذَفُوْنَ مِنْ كُلِّ جَانِبٍ ۟ۗۖ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 9,
      "text": "دُحُوْرًا وَّلَهُمْ عَذَابٌ وَّاصِبٌ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَاَتْبَعَهٗ شِهَابٌ ثَاقِبٌ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَاسْتَفْتِهِمْ اَهُمْ اَشَدُّ خَلْقًا اَمْ مَّنْ خَلَقْنَا ؕ اِنَّا خَلَقْنٰهُمْ مِّنْ طِیْنٍ لَّازِبٍ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 12,
      "text": "بَلْ عَجِبْتَ وَیَسْخَرُوْنَ ۪۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاِذَا ذُكِّرُوْا لَا یَذْكُرُوْنَ ۪۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَاِذَا رَاَوْا اٰیَةً یَّسْتَسْخِرُوْنَ ۪۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَقَالُوْۤا اِنْ هٰذَاۤ اِلَّا سِحْرٌ مُّبِیْنٌ ۟ۚۖ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ءَاِذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبْعُوْثُوْنَ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اَوَاٰبَآؤُنَا الْاَوَّلُوْنَ ۟ؕ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قُلْ نَعَمْ وَاَنْتُمْ دَاخِرُوْنَ ۟ۚ",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَاِنَّمَا هِیَ زَجْرَةٌ وَّاحِدَةٌ فَاِذَا هُمْ یَنْظُرُوْنَ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَقَالُوْا یٰوَیْلَنَا هٰذَا یَوْمُ الدِّیْنِ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 21,
      "text": "هٰذَا یَوْمُ الْفَصْلِ الَّذِیْ كُنْتُمْ بِهٖ تُكَذِّبُوْنَ ۟۠",
      "juz": 23,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اُحْشُرُوا الَّذِیْنَ ظَلَمُوْا وَاَزْوَاجَهُمْ وَمَا كَانُوْا یَعْبُدُوْنَ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 23,
      "text": "مِنْ دُوْنِ اللّٰهِ فَاهْدُوْهُمْ اِلٰی صِرَاطِ الْجَحِیْمِ ۟",
      "juz": 23,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَقِفُوْهُمْ اِنَّهُمْ مَّسْـُٔوْلُوْنَ ۟ۙ",
      "juz": 23,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 25,
      "text": "مَا لَكُمْ لَا تَنَاصَرُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 26,
      "text": "بَلْ هُمُ الْیَوْمَ مُسْتَسْلِمُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَاَقْبَلَ بَعْضُهُمْ عَلٰی بَعْضٍ یَّتَسَآءَلُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قَالُوْۤا اِنَّكُمْ كُنْتُمْ تَاْتُوْنَنَا عَنِ الْیَمِیْنِ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قَالُوْا بَلْ لَّمْ تَكُوْنُوْا مُؤْمِنِیْنَ ۟ۚ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَمَا كَانَ لَنَا عَلَیْكُمْ مِّنْ سُلْطٰنٍ ۚ بَلْ كُنْتُمْ قَوْمًا طٰغِیْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَحَقَّ عَلَیْنَا قَوْلُ رَبِّنَاۤ ۖۗ اِنَّا لَذَآىِٕقُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فَاَغْوَیْنٰكُمْ اِنَّا كُنَّا غٰوِیْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 33,
      "text": "فَاِنَّهُمْ یَوْمَىِٕذٍ فِی الْعَذَابِ مُشْتَرِكُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اِنَّا كَذٰلِكَ نَفْعَلُ بِالْمُجْرِمِیْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اِنَّهُمْ كَانُوْۤا اِذَا قِیْلَ لَهُمْ لَاۤ اِلٰهَ اِلَّا اللّٰهُۙ یَسْتَكْبِرُوْنَ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَیَقُوْلُوْنَ اَىِٕنَّا لَتَارِكُوْۤا اٰلِهَتِنَا لِشَاعِرٍ مَّجْنُوْنٍ ۟ؕ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 37,
      "text": "بَلْ جَآءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِیْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اِنَّكُمْ لَذَآىِٕقُوا الْعَذَابِ الْاَلِیْمِ ۟ۚ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَمَا تُجْزَوْنَ اِلَّا مَا كُنْتُمْ تَعْمَلُوْنَ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اِلَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اُولٰٓىِٕكَ لَهُمْ رِزْقٌ مَّعْلُوْمٌ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فَوَاكِهُ ۚ وَهُمْ مُّكْرَمُوْنَ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 43,
      "text": "فِیْ جَنّٰتِ النَّعِیْمِ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 44,
      "text": "عَلٰی سُرُرٍ مُّتَقٰبِلِیْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 45,
      "text": "یُطَافُ عَلَیْهِمْ بِكَاْسٍ مِّنْ مَّعِیْنٍ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 46,
      "text": "بَیْضَآءَ لَذَّةٍ لِّلشّٰرِبِیْنَ ۟ۚۖ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 47,
      "text": "لَا فِیْهَا غَوْلٌ وَّلَا هُمْ عَنْهَا یُنْزَفُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَعِنْدَهُمْ قٰصِرٰتُ الطَّرْفِ عِیْنٌ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 49,
      "text": "كَاَنَّهُنَّ بَیْضٌ مَّكْنُوْنٌ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَاَقْبَلَ بَعْضُهُمْ عَلٰی بَعْضٍ یَّتَسَآءَلُوْنَ ۟",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 51,
      "text": "قَالَ قَآىِٕلٌ مِّنْهُمْ اِنِّیْ كَانَ لِیْ قَرِیْنٌ ۟ۙ",
      "juz": 23,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 52,
      "text": "یَّقُوْلُ اَىِٕنَّكَ لَمِنَ الْمُصَدِّقِیْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 53,
      "text": "ءَاِذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَدِیْنُوْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 54,
      "text": "قَالَ هَلْ اَنْتُمْ مُّطَّلِعُوْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فَاطَّلَعَ فَرَاٰهُ فِیْ سَوَآءِ الْجَحِیْمِ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 56,
      "text": "قَالَ تَاللّٰهِ اِنْ كِدْتَّ لَتُرْدِیْنِ ۟ۙ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 57,
      "text": "وَلَوْلَا نِعْمَةُ رَبِّیْ لَكُنْتُ مِنَ الْمُحْضَرِیْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 58,
      "text": "اَفَمَا نَحْنُ بِمَیِّتِیْنَ ۟ۙ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 59,
      "text": "اِلَّا مَوْتَتَنَا الْاُوْلٰی وَمَا نَحْنُ بِمُعَذَّبِیْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 60,
      "text": "اِنَّ هٰذَا لَهُوَ الْفَوْزُ الْعَظِیْمُ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 61,
      "text": "لِمِثْلِ هٰذَا فَلْیَعْمَلِ الْعٰمِلُوْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 62,
      "text": "اَذٰلِكَ خَیْرٌ نُّزُلًا اَمْ شَجَرَةُ الزَّقُّوْمِ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 63,
      "text": "اِنَّا جَعَلْنٰهَا فِتْنَةً لِّلظّٰلِمِیْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 64,
      "text": "اِنَّهَا شَجَرَةٌ تَخْرُجُ فِیْۤ اَصْلِ الْجَحِیْمِ ۟ۙ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 65,
      "text": "طَلْعُهَا كَاَنَّهٗ رُءُوْسُ الشَّیٰطِیْنِ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 66,
      "text": "فَاِنَّهُمْ لَاٰكِلُوْنَ مِنْهَا فَمَالِـُٔوْنَ مِنْهَا الْبُطُوْنَ ۟ؕ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 67,
      "text": "ثُمَّ اِنَّ لَهُمْ عَلَیْهَا لَشَوْبًا مِّنْ حَمِیْمٍ ۟ۚ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 68,
      "text": "ثُمَّ اِنَّ مَرْجِعَهُمْ لَاۡاِلَی الْجَحِیْمِ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 69,
      "text": "اِنَّهُمْ اَلْفَوْا اٰبَآءَهُمْ ضَآلِّیْنَ ۟ۙ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 70,
      "text": "فَهُمْ عَلٰۤی اٰثٰرِهِمْ یُهْرَعُوْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 71,
      "text": "وَلَقَدْ ضَلَّ قَبْلَهُمْ اَكْثَرُ الْاَوَّلِیْنَ ۟ۙ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 72,
      "text": "وَلَقَدْ اَرْسَلْنَا فِیْهِمْ مُّنْذِرِیْنَ ۟",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 73,
      "text": "فَانْظُرْ كَیْفَ كَانَ عَاقِبَةُ الْمُنْذَرِیْنَ ۟ۙ",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 74,
      "text": "اِلَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟۠",
      "juz": 23,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 75,
      "text": "وَلَقَدْ نَادٰىنَا نُوْحٌ فَلَنِعْمَ الْمُجِیْبُوْنَ ۟ؗۖ",
      "juz": 23,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وَنَجَّیْنٰهُ وَاَهْلَهٗ مِنَ الْكَرْبِ الْعَظِیْمِ ۟ؗۖ",
      "juz": 23,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 77,
      "text": "وَجَعَلْنَا ذُرِّیَّتَهٗ هُمُ الْبٰقِیْنَ ۟ؗۖ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 78,
      "text": "وَتَرَكْنَا عَلَیْهِ فِی الْاٰخِرِیْنَ ۟ؗۖ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 79,
      "text": "سَلٰمٌ عَلٰی نُوْحٍ فِی الْعٰلَمِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 80,
      "text": "اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 81,
      "text": "اِنَّهٗ مِنْ عِبَادِنَا الْمُؤْمِنِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 82,
      "text": "ثُمَّ اَغْرَقْنَا الْاٰخَرِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 83,
      "text": "وَاِنَّ مِنْ شِیْعَتِهٖ لَاِبْرٰهِیْمَ ۟ۘ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 84,
      "text": "اِذْ جَآءَ رَبَّهٗ بِقَلْبٍ سَلِیْمٍ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 85,
      "text": "اِذْ قَالَ لِاَبِیْهِ وَقَوْمِهٖ مَاذَا تَعْبُدُوْنَ ۟ۚ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 86,
      "text": "اَىِٕفْكًا اٰلِهَةً دُوْنَ اللّٰهِ تُرِیْدُوْنَ ۟ؕ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 87,
      "text": "فَمَا ظَنُّكُمْ بِرَبِّ الْعٰلَمِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 88,
      "text": "فَنَظَرَ نَظْرَةً فِی النُّجُوْمِ ۟ۙ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 89,
      "text": "فَقَالَ اِنِّیْ سَقِیْمٌ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 90,
      "text": "فَتَوَلَّوْا عَنْهُ مُدْبِرِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 91,
      "text": "فَرَاغَ اِلٰۤی اٰلِهَتِهِمْ فَقَالَ اَلَا تَاْكُلُوْنَ ۟ۚ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 92,
      "text": "مَا لَكُمْ لَا تَنْطِقُوْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 93,
      "text": "فَرَاغَ عَلَیْهِمْ ضَرْبًا بِالْیَمِیْنِ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 94,
      "text": "فَاَقْبَلُوْۤا اِلَیْهِ یَزِفُّوْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 95,
      "text": "قَالَ اَتَعْبُدُوْنَ مَا تَنْحِتُوْنَ ۟ۙ",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 96,
      "text": "وَاللّٰهُ خَلَقَكُمْ وَمَا تَعْمَلُوْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 97,
      "text": "قَالُوا ابْنُوْا لَهٗ بُنْیَانًا فَاَلْقُوْهُ فِی الْجَحِیْمِ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 98,
      "text": "فَاَرَادُوْا بِهٖ كَیْدًا فَجَعَلْنٰهُمُ الْاَسْفَلِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 99,
      "text": "وَقَالَ اِنِّیْ ذَاهِبٌ اِلٰی رَبِّیْ سَیَهْدِیْنِ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 100,
      "text": "رَبِّ هَبْ لِیْ مِنَ الصّٰلِحِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 101,
      "text": "فَبَشَّرْنٰهُ بِغُلٰمٍ حَلِیْمٍ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 102,
      "text": "فَلَمَّا بَلَغَ مَعَهُ السَّعْیَ قَالَ یٰبُنَیَّ اِنِّیْۤ اَرٰی فِی الْمَنَامِ اَنِّیْۤ اَذْبَحُكَ فَانْظُرْ مَاذَا تَرٰی ؕ قَالَ یٰۤاَبَتِ افْعَلْ مَا تُؤْمَرُ ؗ سَتَجِدُنِیْۤ اِنْ شَآءَ اللّٰهُ مِنَ الصّٰبِرِیْنَ ۟",
      "juz": 23,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 103,
      "text": "فَلَمَّاۤ اَسْلَمَا وَتَلَّهٗ لِلْجَبِیْنِ ۟ۚ",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 104,
      "text": "وَنَادَیْنٰهُ اَنْ یّٰۤاِبْرٰهِیْمُ ۟ۙ",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 105,
      "text": "قَدْ صَدَّقْتَ الرُّءْیَا ۚ اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 106,
      "text": "اِنَّ هٰذَا لَهُوَ الْبَلٰٓؤُا الْمُبِیْنُ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 107,
      "text": "وَفَدَیْنٰهُ بِذِبْحٍ عَظِیْمٍ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 108,
      "text": "وَتَرَكْنَا عَلَیْهِ فِی الْاٰخِرِیْنَ ۟ۖ",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 109,
      "text": "سَلٰمٌ عَلٰۤی اِبْرٰهِیْمَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 110,
      "text": "كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 111,
      "text": "اِنَّهٗ مِنْ عِبَادِنَا الْمُؤْمِنِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 112,
      "text": "وَبَشَّرْنٰهُ بِاِسْحٰقَ نَبِیًّا مِّنَ الصّٰلِحِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 113,
      "text": "وَبٰرَكْنَا عَلَیْهِ وَعَلٰۤی اِسْحٰقَ ؕ وَمِنْ ذُرِّیَّتِهِمَا مُحْسِنٌ وَّظَالِمٌ لِّنَفْسِهٖ مُبِیْنٌ ۟۠",
      "juz": 23,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 114,
      "text": "وَلَقَدْ مَنَنَّا عَلٰی مُوْسٰی وَهٰرُوْنَ ۟ۚ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 115,
      "text": "وَنَجَّیْنٰهُمَا وَقَوْمَهُمَا مِنَ الْكَرْبِ الْعَظِیْمِ ۟ۚ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 116,
      "text": "وَنَصَرْنٰهُمْ فَكَانُوْا هُمُ الْغٰلِبِیْنَ ۟ۚ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 117,
      "text": "وَاٰتَیْنٰهُمَا الْكِتٰبَ الْمُسْتَبِیْنَ ۟ۚ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 118,
      "text": "وَهَدَیْنٰهُمَا الصِّرَاطَ الْمُسْتَقِیْمَ ۟ۚ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 119,
      "text": "وَتَرَكْنَا عَلَیْهِمَا فِی الْاٰخِرِیْنَ ۟ۙ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 120,
      "text": "سَلٰمٌ عَلٰی مُوْسٰی وَهٰرُوْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 121,
      "text": "اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 122,
      "text": "اِنَّهُمَا مِنْ عِبَادِنَا الْمُؤْمِنِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 123,
      "text": "وَاِنَّ اِلْیَاسَ لَمِنَ الْمُرْسَلِیْنَ ۟ؕ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 124,
      "text": "اِذْ قَالَ لِقَوْمِهٖۤ اَلَا تَتَّقُوْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 125,
      "text": "اَتَدْعُوْنَ بَعْلًا وَّتَذَرُوْنَ اَحْسَنَ الْخَالِقِیْنَ ۟ۙ",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 126,
      "text": "اللّٰهَ رَبَّكُمْ وَرَبَّ اٰبَآىِٕكُمُ الْاَوَّلِیْنَ ۟",
      "juz": 23,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 127,
      "text": "فَكَذَّبُوْهُ فَاِنَّهُمْ لَمُحْضَرُوْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 128,
      "text": "اِلَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 129,
      "text": "وَتَرَكْنَا عَلَیْهِ فِی الْاٰخِرِیْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 130,
      "text": "سَلٰمٌ عَلٰۤی اِلْ یَاسِیْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 131,
      "text": "اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 132,
      "text": "اِنَّهٗ مِنْ عِبَادِنَا الْمُؤْمِنِیْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 133,
      "text": "وَاِنَّ لُوْطًا لَّمِنَ الْمُرْسَلِیْنَ ۟ؕ",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 134,
      "text": "اِذْ نَجَّیْنٰهُ وَاَهْلَهٗۤ اَجْمَعِیْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 135,
      "text": "اِلَّا عَجُوْزًا فِی الْغٰبِرِیْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 136,
      "text": "ثُمَّ دَمَّرْنَا الْاٰخَرِیْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 137,
      "text": "وَاِنَّكُمْ لَتَمُرُّوْنَ عَلَیْهِمْ مُّصْبِحِیْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 138,
      "text": "وَبِالَّیْلِ ؕ اَفَلَا تَعْقِلُوْنَ ۟۠",
      "juz": 23,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 139,
      "text": "وَاِنَّ یُوْنُسَ لَمِنَ الْمُرْسَلِیْنَ ۟ؕ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 140,
      "text": "اِذْ اَبَقَ اِلَی الْفُلْكِ الْمَشْحُوْنِ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 141,
      "text": "فَسَاهَمَ فَكَانَ مِنَ الْمُدْحَضِیْنَ ۟ۚ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 142,
      "text": "فَالْتَقَمَهُ الْحُوْتُ وَهُوَ مُلِیْمٌ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 143,
      "text": "فَلَوْلَاۤ اَنَّهٗ كَانَ مِنَ الْمُسَبِّحِیْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 144,
      "text": "لَلَبِثَ فِیْ بَطْنِهٖۤ اِلٰی یَوْمِ یُبْعَثُوْنَ ۟ۚ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 145,
      "text": "فَنَبَذْنٰهُ بِالْعَرَآءِ وَهُوَ سَقِیْمٌ ۟ۚ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 146,
      "text": "وَاَنْۢبَتْنَا عَلَیْهِ شَجَرَةً مِّنْ یَّقْطِیْنٍ ۟ۚ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 147,
      "text": "وَاَرْسَلْنٰهُ اِلٰی مِائَةِ اَلْفٍ اَوْ یَزِیْدُوْنَ ۟ۚ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 148,
      "text": "فَاٰمَنُوْا فَمَتَّعْنٰهُمْ اِلٰی حِیْنٍ ۟ؕ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 149,
      "text": "فَاسْتَفْتِهِمْ اَلِرَبِّكَ الْبَنَاتُ وَلَهُمُ الْبَنُوْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 150,
      "text": "اَمْ خَلَقْنَا الْمَلٰٓىِٕكَةَ اِنَاثًا وَّهُمْ شٰهِدُوْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 151,
      "text": "اَلَاۤ اِنَّهُمْ مِّنْ اِفْكِهِمْ لَیَقُوْلُوْنَ ۟ۙ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 152,
      "text": "وَلَدَ اللّٰهُ ۙ وَاِنَّهُمْ لَكٰذِبُوْنَ ۟",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 153,
      "text": "اَصْطَفَی الْبَنَاتِ عَلَی الْبَنِیْنَ ۟ؕ",
      "juz": 23,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 154,
      "text": "مَا لَكُمْ ۫ كَیْفَ تَحْكُمُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 155,
      "text": "اَفَلَا تَذَكَّرُوْنَ ۟ۚ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 156,
      "text": "اَمْ لَكُمْ سُلْطٰنٌ مُّبِیْنٌ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 157,
      "text": "فَاْتُوْا بِكِتٰبِكُمْ اِنْ كُنْتُمْ صٰدِقِیْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 158,
      "text": "وَجَعَلُوْا بَیْنَهٗ وَبَیْنَ الْجِنَّةِ نَسَبًا ؕ وَلَقَدْ عَلِمَتِ الْجِنَّةُ اِنَّهُمْ لَمُحْضَرُوْنَ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 159,
      "text": "سُبْحٰنَ اللّٰهِ عَمَّا یَصِفُوْنَ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 160,
      "text": "اِلَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 161,
      "text": "فَاِنَّكُمْ وَمَا تَعْبُدُوْنَ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 162,
      "text": "مَاۤ اَنْتُمْ عَلَیْهِ بِفٰتِنِیْنَ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 163,
      "text": "اِلَّا مَنْ هُوَ صَالِ الْجَحِیْمِ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 164,
      "text": "وَمَا مِنَّاۤ اِلَّا لَهٗ مَقَامٌ مَّعْلُوْمٌ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 165,
      "text": "وَّاِنَّا لَنَحْنُ الصَّآفُّوْنَ ۟ۚ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 166,
      "text": "وَاِنَّا لَنَحْنُ الْمُسَبِّحُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 167,
      "text": "وَاِنْ كَانُوْا لَیَقُوْلُوْنَ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 168,
      "text": "لَوْ اَنَّ عِنْدَنَا ذِكْرًا مِّنَ الْاَوَّلِیْنَ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 169,
      "text": "لَكُنَّا عِبَادَ اللّٰهِ الْمُخْلَصِیْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 170,
      "text": "فَكَفَرُوْا بِهٖ فَسَوْفَ یَعْلَمُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 171,
      "text": "وَلَقَدْ سَبَقَتْ كَلِمَتُنَا لِعِبَادِنَا الْمُرْسَلِیْنَ ۟ۚۖ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 172,
      "text": "اِنَّهُمْ لَهُمُ الْمَنْصُوْرُوْنَ ۪۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 173,
      "text": "وَاِنَّ جُنْدَنَا لَهُمُ الْغٰلِبُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 174,
      "text": "فَتَوَلَّ عَنْهُمْ حَتّٰی حِیْنٍ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 175,
      "text": "وَّاَبْصِرْهُمْ فَسَوْفَ یُبْصِرُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 176,
      "text": "اَفَبِعَذَابِنَا یَسْتَعْجِلُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 177,
      "text": "فَاِذَا نَزَلَ بِسَاحَتِهِمْ فَسَآءَ صَبَاحُ الْمُنْذَرِیْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 178,
      "text": "وَتَوَلَّ عَنْهُمْ حَتّٰی حِیْنٍ ۟ۙ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 179,
      "text": "وَّاَبْصِرْ فَسَوْفَ یُبْصِرُوْنَ ۟",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 180,
      "text": "سُبْحٰنَ رَبِّكَ رَبِّ الْعِزَّةِ عَمَّا یَصِفُوْنَ ۟ۚ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 181,
      "text": "وَسَلٰمٌ عَلَی الْمُرْسَلِیْنَ ۟ۚ",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 182,
      "text": "وَالْحَمْدُ لِلّٰهِ رَبِّ الْعٰلَمِیْنَ ۟۠",
      "juz": 23,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    }
  ]
