{
  "number": 69,
  "name": "سُورَةُ الحَاقَّةِ",
  "englishName": "Al-Haaqqa",
  "ayahs": [
    {
      "number": 1,
      "text": "اَلْحَآقَّةُ ۟ۙ",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَا الْحَآقَّةُ ۟ۚ",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَاۤ اَدْرٰىكَ مَا الْحَآقَّةُ ۟ؕ",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَذَّبَتْ ثَمُوْدُ وَعَادٌ بِالْقَارِعَةِ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَاَمَّا ثَمُوْدُ فَاُهْلِكُوْا بِالطَّاغِیَةِ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَاَمَّا عَادٌ فَاُهْلِكُوْا بِرِیْحٍ صَرْصَرٍ عَاتِیَةٍ ۟ۙ",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 7,
      "text": "سَخَّرَهَا عَلَیْهِمْ سَبْعَ لَیَالٍ وَّثَمٰنِیَةَ اَیَّامٍ ۙ حُسُوْمًاۙ فَتَرَی الْقَوْمَ فِیْهَا صَرْعٰی ۙ كَاَنَّهُمْ اَعْجَازُ نَخْلٍ خَاوِیَةٍ ۟ۚ",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَهَلْ تَرٰی لَهُمْ مِّنْ بَاقِیَةٍ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَجَآءَ فِرْعَوْنُ وَمَنْ قَبْلَهٗ وَالْمُؤْتَفِكٰتُ بِالْخَاطِئَةِ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَعَصَوْا رَسُوْلَ رَبِّهِمْ فَاَخَذَهُمْ اَخْذَةً رَّابِیَةً ۟",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اِنَّا لَمَّا طَغَا الْمَآءُ حَمَلْنٰكُمْ فِی الْجَارِیَةِ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 12,
      "text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَّتَعِیَهَاۤ اُذُنٌ وَّاعِیَةٌ ۟",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَاِذَا نُفِخَ فِی الصُّوْرِ نَفْخَةٌ وَّاحِدَةٌ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّحُمِلَتِ الْاَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَّاحِدَةً ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَیَوْمَىِٕذٍ وَّقَعَتِ الْوَاقِعَةُ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَانْشَقَّتِ السَّمَآءُ فَهِیَ یَوْمَىِٕذٍ وَّاهِیَةٌ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَّالْمَلَكُ عَلٰۤی اَرْجَآىِٕهَا ؕ وَیَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ یَوْمَىِٕذٍ ثَمٰنِیَةٌ ۟ؕ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 18,
      "text": "یَوْمَىِٕذٍ تُعْرَضُوْنَ لَا تَخْفٰی مِنْكُمْ خَافِیَةٌ ۟",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَاَمَّا مَنْ اُوْتِیَ كِتٰبَهٗ بِیَمِیْنِهٖ ۙ فَیَقُوْلُ هَآؤُمُ اقْرَءُوْا كِتٰبِیَهْ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اِنِّیْ ظَنَنْتُ اَنِّیْ مُلٰقٍ حِسَابِیَهْ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَهُوَ فِیْ عِیْشَةٍ رَّاضِیَةٍ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 22,
      "text": "فِیْ جَنَّةٍ عَالِیَةٍ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 23,
      "text": "قُطُوْفُهَا دَانِیَةٌ ۟",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 24,
      "text": "كُلُوْا وَاشْرَبُوْا هَنِیْٓـًٔا بِمَاۤ اَسْلَفْتُمْ فِی الْاَیَّامِ الْخَالِیَةِ ۟",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَاَمَّا مَنْ اُوْتِیَ كِتٰبَهٗ بِشِمَالِهٖ ۙ۬ فَیَقُوْلُ یٰلَیْتَنِیْ لَمْ اُوْتَ كِتٰبِیَهْ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلَمْ اَدْرِ مَا حِسَابِیَهْ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 27,
      "text": "یٰلَیْتَهَا كَانَتِ الْقَاضِیَةَ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 28,
      "text": "مَاۤ اَغْنٰی عَنِّیْ مَالِیَهْ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 29,
      "text": "هَلَكَ عَنِّیْ سُلْطٰنِیَهْ ۟ۚ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 30,
      "text": "خُذُوْهُ فَغُلُّوْهُ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 31,
      "text": "ثُمَّ الْجَحِیْمَ صَلُّوْهُ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 32,
      "text": "ثُمَّ فِیْ سِلْسِلَةٍ ذَرْعُهَا سَبْعُوْنَ ذِرَاعًا فَاسْلُكُوْهُ ۟ؕ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اِنَّهٗ كَانَ لَا یُؤْمِنُ بِاللّٰهِ الْعَظِیْمِ ۟ۙ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَلَا یَحُضُّ عَلٰی طَعَامِ الْمِسْكِیْنِ ۟ؕ",
      "juz": 29,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 35,
      "text": "فَلَیْسَ لَهُ الْیَوْمَ هٰهُنَا حَمِیْمٌ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَّلَا طَعَامٌ اِلَّا مِنْ غِسْلِیْنٍ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 37,
      "text": "لَّا یَاْكُلُهٗۤ اِلَّا الْخَاطِـُٔوْنَ ۟۠",
      "juz": 29,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 38,
      "text": "فَلَاۤ اُقْسِمُ بِمَا تُبْصِرُوْنَ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَمَا لَا تُبْصِرُوْنَ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اِنَّهٗ لَقَوْلُ رَسُوْلٍ كَرِیْمٍ ۟ۚۙ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَّمَا هُوَ بِقَوْلِ شَاعِرٍ ؕ قَلِیْلًا مَّا تُؤْمِنُوْنَ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وَلَا بِقَوْلِ كَاهِنٍ ؕ قَلِیْلًا مَّا تَذَكَّرُوْنَ ۟ؕ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 43,
      "text": "تَنْزِیْلٌ مِّنْ رَّبِّ الْعٰلَمِیْنَ ۟",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وَلَوْ تَقَوَّلَ عَلَیْنَا بَعْضَ الْاَقَاوِیْلِ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 45,
      "text": "لَاَخَذْنَا مِنْهُ بِالْیَمِیْنِ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 46,
      "text": "ثُمَّ لَقَطَعْنَا مِنْهُ الْوَتِیْنَ ۟ؗۖ",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 47,
      "text": "فَمَا مِنْكُمْ مِّنْ اَحَدٍ عَنْهُ حٰجِزِیْنَ ۟",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَاِنَّهٗ لَتَذْكِرَةٌ لِّلْمُتَّقِیْنَ ۟",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَاِنَّا لَنَعْلَمُ اَنَّ مِنْكُمْ مُّكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَاِنَّهٗ لَحَسْرَةٌ عَلَی الْكٰفِرِیْنَ ۟",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَاِنَّهٗ لَحَقُّ الْیَقِیْنِ ۟",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 52,
      "text": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِیْمِ ۟۠",
      "juz": 29,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    }
  ]
}