{
  "number": 82,
  "name": "سُورَةُ الانفِطَارِ",
  "englishName": "Al-Infitaar",
  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا السَّمَآءُ انْفَطَرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاِذَا الْكَوَاكِبُ انْتَثَرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِذَا الْبِحَارُ فُجِّرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاِذَا الْقُبُوْرُ بُعْثِرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَاَخَّرَتْ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یٰۤاَیُّهَا الْاِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِیْمِ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 7,
      "text": "الَّذِیْ خَلَقَكَ فَسَوّٰىكَ فَعَدَلَكَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فِیْۤ اَیِّ صُوْرَةٍ مَّا شَآءَ رَكَّبَكَ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كَلَّا بَلْ تُكَذِّبُوْنَ بِالدِّیْنِ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاِنَّ عَلَیْكُمْ لَحٰفِظِیْنَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كِرَامًا كَاتِبِیْنَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 12,
      "text": "یَعْلَمُوْنَ مَا تَفْعَلُوْنَ ۟",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِنَّ الْاَبْرَارَ لَفِیْ نَعِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَاِنَّ الْفُجَّارَ لَفِیْ جَحِیْمٍ ۟ۚۖ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 15,
      "text": "یَّصْلَوْنَهَا یَوْمَ الدِّیْنِ ۟",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَمَا هُمْ عَنْهَا بِغَآىِٕبِیْنَ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَمَاۤ اَدْرٰىكَ مَا یَوْمُ الدِّیْنِ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ثُمَّ مَاۤ اَدْرٰىكَ مَا یَوْمُ الدِّیْنِ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 19,
      "text": "یَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَیْـًٔا ؕ وَالْاَمْرُ یَوْمَىِٕذٍ لِّلّٰهِ ۟۠",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    }
  ]
}