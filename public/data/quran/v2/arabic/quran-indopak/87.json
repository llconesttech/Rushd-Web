{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "ayahs": [
    {
      "number": 1,
      "text": "سَبِّحِ اسْمَ رَبِّكَ الْاَعْلَی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الَّذِیْ خَلَقَ فَسَوّٰی ۟",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالَّذِیْ قَدَّرَ فَهَدٰی ۟",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّذِیْۤ اَخْرَجَ الْمَرْعٰی ۟",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَجَعَلَهٗ غُثَآءً اَحْوٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "سَنُقْرِئُكَ فَلَا تَنْسٰۤی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِلَّا مَا شَآءَ اللّٰهُ ؕ اِنَّهٗ یَعْلَمُ الْجَهْرَ وَمَا یَخْفٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَنُیَسِّرُكَ لِلْیُسْرٰی ۟ۚۖ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَذَكِّرْ اِنْ نَّفَعَتِ الذِّكْرٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "سَیَذَّكَّرُ مَنْ یَّخْشٰی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَیَتَجَنَّبُهَا الْاَشْقَی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "الَّذِیْ یَصْلَی النَّارَ الْكُبْرٰی ۟ۚ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُمَّ لَا یَمُوْتُ فِیْهَا وَلَا یَحْیٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قَدْ اَفْلَحَ مَنْ تَزَكّٰی ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَذَكَرَ اسْمَ رَبِّهٖ فَصَلّٰی ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "بَلْ تُؤْثِرُوْنَ الْحَیٰوةَ الدُّنْیَا ۟ۚۖ",
      "juz": 30,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَالْاٰخِرَةُ خَیْرٌ وَّاَبْقٰی ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اِنَّ هٰذَا لَفِی الصُّحُفِ الْاُوْلٰی ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "صُحُفِ اِبْرٰهِیْمَ وَمُوْسٰی ۟۠",
      "juz": 30,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}