{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "ayahs": [
    {
      "number": 1,
      "text": "اَرَءَیْتَ الَّذِیْ یُكَذِّبُ بِالدِّیْنِ ۟ؕ",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَذٰلِكَ الَّذِیْ یَدُعُّ الْیَتِیْمَ ۟ۙ",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَلَا یَحُضُّ عَلٰی طَعَامِ الْمِسْكِیْنِ ۟ؕ",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَوَیْلٌ لِّلْمُصَلِّیْنَ ۟ۙ",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "الَّذِیْنَ هُمْ عَنْ صَلَاتِهِمْ سَاهُوْنَ ۟ۙ",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "الَّذِیْنَ هُمْ یُرَآءُوْنَ ۟ۙ",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَیَمْنَعُوْنَ الْمَاعُوْنَ ۟۠",
      "juz": 30,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}