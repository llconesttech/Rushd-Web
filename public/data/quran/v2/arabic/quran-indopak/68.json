{
  "number": 68,
  "name": "سُورَةُ القَلَمِ",
  "englishName": "Al-Qalam",
  "ayahs": [
    {
      "number": 1,
      "text": "نٓ وَالْقَلَمِ وَمَا یَسْطُرُوْنَ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَاۤ اَنْتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُوْنٍ ۟ۚ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِنَّ لَكَ لَاَجْرًا غَیْرَ مَمْنُوْنٍ ۟ۚ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاِنَّكَ لَعَلٰی خُلُقٍ عَظِیْمٍ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَسَتُبْصِرُ وَیُبْصِرُوْنَ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بِاَیِّىكُمُ الْمَفْتُوْنُ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِنَّ رَبَّكَ هُوَ اَعْلَمُ بِمَنْ ضَلَّ عَنْ سَبِیْلِهٖ ۪ وَهُوَ اَعْلَمُ بِالْمُهْتَدِیْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَلَا تُطِعِ الْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَدُّوْا لَوْ تُدْهِنُ فَیُدْهِنُوْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِیْنٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 11,
      "text": "هَمَّازٍ مَّشَّآءٍ بِنَمِیْمٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 12,
      "text": "مَّنَّاعٍ لِّلْخَیْرِ مُعْتَدٍ اَثِیْمٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 13,
      "text": "عُتُلٍّ بَعْدَ ذٰلِكَ زَنِیْمٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اَنْ كَانَ ذَا مَالٍ وَّبَنِیْنَ ۟ؕ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِذَا تُتْلٰی عَلَیْهِ اٰیٰتُنَا قَالَ اَسَاطِیْرُ الْاَوَّلِیْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 16,
      "text": "سَنَسِمُهٗ عَلَی الْخُرْطُوْمِ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِنَّا بَلَوْنٰهُمْ كَمَا بَلَوْنَاۤ اَصْحٰبَ الْجَنَّةِ ۚ اِذْ اَقْسَمُوْا لَیَصْرِمُنَّهَا مُصْبِحِیْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلَا یَسْتَثْنُوْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَطَافَ عَلَیْهَا طَآىِٕفٌ مِّنْ رَّبِّكَ وَهُمْ نَآىِٕمُوْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَاَصْبَحَتْ كَالصَّرِیْمِ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَتَنَادَوْا مُصْبِحِیْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اَنِ اغْدُوْا عَلٰی حَرْثِكُمْ اِنْ كُنْتُمْ صٰرِمِیْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَانْطَلَقُوْا وَهُمْ یَتَخَافَتُوْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اَنْ لَّا یَدْخُلَنَّهَا الْیَوْمَ عَلَیْكُمْ مِّسْكِیْنٌ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَّغَدَوْا عَلٰی حَرْدٍ قٰدِرِیْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَلَمَّا رَاَوْهَا قَالُوْۤا اِنَّا لَضَآلُّوْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 27,
      "text": "بَلْ نَحْنُ مَحْرُوْمُوْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قَالَ اَوْسَطُهُمْ اَلَمْ اَقُلْ لَّكُمْ لَوْلَا تُسَبِّحُوْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قَالُوْا سُبْحٰنَ رَبِّنَاۤ اِنَّا كُنَّا ظٰلِمِیْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَاَقْبَلَ بَعْضُهُمْ عَلٰی بَعْضٍ یَّتَلَاوَمُوْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قَالُوْا یٰوَیْلَنَاۤ اِنَّا كُنَّا طٰغِیْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 32,
      "text": "عَسٰی رَبُّنَاۤ اَنْ یُّبْدِلَنَا خَیْرًا مِّنْهَاۤ اِنَّاۤ اِلٰی رَبِّنَا رٰغِبُوْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَذٰلِكَ الْعَذَابُ ؕ وَلَعَذَابُ الْاٰخِرَةِ اَكْبَرُ ۘ لَوْ كَانُوْا یَعْلَمُوْنَ ۟۠",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اِنَّ لِلْمُتَّقِیْنَ عِنْدَ رَبِّهِمْ جَنّٰتِ النَّعِیْمِ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اَفَنَجْعَلُ الْمُسْلِمِیْنَ كَالْمُجْرِمِیْنَ ۟ؕ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 36,
      "text": "مَا لَكُمْ ۥ كَیْفَ تَحْكُمُوْنَ ۟ۚ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اَمْ لَكُمْ كِتٰبٌ فِیْهِ تَدْرُسُوْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اِنَّ لَكُمْ فِیْهِ لَمَا تَخَیَّرُوْنَ ۟ۚ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اَمْ لَكُمْ اَیْمَانٌ عَلَیْنَا بَالِغَةٌ اِلٰی یَوْمِ الْقِیٰمَةِ ۙ اِنَّ لَكُمْ لَمَا تَحْكُمُوْنَ ۟ۚ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 40,
      "text": "سَلْهُمْ اَیُّهُمْ بِذٰلِكَ زَعِیْمٌ ۟ۚۛ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اَمْ لَهُمْ شُرَكَآءُ ۛۚ فَلْیَاْتُوْا بِشُرَكَآىِٕهِمْ اِنْ كَانُوْا صٰدِقِیْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 42,
      "text": "یَوْمَ یُكْشَفُ عَنْ سَاقٍ وَّیُدْعَوْنَ اِلَی السُّجُوْدِ فَلَا یَسْتَطِیْعُوْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 43,
      "text": "خَاشِعَةً اَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ؕ وَقَدْ كَانُوْا یُدْعَوْنَ اِلَی السُّجُوْدِ وَهُمْ سٰلِمُوْنَ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَذَرْنِیْ وَمَنْ یُّكَذِّبُ بِهٰذَا الْحَدِیْثِ ؕ سَنَسْتَدْرِجُهُمْ مِّنْ حَیْثُ لَا یَعْلَمُوْنَ ۟ۙ",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَاُمْلِیْ لَهُمْ ؕ اِنَّ كَیْدِیْ مَتِیْنٌ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 46,
      "text": "اَمْ تَسْـَٔلُهُمْ اَجْرًا فَهُمْ مِّنْ مَّغْرَمٍ مُّثْقَلُوْنَ ۟ۚ",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 47,
      "text": "اَمْ عِنْدَهُمُ الْغَیْبُ فَهُمْ یَكْتُبُوْنَ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 48,
      "text": "فَاصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُنْ كَصَاحِبِ الْحُوْتِ ۘ اِذْ نَادٰی وَهُوَ مَكْظُوْمٌ ۟ؕ",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 49,
      "text": "لَوْلَاۤ اَنْ تَدٰرَكَهٗ نِعْمَةٌ مِّنْ رَّبِّهٖ لَنُبِذَ بِالْعَرَآءِ وَهُوَ مَذْمُوْمٌ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَاجْتَبٰىهُ رَبُّهٗ فَجَعَلَهٗ مِنَ الصّٰلِحِیْنَ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَاِنْ یَّكَادُ الَّذِیْنَ كَفَرُوْا لَیُزْلِقُوْنَكَ بِاَبْصَارِهِمْ لَمَّا سَمِعُوا الذِّكْرَ وَیَقُوْلُوْنَ اِنَّهٗ لَمَجْنُوْنٌ ۟ۘ",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَمَا هُوَ اِلَّا ذِكْرٌ لِّلْعٰلَمِیْنَ ۟۠",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    }
  ]
}