      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَمَاۤ اَدْرٰىكَ مَا لَیْلَةُ الْقَدْرِ ۟ؕ",
      "juz": 30,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لَیْلَةُ الْقَدْرِ ۙ۬ خَیْرٌ مِّنْ اَلْفِ شَهْرٍ ۟ؕؔ",
      "juz": 30,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَنَزَّلُ الْمَلٰٓىِٕكَةُ وَالرُّوْحُ فِیْهَا بِاِذْنِ رَبِّهِمْ ۚ مِنْ كُلِّ اَمْرٍ ۟ۙۛ",
      "juz": 30,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "سَلٰمٌ ۛ۫ هِیَ حَتّٰی مَطْلَعِ الْفَجْرِ ۟۠",
      "juz": 30,
      "page": 598,
      "ruku": 539,
