    },
    {
      "number": 2,
      "text": "مَا الْقَارِعَةُ ۟ۚ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَاۤ اَدْرٰىكَ مَا الْقَارِعَةُ ۟ؕ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "یَوْمَ یَكُوْنُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوْثِ ۟ۙ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَتَكُوْنُ الْجِبَالُ كَالْعِهْنِ الْمَنْفُوْشِ ۟ؕ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَاَمَّا مَنْ ثَقُلَتْ مَوَازِیْنُهٗ ۟ۙ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَهُوَ فِیْ عِیْشَةٍ رَّاضِیَةٍ ۟ؕ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَاَمَّا مَنْ خَفَّتْ مَوَازِیْنُهٗ ۟ۙ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَاُمُّهٗ هَاوِیَةٌ ۟ؕ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَمَاۤ اَدْرٰىكَ مَا هِیَهْ ۟ؕ",
      "juz": 30,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "نَارٌ حَامِیَةٌ ۟۠",
      "juz": 30,
      "page": 600,
