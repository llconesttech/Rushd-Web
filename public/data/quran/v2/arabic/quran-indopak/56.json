{
  "number": 56,
  "name": "سُورَةُ الوَاقِعَةِ",
  "englishName": "Al-Waaqia",
  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا وَقَعَتِ الْوَاقِعَةُ ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 2,
      "text": "لَیْسَ لِوَقْعَتِهَا كَاذِبَةٌ ۟ۘ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 3,
      "text": "خَافِضَةٌ رَّافِعَةٌ ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِذَا رُجَّتِ الْاَرْضُ رَجًّا ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَّبُسَّتِ الْجِبَالُ بَسًّا ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَكَانَتْ هَبَآءً مُّنْۢبَثًّا ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّكُنْتُمْ اَزْوَاجًا ثَلٰثَةً ۟ؕ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَاَصْحٰبُ الْمَیْمَنَةِ ۙ۬ مَاۤ اَصْحٰبُ الْمَیْمَنَةِ ۟ؕ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَاَصْحٰبُ الْمَشْـَٔمَةِ ۙ۬ مَاۤ اَصْحٰبُ الْمَشْـَٔمَةِ ۟ؕ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَالسّٰبِقُوْنَ السّٰبِقُوْنَ ۟ۚۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اُولٰٓىِٕكَ الْمُقَرَّبُوْنَ ۟ۚ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فِیْ جَنّٰتِ النَّعِیْمِ ۟",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُلَّةٌ مِّنَ الْاَوَّلِیْنَ ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَقَلِیْلٌ مِّنَ الْاٰخِرِیْنَ ۟ؕ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 15,
      "text": "عَلٰی سُرُرٍ مَّوْضُوْنَةٍ ۟ۙ",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 16,
      "text": "مُّتَّكِـِٕیْنَ عَلَیْهَا مُتَقٰبِلِیْنَ ۟",
      "juz": 27,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 17,
      "text": "یَطُوْفُ عَلَیْهِمْ وِلْدَانٌ مُّخَلَّدُوْنَ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 18,
      "text": "بِاَكْوَابٍ وَّاَبَارِیْقَ ۙ۬ وَكَاْسٍ مِّنْ مَّعِیْنٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 19,
      "text": "لَّا یُصَدَّعُوْنَ عَنْهَا وَلَا یُنْزِفُوْنَ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَفَاكِهَةٍ مِّمَّا یَتَخَیَّرُوْنَ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَلَحْمِ طَیْرٍ مِّمَّا یَشْتَهُوْنَ ۟ؕ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَحُوْرٌ عِیْنٌ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 23,
      "text": "كَاَمْثَالِ اللُّؤْلُوءِ الْمَكْنُوْنِ ۟ۚ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 24,
      "text": "جَزَآءً بِمَا كَانُوْا یَعْمَلُوْنَ ۟",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 25,
      "text": "لَا یَسْمَعُوْنَ فِیْهَا لَغْوًا وَّلَا تَاْثِیْمًا ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 26,
      "text": "اِلَّا قِیْلًا سَلٰمًا سَلٰمًا ۟",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَاَصْحٰبُ الْیَمِیْنِ ۙ۬ مَاۤ اَصْحٰبُ الْیَمِیْنِ ۟ؕ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 28,
      "text": "فِیْ سِدْرٍ مَّخْضُوْدٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَّطَلْحٍ مَّنْضُوْدٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَّظِلٍّ مَّمْدُوْدٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وَّمَآءٍ مَّسْكُوْبٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَّفَاكِهَةٍ كَثِیْرَةٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 33,
      "text": "لَّا مَقْطُوْعَةٍ وَّلَا مَمْنُوْعَةٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَّفُرُشٍ مَّرْفُوْعَةٍ ۟ؕ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اِنَّاۤ اَنْشَاْنٰهُنَّ اِنْشَآءً ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَجَعَلْنٰهُنَّ اَبْكَارًا ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 37,
      "text": "عُرُبًا اَتْرَابًا ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 38,
      "text": "لِّاَصْحٰبِ الْیَمِیْنِ ۟ؕ۠",
      "juz": 27,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ثُلَّةٌ مِّنَ الْاَوَّلِیْنَ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَثُلَّةٌ مِّنَ الْاٰخِرِیْنَ ۟ؕ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَاَصْحٰبُ الشِّمَالِ ۙ۬ مَاۤ اَصْحٰبُ الشِّمَالِ ۟ؕ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فِیْ سَمُوْمٍ وَّحَمِیْمٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وَّظِلٍّ مِّنْ یَّحْمُوْمٍ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 44,
      "text": "لَّا بَارِدٍ وَّلَا كَرِیْمٍ ۟",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 45,
      "text": "اِنَّهُمْ كَانُوْا قَبْلَ ذٰلِكَ مُتْرَفِیْنَ ۟ۚۖ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 46,
      "text": "وَكَانُوْا یُصِرُّوْنَ عَلَی الْحِنْثِ الْعَظِیْمِ ۟ۚ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَكَانُوْا یَقُوْلُوْنَ ۙ۬ اَىِٕذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبْعُوْثُوْنَ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 48,
      "text": "اَوَاٰبَآؤُنَا الْاَوَّلُوْنَ ۟",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 49,
      "text": "قُلْ اِنَّ الْاَوَّلِیْنَ وَالْاٰخِرِیْنَ ۟ۙ",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 50,
      "text": "لَمَجْمُوْعُوْنَ ۙ۬ اِلٰی مِیْقَاتِ یَوْمٍ مَّعْلُوْمٍ ۟",
      "juz": 27,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ثُمَّ اِنَّكُمْ اَیُّهَا الضَّآلُّوْنَ الْمُكَذِّبُوْنَ ۟ۙ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 52,
      "text": "لَاٰكِلُوْنَ مِنْ شَجَرٍ مِّنْ زَقُّوْمٍ ۟ۙ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فَمَالِـُٔوْنَ مِنْهَا الْبُطُوْنَ ۟ۚ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فَشٰرِبُوْنَ عَلَیْهِ مِنَ الْحَمِیْمِ ۟ۚ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فَشٰرِبُوْنَ شُرْبَ الْهِیْمِ ۟ؕ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 56,
      "text": "هٰذَا نُزُلُهُمْ یَوْمَ الدِّیْنِ ۟ؕ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 57,
      "text": "نَحْنُ خَلَقْنٰكُمْ فَلَوْلَا تُصَدِّقُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 58,
      "text": "اَفَرَءَیْتُمْ مَّا تُمْنُوْنَ ۟ؕ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 59,
      "text": "ءَاَنْتُمْ تَخْلُقُوْنَهٗۤ اَمْ نَحْنُ الْخٰلِقُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 60,
      "text": "نَحْنُ قَدَّرْنَا بَیْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوْقِیْنَ ۟ۙ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 61,
      "text": "عَلٰۤی اَنْ نُّبَدِّلَ اَمْثَالَكُمْ وَنُنْشِئَكُمْ فِیْ مَا لَا تَعْلَمُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 62,
      "text": "وَلَقَدْ عَلِمْتُمُ النَّشْاَةَ الْاُوْلٰی فَلَوْلَا تَذَكَّرُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 63,
      "text": "اَفَرَءَیْتُمْ مَّا تَحْرُثُوْنَ ۟ؕ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 64,
      "text": "ءَاَنْتُمْ تَزْرَعُوْنَهٗۤ اَمْ نَحْنُ الزّٰرِعُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 65,
      "text": "لَوْ نَشَآءُ لَجَعَلْنٰهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 66,
      "text": "اِنَّا لَمُغْرَمُوْنَ ۟ۙ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 67,
      "text": "بَلْ نَحْنُ مَحْرُوْمُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 68,
      "text": "اَفَرَءَیْتُمُ الْمَآءَ الَّذِیْ تَشْرَبُوْنَ ۟ؕ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 69,
      "text": "ءَاَنْتُمْ اَنْزَلْتُمُوْهُ مِنَ الْمُزْنِ اَمْ نَحْنُ الْمُنْزِلُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 70,
      "text": "لَوْ نَشَآءُ جَعَلْنٰهُ اُجَاجًا فَلَوْلَا تَشْكُرُوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 71,
      "text": "اَفَرَءَیْتُمُ النَّارَ الَّتِیْ تُوْرُوْنَ ۟ؕ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 72,
      "text": "ءَاَنْتُمْ اَنْشَاْتُمْ شَجَرَتَهَاۤ اَمْ نَحْنُ الْمُنْشِـُٔوْنَ ۟",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 73,
      "text": "نَحْنُ جَعَلْنٰهَا تَذْكِرَةً وَّمَتَاعًا لِّلْمُقْوِیْنَ ۟ۚ",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 74,
      "text": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِیْمِ ۟۠",
      "juz": 27,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 75,
      "text": "فَلَاۤ اُقْسِمُ بِمَوٰقِعِ النُّجُوْمِ ۟ۙ",
      "juz": 27,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وَاِنَّهٗ لَقَسَمٌ لَّوْ تَعْلَمُوْنَ عَظِیْمٌ ۟ۙ",
      "juz": 27,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 77,
      "text": "اِنَّهٗ لَقُرْاٰنٌ كَرِیْمٌ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 78,
      "text": "فِیْ كِتٰبٍ مَّكْنُوْنٍ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 79,
      "text": "لَّا یَمَسُّهٗۤ اِلَّا الْمُطَهَّرُوْنَ ۟ؕ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 80,
      "text": "تَنْزِیْلٌ مِّنْ رَّبِّ الْعٰلَمِیْنَ ۟",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 81,
      "text": "اَفَبِهٰذَا الْحَدِیْثِ اَنْتُمْ مُّدْهِنُوْنَ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 82,
      "text": "وَتَجْعَلُوْنَ رِزْقَكُمْ اَنَّكُمْ تُكَذِّبُوْنَ ۟",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 83,
      "text": "فَلَوْلَاۤ اِذَا بَلَغَتِ الْحُلْقُوْمَ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 84,
      "text": "وَاَنْتُمْ حِیْنَىِٕذٍ تَنْظُرُوْنَ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وَنَحْنُ اَقْرَبُ اِلَیْهِ مِنْكُمْ وَلٰكِنْ لَّا تُبْصِرُوْنَ ۟",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 86,
      "text": "فَلَوْلَاۤ اِنْ كُنْتُمْ غَیْرَ مَدِیْنِیْنَ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 87,
      "text": "تَرْجِعُوْنَهَاۤ اِنْ كُنْتُمْ صٰدِقِیْنَ ۟",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 88,
      "text": "فَاَمَّاۤ اِنْ كَانَ مِنَ الْمُقَرَّبِیْنَ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 89,
      "text": "فَرَوْحٌ وَّرَیْحَانٌ ۙ۬ وَّجَنَّتُ نَعِیْمٍ ۟",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 90,
      "text": "وَاَمَّاۤ اِنْ كَانَ مِنْ اَصْحٰبِ الْیَمِیْنِ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 91,
      "text": "فَسَلٰمٌ لَّكَ مِنْ اَصْحٰبِ الْیَمِیْنِ ۟ؕ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 92,
      "text": "وَاَمَّاۤ اِنْ كَانَ مِنَ الْمُكَذِّبِیْنَ الضَّآلِّیْنَ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 93,
      "text": "فَنُزُلٌ مِّنْ حَمِیْمٍ ۟ۙ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 94,
      "text": "وَّتَصْلِیَةُ جَحِیْمٍ ۟",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 95,
      "text": "اِنَّ هٰذَا لَهُوَ حَقُّ الْیَقِیْنِ ۟ۚ",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 96,
      "text": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِیْمِ ۟۠",
      "juz": 27,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    }
  ]
