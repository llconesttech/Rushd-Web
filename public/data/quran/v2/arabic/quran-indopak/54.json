{
  "number": 54,
  "name": "سُورَةُ القَمَرِ",
  "englishName": "Al-Qamar",
  "ayahs": [
    {
      "number": 1,
      "text": "اِقْتَرَبَتِ السَّاعَةُ وَانْشَقَّ الْقَمَرُ ۟",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاِنْ یَّرَوْا اٰیَةً یُّعْرِضُوْا وَیَقُوْلُوْا سِحْرٌ مُّسْتَمِرٌّ ۟",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَكَذَّبُوْا وَاتَّبَعُوْۤا اَهْوَآءَهُمْ وَكُلُّ اَمْرٍ مُّسْتَقِرٌّ ۟",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَلَقَدْ جَآءَهُمْ مِّنَ الْاَنْۢبَآءِ مَا فِیْهِ مُزْدَجَرٌ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 5,
      "text": "حِكْمَةٌ بَالِغَةٌ فَمَا تُغْنِ النُّذُرُ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَتَوَلَّ عَنْهُمْ ۘ یَوْمَ یَدْعُ الدَّاعِ اِلٰی شَیْءٍ نُّكُرٍ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 7,
      "text": "خُشَّعًا اَبْصَارُهُمْ یَخْرُجُوْنَ مِنَ الْاَجْدَاثِ كَاَنَّهُمْ جَرَادٌ مُّنْتَشِرٌ ۟ۙ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 8,
      "text": "مُّهْطِعِیْنَ اِلَی الدَّاعِ ؕ یَقُوْلُ الْكٰفِرُوْنَ هٰذَا یَوْمٌ عَسِرٌ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوْحٍ فَكَذَّبُوْا عَبْدَنَا وَقَالُوْا مَجْنُوْنٌ وَّازْدُجِرَ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَدَعَا رَبَّهٗۤ اَنِّیْ مَغْلُوْبٌ فَانْتَصِرْ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَفَتَحْنَاۤ اَبْوَابَ السَّمَآءِ بِمَآءٍ مُّنْهَمِرٍ ۟ؗۖ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَّفَجَّرْنَا الْاَرْضَ عُیُوْنًا فَالْتَقَی الْمَآءُ عَلٰۤی اَمْرٍ قَدْ قُدِرَ ۟ۚ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَحَمَلْنٰهُ عَلٰی ذَاتِ اَلْوَاحٍ وَّدُسُرٍ ۟ۙ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 14,
      "text": "تَجْرِیْ بِاَعْیُنِنَا ۚ جَزَآءً لِّمَنْ كَانَ كُفِرَ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَقَدْ تَّرَكْنٰهَاۤ اٰیَةً فَهَلْ مِنْ مُّدَّكِرٍ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَكَیْفَ كَانَ عَذَابِیْ وَنُذُرِ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَلَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 18,
      "text": "كَذَّبَتْ عَادٌ فَكَیْفَ كَانَ عَذَابِیْ وَنُذُرِ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اِنَّاۤ اَرْسَلْنَا عَلَیْهِمْ رِیْحًا صَرْصَرًا فِیْ یَوْمِ نَحْسٍ مُّسْتَمِرٍّ ۟ۙ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 20,
      "text": "تَنْزِعُ النَّاسَ ۙ كَاَنَّهُمْ اَعْجَازُ نَخْلٍ مُّنْقَعِرٍ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَكَیْفَ كَانَ عَذَابِیْ وَنُذُرِ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَلَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ ۟۠",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 23,
      "text": "كَذَّبَتْ ثَمُوْدُ بِالنُّذُرِ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَقَالُوْۤا اَبَشَرًا مِّنَّا وَاحِدًا نَّتَّبِعُهٗۤ ۙ اِنَّاۤ اِذًا لَّفِیْ ضَلٰلٍ وَّسُعُرٍ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 25,
      "text": "ءَاُلْقِیَ الذِّكْرُ عَلَیْهِ مِنْ بَیْنِنَا بَلْ هُوَ كَذَّابٌ اَشِرٌ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 26,
      "text": "سَیَعْلَمُوْنَ غَدًا مَّنِ الْكَذَّابُ الْاَشِرُ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اِنَّا مُرْسِلُوا النَّاقَةِ فِتْنَةً لَّهُمْ فَارْتَقِبْهُمْ وَاصْطَبِرْ ۟ؗ",
      "juz": 27,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَنَبِّئْهُمْ اَنَّ الْمَآءَ قِسْمَةٌ بَیْنَهُمْ ۚ كُلُّ شِرْبٍ مُّحْتَضَرٌ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَنَادَوْا صَاحِبَهُمْ فَتَعَاطٰی فَعَقَرَ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَكَیْفَ كَانَ عَذَابِیْ وَنُذُرِ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اِنَّاۤ اَرْسَلْنَا عَلَیْهِمْ صَیْحَةً وَّاحِدَةً فَكَانُوْا كَهَشِیْمِ الْمُحْتَظِرِ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَذَّبَتْ قَوْمُ لُوْطٍ بِالنُّذُرِ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اِنَّاۤ اَرْسَلْنَا عَلَیْهِمْ حَاصِبًا اِلَّاۤ اٰلَ لُوْطٍ ؕ نَجَّیْنٰهُمْ بِسَحَرٍ ۟ۙ",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 35,
      "text": "نِّعْمَةً مِّنْ عِنْدِنَا ؕ كَذٰلِكَ نَجْزِیْ مَنْ شَكَرَ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَلَقَدْ اَنْذَرَهُمْ بَطْشَتَنَا فَتَمَارَوْا بِالنُّذُرِ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَلَقَدْ رَاوَدُوْهُ عَنْ ضَیْفِهٖ فَطَمَسْنَاۤ اَعْیُنَهُمْ فَذُوْقُوْا عَذَابِیْ وَنُذُرِ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَلَقَدْ صَبَّحَهُمْ بُكْرَةً عَذَابٌ مُّسْتَقِرٌّ ۟ۚ",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَذُوْقُوْا عَذَابِیْ وَنُذُرِ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَلَقَدْ یَسَّرْنَا الْقُرْاٰنَ لِلذِّكْرِ فَهَلْ مِنْ مُّدَّكِرٍ ۟۠",
      "juz": 27,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَلَقَدْ جَآءَ اٰلَ فِرْعَوْنَ النُّذُرُ ۟ۚ",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 42,
      "text": "كَذَّبُوْا بِاٰیٰتِنَا كُلِّهَا فَاَخَذْنٰهُمْ اَخْذَ عَزِیْزٍ مُّقْتَدِرٍ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اَكُفَّارُكُمْ خَیْرٌ مِّنْ اُولٰٓىِٕكُمْ اَمْ لَكُمْ بَرَآءَةٌ فِی الزُّبُرِ ۟ۚ",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 44,
      "text": "اَمْ یَقُوْلُوْنَ نَحْنُ جَمِیْعٌ مُّنْتَصِرٌ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 45,
      "text": "سَیُهْزَمُ الْجَمْعُ وَیُوَلُّوْنَ الدُّبُرَ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 46,
      "text": "بَلِ السَّاعَةُ مَوْعِدُهُمْ وَالسَّاعَةُ اَدْهٰی وَاَمَرُّ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 47,
      "text": "اِنَّ الْمُجْرِمِیْنَ فِیْ ضَلٰلٍ وَّسُعُرٍ ۟ۘ",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 48,
      "text": "یَوْمَ یُسْحَبُوْنَ فِی النَّارِ عَلٰی وُجُوْهِهِمْ ؕ ذُوْقُوْا مَسَّ سَقَرَ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 49,
      "text": "اِنَّا كُلَّ شَیْءٍ خَلَقْنٰهُ بِقَدَرٍ ۟",
      "juz": 27,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَمَاۤ اَمْرُنَاۤ اِلَّا وَاحِدَةٌ كَلَمْحٍ بِالْبَصَرِ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَلَقَدْ اَهْلَكْنَاۤ اَشْیَاعَكُمْ فَهَلْ مِنْ مُّدَّكِرٍ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَكُلُّ شَیْءٍ فَعَلُوْهُ فِی الزُّبُرِ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 53,
      "text": "وَكُلُّ صَغِیْرٍ وَّكَبِیْرٍ مُّسْتَطَرٌ ۟",
      "juz": 27,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 54,
      "text": "اِنَّ الْمُتَّقِیْنَ فِیْ جَنّٰتٍ وَّنَهَرٍ ۟ۙ",
      "juz": 27,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فِیْ مَقْعَدِ صِدْقٍ عِنْدَ مَلِیْكٍ مُّقْتَدِرٍ ۟۠",
      "juz": 27,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    }
  ]
}