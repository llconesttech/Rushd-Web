{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالسَّمَآءِ وَالطَّارِقِ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَمَاۤ اَدْرٰىكَ مَا الطَّارِقُ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "النَّجْمُ الثَّاقِبُ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِنْ كُلُّ نَفْسٍ لَّمَّا عَلَیْهَا حَافِظٌ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَلْیَنْظُرِ الْاِنْسَانُ مِمَّ خُلِقَ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "خُلِقَ مِنْ مَّآءٍ دَافِقٍ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "یَّخْرُجُ مِنْ بَیْنِ الصُّلْبِ وَالتَّرَآىِٕبِ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِنَّهٗ عَلٰی رَجْعِهٖ لَقَادِرٌ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یَوْمَ تُبْلَی السَّرَآىِٕرُ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَمَا لَهٗ مِنْ قُوَّةٍ وَّلَا نَاصِرٍ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَالسَّمَآءِ ذَاتِ الرَّجْعِ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَالْاَرْضِ ذَاتِ الصَّدْعِ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِنَّهٗ لَقَوْلٌ فَصْلٌ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّمَا هُوَ بِالْهَزْلِ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِنَّهُمْ یَكِیْدُوْنَ كَیْدًا ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَّاَكِیْدُ كَیْدًا ۟ۚۖ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَمَهِّلِ الْكٰفِرِیْنَ اَمْهِلْهُمْ رُوَیْدًا ۟۠",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}