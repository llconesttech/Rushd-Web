{
  "number": 89,
  "name": "سُورَةُ الفَجۡرِ",
  "englishName": "Al-Fajr",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالْفَجْرِ ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلَیَالٍ عَشْرٍ ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّالشَّفْعِ وَالْوَتْرِ ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّیْلِ اِذَا یَسْرِ ۟ۚ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "هَلْ فِیْ ذٰلِكَ قَسَمٌ لِّذِیْ حِجْرٍ ۟ؕ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اَلَمْ تَرَ كَیْفَ فَعَلَ رَبُّكَ بِعَادٍ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِرَمَ ذَاتِ الْعِمَادِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "الَّتِیْ لَمْ یُخْلَقْ مِثْلُهَا فِی الْبِلَادِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَثَمُوْدَ الَّذِیْنَ جَابُوا الصَّخْرَ بِالْوَادِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَفِرْعَوْنَ ذِی الْاَوْتَادِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الَّذِیْنَ طَغَوْا فِی الْبِلَادِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فَاَكْثَرُوْا فِیْهَا الْفَسَادَ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَصَبَّ عَلَیْهِمْ رَبُّكَ سَوْطَ عَذَابٍ ۟ۚۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اِنَّ رَبَّكَ لَبِالْمِرْصَادِ ۟ؕ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَاَمَّا الْاِنْسَانُ اِذَا مَا ابْتَلٰىهُ رَبُّهٗ فَاَكْرَمَهٗ وَنَعَّمَهٗ ۙ۬ فَیَقُوْلُ رَبِّیْۤ اَكْرَمَنِ ۟ؕ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَاَمَّاۤ اِذَا مَا ابْتَلٰىهُ فَقَدَرَ عَلَیْهِ رِزْقَهٗ ۙ۬ فَیَقُوْلُ رَبِّیْۤ اَهَانَنِ ۟ۚ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "كَلَّا بَلْ لَّا تُكْرِمُوْنَ الْیَتِیْمَ ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلَا تَحٰٓضُّوْنَ عَلٰی طَعَامِ الْمِسْكِیْنِ ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَتَاْكُلُوْنَ التُّرَاثَ اَكْلًا لَّمًّا ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَّتُحِبُّوْنَ الْمَالَ حُبًّا جَمًّا ۟ؕ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "كَلَّاۤ اِذَا دُكَّتِ الْاَرْضُ دَكًّا دَكًّا ۟ۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَّجَآءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا ۟ۚ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَجِایْٓءَ یَوْمَىِٕذٍۭ بِجَهَنَّمَ ۙ۬ یَوْمَىِٕذٍ یَّتَذَكَّرُ الْاِنْسَانُ وَاَنّٰی لَهُ الذِّكْرٰی ۟ؕ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "یَقُوْلُ یٰلَیْتَنِیْ قَدَّمْتُ لِحَیَاتِیْ ۟ۚ",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَیَوْمَىِٕذٍ لَّا یُعَذِّبُ عَذَابَهٗۤ اَحَدٌ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَّلَا یُوْثِقُ وَثَاقَهٗۤ اَحَدٌ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 27,
      "text": "یٰۤاَیَّتُهَا النَّفْسُ الْمُطْمَىِٕنَّةُ ۟ۗۖ",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 28,
      "text": "ارْجِعِیْۤ اِلٰی رَبِّكِ رَاضِیَةً مَّرْضِیَّةً ۟ۚ",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَادْخُلِیْ فِیْ عِبٰدِیْ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَادْخُلِیْ جَنَّتِیْ ۟۠",
      "juz": 30,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}