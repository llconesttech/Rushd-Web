{
  "number": 90,
  "name": "سُورَةُ البَلَدِ",
  "englishName": "Al-Balad",
  "ayahs": [
    {
      "number": 1,
      "text": "لَاۤ اُقْسِمُ بِهٰذَا الْبَلَدِ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاَنْتَ حِلٌّۢ بِهٰذَا الْبَلَدِ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَوَالِدٍ وَّمَا وَلَدَ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "لَقَدْ خَلَقْنَا الْاِنْسَانَ فِیْ كَبَدٍ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اَیَحْسَبُ اَنْ لَّنْ یَّقْدِرَ عَلَیْهِ اَحَدٌ ۟ۘ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَقُوْلُ اَهْلَكْتُ مَالًا لُّبَدًا ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اَیَحْسَبُ اَنْ لَّمْ یَرَهٗۤ اَحَدٌ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اَلَمْ نَجْعَلْ لَّهٗ عَیْنَیْنِ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَلِسَانًا وَّشَفَتَیْنِ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَهَدَیْنٰهُ النَّجْدَیْنِ ۟ۚ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَلَا اقْتَحَمَ الْعَقَبَةَ ۟ؗۖ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَمَاۤ اَدْرٰىكَ مَا الْعَقَبَةُ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَكُّ رَقَبَةٍ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اَوْ اِطْعٰمٌ فِیْ یَوْمٍ ذِیْ مَسْغَبَةٍ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "یَّتِیْمًا ذَا مَقْرَبَةٍ ۟ۙ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اَوْ مِسْكِیْنًا ذَا مَتْرَبَةٍ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ثُمَّ كَانَ مِنَ الَّذِیْنَ اٰمَنُوْا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اُولٰٓىِٕكَ اَصْحٰبُ الْمَیْمَنَةِ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَالَّذِیْنَ كَفَرُوْا بِاٰیٰتِنَا هُمْ اَصْحٰبُ الْمَشْـَٔمَةِ ۟ؕ",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 20,
      "text": "عَلَیْهِمْ نَارٌ مُّؤْصَدَةٌ ۟۠",
      "juz": 30,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}