{
  "number": 83,
  "name": "سُورَةُ المُطَفِّفِينَ",
  "englishName": "Al-Mutaffifin",
  "ayahs": [
    {
      "number": 1,
      "text": "وَیْلٌ لِّلْمُطَفِّفِیْنَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الَّذِیْنَ اِذَا اكْتَالُوْا عَلَی النَّاسِ یَسْتَوْفُوْنَ ۟ؗۖ",
      "juz": 30,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِذَا كَالُوْهُمْ اَوْ وَّزَنُوْهُمْ یُخْسِرُوْنَ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اَلَا یَظُنُّ اُولٰٓىِٕكَ اَنَّهُمْ مَّبْعُوْثُوْنَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5,
      "text": "لِیَوْمٍ عَظِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَّوْمَ یَقُوْمُ النَّاسُ لِرَبِّ الْعٰلَمِیْنَ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 7,
      "text": "كَلَّاۤ اِنَّ كِتٰبَ الْفُجَّارِ لَفِیْ سِجِّیْنٍ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَمَاۤ اَدْرٰىكَ مَا سِجِّیْنٌ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كِتٰبٌ مَّرْقُوْمٌ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الَّذِیْنَ یُكَذِّبُوْنَ بِیَوْمِ الدِّیْنِ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَمَا یُكَذِّبُ بِهٖۤ اِلَّا كُلُّ مُعْتَدٍ اَثِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِذَا تُتْلٰی عَلَیْهِ اٰیٰتُنَا قَالَ اَسَاطِیْرُ الْاَوَّلِیْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 14,
      "text": "كَلَّا بَلْ ٚ رَانَ عَلٰی قُلُوْبِهِمْ مَّا كَانُوْا یَكْسِبُوْنَ ۟",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلَّاۤ اِنَّهُمْ عَنْ رَّبِّهِمْ یَوْمَىِٕذٍ لَّمَحْجُوْبُوْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ثُمَّ اِنَّهُمْ لَصَالُوا الْجَحِیْمِ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ثُمَّ یُقَالُ هٰذَا الَّذِیْ كُنْتُمْ بِهٖ تُكَذِّبُوْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 18,
      "text": "كَلَّاۤ اِنَّ كِتٰبَ الْاَبْرَارِ لَفِیْ عِلِّیِّیْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَمَاۤ اَدْرٰىكَ مَا عِلِّیُّوْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 20,
      "text": "كِتٰبٌ مَّرْقُوْمٌ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 21,
      "text": "یَّشْهَدُهُ الْمُقَرَّبُوْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اِنَّ الْاَبْرَارَ لَفِیْ نَعِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 23,
      "text": "عَلَی الْاَرَآىِٕكِ یَنْظُرُوْنَ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 24,
      "text": "تَعْرِفُ فِیْ وُجُوْهِهِمْ نَضْرَةَ النَّعِیْمِ ۟ۚ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 25,
      "text": "یُسْقَوْنَ مِنْ رَّحِیْقٍ مَّخْتُوْمٍ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 26,
      "text": "خِتٰمُهٗ مِسْكٌ ؕ وَفِیْ ذٰلِكَ فَلْیَتَنَافَسِ الْمُتَنَافِسُوْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَمِزَاجُهٗ مِنْ تَسْنِیْمٍ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 28,
      "text": "عَیْنًا یَّشْرَبُ بِهَا الْمُقَرَّبُوْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اِنَّ الَّذِیْنَ اَجْرَمُوْا كَانُوْا مِنَ الَّذِیْنَ اٰمَنُوْا یَضْحَكُوْنَ ۟ؗۖ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَاِذَا مَرُّوْا بِهِمْ یَتَغَامَزُوْنَ ۟ؗۖ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وَاِذَا انْقَلَبُوْۤا اِلٰۤی اَهْلِهِمُ انْقَلَبُوْا فَكِهِیْنَ ۟ؗۖ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَاِذَا رَاَوْهُمْ قَالُوْۤا اِنَّ هٰۤؤُلَآءِ لَضَآلُّوْنَ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَمَاۤ اُرْسِلُوْا عَلَیْهِمْ حٰفِظِیْنَ ۟ؕ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَالْیَوْمَ الَّذِیْنَ اٰمَنُوْا مِنَ الْكُفَّارِ یَضْحَكُوْنَ ۟ۙ",
      "juz": 30,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 35,
      "text": "عَلَی الْاَرَآىِٕكِ ۙ یَنْظُرُوْنَ ۟ؕ",
      "juz": 30,
      "page": 589,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 36,
      "text": "هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوْا یَفْعَلُوْنَ ۟۠",
      "juz": 30,
      "page": 589,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    }
  ]
}