{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "ayahs": [
    {
      "number": 1,
      "text": "وَالْمُرْسَلٰتِ عُرْفًا ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالْعٰصِفٰتِ عَصْفًا ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّالنّٰشِرٰتِ نَشْرًا ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَالْفٰرِقٰتِ فَرْقًا ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَالْمُلْقِیٰتِ ذِكْرًا ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 6,
      "text": "عُذْرًا اَوْ نُذْرًا ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِنَّمَا تُوْعَدُوْنَ لَوَاقِعٌ ۟ؕ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَاِذَا النُّجُوْمُ طُمِسَتْ ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَاِذَا السَّمَآءُ فُرِجَتْ ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاِذَا الْجِبَالُ نُسِفَتْ ۟ۙ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَاِذَا الرُّسُلُ اُقِّتَتْ ۟ؕ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 12,
      "text": "لِاَیِّ یَوْمٍ اُجِّلَتْ ۟ؕ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 13,
      "text": "لِیَوْمِ الْفَصْلِ ۟ۚ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَمَاۤ اَدْرٰىكَ مَا یَوْمُ الْفَصْلِ ۟ؕ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اَلَمْ نُهْلِكِ الْاَوَّلِیْنَ ۟ؕ",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ثُمَّ نُتْبِعُهُمُ الْاٰخِرِیْنَ ۟",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 18,
      "text": "كَذٰلِكَ نَفْعَلُ بِالْمُجْرِمِیْنَ ۟",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اَلَمْ نَخْلُقْكُّمْ مِّنْ مَّآءٍ مَّهِیْنٍ ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَجَعَلْنٰهُ فِیْ قَرَارٍ مَّكِیْنٍ ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اِلٰی قَدَرٍ مَّعْلُوْمٍ ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَقَدَرْنَا ۖۗ فَنِعْمَ الْقٰدِرُوْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اَلَمْ نَجْعَلِ الْاَرْضَ كِفَاتًا ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 26,
      "text": "اَحْیَآءً وَّاَمْوَاتًا ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَّجَعَلْنَا فِیْهَا رَوَاسِیَ شٰمِخٰتٍ وَّاَسْقَیْنٰكُمْ مَّآءً فُرَاتًا ۟ؕ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اِنْطَلِقُوْۤا اِلٰی مَا كُنْتُمْ بِهٖ تُكَذِّبُوْنَ ۟ۚ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اِنْطَلِقُوْۤا اِلٰی ظِلٍّ ذِیْ ثَلٰثِ شُعَبٍ ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 31,
      "text": "لَّا ظَلِیْلٍ وَّلَا یُغْنِیْ مِنَ اللَّهَبِ ۟ؕ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اِنَّهَا تَرْمِیْ بِشَرَرٍ كَالْقَصْرِ ۟ۚ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَاَنَّهٗ جِمٰلَتٌ صُفْرٌ ۟ؕ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 35,
      "text": "هٰذَا یَوْمُ لَا یَنْطِقُوْنَ ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَلَا یُؤْذَنُ لَهُمْ فَیَعْتَذِرُوْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 38,
      "text": "هٰذَا یَوْمُ الْفَصْلِ ۚ جَمَعْنٰكُمْ وَالْاَوَّلِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَاِنْ كَانَ لَكُمْ كَیْدٌ فَكِیْدُوْنِ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟۠",
      "juz": 29,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اِنَّ الْمُتَّقِیْنَ فِیْ ظِلٰلٍ وَّعُیُوْنٍ ۟ۙ",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وَّفَوَاكِهَ مِمَّا یَشْتَهُوْنَ ۟ؕ",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 43,
      "text": "كُلُوْا وَاشْرَبُوْا هَنِیْٓـًٔا بِمَا كُنْتُمْ تَعْمَلُوْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 44,
      "text": "اِنَّا كَذٰلِكَ نَجْزِی الْمُحْسِنِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كُلُوْا وَتَمَتَّعُوْا قَلِیْلًا اِنَّكُمْ مُّجْرِمُوْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَاِذَا قِیْلَ لَهُمُ ارْكَعُوْا لَا یَرْكَعُوْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَیْلٌ یَّوْمَىِٕذٍ لِّلْمُكَذِّبِیْنَ ۟",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَبِاَیِّ حَدِیْثٍ بَعْدَهٗ یُؤْمِنُوْنَ ۟۠",
      "juz": 29,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    }
  ]
}