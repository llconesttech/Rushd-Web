{
  "number": 54,
  "name": "سُورَةُ القَمَرِ",
  "englishName": "Al-Qamar",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ اقتَرَبَتِ السّاعَةُ وَانشَقَّ القَمَرُ",
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَإِن يَرَوا ءايَةً يُعرِضوا وَيَقولوا سِحرٌ مُستَمِرٌّ",
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَكَذَّبوا وَاتَّبَعوا أَهواءَهُم ۚ وَكُلُّ أَمرٍ مُستَقِرٌّ",
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَلَقَد جاءَهُم مِنَ الأَنباءِ ما فيهِ مُزدَجَرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 5,
      "text": "حِكمَةٌ بٰلِغَةٌ ۖ فَما تُغنِ النُّذُرُ",
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَتَوَلَّ عَنهُم ۘ يَومَ يَدعُ الدّاعِ إِلىٰ شَيءٍ نُكُرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 7,
      "text": "خُشَّعًا أَبصٰرُهُم يَخرُجونَ مِنَ الأَجداثِ كَأَنَّهُم جَرادٌ مُنتَشِرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 8,
      "text": "مُهطِعينَ إِلَى الدّاعِ ۖ يَقولُ الكٰفِرونَ هٰذا يَومٌ عَسِرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 9,
      "text": "۞ كَذَّبَت قَبلَهُم قَومُ نوحٍ فَكَذَّبوا عَبدَنا وَقالوا مَجنونٌ وَازدُجِرَ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَدَعا رَبَّهُ أَنّى مَغلوبٌ فَانتَصِر",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَفَتَحنا أَبوٰبَ السَّماءِ بِماءٍ مُنهَمِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَفَجَّرنَا الأَرضَ عُيونًا فَالتَقَى الماءُ عَلىٰ أَمرٍ قَد قُدِرَ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَحَمَلنٰهُ عَلىٰ ذاتِ أَلوٰحٍ وَدُسُرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 14,
      "text": "تَجرى بِأَعيُنِنا جَزاءً لِمَن كانَ كُفِرَ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَقَد تَرَكنٰها ءايَةً فَهَل مِن مُدَّكِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَكَيفَ كانَ عَذابى وَنُذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَلَقَد يَسَّرنَا القُرءانَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 18,
      "text": "كَذَّبَت عادٌ فَكَيفَ كانَ عَذابى وَنُذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 19,
      "text": "إِنّا أَرسَلنا عَلَيهِم ريحًا صَرصَرًا فى يَومِ نَحسٍ مُستَمِرٍّ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 20,
      "text": "تَنزِعُ النّاسَ كَأَنَّهُم أَعجازُ نَخلٍ مُنقَعِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَكَيفَ كانَ عَذابى وَنُذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَلَقَد يَسَّرنَا القُرءانَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 23,
      "text": "كَذَّبَت ثَمودُ بِالنُّذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَقالوا أَبَشَرًا مِنّا وٰحِدًا نَتَّبِعُهُ إِنّا إِذًا لَفى ضَلٰلٍ وَسُعُرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 25,
      "text": "أَءُلقِىَ الذِّكرُ عَلَيهِ مِن بَينِنا بَل هُوَ كَذّابٌ أَشِرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 26,
      "text": "سَيَعلَمونَ غَدًا مَنِ الكَذّابُ الأَشِرُ",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إِنّا مُرسِلُوا النّاقَةِ فِتنَةً لَهُم فَارتَقِبهُم وَاصطَبِر",
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَنَبِّئهُم أَنَّ الماءَ قِسمَةٌ بَينَهُم ۖ كُلُّ شِربٍ مُحتَضَرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَنادَوا صاحِبَهُم فَتَعاطىٰ فَعَقَرَ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَكَيفَ كانَ عَذابى وَنُذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 31,
      "text": "إِنّا أَرسَلنا عَلَيهِم صَيحَةً وٰحِدَةً فَكانوا كَهَشيمِ المُحتَظِرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلَقَد يَسَّرنَا القُرءانَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَذَّبَت قَومُ لوطٍ بِالنُّذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 34,
      "text": "إِنّا أَرسَلنا عَلَيهِم حاصِبًا إِلّا ءالَ لوطٍ ۖ نَجَّينٰهُم بِسَحَرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 35,
      "text": "نِعمَةً مِن عِندِنا ۚ كَذٰلِكَ نَجزى مَن شَكَرَ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَلَقَد أَنذَرَهُم بَطشَتَنا فَتَمارَوا بِالنُّذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَلَقَد رٰوَدوهُ عَن ضَيفِهِ فَطَمَسنا أَعيُنَهُم فَذوقوا عَذابى وَنُذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَلَقَد صَبَّحَهُم بُكرَةً عَذابٌ مُستَقِرٌّ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَذوقوا عَذابى وَنُذُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَلَقَد يَسَّرنَا القُرءانَ لِلذِّكرِ فَهَل مِن مُدَّكِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَلَقَد جاءَ ءالَ فِرعَونَ النُّذُرُ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 42,
      "text": "كَذَّبوا بِـٔايٰتِنا كُلِّها فَأَخَذنٰهُم أَخذَ عَزيزٍ مُقتَدِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 43,
      "text": "أَكُفّارُكُم خَيرٌ مِن أُولٰئِكُم أَم لَكُم بَراءَةٌ فِى الزُّبُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 44,
      "text": "أَم يَقولونَ نَحنُ جَميعٌ مُنتَصِرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 45,
      "text": "سَيُهزَمُ الجَمعُ وَيُوَلّونَ الدُّبُرَ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 46,
      "text": "بَلِ السّاعَةُ مَوعِدُهُم وَالسّاعَةُ أَدهىٰ وَأَمَرُّ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 47,
      "text": "إِنَّ المُجرِمينَ فى ضَلٰلٍ وَسُعُرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 48,
      "text": "يَومَ يُسحَبونَ فِى النّارِ عَلىٰ وُجوهِهِم ذوقوا مَسَّ سَقَرَ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 49,
      "text": "إِنّا كُلَّ شَيءٍ خَلَقنٰهُ بِقَدَرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَما أَمرُنا إِلّا وٰحِدَةٌ كَلَمحٍ بِالبَصَرِ",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَلَقَد أَهلَكنا أَشياعَكُم فَهَل مِن مُدَّكِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَكُلُّ شَيءٍ فَعَلوهُ فِى الزُّبُرِ",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 53,
      "text": "وَكُلُّ صَغيرٍ وَكَبيرٍ مُستَطَرٌ",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 54,
      "text": "إِنَّ المُتَّقينَ فى جَنّٰتٍ وَنَهَرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فى مَقعَدِ صِدقٍ عِندَ مَليكٍ مُقتَدِرٍ",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    }
  ]
}