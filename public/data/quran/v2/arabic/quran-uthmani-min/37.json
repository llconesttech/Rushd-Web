{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالصّٰفّٰتِ صَفًّا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالزّٰجِرٰتِ زَجرًا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فَالتّٰلِيٰتِ ذِكرًا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِنَّ إِلٰهَكُم لَوٰحِدٌ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 5,
      "text": "رَبُّ السَّمٰوٰتِ وَالأَرضِ وَما بَينَهُما وَرَبُّ المَشٰرِقِ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 6,
      "text": "إِنّا زَيَّنَّا السَّماءَ الدُّنيا بِزينَةٍ الكَواكِبِ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَحِفظًا مِن كُلِّ شَيطٰنٍ مارِدٍ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 8,
      "text": "لا يَسَّمَّعونَ إِلَى المَلَإِ الأَعلىٰ وَيُقذَفونَ مِن كُلِّ جانِبٍ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 9,
      "text": "دُحورًا ۖ وَلَهُم عَذابٌ واصِبٌ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 10,
      "text": "إِلّا مَن خَطِفَ الخَطفَةَ فَأَتبَعَهُ شِهابٌ ثاقِبٌ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَاستَفتِهِم أَهُم أَشَدُّ خَلقًا أَم مَن خَلَقنا ۚ إِنّا خَلَقنٰهُم مِن طينٍ لازِبٍ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 12,
      "text": "بَل عَجِبتَ وَيَسخَرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَإِذا ذُكِّروا لا يَذكُرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَإِذا رَأَوا ءايَةً يَستَسخِرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَقالوا إِن هٰذا إِلّا سِحرٌ مُبينٌ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 16,
      "text": "أَءِذا مِتنا وَكُنّا تُرابًا وَعِظٰمًا أَءِنّا لَمَبعوثونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 17,
      "text": "أَوَءاباؤُنَا الأَوَّلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قُل نَعَم وَأَنتُم دٰخِرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَإِنَّما هِىَ زَجرَةٌ وٰحِدَةٌ فَإِذا هُم يَنظُرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَقالوا يٰوَيلَنا هٰذا يَومُ الدّينِ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 21,
      "text": "هٰذا يَومُ الفَصلِ الَّذى كُنتُم بِهِ تُكَذِّبونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 22,
      "text": "۞ احشُرُوا الَّذينَ ظَلَموا وَأَزوٰجَهُم وَما كانوا يَعبُدونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 23,
      "text": "مِن دونِ اللَّهِ فَاهدوهُم إِلىٰ صِرٰطِ الجَحيمِ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَقِفوهُم ۖ إِنَّهُم مَسـٔولونَ",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 25,
      "text": "ما لَكُم لا تَناصَرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 26,
      "text": "بَل هُمُ اليَومَ مُستَسلِمونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَساءَلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قالوا إِنَّكُم كُنتُم تَأتونَنا عَنِ اليَمينِ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قالوا بَل لَم تَكونوا مُؤمِنينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَما كانَ لَنا عَلَيكُم مِن سُلطٰنٍ ۖ بَل كُنتُم قَومًا طٰغينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَحَقَّ عَلَينا قَولُ رَبِّنا ۖ إِنّا لَذائِقونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فَأَغوَينٰكُم إِنّا كُنّا غٰوينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 33,
      "text": "فَإِنَّهُم يَومَئِذٍ فِى العَذابِ مُشتَرِكونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 34,
      "text": "إِنّا كَذٰلِكَ نَفعَلُ بِالمُجرِمينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 35,
      "text": "إِنَّهُم كانوا إِذا قيلَ لَهُم لا إِلٰهَ إِلَّا اللَّهُ يَستَكبِرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَيَقولونَ أَئِنّا لَتارِكوا ءالِهَتِنا لِشاعِرٍ مَجنونٍ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 37,
      "text": "بَل جاءَ بِالحَقِّ وَصَدَّقَ المُرسَلينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 38,
      "text": "إِنَّكُم لَذائِقُوا العَذابِ الأَليمِ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَما تُجزَونَ إِلّا ما كُنتُم تَعمَلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إِلّا عِبادَ اللَّهِ المُخلَصينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 41,
      "text": "أُولٰئِكَ لَهُم رِزقٌ مَعلومٌ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فَوٰكِهُ ۖ وَهُم مُكرَمونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 43,
      "text": "فى جَنّٰتِ النَّعيمِ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 44,
      "text": "عَلىٰ سُرُرٍ مُتَقٰبِلينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 45,
      "text": "يُطافُ عَلَيهِم بِكَأسٍ مِن مَعينٍ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 46,
      "text": "بَيضاءَ لَذَّةٍ لِلشّٰرِبينَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 47,
      "text": "لا فيها غَولٌ وَلا هُم عَنها يُنزَفونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَعِندَهُم قٰصِرٰتُ الطَّرفِ عينٌ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 49,
      "text": "كَأَنَّهُنَّ بَيضٌ مَكنونٌ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَساءَلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 51,
      "text": "قالَ قائِلٌ مِنهُم إِنّى كانَ لى قَرينٌ",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 52,
      "text": "يَقولُ أَءِنَّكَ لَمِنَ المُصَدِّقينَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 53,
      "text": "أَءِذا مِتنا وَكُنّا تُرابًا وَعِظٰمًا أَءِنّا لَمَدينونَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 54,
      "text": "قالَ هَل أَنتُم مُطَّلِعونَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فَاطَّلَعَ فَرَءاهُ فى سَواءِ الجَحيمِ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 56,
      "text": "قالَ تَاللَّهِ إِن كِدتَ لَتُردينِ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 57,
      "text": "وَلَولا نِعمَةُ رَبّى لَكُنتُ مِنَ المُحضَرينَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 58,
      "text": "أَفَما نَحنُ بِمَيِّتينَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 59,
      "text": "إِلّا مَوتَتَنَا الأولىٰ وَما نَحنُ بِمُعَذَّبينَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 60,
      "text": "إِنَّ هٰذا لَهُوَ الفَوزُ العَظيمُ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 61,
      "text": "لِمِثلِ هٰذا فَليَعمَلِ العٰمِلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 62,
      "text": "أَذٰلِكَ خَيرٌ نُزُلًا أَم شَجَرَةُ الزَّقّومِ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 63,
      "text": "إِنّا جَعَلنٰها فِتنَةً لِلظّٰلِمينَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 64,
      "text": "إِنَّها شَجَرَةٌ تَخرُجُ فى أَصلِ الجَحيمِ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 65,
      "text": "طَلعُها كَأَنَّهُ رُءوسُ الشَّيٰطينِ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 66,
      "text": "فَإِنَّهُم لَءاكِلونَ مِنها فَمالِـٔونَ مِنهَا البُطونَ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 67,
      "text": "ثُمَّ إِنَّ لَهُم عَلَيها لَشَوبًا مِن حَميمٍ",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 68,
