      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 137,
      "text": "وَإِنَّكُم لَتَمُرّونَ عَلَيهِم مُصبِحينَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 138,
      "text": "وَبِالَّيلِ ۗ أَفَلا تَعقِلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 139,
      "text": "وَإِنَّ يونُسَ لَمِنَ المُرسَلينَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 140,
      "text": "إِذ أَبَقَ إِلَى الفُلكِ المَشحونِ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 141,
      "text": "فَساهَمَ فَكانَ مِنَ المُدحَضينَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 142,
      "text": "فَالتَقَمَهُ الحوتُ وَهُوَ مُليمٌ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 143,
      "text": "فَلَولا أَنَّهُ كانَ مِنَ المُسَبِّحينَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 144,
      "text": "لَلَبِثَ فى بَطنِهِ إِلىٰ يَومِ يُبعَثونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 145,
      "text": "۞ فَنَبَذنٰهُ بِالعَراءِ وَهُوَ سَقيمٌ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 146,
      "text": "وَأَنبَتنا عَلَيهِ شَجَرَةً مِن يَقطينٍ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 147,
      "text": "وَأَرسَلنٰهُ إِلىٰ مِا۟ئَةِ أَلفٍ أَو يَزيدونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 148,
      "text": "فَـٔامَنوا فَمَتَّعنٰهُم إِلىٰ حينٍ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 149,
      "text": "فَاستَفتِهِم أَلِرَبِّكَ البَناتُ وَلَهُمُ البَنونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 150,
      "text": "أَم خَلَقنَا المَلٰئِكَةَ إِنٰثًا وَهُم شٰهِدونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 151,
      "text": "أَلا إِنَّهُم مِن إِفكِهِم لَيَقولونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 152,
      "text": "وَلَدَ اللَّهُ وَإِنَّهُم لَكٰذِبونَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 153,
      "text": "أَصطَفَى البَناتِ عَلَى البَنينَ",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 154,
      "text": "ما لَكُم كَيفَ تَحكُمونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 155,
      "text": "أَفَلا تَذَكَّرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 156,
      "text": "أَم لَكُم سُلطٰنٌ مُبينٌ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 157,
      "text": "فَأتوا بِكِتٰبِكُم إِن كُنتُم صٰدِقينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 158,
      "text": "وَجَعَلوا بَينَهُ وَبَينَ الجِنَّةِ نَسَبًا ۚ وَلَقَد عَلِمَتِ الجِنَّةُ إِنَّهُم لَمُحضَرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 159,
      "text": "سُبحٰنَ اللَّهِ عَمّا يَصِفونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 160,
      "text": "إِلّا عِبادَ اللَّهِ المُخلَصينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 161,
      "text": "فَإِنَّكُم وَما تَعبُدونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 162,
      "text": "ما أَنتُم عَلَيهِ بِفٰتِنينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 163,
      "text": "إِلّا مَن هُوَ صالِ الجَحيمِ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 164,
      "text": "وَما مِنّا إِلّا لَهُ مَقامٌ مَعلومٌ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 165,
      "text": "وَإِنّا لَنَحنُ الصّافّونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 166,
      "text": "وَإِنّا لَنَحنُ المُسَبِّحونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 167,
      "text": "وَإِن كانوا لَيَقولونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 168,
      "text": "لَو أَنَّ عِندَنا ذِكرًا مِنَ الأَوَّلينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 169,
      "text": "لَكُنّا عِبادَ اللَّهِ المُخلَصينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 170,
      "text": "فَكَفَروا بِهِ ۖ فَسَوفَ يَعلَمونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 171,
      "text": "وَلَقَد سَبَقَت كَلِمَتُنا لِعِبادِنَا المُرسَلينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 172,
      "text": "إِنَّهُم لَهُمُ المَنصورونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 173,
      "text": "وَإِنَّ جُندَنا لَهُمُ الغٰلِبونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 174,
      "text": "فَتَوَلَّ عَنهُم حَتّىٰ حينٍ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 175,
      "text": "وَأَبصِرهُم فَسَوفَ يُبصِرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 176,
      "text": "أَفَبِعَذابِنا يَستَعجِلونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 177,
      "text": "فَإِذا نَزَلَ بِساحَتِهِم فَساءَ صَباحُ المُنذَرينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 178,
      "text": "وَتَوَلَّ عَنهُم حَتّىٰ حينٍ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 179,
      "text": "وَأَبصِر فَسَوفَ يُبصِرونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 180,
      "text": "سُبحٰنَ رَبِّكَ رَبِّ العِزَّةِ عَمّا يَصِفونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 181,
      "text": "وَسَلٰمٌ عَلَى المُرسَلينَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 182,
