{
  "number": 52,
  "name": "سُورَةُ الطُّورِ",
  "englishName": "At-Tur",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالطّورِ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَكِتٰبٍ مَسطورٍ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فى رَقٍّ مَنشورٍ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالبَيتِ المَعمورِ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَالسَّقفِ المَرفوعِ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَالبَحرِ المَسجورِ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِنَّ عَذابَ رَبِّكَ لَوٰقِعٌ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ما لَهُ مِن دافِعٍ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 9,
      "text": "يَومَ تَمورُ السَّماءُ مَورًا",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَتَسيرُ الجِبالُ سَيرًا",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَوَيلٌ يَومَئِذٍ لِلمُكَذِّبينَ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 12,
      "text": "الَّذينَ هُم فى خَوضٍ يَلعَبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 13,
      "text": "يَومَ يُدَعّونَ إِلىٰ نارِ جَهَنَّمَ دَعًّا",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 14,
      "text": "هٰذِهِ النّارُ الَّتى كُنتُم بِها تُكَذِّبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 523,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 15,
      "text": "أَفَسِحرٌ هٰذا أَم أَنتُم لا تُبصِرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اصلَوها فَاصبِروا أَو لا تَصبِروا سَواءٌ عَلَيكُم ۖ إِنَّما تُجزَونَ ما كُنتُم تَعمَلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إِنَّ المُتَّقينَ فى جَنّٰتٍ وَنَعيمٍ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فٰكِهينَ بِما ءاتىٰهُم رَبُّهُم وَوَقىٰهُم رَبُّهُم عَذابَ الجَحيمِ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 19,
      "text": "كُلوا وَاشرَبوا هَنيـًٔا بِما كُنتُم تَعمَلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 20,
      "text": "مُتَّكِـٔينَ عَلىٰ سُرُرٍ مَصفوفَةٍ ۖ وَزَوَّجنٰهُم بِحورٍ عينٍ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَالَّذينَ ءامَنوا وَاتَّبَعَتهُم ذُرِّيَّتُهُم بِإيمٰنٍ أَلحَقنا بِهِم ذُرِّيَّتَهُم وَما أَلَتنٰهُم مِن عَمَلِهِم مِن شَيءٍ ۚ كُلُّ امرِئٍ بِما كَسَبَ رَهينٌ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَأَمدَدنٰهُم بِفٰكِهَةٍ وَلَحمٍ مِمّا يَشتَهونَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 23,
      "text": "يَتَنٰزَعونَ فيها كَأسًا لا لَغوٌ فيها وَلا تَأثيمٌ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 209,
      "sajda": false
    },
    {
      "number": 24,
      "text": "۞ وَيَطوفُ عَلَيهِم غِلمانٌ لَهُم كَأَنَّهُم لُؤلُؤٌ مَكنونٌ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَساءَلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قالوا إِنّا كُنّا قَبلُ فى أَهلِنا مُشفِقينَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فَمَنَّ اللَّهُ عَلَينا وَوَقىٰنا عَذابَ السَّمومِ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 28,
      "text": "إِنّا كُنّا مِن قَبلُ نَدعوهُ ۖ إِنَّهُ هُوَ البَرُّ الرَّحيمُ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 458,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَذَكِّر فَما أَنتَ بِنِعمَتِ رَبِّكَ بِكاهِنٍ وَلا مَجنونٍ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 30,
      "text": "أَم يَقولونَ شاعِرٌ نَتَرَبَّصُ بِهِ رَيبَ المَنونِ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قُل تَرَبَّصوا فَإِنّى مَعَكُم مِنَ المُتَرَبِّصينَ",
      "juz": 27,
      "manzil": 7,
      "page": 524,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 32,
      "text": "أَم تَأمُرُهُم أَحلٰمُهُم بِهٰذا ۚ أَم هُم قَومٌ طاغونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 33,
      "text": "أَم يَقولونَ تَقَوَّلَهُ ۚ بَل لا يُؤمِنونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَليَأتوا بِحَديثٍ مِثلِهِ إِن كانوا صٰدِقينَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 35,
      "text": "أَم خُلِقوا مِن غَيرِ شَيءٍ أَم هُمُ الخٰلِقونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 36,
      "text": "أَم خَلَقُوا السَّمٰوٰتِ وَالأَرضَ ۚ بَل لا يوقِنونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 37,
      "text": "أَم عِندَهُم خَزائِنُ رَبِّكَ أَم هُمُ المُصَۣيطِرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 38,
      "text": "أَم لَهُم سُلَّمٌ يَستَمِعونَ فيهِ ۖ فَليَأتِ مُستَمِعُهُم بِسُلطٰنٍ مُبينٍ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 39,
      "text": "أَم لَهُ البَنٰتُ وَلَكُمُ البَنونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 40,
      "text": "أَم تَسـَٔلُهُم أَجرًا فَهُم مِن مَغرَمٍ مُثقَلونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 41,
      "text": "أَم عِندَهُمُ الغَيبُ فَهُم يَكتُبونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 42,
      "text": "أَم يُريدونَ كَيدًا ۖ فَالَّذينَ كَفَروا هُمُ المَكيدونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 43,
      "text": "أَم لَهُم إِلٰهٌ غَيرُ اللَّهِ ۚ سُبحٰنَ اللَّهِ عَمّا يُشرِكونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وَإِن يَرَوا كِسفًا مِنَ السَّماءِ ساقِطًا يَقولوا سَحابٌ مَركومٌ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 45,
      "text": "فَذَرهُم حَتّىٰ يُلٰقوا يَومَهُمُ الَّذى فيهِ يُصعَقونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 46,
      "text": "يَومَ لا يُغنى عَنهُم كَيدُهُم شَيـًٔا وَلا هُم يُنصَرونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وَإِنَّ لِلَّذينَ ظَلَموا عَذابًا دونَ ذٰلِكَ وَلٰكِنَّ أَكثَرَهُم لا يَعلَمونَ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَاصبِر لِحُكمِ رَبِّكَ فَإِنَّكَ بِأَعيُنِنا ۖ وَسَبِّح بِحَمدِ رَبِّكَ حينَ تَقومُ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَمِنَ الَّيلِ فَسَبِّحهُ وَإِدبٰرَ النُّجومِ",
      "juz": 27,
      "manzil": 7,
      "page": 525,
      "ruku": 459,
      "hizbQuarter": 210,
      "sajda": false
    }
  ]
}