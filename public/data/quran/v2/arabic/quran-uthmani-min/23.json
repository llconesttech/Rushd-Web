{
  "number": 23,
  "name": "سُورَةُ المُؤۡمِنُونَ",
  "englishName": "Al-Muminoon",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ قَد أَفلَحَ المُؤمِنونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الَّذينَ هُم فى صَلاتِهِم خٰشِعونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالَّذينَ هُم عَنِ اللَّغوِ مُعرِضونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّذينَ هُم لِلزَّكوٰةِ فٰعِلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَالَّذينَ هُم لِفُروجِهِم حٰفِظونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 6,
      "text": "إِلّا عَلىٰ أَزوٰجِهِم أَو ما مَلَكَت أَيمٰنُهُم فَإِنَّهُم غَيرُ مَلومينَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَمَنِ ابتَغىٰ وَراءَ ذٰلِكَ فَأُولٰئِكَ هُمُ العادونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَالَّذينَ هُم لِأَمٰنٰتِهِم وَعَهدِهِم رٰعونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَالَّذينَ هُم عَلىٰ صَلَوٰتِهِم يُحافِظونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 10,
      "text": "أُولٰئِكَ هُمُ الوٰرِثونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الَّذينَ يَرِثونَ الفِردَوسَ هُم فيها خٰلِدونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَلَقَد خَلَقنَا الإِنسٰنَ مِن سُلٰلَةٍ مِن طينٍ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُمَّ جَعَلنٰهُ نُطفَةً فى قَرارٍ مَكينٍ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ثُمَّ خَلَقنَا النُّطفَةَ عَلَقَةً فَخَلَقنَا العَلَقَةَ مُضغَةً فَخَلَقنَا المُضغَةَ عِظٰمًا فَكَسَونَا العِظٰمَ لَحمًا ثُمَّ أَنشَأنٰهُ خَلقًا ءاخَرَ ۚ فَتَبارَكَ اللَّهُ أَحسَنُ الخٰلِقينَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 15,
      "text": "ثُمَّ إِنَّكُم بَعدَ ذٰلِكَ لَمَيِّتونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ثُمَّ إِنَّكُم يَومَ القِيٰمَةِ تُبعَثونَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَلَقَد خَلَقنا فَوقَكُم سَبعَ طَرائِقَ وَما كُنّا عَنِ الخَلقِ غٰفِلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 342,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَأَنزَلنا مِنَ السَّماءِ ماءً بِقَدَرٍ فَأَسكَنّٰهُ فِى الأَرضِ ۖ وَإِنّا عَلىٰ ذَهابٍ بِهِ لَقٰدِرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَأَنشَأنا لَكُم بِهِ جَنّٰتٍ مِن نَخيلٍ وَأَعنٰبٍ لَكُم فيها فَوٰكِهُ كَثيرَةٌ وَمِنها تَأكُلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَشَجَرَةً تَخرُجُ مِن طورِ سَيناءَ تَنبُتُ بِالدُّهنِ وَصِبغٍ لِلءاكِلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَإِنَّ لَكُم فِى الأَنعٰمِ لَعِبرَةً ۖ نُسقيكُم مِمّا فى بُطونِها وَلَكُم فيها مَنٰفِعُ كَثيرَةٌ وَمِنها تَأكُلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَعَلَيها وَعَلَى الفُلكِ تُحمَلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 295,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَلَقَد أَرسَلنا نوحًا إِلىٰ قَومِهِ فَقالَ يٰقَومِ اعبُدُوا اللَّهَ ما لَكُم مِن إِلٰهٍ غَيرُهُ ۖ أَفَلا تَتَّقونَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَقالَ المَلَؤُا۟ الَّذينَ كَفَروا مِن قَومِهِ ما هٰذا إِلّا بَشَرٌ مِثلُكُم يُريدُ أَن يَتَفَضَّلَ عَلَيكُم وَلَو شاءَ اللَّهُ لَأَنزَلَ مَلٰئِكَةً ما سَمِعنا بِهٰذا فى ءابائِنَا الأَوَّلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 25,
      "text": "إِن هُوَ إِلّا رَجُلٌ بِهِ جِنَّةٌ فَتَرَبَّصوا بِهِ حَتّىٰ حينٍ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قالَ رَبِّ انصُرنى بِما كَذَّبونِ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فَأَوحَينا إِلَيهِ أَنِ اصنَعِ الفُلكَ بِأَعيُنِنا وَوَحيِنا فَإِذا جاءَ أَمرُنا وَفارَ التَّنّورُ ۙ فَاسلُك فيها مِن كُلٍّ زَوجَينِ اثنَينِ وَأَهلَكَ إِلّا مَن سَبَقَ عَلَيهِ القَولُ مِنهُم ۖ وَلا تُخٰطِبنى فِى الَّذينَ ظَلَموا ۖ إِنَّهُم مُغرَقونَ",
      "juz": 18,
      "manzil": 4,
      "page": 343,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 28,
      "text": "فَإِذَا استَوَيتَ أَنتَ وَمَن مَعَكَ عَلَى الفُلكِ فَقُلِ الحَمدُ لِلَّهِ الَّذى نَجّىٰنا مِنَ القَومِ الظّٰلِمينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَقُل رَبِّ أَنزِلنى مُنزَلًا مُبارَكًا وَأَنتَ خَيرُ المُنزِلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 30,
      "text": "إِنَّ فى ذٰلِكَ لَءايٰتٍ وَإِن كُنّا لَمُبتَلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 31,
      "text": "ثُمَّ أَنشَأنا مِن بَعدِهِم قَرنًا ءاخَرينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فَأَرسَلنا فيهِم رَسولًا مِنهُم أَنِ اعبُدُوا اللَّهَ ما لَكُم مِن إِلٰهٍ غَيرُهُ ۖ أَفَلا تَتَّقونَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 296,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَقالَ المَلَأُ مِن قَومِهِ الَّذينَ كَفَروا وَكَذَّبوا بِلِقاءِ الءاخِرَةِ وَأَترَفنٰهُم فِى الحَيوٰةِ الدُّنيا ما هٰذا إِلّا بَشَرٌ مِثلُكُم يَأكُلُ مِمّا تَأكُلونَ مِنهُ وَيَشرَبُ مِمّا تَشرَبونَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَلَئِن أَطَعتُم بَشَرًا مِثلَكُم إِنَّكُم إِذًا لَخٰسِرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 35,
      "text": "أَيَعِدُكُم أَنَّكُم إِذا مِتُّم وَكُنتُم تُرابًا وَعِظٰمًا أَنَّكُم مُخرَجونَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 137,
      "sajda": false
    },
    {
      "number": 36,
      "text": "۞ هَيهاتَ هَيهاتَ لِما توعَدونَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 37,
      "text": "إِن هِىَ إِلّا حَياتُنَا الدُّنيا نَموتُ وَنَحيا وَما نَحنُ بِمَبعوثينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 38,
      "text": "إِن هُوَ إِلّا رَجُلٌ افتَرىٰ عَلَى اللَّهِ كَذِبًا وَما نَحنُ لَهُ بِمُؤمِنينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 39,
      "text": "قالَ رَبِّ انصُرنى بِما كَذَّبونِ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 40,
      "text": "قالَ عَمّا قَليلٍ لَيُصبِحُنَّ نٰدِمينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فَأَخَذَتهُمُ الصَّيحَةُ بِالحَقِّ فَجَعَلنٰهُم غُثاءً ۚ فَبُعدًا لِلقَومِ الظّٰلِمينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 42,
      "text": "ثُمَّ أَنشَأنا مِن بَعدِهِم قُرونًا ءاخَرينَ",
      "juz": 18,
      "manzil": 4,
      "page": 344,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 43,
      "text": "ما تَسبِقُ مِن أُمَّةٍ أَجَلَها وَما يَستَـٔخِرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 44,
      "text": "ثُمَّ أَرسَلنا رُسُلَنا تَترا ۖ كُلَّ ما جاءَ أُمَّةً رَسولُها كَذَّبوهُ ۚ فَأَتبَعنا بَعضَهُم بَعضًا وَجَعَلنٰهُم أَحاديثَ ۚ فَبُعدًا لِقَومٍ لا يُؤمِنونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 45,
      "text": "ثُمَّ أَرسَلنا موسىٰ وَأَخاهُ هٰرونَ بِـٔايٰتِنا وَسُلطٰنٍ مُبينٍ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 46,
      "text": "إِلىٰ فِرعَونَ وَمَلَإِي۟هِ فَاستَكبَروا وَكانوا قَومًا عالينَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 47,
      "text": "فَقالوا أَنُؤمِنُ لِبَشَرَينِ مِثلِنا وَقَومُهُما لَنا عٰبِدونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 48,
      "text": "فَكَذَّبوهُما فَكانوا مِنَ المُهلَكينَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَلَقَد ءاتَينا موسَى الكِتٰبَ لَعَلَّهُم يَهتَدونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَجَعَلنَا ابنَ مَريَمَ وَأُمَّهُ ءايَةً وَءاوَينٰهُما إِلىٰ رَبوَةٍ ذاتِ قَرارٍ وَمَعينٍ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 297,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 51,
      "text": "يٰأَيُّهَا الرُّسُلُ كُلوا مِنَ الطَّيِّبٰتِ وَاعمَلوا صٰلِحًا ۖ إِنّى بِما تَعمَلونَ عَليمٌ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَإِنَّ هٰذِهِ أُمَّتُكُم أُمَّةً وٰحِدَةً وَأَنا۠ رَبُّكُم فَاتَّقونِ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فَتَقَطَّعوا أَمرَهُم بَينَهُم زُبُرًا ۖ كُلُّ حِزبٍ بِما لَدَيهِم فَرِحونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فَذَرهُم فى غَمرَتِهِم حَتّىٰ حينٍ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 55,
      "text": "أَيَحسَبونَ أَنَّما نُمِدُّهُم بِهِ مِن مالٍ وَبَنينَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 56,
      "text": "نُسارِعُ لَهُم فِى الخَيرٰتِ ۚ بَل لا يَشعُرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 57,
      "text": "إِنَّ الَّذينَ هُم مِن خَشيَةِ رَبِّهِم مُشفِقونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 58,
      "text": "وَالَّذينَ هُم بِـٔايٰتِ رَبِّهِم يُؤمِنونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 59,
      "text": "وَالَّذينَ هُم بِرَبِّهِم لا يُشرِكونَ",
      "juz": 18,
      "manzil": 4,
      "page": 345,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 60,
      "text": "وَالَّذينَ يُؤتونَ ما ءاتَوا وَقُلوبُهُم وَجِلَةٌ أَنَّهُم إِلىٰ رَبِّهِم رٰجِعونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 61,
      "text": "أُولٰئِكَ يُسٰرِعونَ فِى الخَيرٰتِ وَهُم لَها سٰبِقونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 62,
      "text": "وَلا نُكَلِّفُ نَفسًا إِلّا وُسعَها ۖ وَلَدَينا كِتٰبٌ يَنطِقُ بِالحَقِّ ۚ وَهُم لا يُظلَمونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 63,
      "text": "بَل قُلوبُهُم فى غَمرَةٍ مِن هٰذا وَلَهُم أَعمٰلٌ مِن دونِ ذٰلِكَ هُم لَها عٰمِلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 64,
      "text": "حَتّىٰ إِذا أَخَذنا مُترَفيهِم بِالعَذابِ إِذا هُم يَجـَٔرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 65,
      "text": "لا تَجـَٔرُوا اليَومَ ۖ إِنَّكُم مِنّا لا تُنصَرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 66,
      "text": "قَد كانَت ءايٰتى تُتلىٰ عَلَيكُم فَكُنتُم عَلىٰ أَعقٰبِكُم تَنكِصونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 67,
      "text": "مُستَكبِرينَ بِهِ سٰمِرًا تَهجُرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 68,
      "text": "أَفَلَم يَدَّبَّرُوا القَولَ أَم جاءَهُم ما لَم يَأتِ ءاباءَهُمُ الأَوَّلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 69,
      "text": "أَم لَم يَعرِفوا رَسولَهُم فَهُم لَهُ مُنكِرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 70,
      "text": "أَم يَقولونَ بِهِ جِنَّةٌ ۚ بَل جاءَهُم بِالحَقِّ وَأَكثَرُهُم لِلحَقِّ كٰرِهونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 71,
      "text": "وَلَوِ اتَّبَعَ الحَقُّ أَهواءَهُم لَفَسَدَتِ السَّمٰوٰتُ وَالأَرضُ وَمَن فيهِنَّ ۚ بَل أَتَينٰهُم بِذِكرِهِم فَهُم عَن ذِكرِهِم مُعرِضونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 72,
      "text": "أَم تَسـَٔلُهُم خَرجًا فَخَراجُ رَبِّكَ خَيرٌ ۖ وَهُوَ خَيرُ الرّٰزِقينَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 73,
      "text": "وَإِنَّكَ لَتَدعوهُم إِلىٰ صِرٰطٍ مُستَقيمٍ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 74,
      "text": "وَإِنَّ الَّذينَ لا يُؤمِنونَ بِالءاخِرَةِ عَنِ الصِّرٰطِ لَنٰكِبونَ",
      "juz": 18,
      "manzil": 4,
      "page": 346,
      "ruku": 298,
      "hizbQuarter": 138,
      "sajda": false
    },
    {
      "number": 75,
      "text": "۞ وَلَو رَحِمنٰهُم وَكَشَفنا ما بِهِم مِن ضُرٍّ لَلَجّوا فى طُغيٰنِهِم يَعمَهونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 298,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وَلَقَد أَخَذنٰهُم بِالعَذابِ فَمَا استَكانوا لِرَبِّهِم وَما يَتَضَرَّعونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 298,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 77,
      "text": "حَتّىٰ إِذا فَتَحنا عَلَيهِم بابًا ذا عَذابٍ شَديدٍ إِذا هُم فيهِ مُبلِسونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 298,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 78,
      "text": "وَهُوَ الَّذى أَنشَأَ لَكُمُ السَّمعَ وَالأَبصٰرَ وَالأَفـِٔدَةَ ۚ قَليلًا ما تَشكُرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 79,
      "text": "وَهُوَ الَّذى ذَرَأَكُم فِى الأَرضِ وَإِلَيهِ تُحشَرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 80,
      "text": "وَهُوَ الَّذى يُحيۦ وَيُميتُ وَلَهُ اختِلٰفُ الَّيلِ وَالنَّهارِ ۚ أَفَلا تَعقِلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 81,
      "text": "بَل قالوا مِثلَ ما قالَ الأَوَّلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 82,
      "text": "قالوا أَءِذا مِتنا وَكُنّا تُرابًا وَعِظٰمًا أَءِنّا لَمَبعوثونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 83,
      "text": "لَقَد وُعِدنا نَحنُ وَءاباؤُنا هٰذا مِن قَبلُ إِن هٰذا إِلّا أَسٰطيرُ الأَوَّلينَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 84,
      "text": "قُل لِمَنِ الأَرضُ وَمَن فيها إِن كُنتُم تَعلَمونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 85,
      "text": "سَيَقولونَ لِلَّهِ ۚ قُل أَفَلا تَذَكَّرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 86,
      "text": "قُل مَن رَبُّ السَّمٰوٰتِ السَّبعِ وَرَبُّ العَرشِ العَظيمِ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 87,
      "text": "سَيَقولونَ لِلَّهِ ۚ قُل أَفَلا تَتَّقونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 88,
      "text": "قُل مَن بِيَدِهِ مَلَكوتُ كُلِّ شَيءٍ وَهُوَ يُجيرُ وَلا يُجارُ عَلَيهِ إِن كُنتُم تَعلَمونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 89,
      "text": "سَيَقولونَ لِلَّهِ ۚ قُل فَأَنّىٰ تُسحَرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 347,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 90,
      "text": "بَل أَتَينٰهُم بِالحَقِّ وَإِنَّهُم لَكٰذِبونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 91,
      "text": "مَا اتَّخَذَ اللَّهُ مِن وَلَدٍ وَما كانَ مَعَهُ مِن إِلٰهٍ ۚ إِذًا لَذَهَبَ كُلُّ إِلٰهٍ بِما خَلَقَ وَلَعَلا بَعضُهُم عَلىٰ بَعضٍ ۚ سُبحٰنَ اللَّهِ عَمّا يَصِفونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 92,
      "text": "عٰلِمِ الغَيبِ وَالشَّهٰدَةِ فَتَعٰلىٰ عَمّا يُشرِكونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 299,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 93,
      "text": "قُل رَبِّ إِمّا تُرِيَنّى ما يوعَدونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 94,
      "text": "رَبِّ فَلا تَجعَلنى فِى القَومِ الظّٰلِمينَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 95,
      "text": "وَإِنّا عَلىٰ أَن نُرِيَكَ ما نَعِدُهُم لَقٰدِرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 96,
      "text": "ادفَع بِالَّتى هِىَ أَحسَنُ السَّيِّئَةَ ۚ نَحنُ أَعلَمُ بِما يَصِفونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 97,
      "text": "وَقُل رَبِّ أَعوذُ بِكَ مِن هَمَزٰتِ الشَّيٰطينِ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 98,
      "text": "وَأَعوذُ بِكَ رَبِّ أَن يَحضُرونِ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 99,
      "text": "حَتّىٰ إِذا جاءَ أَحَدَهُمُ المَوتُ قالَ رَبِّ ارجِعونِ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 100,
      "text": "لَعَلّى أَعمَلُ صٰلِحًا فيما تَرَكتُ ۚ كَلّا ۚ إِنَّها كَلِمَةٌ هُوَ قائِلُها ۖ وَمِن وَرائِهِم بَرزَخٌ إِلىٰ يَومِ يُبعَثونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 101,
      "text": "فَإِذا نُفِخَ فِى الصّورِ فَلا أَنسابَ بَينَهُم يَومَئِذٍ وَلا يَتَساءَلونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 102,
      "text": "فَمَن ثَقُلَت مَوٰزينُهُ فَأُولٰئِكَ هُمُ المُفلِحونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 103,
      "text": "وَمَن خَفَّت مَوٰزينُهُ فَأُولٰئِكَ الَّذينَ خَسِروا أَنفُسَهُم فى جَهَنَّمَ خٰلِدونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 104,
      "text": "تَلفَحُ وُجوهَهُمُ النّارُ وَهُم فيها كٰلِحونَ",
      "juz": 18,
      "manzil": 4,
      "page": 348,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 105,
      "text": "أَلَم تَكُن ءايٰتى تُتلىٰ عَلَيكُم فَكُنتُم بِها تُكَذِّبونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 106,
      "text": "قالوا رَبَّنا غَلَبَت عَلَينا شِقوَتُنا وَكُنّا قَومًا ضالّينَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 107,
      "text": "رَبَّنا أَخرِجنا مِنها فَإِن عُدنا فَإِنّا ظٰلِمونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 108,
      "text": "قالَ اخسَـٔوا فيها وَلا تُكَلِّمونِ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 109,
      "text": "إِنَّهُ كانَ فَريقٌ مِن عِبادى يَقولونَ رَبَّنا ءامَنّا فَاغفِر لَنا وَارحَمنا وَأَنتَ خَيرُ الرّٰحِمينَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 110,
      "text": "فَاتَّخَذتُموهُم سِخرِيًّا حَتّىٰ أَنسَوكُم ذِكرى وَكُنتُم مِنهُم تَضحَكونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 111,
      "text": "إِنّى جَزَيتُهُمُ اليَومَ بِما صَبَروا أَنَّهُم هُمُ الفائِزونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 112,
      "text": "قٰلَ كَم لَبِثتُم فِى الأَرضِ عَدَدَ سِنينَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 113,
      "text": "قالوا لَبِثنا يَومًا أَو بَعضَ يَومٍ فَسـَٔلِ العادّينَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 114,
      "text": "قٰلَ إِن لَبِثتُم إِلّا قَليلًا ۖ لَو أَنَّكُم كُنتُم تَعلَمونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 115,
      "text": "أَفَحَسِبتُم أَنَّما خَلَقنٰكُم عَبَثًا وَأَنَّكُم إِلَينا لا تُرجَعونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 116,
      "text": "فَتَعٰلَى اللَّهُ المَلِكُ الحَقُّ ۖ لا إِلٰهَ إِلّا هُوَ رَبُّ العَرشِ الكَريمِ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 117,
      "text": "وَمَن يَدعُ مَعَ اللَّهِ إِلٰهًا ءاخَرَ لا بُرهٰنَ لَهُ بِهِ فَإِنَّما حِسابُهُ عِندَ رَبِّهِ ۚ إِنَّهُ لا يُفلِحُ الكٰفِرونَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    },
    {
      "number": 118,
      "text": "وَقُل رَبِّ اغفِر وَارحَم وَأَنتَ خَيرُ الرّٰحِمينَ",
      "juz": 18,
      "manzil": 4,
      "page": 349,
      "ruku": 300,
      "hizbQuarter": 139,
      "sajda": false
    }
  ]
}