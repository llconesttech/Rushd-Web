{
  "number": 91,
  "name": "سُورَةُ الشَّمۡسِ",
  "englishName": "Ash-Shams",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالشَّمسِ وَضُحىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالقَمَرِ إِذا تَلىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالنَّهارِ إِذا جَلّىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّيلِ إِذا يَغشىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَالسَّماءِ وَما بَنىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَالأَرضِ وَما طَحىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَنَفسٍ وَما سَوّىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَأَلهَمَها فُجورَها وَتَقوىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "قَد أَفلَحَ مَن زَكّىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَقَد خابَ مَن دَسّىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَذَّبَت ثَمودُ بِطَغوىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِذِ انبَعَثَ أَشقىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَقالَ لَهُم رَسولُ اللَّهِ ناقَةَ اللَّهِ وَسُقيٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَكَذَّبوهُ فَعَقَروها فَدَمدَمَ عَلَيهِم رَبُّهُم بِذَنبِهِم فَسَوّىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلا يَخافُ عُقبٰها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}