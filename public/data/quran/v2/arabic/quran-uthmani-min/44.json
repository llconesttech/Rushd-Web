{
  "number": 44,
  "name": "سُورَةُ الدُّخَانِ",
  "englishName": "Ad-Dukhaan",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ حم",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالكِتٰبِ المُبينِ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إِنّا أَنزَلنٰهُ فى لَيلَةٍ مُبٰرَكَةٍ ۚ إِنّا كُنّا مُنذِرينَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فيها يُفرَقُ كُلُّ أَمرٍ حَكيمٍ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 5,
      "text": "أَمرًا مِن عِندِنا ۚ إِنّا كُنّا مُرسِلينَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 6,
      "text": "رَحمَةً مِن رَبِّكَ ۚ إِنَّهُ هُوَ السَّميعُ العَليمُ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 7,
      "text": "رَبِّ السَّمٰوٰتِ وَالأَرضِ وَما بَينَهُما ۖ إِن كُنتُم موقِنينَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 8,
      "text": "لا إِلٰهَ إِلّا هُوَ يُحيۦ وَيُميتُ ۖ رَبُّكُم وَرَبُّ ءابائِكُمُ الأَوَّلينَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 9,
      "text": "بَل هُم فى شَكٍّ يَلعَبونَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَارتَقِب يَومَ تَأتِى السَّماءُ بِدُخانٍ مُبينٍ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 11,
      "text": "يَغشَى النّاسَ ۖ هٰذا عَذابٌ أَليمٌ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 12,
      "text": "رَبَّنَا اكشِف عَنَّا العَذابَ إِنّا مُؤمِنونَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 13,
      "text": "أَنّىٰ لَهُمُ الذِّكرىٰ وَقَد جاءَهُم رَسولٌ مُبينٌ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ثُمَّ تَوَلَّوا عَنهُ وَقالوا مُعَلَّمٌ مَجنونٌ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 15,
      "text": "إِنّا كاشِفُوا العَذابِ قَليلًا ۚ إِنَّكُم عائِدونَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 16,
      "text": "يَومَ نَبطِشُ البَطشَةَ الكُبرىٰ إِنّا مُنتَقِمونَ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 17,
      "text": "۞ وَلَقَد فَتَنّا قَبلَهُم قَومَ فِرعَونَ وَجاءَهُم رَسولٌ كَريمٌ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 18,
      "text": "أَن أَدّوا إِلَىَّ عِبادَ اللَّهِ ۖ إِنّى لَكُم رَسولٌ أَمينٌ",
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَأَن لا تَعلوا عَلَى اللَّهِ ۖ إِنّى ءاتيكُم بِسُلطٰنٍ مُبينٍ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَإِنّى عُذتُ بِرَبّى وَرَبِّكُم أَن تَرجُمونِ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَإِن لَم تُؤمِنوا لى فَاعتَزِلونِ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 22,
      "text": "فَدَعا رَبَّهُ أَنَّ هٰؤُلاءِ قَومٌ مُجرِمونَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَأَسرِ بِعِبادى لَيلًا إِنَّكُم مُتَّبَعونَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَاترُكِ البَحرَ رَهوًا ۖ إِنَّهُم جُندٌ مُغرَقونَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 25,
      "text": "كَم تَرَكوا مِن جَنّٰتٍ وَعُيونٍ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَزُروعٍ وَمَقامٍ كَريمٍ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَنَعمَةٍ كانوا فيها فٰكِهينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 28,
      "text": "كَذٰلِكَ ۖ وَأَورَثنٰها قَومًا ءاخَرينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَما بَكَت عَلَيهِمُ السَّماءُ وَالأَرضُ وَما كانوا مُنظَرينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَلَقَد نَجَّينا بَنى إِسرٰءيلَ مِنَ العَذابِ المُهينِ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 31,
      "text": "مِن فِرعَونَ ۚ إِنَّهُ كانَ عالِيًا مِنَ المُسرِفينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلَقَدِ اختَرنٰهُم عَلىٰ عِلمٍ عَلَى العٰلَمينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَءاتَينٰهُم مِنَ الءايٰتِ ما فيهِ بَلٰؤٌا۟ مُبينٌ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 34,
      "text": "إِنَّ هٰؤُلاءِ لَيَقولونَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 35,
      "text": "إِن هِىَ إِلّا مَوتَتُنَا الأولىٰ وَما نَحنُ بِمُنشَرينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَأتوا بِـٔابائِنا إِن كُنتُم صٰدِقينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 37,
      "text": "أَهُم خَيرٌ أَم قَومُ تُبَّعٍ وَالَّذينَ مِن قَبلِهِم ۚ أَهلَكنٰهُم ۖ إِنَّهُم كانوا مُجرِمينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَما خَلَقنَا السَّمٰوٰتِ وَالأَرضَ وَما بَينَهُما لٰعِبينَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ما خَلَقنٰهُما إِلّا بِالحَقِّ وَلٰكِنَّ أَكثَرَهُم لا يَعلَمونَ",
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إِنَّ يَومَ الفَصلِ ميقٰتُهُم أَجمَعينَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 41,
      "text": "يَومَ لا يُغنى مَولًى عَن مَولًى شَيـًٔا وَلا هُم يُنصَرونَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 42,
      "text": "إِلّا مَن رَحِمَ اللَّهُ ۚ إِنَّهُ هُوَ العَزيزُ الرَّحيمُ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 43,
      "text": "إِنَّ شَجَرَتَ الزَّقّومِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 44,
      "text": "طَعامُ الأَثيمِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 45,
      "text": "كَالمُهلِ يَغلى فِى البُطونِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كَغَلىِ الحَميمِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 47,
      "text": "خُذوهُ فَاعتِلوهُ إِلىٰ سَواءِ الجَحيمِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 48,
      "text": "ثُمَّ صُبّوا فَوقَ رَأسِهِ مِن عَذابِ الحَميمِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 49,
      "text": "ذُق إِنَّكَ أَنتَ العَزيزُ الكَريمُ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 50,
      "text": "إِنَّ هٰذا ما كُنتُم بِهِ تَمتَرونَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 51,
      "text": "إِنَّ المُتَّقينَ فى مَقامٍ أَمينٍ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 52,
      "text": "فى جَنّٰتٍ وَعُيونٍ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 53,
      "text": "يَلبَسونَ مِن سُندُسٍ وَإِستَبرَقٍ مُتَقٰبِلينَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 54,
      "text": "كَذٰلِكَ وَزَوَّجنٰهُم بِحورٍ عينٍ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 55,
      "text": "يَدعونَ فيها بِكُلِّ فٰكِهَةٍ ءامِنينَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 56,
      "text": "لا يَذوقونَ فيهَا المَوتَ إِلَّا المَوتَةَ الأولىٰ ۖ وَوَقىٰهُم عَذابَ الجَحيمِ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 57,
      "text": "فَضلًا مِن رَبِّكَ ۚ ذٰلِكَ هُوَ الفَوزُ العَظيمُ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 58,
      "text": "فَإِنَّما يَسَّرنٰهُ بِلِسانِكَ لَعَلَّهُم يَتَذَكَّرونَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 59,
      "text": "فَارتَقِب إِنَّهُم مُرتَقِبونَ",
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    }
  ]
}