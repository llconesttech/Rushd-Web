{
  "number": 89,
  "name": "سُورَةُ الفَجۡرِ",
  "englishName": "Al-Fajr",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالفَجرِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلَيالٍ عَشرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالشَّفعِ وَالوَترِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّيلِ إِذا يَسرِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "هَل فى ذٰلِكَ قَسَمٌ لِذى حِجرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "أَلَم تَرَ كَيفَ فَعَلَ رَبُّكَ بِعادٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِرَمَ ذاتِ العِمادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "الَّتى لَم يُخلَق مِثلُها فِى البِلٰدِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَثَمودَ الَّذينَ جابُوا الصَّخرَ بِالوادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَفِرعَونَ ذِى الأَوتادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الَّذينَ طَغَوا فِى البِلٰدِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فَأَكثَروا فيهَا الفَسادَ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَصَبَّ عَلَيهِم رَبُّكَ سَوطَ عَذابٍ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "إِنَّ رَبَّكَ لَبِالمِرصادِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَأَمَّا الإِنسٰنُ إِذا مَا ابتَلىٰهُ رَبُّهُ فَأَكرَمَهُ وَنَعَّمَهُ فَيَقولُ رَبّى أَكرَمَنِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَأَمّا إِذا مَا ابتَلىٰهُ فَقَدَرَ عَلَيهِ رِزقَهُ فَيَقولُ رَبّى أَهٰنَنِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "كَلّا ۖ بَل لا تُكرِمونَ اليَتيمَ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلا تَحٰضّونَ عَلىٰ طَعامِ المِسكينِ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَتَأكُلونَ التُّراثَ أَكلًا لَمًّا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَتُحِبّونَ المالَ حُبًّا جَمًّا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "كَلّا إِذا دُكَّتِ الأَرضُ دَكًّا دَكًّا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَجاءَ رَبُّكَ وَالمَلَكُ صَفًّا صَفًّا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَجِا۟يءَ يَومَئِذٍ بِجَهَنَّمَ ۚ يَومَئِذٍ يَتَذَكَّرُ الإِنسٰنُ وَأَنّىٰ لَهُ الذِّكرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "يَقولُ يٰلَيتَنى قَدَّمتُ لِحَياتى",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَيَومَئِذٍ لا يُعَذِّبُ عَذابَهُ أَحَدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلا يوثِقُ وَثاقَهُ أَحَدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 27,
      "text": "يٰأَيَّتُهَا النَّفسُ المُطمَئِنَّةُ",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 28,
      "text": "ارجِعى إِلىٰ رَبِّكِ راضِيَةً مَرضِيَّةً",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فَادخُلى فى عِبٰدى",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَادخُلى جَنَّتى",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}