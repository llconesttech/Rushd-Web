{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالنّٰزِعٰتِ غَرقًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالنّٰشِطٰتِ نَشطًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالسّٰبِحٰتِ سَبحًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَالسّٰبِقٰتِ سَبقًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَالمُدَبِّرٰتِ أَمرًا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يَومَ تَرجُفُ الرّاجِفَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تَتبَعُهَا الرّادِفَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "قُلوبٌ يَومَئِذٍ واجِفَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "أَبصٰرُها خٰشِعَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "يَقولونَ أَءِنّا لَمَردودونَ فِى الحافِرَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "أَءِذا كُنّا عِظٰمًا نَخِرَةً",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "قالوا تِلكَ إِذًا كَرَّةٌ خاسِرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَإِنَّما هِىَ زَجرَةٌ وٰحِدَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَإِذا هُم بِالسّاهِرَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "هَل أَتىٰكَ حَديثُ موسىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "إِذ نادىٰهُ رَبُّهُ بِالوادِ المُقَدَّسِ طُوًى",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اذهَب إِلىٰ فِرعَونَ إِنَّهُ طَغىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَقُل هَل لَكَ إِلىٰ أَن تَزَكّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَأَهدِيَكَ إِلىٰ رَبِّكَ فَتَخشىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَأَرىٰهُ الءايَةَ الكُبرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَكَذَّبَ وَعَصىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ثُمَّ أَدبَرَ يَسعىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَحَشَرَ فَنادىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَقالَ أَنا۠ رَبُّكُمُ الأَعلىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فَأَخَذَهُ اللَّهُ نَكالَ الءاخِرَةِ وَالأولىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "إِنَّ فى ذٰلِكَ لَعِبرَةً لِمَن يَخشىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "ءَأَنتُم أَشَدُّ خَلقًا أَمِ السَّماءُ ۚ بَنىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "رَفَعَ سَمكَها فَسَوّىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَأَغطَشَ لَيلَها وَأَخرَجَ ضُحىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَالأَرضَ بَعدَ ذٰلِكَ دَحىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "أَخرَجَ مِنها ماءَها وَمَرعىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَالجِبالَ أَرسىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "مَتٰعًا لَكُم وَلِأَنعٰمِكُم",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فَإِذا جاءَتِ الطّامَّةُ الكُبرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "يَومَ يَتَذَكَّرُ الإِنسٰنُ ما سَعىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَبُرِّزَتِ الجَحيمُ لِمَن يَرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "فَأَمّا مَن طَغىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وَءاثَرَ الحَيوٰةَ الدُّنيا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَإِنَّ الجَحيمَ هِىَ المَأوىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَأَمّا مَن خافَ مَقامَ رَبِّهِ وَنَهَى النَّفسَ عَنِ الهَوىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فَإِنَّ الجَنَّةَ هِىَ المَأوىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 42,
      "text": "يَسـَٔلونَكَ عَنِ السّاعَةِ أَيّانَ مُرسىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 43,
      "text": "فيمَ أَنتَ مِن ذِكرىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 44,
      "text": "إِلىٰ رَبِّكَ مُنتَهىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 45,
      "text": "إِنَّما أَنتَ مُنذِرُ مَن يَخشىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 46,
      "text": "كَأَنَّهُم يَومَ يَرَونَها لَم يَلبَثوا إِلّا عَشِيَّةً أَو ضُحىٰها",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}