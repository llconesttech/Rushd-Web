{
  "number": 104,
  "name": "سُورَةُ الهُمَزَةِ",
  "englishName": "Al-Humaza",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَيلٌ لِكُلِّ هُمَزَةٍ لُمَزَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الَّذى جَمَعَ مالًا وَعَدَّدَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "يَحسَبُ أَنَّ مالَهُ أَخلَدَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَلّا ۖ لَيُنبَذَنَّ فِى الحُطَمَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَما أَدرىٰكَ مَا الحُطَمَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "نارُ اللَّهِ الموقَدَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "الَّتى تَطَّلِعُ عَلَى الأَفـِٔدَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّها عَلَيهِم مُؤصَدَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فى عَمَدٍ مُمَدَّدَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}