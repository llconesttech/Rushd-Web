{
  "number": 76,
  "name": "سُورَةُ الإِنسَانِ",
  "englishName": "Al-Insaan",
  "ayahs": [
    {
      "number": 1,
      "text": "هَلْ أَتَىٰ عَلَى [h:7873[ٱ]لْإِ[f:7874[نس]َ[n[ـٰ]نِ حِي[a:14153[نٌ م]ِّنَ [h:14154[ٱ][l[ل]دَّهْرِ لَمْ يَكُ[f:14155[ن ش]َيْـ[a:14156[ـًٔا م]َّذْكُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "إِ[g[نّ]َا خَلَ[q:5159[قْ]نَا [h:7228[ٱ]لْإِ[f:5969[نس]َ[n[ـٰ]نَ م[a:7325[ِن ن]ُّ[q:7326[طْ]فَةٍ أَمْشَا[a:14157[جٍ ن]َّ[q:14158[بْ]تَلِيهِ فَجَعَلْنَ[n[ـٰ]هُ سَمِيع[i:2864[َۢا ب]َصِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إِ[g[نّ]َا هَدَيْنَ[n[ـٰ]هُ [h:14159[ٱ][l[ل]سَّبِيلَ إِ[g[مّ]َا شَاكِ[a:14160[رًا و]َإِ[g[مّ]َا كَفُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِ[g[نّ][o[َآ] أَعْتَ[q:2660[دْ]نَا لِلْكَ[n[ـٰ]فِرِينَ سَلَ[n[ـٰ]سِلَ[s[اْ] وَأَغْلَ[n[ـٰ]ل[a:14161[اً و]َسَعِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "إِ[g[نّ]َ [h:14162[ٱ]لْأَ[q:14163[بْ]رَارَ يَشْرَبُونَ مِ[f:14164[ن ك]َأْ[f:14165[سٍ ك]َانَ مِزَاجُهَا كَافُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "عَيْ[a:14166[نًا ي]َشْرَبُ بِهَا عِبَادُ [h:14167[ٱ]للَّهِ يُفَجِّرُونَهَا تَفْجِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
      "text": "يُوفُونَ بِ[h:14168[ٱ][l[ل][g[نّ]َذْرِ وَيَخَافُونَ يَوْ[f:14169[مًا ك]َانَ شَرُّهُ[n[ۥ] مُسْتَطِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَيُ[q:14170[طْ]عِمُونَ [h:14171[ٱ][l[ل]طَّعَامَ عَلَىٰ حُبِّه[n[ِۦ] مِسْكِي[a:14172[نًا و]َيَتِي[a:14173[مًا و]َأَسِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 9,
      "text": "إِ[g[نّ]َمَا نُ[q:14174[طْ]عِمُكُمْ لِوَ[q:14175[جْ]هِ [h:14176[ٱ]للَّهِ لَا نُرِيدُ مِ[f:344[نك]ُمْ جَز[o[َا]ٓ[a:14177[ءً و]َلَا شُكُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 10,
      "text": "إِ[g[نّ]َا نَخَافُ م[u:10191[ِن ر]َّبِّنَا يَوْمًا عَبُو[f:14178[سًا ق]َمْطَرِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَوَقَ[n[ـٰ]هُمُ [h:14179[ٱ]للَّهُ شَرَّ ذ[n[َٲ]لِكَ [h:14180[ٱ]لْيَوْمِ وَلَقَّ[n[ـٰ]هُمْ نَضْرَ[a:14181[ةً و]َسُرُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَجَزَ[n[ٮٰ]ه[c:14182[ُم ب]ِمَا صَبَرُو[s[اْ] جَ[g[نّ]َ[a:14183[ةً و]َحَرِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 13,
      "text": "مُّتَّكِــِٔينَ فِيهَا عَلَى [h:8058[ٱ]لْأَر[o[َا]ٓئِكِ‌ۖ لَا يَرَوْنَ فِيهَا شَمْ[a:14184[سًا و]َلَا زَمْهَرِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَدَانِيَةً عَلَيْهِمْ ظِلَ[n[ـٰ]لُهَا وَذُلِّلَتْ قُطُوفُهَا تَذْلِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَيُطَافُ عَلَيْه[c:14185[ِم ب]ِـَٔـانِيَ[a:14186[ةٍ م]ِّ[f:12249[ن ف]ِضَّ[a:14187[ةٍ و]َأَكْوَا[f:14188[بٍ ك]َانَتْ قَوَارِيرَا۟",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 16,
      "text": "قَوَارِيرَ[s[اْ] مِ[f:12249[ن ف]ِضَّ[f:14189[ةٍ ق]َدَّرُوهَا تَ[q:9559[قْ]دِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَيُسْقَوْنَ فِيهَا كَأْ[f:14190[سًا ك]َانَ مِزَاجُهَا زَ[f:14191[نج]َبِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 18,
      "text": "عَيْ[f:14192[نًا ف]ِيهَا تُسَ[g[مّ]َىٰ سَلْسَبِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 19,
      "text": "۞ وَيَطُوفُ عَلَيْهِمْ وِلْد[n[َٲ][a:13218[نٌ م]ُّخَلَّدُونَ إِذَا رَأَيْتَهُمْ حَسِ[q:14193[بْ]تَهُمْ لُؤْلُ[a:14194[ؤًا م]َّ[f:9614[نث]ُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَإِذَا رَأَيْتَ ثَ[g[مّ]َ رَأَيْتَ نَعِي[a:14195[مًا و]َمُلْ[f:14196[كًا ك]َبِيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 21,
      "text": "عَ[n[ـٰ]لِيَهُمْ ثِيَابُ سُ[f:8054[ند]ُسٍ خُضْ[a:14197[رٌ و]َإِسْتَ[q:14198[بْ]رَ[a:14199[قٌ‌ۖ و]َحُلّ[o[ُوٓ][s[اْ] أَسَاوِرَ مِ[f:12249[ن ف]ِضَّ[a:14200[ةٍ و]َسَقَ[n[ـٰ]هُمْ رَبُّهُمْ شَرَا[f:14201[بًا ط]َهُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 22,
      "text": "إِ[g[نّ]َ هَ[n[ـٰ]ذَا كَانَ لَكُمْ جَز[o[َا]ٓ[a:14202[ءً و]َكَانَ سَعْيُك[w:14203[ُم م]َّشْكُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 23,
      "text": "إِ[g[نّ]َا نَحْنُ نَزَّلْنَا عَلَيْكَ [h:8434[ٱ]لْقُرْءَانَ تَ[f:7929[نز]ِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَ[h:9999[ٱ]صْبِرْ لِحُكْمِ رَبِّكَ وَلَا تُطِعْ مِنْهُمْ ءَاثِمًا أَوْ كَفُورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَ[h:9999[ٱ]ذْكُرِ [h:14032[ٱ]سْمَ رَبِّكَ بُكْرَ[a:9566[ةً و]َأَصِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَمِنَ [h:7856[ٱ]لَّيْلِ فَ[h:14204[ٱ]سْجُ[q:14204[دْ] لَهُ[n[ۥ] وَسَبِّحْهُ لَيْل[f:14205[اً ط]َوِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إِ[g[نّ]َ هَ[o[ـٰٓ]ؤُل[o[َا]ٓءِ يُحِبُّونَ [h:14206[ٱ]لْعَاجِلَةَ وَيَذَرُونَ وَر[o[َا]ٓءَهُمْ يَوْ[f:14207[مًا ث]َقِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 28,
      "text": "نَّحْنُ خَلَ[q:11326[قْ]نَ[n[ـٰ]هُمْ وَشَدَ[q:11504[دْ]ن[o[َآ] أَسْرَهُمْ‌ۖ وَإِذَا شِئْنَا بَدَّلْن[o[َآ] أَمْثَ[n[ـٰ]لَهُمْ تَ[q:10752[بْ]دِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 29,
      "text": "إِ[g[نّ]َ هَ[n[ـٰ]ذِه[n[ِۦ] تَذْكِرَ[f:14055[ةٌ‌ۖ ف]َمَ[f:8041[ن ش][o[َا]ٓءَ [h:14056[ٱ]تَّخَذَ إِلَىٰ رَبِّه[n[ِۦ] سَبِيلاً",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَمَا تَش[o[َا]ٓءُونَ إِلّ[o[َآ] أ[a:4201[َن ي]َش[o[َا]ٓءَ [h:4334[ٱ]للَّهُ‌ۚ إِ[g[نّ]َ [h:80[ٱ]للَّهَ كَانَ عَلِيمًا حَكِيمًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 31,
      "text": "يُ[q:8947[دْ]خِلُ م[a:476[َن ي]َش[o[َا]ٓءُ فِى رَحْمَتِه[n[ِۦ]‌ۚ وَ[h:14208[ٱ][l[ل]ظَّ[n[ـٰ]لِمِينَ أَعَدَّ لَهُمْ عَذَابًا أَلِيمَۢا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    }
  ]
}