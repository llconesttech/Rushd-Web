      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "خُلِقَ م[a:840[ِن م]ّ[o[َا]ٓ[f:14423[ءٍ د]َافِ[q[ق]ٍ",
      "juz": 30,
