{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "عَامِلَةٌ نَاصِبَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَصْلَىٰ نَارًا حَامِيَةً",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "لِسَعْيِهَا رَاضِيَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فِي جَنَّةٍ عَالِيَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "لَا تَسْمَعُ فِيهَا لَاغِيَةً",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فِيهَا عَيْنٌ جَارِيَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فِيهَا سُرُرٌ مَرْفُوعَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَأَكْوَابٌ مَوْضُوعَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَنَمَارِقُ مَصْفُوفَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَزَرَابِيُّ مَبْثُوثَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "إِلَّا مَنْ تَوَلَّىٰ وَكَفَرَ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "إِنَّ إِلَيْنَا إِيَابَهُمْ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}