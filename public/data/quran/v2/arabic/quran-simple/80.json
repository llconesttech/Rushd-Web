{
  "number": 80,
  "name": "سُورَةُ عَبَسَ",
  "englishName": "Abasa",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ عَبَسَ وَتَوَلَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 2,
      "text": "أَنْ جَاءَهُ الْأَعْمَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 4,
      "text": "أَوْ يَذَّكَّرُ فَتَنْفَعَهُ الذِّكْرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5,
      "text": "أَمَّا مَنِ اسْتَغْنَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَأَنْتَ لَهُ تَصَدَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَأَمَّا مَنْ جَاءَكَ يَسْعَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَهُوَ يَخْشَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَأَنْتَ عَنْهُ تَلَهَّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَلَّا إِنَّهَا تَذْكِرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فَمَنْ شَاءَ ذَكَرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فِي صُحُفٍ مُكَرَّمَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 14,
      "text": "مَرْفُوعَةٍ مُطَهَّرَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 15,
      "text": "بِأَيْدِي سَفَرَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 16,
      "text": "كِرَامٍ بَرَرَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 17,
      "text": "قُتِلَ الْإِنْسَانُ مَا أَكْفَرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 18,
      "text": "مِنْ أَيِّ شَيْءٍ خَلَقَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 19,
      "text": "مِنْ نُطْفَةٍ خَلَقَهُ فَقَدَّرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 20,
      "text": "ثُمَّ السَّبِيلَ يَسَّرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 21,
      "text": "ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ثُمَّ إِذَا شَاءَ أَنْشَرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 23,
      "text": "كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَلْيَنْظُرِ الْإِنْسَانُ إِلَىٰ طَعَامِهِ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 25,
      "text": "أَنَّا صَبَبْنَا الْمَاءَ صَبًّا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فَأَنْبَتْنَا فِيهَا حَبًّا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَعِنَبًا وَقَضْبًا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَزَيْتُونًا وَنَخْلًا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَحَدَائِقَ غُلْبًا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وَفَاكِهَةً وَأَبًّا",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 32,
      "text": "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 33,
      "text": "فَإِذَا جَاءَتِ الصَّاخَّةُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 34,
      "text": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 35,
      "text": "وَأُمِّهِ وَأَبِيهِ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَصَاحِبَتِهِ وَبَنِيهِ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 37,
      "text": "لِكُلِّ امْرِئٍ مِنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 38,
      "text": "وُجُوهٌ يَوْمَئِذٍ مُسْفِرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ضَاحِكَةٌ مُسْتَبْشِرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 41,
      "text": "تَرْهَقُهَا قَتَرَةٌ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 42,
      "text": "أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    }
  ]
}