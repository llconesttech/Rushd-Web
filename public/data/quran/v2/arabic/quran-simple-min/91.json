{
  "number": 91,
  "name": "سُورَةُ الشَّمۡسِ",
  "englishName": "Ash-Shams",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالشَّمسِ وَضُحاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَالقَمَرِ إِذا تَلاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالنَّهارِ إِذا جَلّاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاللَّيلِ إِذا يَغشاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَالسَّماءِ وَما بَناها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَالأَرضِ وَما طَحاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَنَفسٍ وَما سَوّاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَأَلهَمَها فُجورَها وَتَقواها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "قَد أَفلَحَ مَن زَكّاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَقَد خابَ مَن دَسّاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَذَّبَت ثَمودُ بِطَغواها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِذِ انبَعَثَ أَشقاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَقالَ لَهُم رَسولُ اللَّهِ ناقَةَ اللَّهِ وَسُقياها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَكَذَّبوهُ فَعَقَروها فَدَمدَمَ عَلَيهِم رَبُّهُم بِذَنبِهِم فَسَوّاها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلا يَخافُ عُقباها",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}