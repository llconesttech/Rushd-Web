{
  "number": 76,
  "name": "سُورَةُ الإِنسَانِ",
  "englishName": "Al-Insaan",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ هَل أَتىٰ عَلَى الإِنسانِ حينٌ مِنَ الدَّهرِ لَم يَكُن شَيئًا مَذكورًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "إِنّا خَلَقنَا الإِنسانَ مِن نُطفَةٍ أَمشاجٍ نَبتَليهِ فَجَعَلناهُ سَميعًا بَصيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إِنّا هَدَيناهُ السَّبيلَ إِمّا شاكِرًا وَإِمّا كَفورًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِنّا أَعتَدنا لِلكافِرينَ سَلاسِلَ وَأَغلالًا وَسَعيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "إِنَّ الأَبرارَ يَشرَبونَ مِن كَأسٍ كانَ مِزاجُها كافورًا",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "عَينًا يَشرَبُ بِها عِبادُ اللَّهِ يُفَجِّرونَها تَفجيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
      "text": "يوفونَ بِالنَّذرِ وَيَخافونَ يَومًا كانَ شَرُّهُ مُستَطيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَيُطعِمونَ الطَّعامَ عَلىٰ حُبِّهِ مِسكينًا وَيَتيمًا وَأَسيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 9,
      "text": "إِنَّما نُطعِمُكُم لِوَجهِ اللَّهِ لا نُريدُ مِنكُم جَزاءً وَلا شُكورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 10,
      "text": "إِنّا نَخافُ مِن رَبِّنا يَومًا عَبوسًا قَمطَريرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَوَقاهُمُ اللَّهُ شَرَّ ذٰلِكَ اليَومِ وَلَقّاهُم نَضرَةً وَسُرورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَجَزاهُم بِما صَبَروا جَنَّةً وَحَريرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 13,
      "text": "مُتَّكِئينَ فيها عَلَى الأَرائِكِ ۖ لا يَرَونَ فيها شَمسًا وَلا زَمهَريرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَدانِيَةً عَلَيهِم ظِلالُها وَذُلِّلَت قُطوفُها تَذليلًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَيُطافُ عَلَيهِم بِآنِيَةٍ مِن فِضَّةٍ وَأَكوابٍ كانَت قَواريرا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 16,
      "text": "قَواريرَ مِن فِضَّةٍ قَدَّروها تَقديرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَيُسقَونَ فيها كَأسًا كانَ مِزاجُها زَنجَبيلًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 18,
      "text": "عَينًا فيها تُسَمّىٰ سَلسَبيلًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 19,
      "text": "۞ وَيَطوفُ عَلَيهِم وِلدانٌ مُخَلَّدونَ إِذا رَأَيتَهُم حَسِبتَهُم لُؤلُؤًا مَنثورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَإِذا رَأَيتَ ثَمَّ رَأَيتَ نَعيمًا وَمُلكًا كَبيرًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 21,
      "text": "عالِيَهُم ثِيابُ سُندُسٍ خُضرٌ وَإِستَبرَقٌ ۖ وَحُلّوا أَساوِرَ مِن فِضَّةٍ وَسَقاهُم رَبُّهُم شَرابًا طَهورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 22,
      "text": "إِنَّ هٰذا كانَ لَكُم جَزاءً وَكانَ سَعيُكُم مَشكورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 23,
      "text": "إِنّا نَحنُ نَزَّلنا عَلَيكَ القُرآنَ تَنزيلًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَاصبِر لِحُكمِ رَبِّكَ وَلا تُطِع مِنهُم آثِمًا أَو كَفورًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَاذكُرِ اسمَ رَبِّكَ بُكرَةً وَأَصيلًا",
      "juz": 29,
      "manzil": 7,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَمِنَ اللَّيلِ فَاسجُد لَهُ وَسَبِّحهُ لَيلًا طَويلًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إِنَّ هٰؤُلاءِ يُحِبّونَ العاجِلَةَ وَيَذَرونَ وَراءَهُم يَومًا ثَقيلًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 28,
      "text": "نَحنُ خَلَقناهُم وَشَدَدنا أَسرَهُم ۖ وَإِذا شِئنا بَدَّلنا أَمثالَهُم تَبديلًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 29,
      "text": "إِنَّ هٰذِهِ تَذكِرَةٌ ۖ فَمَن شاءَ اتَّخَذَ إِلىٰ رَبِّهِ سَبيلًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَما تَشاءونَ إِلّا أَن يَشاءَ اللَّهُ ۚ إِنَّ اللَّهَ كانَ عَليمًا حَكيمًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 31,
      "text": "يُدخِلُ مَن يَشاءُ في رَحمَتِهِ ۚ وَالظّالِمينَ أَعَدَّ لَهُم عَذابًا أَليمًا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    }
  ]
}