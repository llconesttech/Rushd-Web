{
  "number": 75,
  "name": "سُورَةُ القِيَامَةِ",
  "englishName": "Al-Qiyaama",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ لا أُقسِمُ بِيَومِ القِيامَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلا أُقسِمُ بِالنَّفسِ اللَّوّامَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "أَيَحسَبُ الإِنسانُ أَلَّن نَجمَعَ عِظامَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "بَلىٰ قادِرينَ عَلىٰ أَن نُسَوِّيَ بَنانَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بَل يُريدُ الإِنسانُ لِيَفجُرَ أَمامَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يَسأَلُ أَيّانَ يَومُ القِيامَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَإِذا بَرِقَ البَصَرُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَخَسَفَ القَمَرُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَجُمِعَ الشَّمسُ وَالقَمَرُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 10,
      "text": "يَقولُ الإِنسانُ يَومَئِذٍ أَينَ المَفَرُّ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَلّا لا وَزَرَ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِلىٰ رَبِّكَ يَومَئِذٍ المُستَقَرُّ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 13,
      "text": "يُنَبَّأُ الإِنسانُ يَومَئِذٍ بِما قَدَّمَ وَأَخَّرَ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 14,
      "text": "بَلِ الإِنسانُ عَلىٰ نَفسِهِ بَصيرَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَو أَلقىٰ مَعاذيرَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 16,
      "text": "لا تُحَرِّك بِهِ لِسانَكَ لِتَعجَلَ بِهِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إِنَّ عَلَينا جَمعَهُ وَقُرآنَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَإِذا قَرَأناهُ فَاتَّبِع قُرآنَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 19,
      "text": "ثُمَّ إِنَّ عَلَينا بَيانَهُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 20,
      "text": "كَلّا بَل تُحِبّونَ العاجِلَةَ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَتَذَرونَ الآخِرَةَ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وُجوهٌ يَومَئِذٍ ناضِرَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 23,
      "text": "إِلىٰ رَبِّها ناظِرَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَوُجوهٌ يَومَئِذٍ باسِرَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تَظُنُّ أَن يُفعَلَ بِها فاقِرَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 26,
      "text": "كَلّا إِذا بَلَغَتِ التَّراقِيَ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَقيلَ مَن ۜ راقٍ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَظَنَّ أَنَّهُ الفِراقُ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَالتَفَّتِ السّاقُ بِالسّاقِ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 30,
      "text": "إِلىٰ رَبِّكَ يَومَئِذٍ المَساقُ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَلا صَدَّقَ وَلا صَلّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلٰكِن كَذَّبَ وَتَوَلّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 33,
      "text": "ثُمَّ ذَهَبَ إِلىٰ أَهلِهِ يَتَمَطّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 34,
      "text": "أَولىٰ لَكَ فَأَولىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ثُمَّ أَولىٰ لَكَ فَأَولىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 36,
      "text": "أَيَحسَبُ الإِنسانُ أَن يُترَكَ سُدًى",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 37,
      "text": "أَلَم يَكُ نُطفَةً مِن مَنِيٍّ يُمنىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 38,
      "text": "ثُمَّ كانَ عَلَقَةً فَخَلَقَ فَسَوّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَجَعَلَ مِنهُ الزَّوجَينِ الذَّكَرَ وَالأُنثىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 40,
      "text": "أَلَيسَ ذٰلِكَ بِقادِرٍ عَلىٰ أَن يُحيِيَ المَوتىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    }
  ]
}