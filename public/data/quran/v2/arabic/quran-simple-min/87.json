{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ سَبِّحِ اسمَ رَبِّكَ الأَعلَى",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الَّذي خَلَقَ فَسَوّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَالَّذي قَدَّرَ فَهَدىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَالَّذي أَخرَجَ المَرعىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَجَعَلَهُ غُثاءً أَحوىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "سَنُقرِئُكَ فَلا تَنسىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِلّا ما شاءَ اللَّهُ ۚ إِنَّهُ يَعلَمُ الجَهرَ وَما يَخفىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَنُيَسِّرُكَ لِليُسرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فَذَكِّر إِن نَفَعَتِ الذِّكرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "سَيَذَّكَّرُ مَن يَخشىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَيَتَجَنَّبُهَا الأَشقَى",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "الَّذي يَصلَى النّارَ الكُبرىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثُمَّ لا يَموتُ فيها وَلا يَحيىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قَد أَفلَحَ مَن تَزَكّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَذَكَرَ اسمَ رَبِّهِ فَصَلّىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "بَل تُؤثِرونَ الحَياةَ الدُّنيا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَالآخِرَةُ خَيرٌ وَأَبقىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "إِنَّ هٰذا لَفِي الصُّحُفِ الأولىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "صُحُفِ إِبراهيمَ وَموسىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}