{
  "number": 68,
  "name": "سُورَةُ القَلَمِ",
  "englishName": "Al-Qalam",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ ن ۚ وَالقَلَمِ وَما يَسطُرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ما أَنتَ بِنِعمَةِ رَبِّكَ بِمَجنونٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَإِنَّ لَكَ لَأَجرًا غَيرَ مَمنونٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَإِنَّكَ لَعَلىٰ خُلُقٍ عَظيمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَسَتُبصِرُ وَيُبصِرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بِأَييِكُمُ المَفتونُ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 7,
      "text": "إِنَّ رَبَّكَ هُوَ أَعلَمُ بِمَن ضَلَّ عَن سَبيلِهِ وَهُوَ أَعلَمُ بِالمُهتَدينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَلا تُطِعِ المُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَدّوا لَو تُدهِنُ فَيُدهِنونَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلا تُطِع كُلَّ حَلّافٍ مَهينٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 11,
      "text": "هَمّازٍ مَشّاءٍ بِنَميمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 12,
      "text": "مَنّاعٍ لِلخَيرِ مُعتَدٍ أَثيمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 13,
      "text": "عُتُلٍّ بَعدَ ذٰلِكَ زَنيمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 14,
      "text": "أَن كانَ ذا مالٍ وَبَنينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 15,
      "text": "إِذا تُتلىٰ عَلَيهِ آياتُنا قالَ أَساطيرُ الأَوَّلينَ",
      "juz": 29,
      "manzil": 7,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 16,
      "text": "سَنَسِمُهُ عَلَى الخُرطومِ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إِنّا بَلَوناهُم كَما بَلَونا أَصحابَ الجَنَّةِ إِذ أَقسَموا لَيَصرِمُنَّها مُصبِحينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلا يَستَثنونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَطافَ عَلَيها طائِفٌ مِن رَبِّكَ وَهُم نائِمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَأَصبَحَت كَالصَّريمِ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَتَنادَوا مُصبِحينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 22,
      "text": "أَنِ اغدوا عَلىٰ حَرثِكُم إِن كُنتُم صارِمينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَانطَلَقوا وَهُم يَتَخافَتونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 24,
      "text": "أَن لا يَدخُلَنَّهَا اليَومَ عَلَيكُم مِسكينٌ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَغَدَوا عَلىٰ حَردٍ قادِرينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَلَمّا رَأَوها قالوا إِنّا لَضالّونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 27,
      "text": "بَل نَحنُ مَحرومونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قالَ أَوسَطُهُم أَلَم أَقُل لَكُم لَولا تُسَبِّحونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قالوا سُبحانَ رَبِّنا إِنّا كُنّا ظالِمينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَأَقبَلَ بَعضُهُم عَلىٰ بَعضٍ يَتَلاوَمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قالوا يا وَيلَنا إِنّا كُنّا طاغينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 32,
      "text": "عَسىٰ رَبُّنا أَن يُبدِلَنا خَيرًا مِنها إِنّا إِلىٰ رَبِّنا راغِبونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَذٰلِكَ العَذابُ ۖ وَلَعَذابُ الآخِرَةِ أَكبَرُ ۚ لَو كانوا يَعلَمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 34,
      "text": "إِنَّ لِلمُتَّقينَ عِندَ رَبِّهِم جَنّاتِ النَّعيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 35,
      "text": "أَفَنَجعَلُ المُسلِمينَ كَالمُجرِمينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 36,
      "text": "ما لَكُم كَيفَ تَحكُمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 37,
      "text": "أَم لَكُم كِتابٌ فيهِ تَدرُسونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 38,
      "text": "إِنَّ لَكُم فيهِ لَما تَخَيَّرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 39,
      "text": "أَم لَكُم أَيمانٌ عَلَينا بالِغَةٌ إِلىٰ يَومِ القِيامَةِ ۙ إِنَّ لَكُم لَما تَحكُمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 40,
      "text": "سَلهُم أَيُّهُم بِذٰلِكَ زَعيمٌ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 41,
      "text": "أَم لَهُم شُرَكاءُ فَليَأتوا بِشُرَكائِهِم إِن كانوا صادِقينَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 42,
      "text": "يَومَ يُكشَفُ عَن ساقٍ وَيُدعَونَ إِلَى السُّجودِ فَلا يَستَطيعونَ",
      "juz": 29,
      "manzil": 7,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 43,
      "text": "خاشِعَةً أَبصارُهُم تَرهَقُهُم ذِلَّةٌ ۖ وَقَد كانوا يُدعَونَ إِلَى السُّجودِ وَهُم سالِمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَذَرني وَمَن يُكَذِّبُ بِهٰذَا الحَديثِ ۖ سَنَستَدرِجُهُم مِن حَيثُ لا يَعلَمونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَأُملي لَهُم ۚ إِنَّ كَيدي مَتينٌ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 46,
      "text": "أَم تَسأَلُهُم أَجرًا فَهُم مِن مَغرَمٍ مُثقَلونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 47,
      "text": "أَم عِندَهُمُ الغَيبُ فَهُم يَكتُبونَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 48,
      "text": "فَاصبِر لِحُكمِ رَبِّكَ وَلا تَكُن كَصاحِبِ الحوتِ إِذ نادىٰ وَهُوَ مَكظومٌ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 49,
      "text": "لَولا أَن تَدارَكَهُ نِعمَةٌ مِن رَبِّهِ لَنُبِذَ بِالعَراءِ وَهُوَ مَذمومٌ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فَاجتَباهُ رَبُّهُ فَجَعَلَهُ مِنَ الصّالِحينَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَإِن يَكادُ الَّذينَ كَفَروا لَيُزلِقونَكَ بِأَبصارِهِم لَمّا سَمِعُوا الذِّكرَ وَيَقولونَ إِنَّهُ لَمَجنونٌ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وَما هُوَ إِلّا ذِكرٌ لِلعالَمينَ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    }
  ]
}