{
  "number": 111,
  "name": "سُورَةُ المَسَدِ",
  "englishName": "Al-Masad",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ تَبَّت يَدا أَبي لَهَبٍ وَتَبَّ",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ما أَغنىٰ عَنهُ مالُهُ وَما كَسَبَ",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "سَيَصلىٰ نارًا ذاتَ لَهَبٍ",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَامرَأَتُهُ حَمّالَةَ الحَطَبِ",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "في جيدِها حَبلٌ مِن مَسَدٍ",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
