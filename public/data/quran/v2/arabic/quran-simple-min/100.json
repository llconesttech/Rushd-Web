{
  "number": 100,
  "name": "سُورَةُ العَادِيَاتِ",
  "englishName": "Al-Aadiyaat",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالعادِياتِ ضَبحًا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَالمورِياتِ قَدحًا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فَالمُغيراتِ صُبحًا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَأَثَرنَ بِهِ نَقعًا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَوَسَطنَ بِهِ جَمعًا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "إِنَّ الإِنسانَ لِرَبِّهِ لَكَنودٌ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَإِنَّهُ عَلىٰ ذٰلِكَ لَشَهيدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَإِنَّهُ لِحُبِّ الخَيرِ لَشَديدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "۞ أَفَلا يَعلَمُ إِذا بُعثِرَ ما فِي القُبورِ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَحُصِّلَ ما فِي الصُّدورِ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "إِنَّ رَبَّهُم بِهِم يَومَئِذٍ لَخَبيرٌ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
