{
  "number": 97,
  "name": "سُورَةُ القَدۡرِ",
  "englishName": "Al-Qadr",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ إِنّا أَنزَلناهُ في لَيلَةِ القَدرِ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَما أَدراكَ ما لَيلَةُ القَدرِ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لَيلَةُ القَدرِ خَيرٌ مِن أَلفِ شَهرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَنَزَّلُ المَلائِكَةُ وَالرّوحُ فيها بِإِذنِ رَبِّهِم مِن كُلِّ أَمرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "سَلامٌ هِيَ حَتّىٰ مَطلَعِ الفَجرِ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
