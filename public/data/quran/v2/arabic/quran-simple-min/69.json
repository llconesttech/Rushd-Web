{
  "number": 69,
  "name": "سُورَةُ الحَاقَّةِ",
  "englishName": "Al-Haaqqa",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ الحاقَّةُ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَا الحاقَّةُ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَما أَدراكَ مَا الحاقَّةُ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَذَّبَت ثَمودُ وَعادٌ بِالقارِعَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَأَمّا ثَمودُ فَأُهلِكوا بِالطّاغِيَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَأَمّا عادٌ فَأُهلِكوا بِريحٍ صَرصَرٍ عاتِيَةٍ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 7,
      "text": "سَخَّرَها عَلَيهِم سَبعَ لَيالٍ وَثَمانِيَةَ أَيّامٍ حُسومًا فَتَرَى القَومَ فيها صَرعىٰ كَأَنَّهُم أَعجازُ نَخلٍ خاوِيَةٍ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَهَل تَرىٰ لَهُم مِن باقِيَةٍ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَجاءَ فِرعَونُ وَمَن قَبلَهُ وَالمُؤتَفِكاتُ بِالخاطِئَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَعَصَوا رَسولَ رَبِّهِم فَأَخَذَهُم أَخذَةً رابِيَةً",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 11,
      "text": "إِنّا لَمّا طَغَى الماءُ حَمَلناكُم فِي الجارِيَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 12,
      "text": "لِنَجعَلَها لَكُم تَذكِرَةً وَتَعِيَها أُذُنٌ واعِيَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَإِذا نُفِخَ فِي الصّورِ نَفخَةٌ واحِدَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَحُمِلَتِ الأَرضُ وَالجِبالُ فَدُكَّتا دَكَّةً واحِدَةً",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَيَومَئِذٍ وَقَعَتِ الواقِعَةُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَانشَقَّتِ السَّماءُ فَهِيَ يَومَئِذٍ واهِيَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَالمَلَكُ عَلىٰ أَرجائِها ۚ وَيَحمِلُ عَرشَ رَبِّكَ فَوقَهُم يَومَئِذٍ ثَمانِيَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 18,
      "text": "يَومَئِذٍ تُعرَضونَ لا تَخفىٰ مِنكُم خافِيَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَأَمّا مَن أوتِيَ كِتابَهُ بِيَمينِهِ فَيَقولُ هاؤُمُ اقرَءوا كِتابِيَه",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 20,
      "text": "إِنّي ظَنَنتُ أَنّي مُلاقٍ حِسابِيَه",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَهُوَ في عيشَةٍ راضِيَةٍ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 22,
      "text": "في جَنَّةٍ عالِيَةٍ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 23,
      "text": "قُطوفُها دانِيَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 24,
      "text": "كُلوا وَاشرَبوا هَنيئًا بِما أَسلَفتُم فِي الأَيّامِ الخالِيَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَأَمّا مَن أوتِيَ كِتابَهُ بِشِمالِهِ فَيَقولُ يا لَيتَني لَم أوتَ كِتابِيَه",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلَم أَدرِ ما حِسابِيَه",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 27,
      "text": "يا لَيتَها كانَتِ القاضِيَةَ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 28,
      "text": "ما أَغنىٰ عَنّي مالِيَه ۜ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 29,
      "text": "هَلَكَ عَنّي سُلطانِيَه",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 30,
      "text": "خُذوهُ فَغُلّوهُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 31,
      "text": "ثُمَّ الجَحيمَ صَلّوهُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 32,
      "text": "ثُمَّ في سِلسِلَةٍ ذَرعُها سَبعونَ ذِراعًا فَاسلُكوهُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 33,
      "text": "إِنَّهُ كانَ لا يُؤمِنُ بِاللَّهِ العَظيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَلا يَحُضُّ عَلىٰ طَعامِ المِسكينِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 35,
      "text": "فَلَيسَ لَهُ اليَومَ هاهُنا حَميمٌ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَلا طَعامٌ إِلّا مِن غِسلينٍ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 37,
      "text": "لا يَأكُلُهُ إِلَّا الخاطِئونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 38,
      "text": "فَلا أُقسِمُ بِما تُبصِرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَما لا تُبصِرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إِنَّهُ لَقَولُ رَسولٍ كَريمٍ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَما هُوَ بِقَولِ شاعِرٍ ۚ قَليلًا ما تُؤمِنونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وَلا بِقَولِ كاهِنٍ ۚ قَليلًا ما تَذَكَّرونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 43,
      "text": "تَنزيلٌ مِن رَبِّ العالَمينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وَلَو تَقَوَّلَ عَلَينا بَعضَ الأَقاويلِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 45,
      "text": "لَأَخَذنا مِنهُ بِاليَمينِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 46,
      "text": "ثُمَّ لَقَطَعنا مِنهُ الوَتينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 47,
      "text": "فَما مِنكُم مِن أَحَدٍ عَنهُ حاجِزينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَإِنَّهُ لَتَذكِرَةٌ لِلمُتَّقينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَإِنّا لَنَعلَمُ أَنَّ مِنكُم مُكَذِّبينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَإِنَّهُ لَحَسرَةٌ عَلَى الكافِرينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَإِنَّهُ لَحَقُّ اليَقينِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 52,
      "text": "فَسَبِّح بِاسمِ رَبِّكَ العَظيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    }
  ]
}