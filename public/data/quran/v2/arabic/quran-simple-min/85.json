{
  "number": 85,
  "name": "سُورَةُ البُرُوجِ",
  "englishName": "Al-Burooj",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ وَالسَّماءِ ذاتِ البُروجِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاليَومِ المَوعودِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَشاهِدٍ وَمَشهودٍ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "قُتِلَ أَصحابُ الأُخدودِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "النّارِ ذاتِ الوَقودِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "إِذ هُم عَلَيها قُعودٌ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَهُم عَلىٰ ما يَفعَلونَ بِالمُؤمِنينَ شُهودٌ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَما نَقَموا مِنهُم إِلّا أَن يُؤمِنوا بِاللَّهِ العَزيزِ الحَميدِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "الَّذي لَهُ مُلكُ السَّماواتِ وَالأَرضِ ۚ وَاللَّهُ عَلىٰ كُلِّ شَيءٍ شَهيدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "إِنَّ الَّذينَ فَتَنُوا المُؤمِنينَ وَالمُؤمِناتِ ثُمَّ لَم يَتوبوا فَلَهُم عَذابُ جَهَنَّمَ وَلَهُم عَذابُ الحَريقِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "إِنَّ الَّذينَ آمَنوا وَعَمِلُوا الصّالِحاتِ لَهُم جَنّاتٌ تَجري مِن تَحتِهَا الأَنهارُ ۚ ذٰلِكَ الفَوزُ الكَبيرُ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِنَّ بَطشَ رَبِّكَ لَشَديدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إِنَّهُ هُوَ يُبدِئُ وَيُعيدُ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَهُوَ الغَفورُ الوَدودُ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "ذُو العَرشِ المَجيدُ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَعّالٌ لِما يُريدُ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "هَل أَتاكَ حَديثُ الجُنودِ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فِرعَونَ وَثَمودَ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 19,
      "text": "بَلِ الَّذينَ كَفَروا في تَكذيبٍ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَاللَّهُ مِن وَرائِهِم مُحيطٌ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 21,
      "text": "بَل هُوَ قُرآنٌ مَجيدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 22,
      "text": "في لَوحٍ مَحفوظٍ",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}