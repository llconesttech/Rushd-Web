{
  "number": 82,
  "name": "سُورَةُ الانفِطَارِ",
  "englishName": "Al-Infitaar",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسمِ اللَّهِ الرَّحمٰنِ الرَّحيمِ إِذَا السَّماءُ انفَطَرَت",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَإِذَا الكَواكِبُ انتَثَرَت",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَإِذَا البِحارُ فُجِّرَت",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَإِذَا القُبورُ بُعثِرَت",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلِمَت نَفسٌ ما قَدَّمَت وَأَخَّرَت",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يا أَيُّهَا الإِنسانُ ما غَرَّكَ بِرَبِّكَ الكَريمِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 7,
      "text": "الَّذي خَلَقَكَ فَسَوّاكَ فَعَدَلَكَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 8,
      "text": "في أَيِّ صورَةٍ ما شاءَ رَكَّبَكَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كَلّا بَل تُكَذِّبونَ بِالدّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَإِنَّ عَلَيكُم لَحافِظينَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كِرامًا كاتِبينَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 12,
      "text": "يَعلَمونَ ما تَفعَلونَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إِنَّ الأَبرارَ لَفي نَعيمٍ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَإِنَّ الفُجّارَ لَفي جَحيمٍ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 15,
      "text": "يَصلَونَها يَومَ الدّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَما هُم عَنها بِغائِبينَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَما أَدراكَ ما يَومُ الدّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ثُمَّ ما أَدراكَ ما يَومُ الدّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 19,
      "text": "يَومَ لا تَملِكُ نَفسٌ لِنَفسٍ شَيئًا ۖ وَالأَمرُ يَومَئِذٍ لِلَّهِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    }
  ]
}