{
  "number": 36,
  "name": "سُورَةُ يسٓ",
  "englishName": "Yaseen",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم يس",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 2,
      "text": "والقرآن الحكيم",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إنك لمن المرسلين",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 4,
      "text": "على صراط مستقيم",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تنزيل العزيز الرحيم",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 6,
      "text": "لتنذر قوما ما أنذر آباؤهم فهم غافلون",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لقد حق القول على أكثرهم فهم لا يؤمنون",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إنا جعلنا في أعناقهم أغلالا فهي إلى الأذقان فهم مقمحون",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وجعلنا من بين أيديهم سدا ومن خلفهم سدا فأغشيناهم فهم لا يبصرون",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وسواء عليهم أأنذرتهم أم لم تنذرهم لا يؤمنون",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 11,
      "text": "إنما تنذر من اتبع الذكر وخشي الرحمن بالغيب ۖ فبشره بمغفرة وأجر كريم",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إنا نحن نحيي الموتى ونكتب ما قدموا وآثارهم ۚ وكل شيء أحصيناه في إمام مبين",
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 13,
      "text": "واضرب لهم مثلا أصحاب القرية إذ جاءها المرسلون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 14,
      "text": "إذ أرسلنا إليهم اثنين فكذبوهما فعززنا بثالث فقالوا إنا إليكم مرسلون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 15,
      "text": "قالوا ما أنتم إلا بشر مثلنا وما أنزل الرحمن من شيء إن أنتم إلا تكذبون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 16,
      "text": "قالوا ربنا يعلم إنا إليكم لمرسلون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وما علينا إلا البلاغ المبين",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قالوا إنا تطيرنا بكم ۖ لئن لم تنتهوا لنرجمنكم وليمسنكم منا عذاب أليم",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 19,
      "text": "قالوا طائركم معكم ۚ أئن ذكرتم ۚ بل أنتم قوم مسرفون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وجاء من أقصى المدينة رجل يسعى قال يا قوم اتبعوا المرسلين",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اتبعوا من لا يسألكم أجرا وهم مهتدون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وما لي لا أعبد الذي فطرني وإليه ترجعون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 23,
      "text": "أأتخذ من دونه آلهة إن يردن الرحمن بضر لا تغن عني شفاعتهم شيئا ولا ينقذون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 24,
      "text": "إني إذا لفي ضلال مبين",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 25,
      "text": "إني آمنت بربكم فاسمعون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قيل ادخل الجنة ۖ قال يا ليت قومي يعلمون",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 27,
      "text": "بما غفر لي ربي وجعلني من المكرمين",
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 28,
      "text": "۞ وما أنزلنا على قومه من بعده من جند من السماء وما كنا منزلين",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 29,
      "text": "إن كانت إلا صيحة واحدة فإذا هم خامدون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 30,
      "text": "يا حسرة على العباد ۚ ما يأتيهم من رسول إلا كانوا به يستهزئون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 31,
      "text": "ألم يروا كم أهلكنا قبلهم من القرون أنهم إليهم لا يرجعون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وإن كل لما جميع لدينا محضرون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وآية لهم الأرض الميتة أحييناها وأخرجنا منها حبا فمنه يأكلون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وجعلنا فيها جنات من نخيل وأعناب وفجرنا فيها من العيون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ليأكلوا من ثمره وما عملته أيديهم ۖ أفلا يشكرون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 36,
      "text": "سبحان الذي خلق الأزواج كلها مما تنبت الأرض ومن أنفسهم ومما لا يعلمون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وآية لهم الليل نسلخ منه النهار فإذا هم مظلمون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 38,
      "text": "والشمس تجري لمستقر لها ۚ ذلك تقدير العزيز العليم",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 39,
      "text": "والقمر قدرناه منازل حتى عاد كالعرجون القديم",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 40,
      "text": "لا الشمس ينبغي لها أن تدرك القمر ولا الليل سابق النهار ۚ وكل في فلك يسبحون",
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وآية لهم أنا حملنا ذريتهم في الفلك المشحون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وخلقنا لهم من مثله ما يركبون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وإن نشأ نغرقهم فلا صريخ لهم ولا هم ينقذون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 44,
      "text": "إلا رحمة منا ومتاعا إلى حين",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وإذا قيل لهم اتقوا ما بين أيديكم وما خلفكم لعلكم ترحمون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 46,
      "text": "وما تأتيهم من آية من آيات ربهم إلا كانوا عنها معرضين",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وإذا قيل لهم أنفقوا مما رزقكم الله قال الذين كفروا للذين آمنوا أنطعم من لو يشاء الله أطعمه إن أنتم إلا في ضلال مبين",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 48,
      "text": "ويقولون متى هذا الوعد إن كنتم صادقين",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 49,
      "text": "ما ينظرون إلا صيحة واحدة تأخذهم وهم يخصمون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فلا يستطيعون توصية ولا إلى أهلهم يرجعون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ونفخ في الصور فإذا هم من الأجداث إلى ربهم ينسلون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 52,
      "text": "قالوا يا ويلنا من بعثنا من مرقدنا ۜ ۗ هذا ما وعد الرحمن وصدق المرسلون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 53,
      "text": "إن كانت إلا صيحة واحدة فإذا هم جميع لدينا محضرون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فاليوم لا تظلم نفس شيئا ولا تجزون إلا ما كنتم تعملون",
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 55,
      "text": "إن أصحاب الجنة اليوم في شغل فاكهون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 56,
      "text": "هم وأزواجهم في ظلال على الأرائك متكئون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 57,
      "text": "لهم فيها فاكهة ولهم ما يدعون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 58,
      "text": "سلام قولا من رب رحيم",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 59,
      "text": "وامتازوا اليوم أيها المجرمون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 60,
      "text": "۞ ألم أعهد إليكم يا بني آدم أن لا تعبدوا الشيطان ۖ إنه لكم عدو مبين",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 61,
      "text": "وأن اعبدوني ۚ هذا صراط مستقيم",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 62,
      "text": "ولقد أضل منكم جبلا كثيرا ۖ أفلم تكونوا تعقلون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 63,
      "text": "هذه جهنم التي كنتم توعدون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 64,
      "text": "اصلوها اليوم بما كنتم تكفرون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 65,
      "text": "اليوم نختم على أفواههم وتكلمنا أيديهم وتشهد أرجلهم بما كانوا يكسبون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 66,
      "text": "ولو نشاء لطمسنا على أعينهم فاستبقوا الصراط فأنى يبصرون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 67,
      "text": "ولو نشاء لمسخناهم على مكانتهم فما استطاعوا مضيا ولا يرجعون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 68,
      "text": "ومن نعمره ننكسه في الخلق ۖ أفلا يعقلون",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 69,
      "text": "وما علمناه الشعر وما ينبغي له ۚ إن هو إلا ذكر وقرآن مبين",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 70,
      "text": "لينذر من كان حيا ويحق القول على الكافرين",
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 71,
      "text": "أولم يروا أنا خلقنا لهم مما عملت أيدينا أنعاما فهم لها مالكون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 72,
      "text": "وذللناها لهم فمنها ركوبهم ومنها يأكلون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 73,
      "text": "ولهم فيها منافع ومشارب ۖ أفلا يشكرون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 74,
      "text": "واتخذوا من دون الله آلهة لعلهم ينصرون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 75,
      "text": "لا يستطيعون نصرهم وهم لهم جند محضرون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 76,
      "text": "فلا يحزنك قولهم ۘ إنا نعلم ما يسرون وما يعلنون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 77,
      "text": "أولم ير الإنسان أنا خلقناه من نطفة فإذا هو خصيم مبين",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 78,
      "text": "وضرب لنا مثلا ونسي خلقه ۖ قال من يحيي العظام وهي رميم",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 79,
      "text": "قل يحييها الذي أنشأها أول مرة ۖ وهو بكل خلق عليم",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 80,
      "text": "الذي جعل لكم من الشجر الأخضر نارا فإذا أنتم منه توقدون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 81,
      "text": "أوليس الذي خلق السماوات والأرض بقادر على أن يخلق مثلهم ۚ بلى وهو الخلاق العليم",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 82,
      "text": "إنما أمره إذا أراد شيئا أن يقول له كن فيكون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 83,
      "text": "فسبحان الذي بيده ملكوت كل شيء وإليه ترجعون",
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    }
  ]
}