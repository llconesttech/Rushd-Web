{
  "number": 90,
  "name": "سُورَةُ البَلَدِ",
  "englishName": "Al-Balad",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم لا أقسم بهذا البلد",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وأنت حل بهذا البلد",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ووالد وما ولد",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "لقد خلقنا الإنسان في كبد",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "أيحسب أن لن يقدر عليه أحد",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يقول أهلكت مالا لبدا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "أيحسب أن لم يره أحد",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ألم نجعل له عينين",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "ولسانا وشفتين",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وهديناه النجدين",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فلا اقتحم العقبة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وما أدراك ما العقبة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فك رقبة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "أو إطعام في يوم ذي مسغبة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "يتيما ذا مقربة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 16,
      "text": "أو مسكينا ذا متربة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ثم كان من الذين آمنوا وتواصوا بالصبر وتواصوا بالمرحمة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 18,
      "text": "أولئك أصحاب الميمنة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 19,
      "text": "والذين كفروا بآياتنا هم أصحاب المشأمة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 20,
      "text": "عليهم نار مؤصدة",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}