{
  "number": 83,
  "name": "سُورَةُ المُطَفِّفِينَ",
  "englishName": "Al-Mutaffifin",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم ويل للمطففين",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 2,
      "text": "الذين إذا اكتالوا على الناس يستوفون",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وإذا كالوهم أو وزنوهم يخسرون",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ألا يظن أولئك أنهم مبعوثون",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ليوم عظيم",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يوم يقوم الناس لرب العالمين",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 7,
      "text": "كلا إن كتاب الفجار لفي سجين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وما أدراك ما سجين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كتاب مرقوم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 10,
      "text": "ويل يومئذ للمكذبين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الذين يكذبون بيوم الدين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وما يكذب به إلا كل معتد أثيم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إذا تتلى عليه آياتنا قال أساطير الأولين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 14,
      "text": "كلا ۖ بل ۜ ران على قلوبهم ما كانوا يكسبون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كلا إنهم عن ربهم يومئذ لمحجوبون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ثم إنهم لصالو الجحيم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 17,
      "text": "ثم يقال هذا الذي كنتم به تكذبون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 18,
      "text": "كلا إن كتاب الأبرار لفي عليين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وما أدراك ما عليون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 20,
      "text": "كتاب مرقوم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 21,
      "text": "يشهده المقربون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 22,
      "text": "إن الأبرار لفي نعيم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 23,
      "text": "على الأرائك ينظرون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 24,
      "text": "تعرف في وجوههم نضرة النعيم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 25,
      "text": "يسقون من رحيق مختوم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ختامه مسك ۚ وفي ذلك فليتنافس المتنافسون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 27,
      "text": "ومزاجه من تسنيم",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 28,
      "text": "عينا يشرب بها المقربون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 29,
      "text": "إن الذين أجرموا كانوا من الذين آمنوا يضحكون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وإذا مروا بهم يتغامزون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وإذا انقلبوا إلى أهلهم انقلبوا فكهين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وإذا رأوهم قالوا إن هؤلاء لضالون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وما أرسلوا عليهم حافظين",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 34,
      "text": "فاليوم الذين آمنوا من الكفار يضحكون",
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 35,
      "text": "على الأرائك ينظرون",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 36,
      "text": "هل ثوب الكفار ما كانوا يفعلون",
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    }
  ]
}