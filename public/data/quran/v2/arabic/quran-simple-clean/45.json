{
  "number": 45,
  "name": "سُورَةُ الجَاثِيَةِ",
  "englishName": "Al-Jaathiya",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم حم",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 2,
      "text": "تنزيل الكتاب من الله العزيز الحكيم",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إن في السماوات والأرض لآيات للمؤمنين",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وفي خلقكم وما يبث من دابة آيات لقوم يوقنون",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 5,
      "text": "واختلاف الليل والنهار وما أنزل الله من السماء من رزق فأحيا به الأرض بعد موتها وتصريف الرياح آيات لقوم يعقلون",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تلك آيات الله نتلوها عليك بالحق ۖ فبأي حديث بعد الله وآياته يؤمنون",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 7,
      "text": "ويل لكل أفاك أثيم",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 8,
      "text": "يسمع آيات الله تتلى عليه ثم يصر مستكبرا كأن لم يسمعها ۖ فبشره بعذاب أليم",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وإذا علم من آياتنا شيئا اتخذها هزوا ۚ أولئك لهم عذاب مهين",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 10,
      "text": "من ورائهم جهنم ۖ ولا يغني عنهم ما كسبوا شيئا ولا ما اتخذوا من دون الله أولياء ۖ ولهم عذاب عظيم",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 11,
      "text": "هذا هدى ۖ والذين كفروا بآيات ربهم لهم عذاب من رجز أليم",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 434,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 12,
      "text": "۞ الله الذي سخر لكم البحر لتجري الفلك فيه بأمره ولتبتغوا من فضله ولعلكم تشكرون",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وسخر لكم ما في السماوات وما في الأرض جميعا منه ۚ إن في ذلك لآيات لقوم يتفكرون",
      "juz": 25,
      "manzil": 6,
      "page": 499,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قل للذين آمنوا يغفروا للذين لا يرجون أيام الله ليجزي قوما بما كانوا يكسبون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 15,
      "text": "من عمل صالحا فلنفسه ۖ ومن أساء فعليها ۖ ثم إلى ربكم ترجعون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ولقد آتينا بني إسرائيل الكتاب والحكم والنبوة ورزقناهم من الطيبات وفضلناهم على العالمين",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وآتيناهم بينات من الأمر ۖ فما اختلفوا إلا من بعد ما جاءهم العلم بغيا بينهم ۚ إن ربك يقضي بينهم يوم القيامة فيما كانوا فيه يختلفون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ثم جعلناك على شريعة من الأمر فاتبعها ولا تتبع أهواء الذين لا يعلمون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 19,
      "text": "إنهم لن يغنوا عنك من الله شيئا ۚ وإن الظالمين بعضهم أولياء بعض ۖ والله ولي المتقين",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 20,
      "text": "هذا بصائر للناس وهدى ورحمة لقوم يوقنون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 21,
      "text": "أم حسب الذين اجترحوا السيئات أن نجعلهم كالذين آمنوا وعملوا الصالحات سواء محياهم ومماتهم ۚ ساء ما يحكمون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 435,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وخلق الله السماوات والأرض بالحق ولتجزى كل نفس بما كسبت وهم لا يظلمون",
      "juz": 25,
      "manzil": 6,
      "page": 500,
      "ruku": 436,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 23,
      "text": "أفرأيت من اتخذ إلهه هواه وأضله الله على علم وختم على سمعه وقلبه وجعل على بصره غشاوة فمن يهديه من بعد الله ۚ أفلا تذكرون",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 436,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وقالوا ما هي إلا حياتنا الدنيا نموت ونحيا وما يهلكنا إلا الدهر ۚ وما لهم بذلك من علم ۖ إن هم إلا يظنون",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 436,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وإذا تتلى عليهم آياتنا بينات ما كان حجتهم إلا أن قالوا ائتوا بآبائنا إن كنتم صادقين",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 436,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قل الله يحييكم ثم يميتكم ثم يجمعكم إلى يوم القيامة لا ريب فيه ولكن أكثر الناس لا يعلمون",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 436,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 27,
      "text": "ولله ملك السماوات والأرض ۚ ويوم تقوم الساعة يومئذ يخسر المبطلون",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وترى كل أمة جاثية ۚ كل أمة تدعى إلى كتابها اليوم تجزون ما كنتم تعملون",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 29,
      "text": "هذا كتابنا ينطق عليكم بالحق ۚ إنا كنا نستنسخ ما كنتم تعملون",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فأما الذين آمنوا وعملوا الصالحات فيدخلهم ربهم في رحمته ۚ ذلك هو الفوز المبين",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وأما الذين كفروا أفلم تكن آياتي تتلى عليكم فاستكبرتم وكنتم قوما مجرمين",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وإذا قيل إن وعد الله حق والساعة لا ريب فيها قلتم ما ندري ما الساعة إن نظن إلا ظنا وما نحن بمستيقنين",
      "juz": 25,
      "manzil": 6,
      "page": 501,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وبدا لهم سيئات ما عملوا وحاق بهم ما كانوا به يستهزئون",
      "juz": 25,
      "manzil": 6,
      "page": 502,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وقيل اليوم ننساكم كما نسيتم لقاء يومكم هذا ومأواكم النار وما لكم من ناصرين",
      "juz": 25,
      "manzil": 6,
      "page": 502,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ذلكم بأنكم اتخذتم آيات الله هزوا وغرتكم الحياة الدنيا ۚ فاليوم لا يخرجون منها ولا هم يستعتبون",
      "juz": 25,
      "manzil": 6,
      "page": 502,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فلله الحمد رب السماوات ورب الأرض رب العالمين",
      "juz": 25,
      "manzil": 6,
      "page": 502,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وله الكبرياء في السماوات والأرض ۖ وهو العزيز الحكيم",
      "juz": 25,
      "manzil": 6,
      "page": 502,
      "ruku": 437,
      "hizbQuarter": 200,
      "sajda": false
    }
  ]
}