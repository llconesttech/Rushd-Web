{
  "number": 50,
  "name": "سُورَةُ قٓ",
  "englishName": "Qaaf",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم ق ۚ والقرآن المجيد",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 2,
      "text": "بل عجبوا أن جاءهم منذر منهم فقال الكافرون هذا شيء عجيب",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 3,
      "text": "أإذا متنا وكنا ترابا ۖ ذلك رجع بعيد",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4,
      "text": "قد علمنا ما تنقص الأرض منهم ۖ وعندنا كتاب حفيظ",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بل كذبوا بالحق لما جاءهم فهم في أمر مريج",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 6,
      "text": "أفلم ينظروا إلى السماء فوقهم كيف بنيناها وزيناها وما لها من فروج",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 7,
      "text": "والأرض مددناها وألقينا فيها رواسي وأنبتنا فيها من كل زوج بهيج",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 8,
      "text": "تبصرة وذكرى لكل عبد منيب",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 9,
      "text": "ونزلنا من السماء ماء مباركا فأنبتنا به جنات وحب الحصيد",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 10,
      "text": "والنخل باسقات لها طلع نضيد",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 11,
      "text": "رزقا للعباد ۖ وأحيينا به بلدة ميتا ۚ كذلك الخروج",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 12,
      "text": "كذبت قبلهم قوم نوح وأصحاب الرس وثمود",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وعاد وفرعون وإخوان لوط",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وأصحاب الأيكة وقوم تبع ۚ كل كذب الرسل فحق وعيد",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 15,
      "text": "أفعيينا بالخلق الأول ۚ بل هم في لبس من خلق جديد",
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 16,
      "text": "ولقد خلقنا الإنسان ونعلم ما توسوس به نفسه ۖ ونحن أقرب إليه من حبل الوريد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إذ يتلقى المتلقيان عن اليمين وعن الشمال قعيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ما يلفظ من قول إلا لديه رقيب عتيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وجاءت سكرة الموت بالحق ۖ ذلك ما كنت منه تحيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 20,
      "text": "ونفخ في الصور ۚ ذلك يوم الوعيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وجاءت كل نفس معها سائق وشهيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لقد كنت في غفلة من هذا فكشفنا عنك غطاءك فبصرك اليوم حديد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وقال قرينه هذا ما لدي عتيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 24,
      "text": "ألقيا في جهنم كل كفار عنيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 25,
      "text": "مناع للخير معتد مريب",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 26,
      "text": "الذي جعل مع الله إلها آخر فألقياه في العذاب الشديد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 27,
      "text": "۞ قال قرينه ربنا ما أطغيته ولكن كان في ضلال بعيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قال لا تختصموا لدي وقد قدمت إليكم بالوعيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 29,
      "text": "ما يبدل القول لدي وما أنا بظلام للعبيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 30,
      "text": "يوم نقول لجهنم هل امتلأت وتقول هل من مزيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وأزلفت الجنة للمتقين غير بعيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 32,
      "text": "هذا ما توعدون لكل أواب حفيظ",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 33,
      "text": "من خشي الرحمن بالغيب وجاء بقلب منيب",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 34,
      "text": "ادخلوها بسلام ۖ ذلك يوم الخلود",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 35,
      "text": "لهم ما يشاءون فيها ولدينا مزيد",
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وكم أهلكنا قبلهم من قرن هم أشد منهم بطشا فنقبوا في البلاد هل من محيص",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 37,
      "text": "إن في ذلك لذكرى لمن كان له قلب أو ألقى السمع وهو شهيد",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 38,
      "text": "ولقد خلقنا السماوات والأرض وما بينهما في ستة أيام وما مسنا من لغوب",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فاصبر على ما يقولون وسبح بحمد ربك قبل طلوع الشمس وقبل الغروب",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 40,
      "text": "ومن الليل فسبحه وأدبار السجود",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 41,
      "text": "واستمع يوم يناد المناد من مكان قريب",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 42,
      "text": "يوم يسمعون الصيحة بالحق ۚ ذلك يوم الخروج",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 43,
      "text": "إنا نحن نحيي ونميت وإلينا المصير",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 44,
      "text": "يوم تشقق الأرض عنهم سراعا ۚ ذلك حشر علينا يسير",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 45,
      "text": "نحن أعلم بما يقولون ۖ وما أنت عليهم بجبار ۖ فذكر بالقرآن من يخاف وعيد",
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    }
  ]
}