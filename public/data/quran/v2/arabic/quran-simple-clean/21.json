{
  "number": 21,
  "name": "سُورَةُ الأَنبِيَاءِ",
  "englishName": "Al-Anbiyaa",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم اقترب للناس حسابهم وهم في غفلة معرضون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ما يأتيهم من ذكر من ربهم محدث إلا استمعوه وهم يلعبون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لاهية قلوبهم ۗ وأسروا النجوى الذين ظلموا هل هذا إلا بشر مثلكم ۖ أفتأتون السحر وأنتم تبصرون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 4,
      "text": "قال ربي يعلم القول في السماء والأرض ۖ وهو السميع العليم",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بل قالوا أضغاث أحلام بل افتراه بل هو شاعر فليأتنا بآية كما أرسل الأولون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ما آمنت قبلهم من قرية أهلكناها ۖ أفهم يؤمنون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وما أرسلنا قبلك إلا رجالا نوحي إليهم ۖ فاسألوا أهل الذكر إن كنتم لا تعلمون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وما جعلناهم جسدا لا يأكلون الطعام وما كانوا خالدين",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 9,
      "text": "ثم صدقناهم الوعد فأنجيناهم ومن نشاء وأهلكنا المسرفين",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 10,
      "text": "لقد أنزلنا إليكم كتابا فيه ذكركم ۖ أفلا تعقلون",
      "juz": 17,
      "manzil": 4,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وكم قصمنا من قرية كانت ظالمة وأنشأنا بعدها قوما آخرين",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فلما أحسوا بأسنا إذا هم منها يركضون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 13,
      "text": "لا تركضوا وارجعوا إلى ما أترفتم فيه ومساكنكم لعلكم تسألون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قالوا يا ويلنا إنا كنا ظالمين",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فما زالت تلك دعواهم حتى جعلناهم حصيدا خامدين",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وما خلقنا السماء والأرض وما بينهما لاعبين",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 17,
      "text": "لو أردنا أن نتخذ لهوا لاتخذناه من لدنا إن كنا فاعلين",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 18,
      "text": "بل نقذف بالحق على الباطل فيدمغه فإذا هو زاهق ۚ ولكم الويل مما تصفون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وله من في السماوات والأرض ۚ ومن عنده لا يستكبرون عن عبادته ولا يستحسرون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 20,
      "text": "يسبحون الليل والنهار لا يفترون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 21,
      "text": "أم اتخذوا آلهة من الأرض هم ينشرون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لو كان فيهما آلهة إلا الله لفسدتا ۚ فسبحان الله رب العرش عما يصفون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 23,
      "text": "لا يسأل عما يفعل وهم يسألون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 24,
      "text": "أم اتخذوا من دونه آلهة ۖ قل هاتوا برهانكم ۖ هذا ذكر من معي وذكر من قبلي ۗ بل أكثرهم لا يعلمون الحق ۖ فهم معرضون",
      "juz": 17,
      "manzil": 4,
      "page": 323,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وما أرسلنا من قبلك من رسول إلا نوحي إليه أنه لا إله إلا أنا فاعبدون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وقالوا اتخذ الرحمن ولدا ۗ سبحانه ۚ بل عباد مكرمون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 27,
      "text": "لا يسبقونه بالقول وهم بأمره يعملون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 28,
      "text": "يعلم ما بين أيديهم وما خلفهم ولا يشفعون إلا لمن ارتضى وهم من خشيته مشفقون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 29,
      "text": "۞ ومن يقل منهم إني إله من دونه فذلك نجزيه جهنم ۚ كذلك نجزي الظالمين",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 279,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 30,
      "text": "أولم ير الذين كفروا أن السماوات والأرض كانتا رتقا ففتقناهما ۖ وجعلنا من الماء كل شيء حي ۖ أفلا يؤمنون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وجعلنا في الأرض رواسي أن تميد بهم وجعلنا فيها فجاجا سبلا لعلهم يهتدون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وجعلنا السماء سقفا محفوظا ۖ وهم عن آياتها معرضون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وهو الذي خلق الليل والنهار والشمس والقمر ۖ كل في فلك يسبحون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وما جعلنا لبشر من قبلك الخلد ۖ أفإن مت فهم الخالدون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 35,
      "text": "كل نفس ذائقة الموت ۗ ونبلوكم بالشر والخير فتنة ۖ وإلينا ترجعون",
      "juz": 17,
      "manzil": 4,
      "page": 324,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وإذا رآك الذين كفروا إن يتخذونك إلا هزوا أهذا الذي يذكر آلهتكم وهم بذكر الرحمن هم كافرون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 37,
      "text": "خلق الإنسان من عجل ۚ سأريكم آياتي فلا تستعجلون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 38,
      "text": "ويقولون متى هذا الوعد إن كنتم صادقين",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 39,
      "text": "لو يعلم الذين كفروا حين لا يكفون عن وجوههم النار ولا عن ظهورهم ولا هم ينصرون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 40,
      "text": "بل تأتيهم بغتة فتبهتهم فلا يستطيعون ردها ولا هم ينظرون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 41,
      "text": "ولقد استهزئ برسل من قبلك فحاق بالذين سخروا منهم ما كانوا به يستهزئون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 280,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 42,
      "text": "قل من يكلؤكم بالليل والنهار من الرحمن ۗ بل هم عن ذكر ربهم معرضون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 43,
      "text": "أم لهم آلهة تمنعهم من دوننا ۚ لا يستطيعون نصر أنفسهم ولا هم منا يصحبون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 44,
      "text": "بل متعنا هؤلاء وآباءهم حتى طال عليهم العمر ۗ أفلا يرون أنا نأتي الأرض ننقصها من أطرافها ۚ أفهم الغالبون",
      "juz": 17,
      "manzil": 4,
      "page": 325,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 45,
      "text": "قل إنما أنذركم بالوحي ۚ ولا يسمع الصم الدعاء إذا ما ينذرون",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 46,
      "text": "ولئن مستهم نفحة من عذاب ربك ليقولن يا ويلنا إنا كنا ظالمين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 47,
      "text": "ونضع الموازين القسط ليوم القيامة فلا تظلم نفس شيئا ۖ وإن كان مثقال حبة من خردل أتينا بها ۗ وكفى بنا حاسبين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 48,
      "text": "ولقد آتينا موسى وهارون الفرقان وضياء وذكرا للمتقين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 49,
      "text": "الذين يخشون ربهم بالغيب وهم من الساعة مشفقون",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وهذا ذكر مبارك أنزلناه ۚ أفأنتم له منكرون",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 281,
      "hizbQuarter": 130,
      "sajda": false
    },
    {
      "number": 51,
      "text": "۞ ولقد آتينا إبراهيم رشده من قبل وكنا به عالمين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 52,
      "text": "إذ قال لأبيه وقومه ما هذه التماثيل التي أنتم لها عاكفون",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 53,
      "text": "قالوا وجدنا آباءنا لها عابدين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 54,
      "text": "قال لقد كنتم أنتم وآباؤكم في ضلال مبين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 55,
      "text": "قالوا أجئتنا بالحق أم أنت من اللاعبين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 56,
      "text": "قال بل ربكم رب السماوات والأرض الذي فطرهن وأنا على ذلكم من الشاهدين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 57,
      "text": "وتالله لأكيدن أصنامكم بعد أن تولوا مدبرين",
      "juz": 17,
      "manzil": 4,
      "page": 326,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 58,
      "text": "فجعلهم جذاذا إلا كبيرا لهم لعلهم إليه يرجعون",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 59,
      "text": "قالوا من فعل هذا بآلهتنا إنه لمن الظالمين",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 60,
      "text": "قالوا سمعنا فتى يذكرهم يقال له إبراهيم",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 61,
      "text": "قالوا فأتوا به على أعين الناس لعلهم يشهدون",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 62,
      "text": "قالوا أأنت فعلت هذا بآلهتنا يا إبراهيم",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 63,
      "text": "قال بل فعله كبيرهم هذا فاسألوهم إن كانوا ينطقون",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 64,
      "text": "فرجعوا إلى أنفسهم فقالوا إنكم أنتم الظالمون",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 65,
      "text": "ثم نكسوا على رءوسهم لقد علمت ما هؤلاء ينطقون",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 66,
      "text": "قال أفتعبدون من دون الله ما لا ينفعكم شيئا ولا يضركم",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 67,
      "text": "أف لكم ولما تعبدون من دون الله ۖ أفلا تعقلون",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 68,
      "text": "قالوا حرقوه وانصروا آلهتكم إن كنتم فاعلين",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 69,
      "text": "قلنا يا نار كوني بردا وسلاما على إبراهيم",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 70,
      "text": "وأرادوا به كيدا فجعلناهم الأخسرين",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 71,
      "text": "ونجيناه ولوطا إلى الأرض التي باركنا فيها للعالمين",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 72,
      "text": "ووهبنا له إسحاق ويعقوب نافلة ۖ وكلا جعلنا صالحين",
      "juz": 17,
      "manzil": 4,
      "page": 327,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 73,
      "text": "وجعلناهم أئمة يهدون بأمرنا وأوحينا إليهم فعل الخيرات وإقام الصلاة وإيتاء الزكاة ۖ وكانوا لنا عابدين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 74,
      "text": "ولوطا آتيناه حكما وعلما ونجيناه من القرية التي كانت تعمل الخبائث ۗ إنهم كانوا قوم سوء فاسقين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 75,
      "text": "وأدخلناه في رحمتنا ۖ إنه من الصالحين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 282,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 76,
      "text": "ونوحا إذ نادى من قبل فاستجبنا له فنجيناه وأهله من الكرب العظيم",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 77,
      "text": "ونصرناه من القوم الذين كذبوا بآياتنا ۚ إنهم كانوا قوم سوء فأغرقناهم أجمعين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 78,
      "text": "وداوود وسليمان إذ يحكمان في الحرث إذ نفشت فيه غنم القوم وكنا لحكمهم شاهدين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 79,
      "text": "ففهمناها سليمان ۚ وكلا آتينا حكما وعلما ۚ وسخرنا مع داوود الجبال يسبحن والطير ۚ وكنا فاعلين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 80,
      "text": "وعلمناه صنعة لبوس لكم لتحصنكم من بأسكم ۖ فهل أنتم شاكرون",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 81,
      "text": "ولسليمان الريح عاصفة تجري بأمره إلى الأرض التي باركنا فيها ۚ وكنا بكل شيء عالمين",
      "juz": 17,
      "manzil": 4,
      "page": 328,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 82,
      "text": "ومن الشياطين من يغوصون له ويعملون عملا دون ذلك ۖ وكنا لهم حافظين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 131,
      "sajda": false
    },
    {
      "number": 83,
      "text": "۞ وأيوب إذ نادى ربه أني مسني الضر وأنت أرحم الراحمين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 84,
      "text": "فاستجبنا له فكشفنا ما به من ضر ۖ وآتيناه أهله ومثلهم معهم رحمة من عندنا وذكرى للعابدين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وإسماعيل وإدريس وذا الكفل ۖ كل من الصابرين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 86,
      "text": "وأدخلناهم في رحمتنا ۖ إنهم من الصالحين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 87,
      "text": "وذا النون إذ ذهب مغاضبا فظن أن لن نقدر عليه فنادى في الظلمات أن لا إله إلا أنت سبحانك إني كنت من الظالمين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 88,
      "text": "فاستجبنا له ونجيناه من الغم ۚ وكذلك ننجي المؤمنين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 89,
      "text": "وزكريا إذ نادى ربه رب لا تذرني فردا وأنت خير الوارثين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 90,
      "text": "فاستجبنا له ووهبنا له يحيى وأصلحنا له زوجه ۚ إنهم كانوا يسارعون في الخيرات ويدعوننا رغبا ورهبا ۖ وكانوا لنا خاشعين",
      "juz": 17,
      "manzil": 4,
      "page": 329,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 91,
      "text": "والتي أحصنت فرجها فنفخنا فيها من روحنا وجعلناها وابنها آية للعالمين",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 92,
      "text": "إن هذه أمتكم أمة واحدة وأنا ربكم فاعبدون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 93,
      "text": "وتقطعوا أمرهم بينهم ۖ كل إلينا راجعون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 283,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 94,
      "text": "فمن يعمل من الصالحات وهو مؤمن فلا كفران لسعيه وإنا له كاتبون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 95,
      "text": "وحرام على قرية أهلكناها أنهم لا يرجعون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 96,
      "text": "حتى إذا فتحت يأجوج ومأجوج وهم من كل حدب ينسلون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 97,
      "text": "واقترب الوعد الحق فإذا هي شاخصة أبصار الذين كفروا يا ويلنا قد كنا في غفلة من هذا بل كنا ظالمين",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 98,
      "text": "إنكم وما تعبدون من دون الله حصب جهنم أنتم لها واردون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 99,
      "text": "لو كان هؤلاء آلهة ما وردوها ۖ وكل فيها خالدون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 100,
      "text": "لهم فيها زفير وهم فيها لا يسمعون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 101,
      "text": "إن الذين سبقت لهم منا الحسنى أولئك عنها مبعدون",
      "juz": 17,
      "manzil": 4,
      "page": 330,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 102,
      "text": "لا يسمعون حسيسها ۖ وهم في ما اشتهت أنفسهم خالدون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 103,
      "text": "لا يحزنهم الفزع الأكبر وتتلقاهم الملائكة هذا يومكم الذي كنتم توعدون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 104,
      "text": "يوم نطوي السماء كطي السجل للكتب ۚ كما بدأنا أول خلق نعيده ۚ وعدا علينا ۚ إنا كنا فاعلين",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 105,
      "text": "ولقد كتبنا في الزبور من بعد الذكر أن الأرض يرثها عبادي الصالحون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 106,
      "text": "إن في هذا لبلاغا لقوم عابدين",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 107,
      "text": "وما أرسلناك إلا رحمة للعالمين",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 108,
      "text": "قل إنما يوحى إلي أنما إلهكم إله واحد ۖ فهل أنتم مسلمون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 109,
      "text": "فإن تولوا فقل آذنتكم على سواء ۖ وإن أدري أقريب أم بعيد ما توعدون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 110,
      "text": "إنه يعلم الجهر من القول ويعلم ما تكتمون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 111,
      "text": "وإن أدري لعله فتنة لكم ومتاع إلى حين",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    },
    {
      "number": 112,
      "text": "قال رب احكم بالحق ۗ وربنا الرحمن المستعان على ما تصفون",
      "juz": 17,
      "manzil": 4,
      "page": 331,
      "ruku": 284,
      "hizbQuarter": 132,
      "sajda": false
    }
  ]
}