{
  "number": 19,
  "name": "سُورَةُ مَرۡيَمَ",
  "englishName": "Maryam",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم كهيعص",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ذكر رحمت ربك عبده زكريا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إذ نادى ربه نداء خفيا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 4,
      "text": "قال رب إني وهن العظم مني واشتعل الرأس شيبا ولم أكن بدعائك رب شقيا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وإني خفت الموالي من ورائي وكانت امرأتي عاقرا فهب لي من لدنك وليا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يرثني ويرث من آل يعقوب ۖ واجعله رب رضيا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 7,
      "text": "يا زكريا إنا نبشرك بغلام اسمه يحيى لم نجعل له من قبل سميا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 8,
      "text": "قال رب أنى يكون لي غلام وكانت امرأتي عاقرا وقد بلغت من الكبر عتيا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 9,
      "text": "قال كذلك قال ربك هو علي هين وقد خلقتك من قبل ولم تك شيئا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 10,
      "text": "قال رب اجعل لي آية ۚ قال آيتك ألا تكلم الناس ثلاث ليال سويا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فخرج على قومه من المحراب فأوحى إليهم أن سبحوا بكرة وعشيا",
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 12,
      "text": "يا يحيى خذ الكتاب بقوة ۖ وآتيناه الحكم صبيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وحنانا من لدنا وزكاة ۖ وكان تقيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وبرا بوالديه ولم يكن جبارا عصيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وسلام عليه يوم ولد ويوم يموت ويوم يبعث حيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 16,
      "text": "واذكر في الكتاب مريم إذ انتبذت من أهلها مكانا شرقيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فاتخذت من دونهم حجابا فأرسلنا إليها روحنا فتمثل لها بشرا سويا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قالت إني أعوذ بالرحمن منك إن كنت تقيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 19,
      "text": "قال إنما أنا رسول ربك لأهب لك غلاما زكيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 20,
      "text": "قالت أنى يكون لي غلام ولم يمسسني بشر ولم أك بغيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 21,
      "text": "قال كذلك قال ربك هو علي هين ۖ ولنجعله آية للناس ورحمة منا ۚ وكان أمرا مقضيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 22,
      "text": "۞ فحملته فانتبذت به مكانا قصيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فأجاءها المخاض إلى جذع النخلة قالت يا ليتني مت قبل هذا وكنت نسيا منسيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فناداها من تحتها ألا تحزني قد جعل ربك تحتك سريا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وهزي إليك بجذع النخلة تساقط عليك رطبا جنيا",
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فكلي واشربي وقري عينا ۖ فإما ترين من البشر أحدا فقولي إني نذرت للرحمن صوما فلن أكلم اليوم إنسيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 27,
      "text": "فأتت به قومها تحمله ۖ قالوا يا مريم لقد جئت شيئا فريا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 28,
      "text": "يا أخت هارون ما كان أبوك امرأ سوء وما كانت أمك بغيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 29,
      "text": "فأشارت إليه ۖ قالوا كيف نكلم من كان في المهد صبيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 30,
      "text": "قال إني عبد الله آتاني الكتاب وجعلني نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وجعلني مباركا أين ما كنت وأوصاني بالصلاة والزكاة ما دمت حيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وبرا بوالدتي ولم يجعلني جبارا شقيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 33,
      "text": "والسلام علي يوم ولدت ويوم أموت ويوم أبعث حيا",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 34,
      "text": "ذلك عيسى ابن مريم ۚ قول الحق الذي فيه يمترون",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ما كان لله أن يتخذ من ولد ۖ سبحانه ۚ إذا قضى أمرا فإنما يقول له كن فيكون",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وإن الله ربي وربكم فاعبدوه ۚ هذا صراط مستقيم",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 37,
      "text": "فاختلف الأحزاب من بينهم ۖ فويل للذين كفروا من مشهد يوم عظيم",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 38,
      "text": "أسمع بهم وأبصر يوم يأتوننا ۖ لكن الظالمون اليوم في ضلال مبين",
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وأنذرهم يوم الحسرة إذ قضي الأمر وهم في غفلة وهم لا يؤمنون",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إنا نحن نرث الأرض ومن عليها وإلينا يرجعون",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 41,
      "text": "واذكر في الكتاب إبراهيم ۚ إنه كان صديقا نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 42,
      "text": "إذ قال لأبيه يا أبت لم تعبد ما لا يسمع ولا يبصر ولا يغني عنك شيئا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 43,
      "text": "يا أبت إني قد جاءني من العلم ما لم يأتك فاتبعني أهدك صراطا سويا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 44,
      "text": "يا أبت لا تعبد الشيطان ۖ إن الشيطان كان للرحمن عصيا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 45,
      "text": "يا أبت إني أخاف أن يمسك عذاب من الرحمن فتكون للشيطان وليا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 46,
      "text": "قال أراغب أنت عن آلهتي يا إبراهيم ۖ لئن لم تنته لأرجمنك ۖ واهجرني مليا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 47,
      "text": "قال سلام عليك ۖ سأستغفر لك ربي ۖ إنه كان بي حفيا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وأعتزلكم وما تدعون من دون الله وأدعو ربي عسى ألا أكون بدعاء ربي شقيا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 49,
      "text": "فلما اعتزلهم وما يعبدون من دون الله وهبنا له إسحاق ويعقوب ۖ وكلا جعلنا نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 50,
      "text": "ووهبنا لهم من رحمتنا وجعلنا لهم لسان صدق عليا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 51,
      "text": "واذكر في الكتاب موسى ۚ إنه كان مخلصا وكان رسولا نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 52,
      "text": "وناديناه من جانب الطور الأيمن وقربناه نجيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 53,
      "text": "ووهبنا له من رحمتنا أخاه هارون نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 54,
      "text": "واذكر في الكتاب إسماعيل ۚ إنه كان صادق الوعد وكان رسولا نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 55,
      "text": "وكان يأمر أهله بالصلاة والزكاة وكان عند ربه مرضيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 56,
      "text": "واذكر في الكتاب إدريس ۚ إنه كان صديقا نبيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 57,
      "text": "ورفعناه مكانا عليا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 58,
      "text": "أولئك الذين أنعم الله عليهم من النبيين من ذرية آدم وممن حملنا مع نوح ومن ذرية إبراهيم وإسرائيل وممن هدينا واجتبينا ۚ إذا تتلى عليهم آيات الرحمن خروا سجدا وبكيا ۩",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": {
        "id": 5,
        "recommended": true,
        "obligatory": false
      }
    },
    {
      "number": 59,
      "text": "۞ فخلف من بعدهم خلف أضاعوا الصلاة واتبعوا الشهوات ۖ فسوف يلقون غيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 60,
      "text": "إلا من تاب وآمن وعمل صالحا فأولئك يدخلون الجنة ولا يظلمون شيئا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 61,
      "text": "جنات عدن التي وعد الرحمن عباده بالغيب ۚ إنه كان وعده مأتيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 62,
      "text": "لا يسمعون فيها لغوا إلا سلاما ۖ ولهم رزقهم فيها بكرة وعشيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 63,
      "text": "تلك الجنة التي نورث من عبادنا من كان تقيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 64,
      "text": "وما نتنزل إلا بأمر ربك ۖ له ما بين أيدينا وما خلفنا وما بين ذلك ۚ وما كان ربك نسيا",
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 65,
      "text": "رب السماوات والأرض وما بينهما فاعبده واصطبر لعبادته ۚ هل تعلم له سميا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 66,
      "text": "ويقول الإنسان أإذا ما مت لسوف أخرج حيا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 67,
      "text": "أولا يذكر الإنسان أنا خلقناه من قبل ولم يك شيئا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 68,
      "text": "فوربك لنحشرنهم والشياطين ثم لنحضرنهم حول جهنم جثيا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 69,
      "text": "ثم لننزعن من كل شيعة أيهم أشد على الرحمن عتيا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 70,
      "text": "ثم لنحن أعلم بالذين هم أولى بها صليا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 71,
      "text": "وإن منكم إلا واردها ۚ كان على ربك حتما مقضيا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 72,
      "text": "ثم ننجي الذين اتقوا ونذر الظالمين فيها جثيا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 73,
      "text": "وإذا تتلى عليهم آياتنا بينات قال الذين كفروا للذين آمنوا أي الفريقين خير مقاما وأحسن نديا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 74,
      "text": "وكم أهلكنا قبلهم من قرن هم أحسن أثاثا ورئيا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 75,
      "text": "قل من كان في الضلالة فليمدد له الرحمن مدا ۚ حتى إذا رأوا ما يوعدون إما العذاب وإما الساعة فسيعلمون من هو شر مكانا وأضعف جندا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 76,
      "text": "ويزيد الله الذين اهتدوا هدى ۗ والباقيات الصالحات خير عند ربك ثوابا وخير مردا",
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 77,
      "text": "أفرأيت الذي كفر بآياتنا وقال لأوتين مالا وولدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 78,
      "text": "أطلع الغيب أم اتخذ عند الرحمن عهدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 79,
      "text": "كلا ۚ سنكتب ما يقول ونمد له من العذاب مدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 80,
      "text": "ونرثه ما يقول ويأتينا فردا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 81,
      "text": "واتخذوا من دون الله آلهة ليكونوا لهم عزا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 82,
      "text": "كلا ۚ سيكفرون بعبادتهم ويكونون عليهم ضدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 83,
      "text": "ألم تر أنا أرسلنا الشياطين على الكافرين تؤزهم أزا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 84,
      "text": "فلا تعجل عليهم ۖ إنما نعد لهم عدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 85,
      "text": "يوم نحشر المتقين إلى الرحمن وفدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 86,
      "text": "ونسوق المجرمين إلى جهنم وردا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 87,
      "text": "لا يملكون الشفاعة إلا من اتخذ عند الرحمن عهدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 88,
      "text": "وقالوا اتخذ الرحمن ولدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 89,
      "text": "لقد جئتم شيئا إدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 90,
      "text": "تكاد السماوات يتفطرن منه وتنشق الأرض وتخر الجبال هدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 91,
      "text": "أن دعوا للرحمن ولدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 92,
      "text": "وما ينبغي للرحمن أن يتخذ ولدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 93,
      "text": "إن كل من في السماوات والأرض إلا آتي الرحمن عبدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 94,
      "text": "لقد أحصاهم وعدهم عدا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 95,
      "text": "وكلهم آتيه يوم القيامة فردا",
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 96,
      "text": "إن الذين آمنوا وعملوا الصالحات سيجعل لهم الرحمن ودا",
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 97,
      "text": "فإنما يسرناه بلسانك لتبشر به المتقين وتنذر به قوما لدا",
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 98,
      "text": "وكم أهلكنا قبلهم من قرن هل تحس منهم من أحد أو تسمع لهم ركزا",
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    }
  ]
}