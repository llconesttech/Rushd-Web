{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم والضحى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "والليل إذا سجى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ما ودعك ربك وما قلى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وللآخرة خير لك من الأولى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ولسوف يعطيك ربك فترضى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ألم يجدك يتيما فآوى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "ووجدك ضالا فهدى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ووجدك عائلا فأغنى",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فأما اليتيم فلا تقهر",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وأما السائل فلا تنهر",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وأما بنعمة ربك فحدث",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}