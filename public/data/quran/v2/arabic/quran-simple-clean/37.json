{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم والصافات صفا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فالزاجرات زجرا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فالتاليات ذكرا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إن إلهكم لواحد",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 5,
      "text": "رب السماوات والأرض وما بينهما ورب المشارق",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 6,
      "text": "إنا زينا السماء الدنيا بزينة الكواكب",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وحفظا من كل شيطان مارد",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 8,
      "text": "لا يسمعون إلى الملإ الأعلى ويقذفون من كل جانب",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 9,
      "text": "دحورا ۖ ولهم عذاب واصب",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 10,
      "text": "إلا من خطف الخطفة فأتبعه شهاب ثاقب",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فاستفتهم أهم أشد خلقا أم من خلقنا ۚ إنا خلقناهم من طين لازب",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 12,
      "text": "بل عجبت ويسخرون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وإذا ذكروا لا يذكرون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وإذا رأوا آية يستسخرون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وقالوا إن هذا إلا سحر مبين",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 16,
      "text": "أإذا متنا وكنا ترابا وعظاما أإنا لمبعوثون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 17,
      "text": "أوآباؤنا الأولون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قل نعم وأنتم داخرون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فإنما هي زجرة واحدة فإذا هم ينظرون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وقالوا يا ويلنا هذا يوم الدين",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 21,
      "text": "هذا يوم الفصل الذي كنتم به تكذبون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 22,
      "text": "۞ احشروا الذين ظلموا وأزواجهم وما كانوا يعبدون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 23,
      "text": "من دون الله فاهدوهم إلى صراط الجحيم",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وقفوهم ۖ إنهم مسئولون",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 25,
      "text": "ما لكم لا تناصرون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 26,
      "text": "بل هم اليوم مستسلمون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وأقبل بعضهم على بعض يتساءلون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قالوا إنكم كنتم تأتوننا عن اليمين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قالوا بل لم تكونوا مؤمنين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وما كان لنا عليكم من سلطان ۖ بل كنتم قوما طاغين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فحق علينا قول ربنا ۖ إنا لذائقون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 32,
      "text": "فأغويناكم إنا كنا غاوين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 33,
      "text": "فإنهم يومئذ في العذاب مشتركون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 34,
      "text": "إنا كذلك نفعل بالمجرمين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 35,
      "text": "إنهم كانوا إذا قيل لهم لا إله إلا الله يستكبرون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 36,
      "text": "ويقولون أئنا لتاركو آلهتنا لشاعر مجنون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 37,
      "text": "بل جاء بالحق وصدق المرسلين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 38,
      "text": "إنكم لذائقو العذاب الأليم",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وما تجزون إلا ما كنتم تعملون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إلا عباد الله المخلصين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 41,
      "text": "أولئك لهم رزق معلوم",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فواكه ۖ وهم مكرمون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 43,
      "text": "في جنات النعيم",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 44,
      "text": "على سرر متقابلين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 45,
      "text": "يطاف عليهم بكأس من معين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 46,
      "text": "بيضاء لذة للشاربين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 47,
      "text": "لا فيها غول ولا هم عنها ينزفون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وعندهم قاصرات الطرف عين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 49,
      "text": "كأنهن بيض مكنون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فأقبل بعضهم على بعض يتساءلون",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 51,
      "text": "قال قائل منهم إني كان لي قرين",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 52,
      "text": "يقول أإنك لمن المصدقين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 53,
      "text": "أإذا متنا وكنا ترابا وعظاما أإنا لمدينون",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 54,
      "text": "قال هل أنتم مطلعون",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فاطلع فرآه في سواء الجحيم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 56,
      "text": "قال تالله إن كدت لتردين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 57,
      "text": "ولولا نعمة ربي لكنت من المحضرين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 58,
      "text": "أفما نحن بميتين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 59,
      "text": "إلا موتتنا الأولى وما نحن بمعذبين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 60,
      "text": "إن هذا لهو الفوز العظيم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 61,
      "text": "لمثل هذا فليعمل العاملون",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 62,
      "text": "أذلك خير نزلا أم شجرة الزقوم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 63,
      "text": "إنا جعلناها فتنة للظالمين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 64,
      "text": "إنها شجرة تخرج في أصل الجحيم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 65,
      "text": "طلعها كأنه رءوس الشياطين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 66,
      "text": "فإنهم لآكلون منها فمالئون منها البطون",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 67,
      "text": "ثم إن لهم عليها لشوبا من حميم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 68,
      "text": "ثم إن مرجعهم لإلى الجحيم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 69,
      "text": "إنهم ألفوا آباءهم ضالين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 70,
      "text": "فهم على آثارهم يهرعون",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 71,
      "text": "ولقد ضل قبلهم أكثر الأولين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 72,
      "text": "ولقد أرسلنا فيهم منذرين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 73,
      "text": "فانظر كيف كان عاقبة المنذرين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 74,
      "text": "إلا عباد الله المخلصين",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 75,
      "text": "ولقد نادانا نوح فلنعم المجيبون",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 76,
      "text": "ونجيناه وأهله من الكرب العظيم",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 77,
      "text": "وجعلنا ذريته هم الباقين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 78,
      "text": "وتركنا عليه في الآخرين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 79,
      "text": "سلام على نوح في العالمين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 80,
      "text": "إنا كذلك نجزي المحسنين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 81,
      "text": "إنه من عبادنا المؤمنين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 82,
      "text": "ثم أغرقنا الآخرين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 83,
      "text": "۞ وإن من شيعته لإبراهيم",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 84,
      "text": "إذ جاء ربه بقلب سليم",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 85,
      "text": "إذ قال لأبيه وقومه ماذا تعبدون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 86,
      "text": "أئفكا آلهة دون الله تريدون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 87,
      "text": "فما ظنكم برب العالمين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 88,
      "text": "فنظر نظرة في النجوم",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 89,
      "text": "فقال إني سقيم",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 90,
      "text": "فتولوا عنه مدبرين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 91,
      "text": "فراغ إلى آلهتهم فقال ألا تأكلون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 92,
      "text": "ما لكم لا تنطقون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 93,
      "text": "فراغ عليهم ضربا باليمين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 94,
      "text": "فأقبلوا إليه يزفون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 95,
      "text": "قال أتعبدون ما تنحتون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 96,
      "text": "والله خلقكم وما تعملون",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 97,
      "text": "قالوا ابنوا له بنيانا فألقوه في الجحيم",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 98,
      "text": "فأرادوا به كيدا فجعلناهم الأسفلين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 99,
      "text": "وقال إني ذاهب إلى ربي سيهدين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 100,
      "text": "رب هب لي من الصالحين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 101,
      "text": "فبشرناه بغلام حليم",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 102,
      "text": "فلما بلغ معه السعي قال يا بني إني أرى في المنام أني أذبحك فانظر ماذا ترى ۚ قال يا أبت افعل ما تؤمر ۖ ستجدني إن شاء الله من الصابرين",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 103,
      "text": "فلما أسلما وتله للجبين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 104,
      "text": "وناديناه أن يا إبراهيم",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 105,
      "text": "قد صدقت الرؤيا ۚ إنا كذلك نجزي المحسنين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 106,
      "text": "إن هذا لهو البلاء المبين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 107,
      "text": "وفديناه بذبح عظيم",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 108,
      "text": "وتركنا عليه في الآخرين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 109,
      "text": "سلام على إبراهيم",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 110,
      "text": "كذلك نجزي المحسنين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 111,
      "text": "إنه من عبادنا المؤمنين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 112,
      "text": "وبشرناه بإسحاق نبيا من الصالحين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 113,
      "text": "وباركنا عليه وعلى إسحاق ۚ ومن ذريتهما محسن وظالم لنفسه مبين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 114,
      "text": "ولقد مننا على موسى وهارون",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 115,
      "text": "ونجيناهما وقومهما من الكرب العظيم",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 116,
      "text": "ونصرناهم فكانوا هم الغالبين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 117,
      "text": "وآتيناهما الكتاب المستبين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 118,
      "text": "وهديناهما الصراط المستقيم",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 119,
      "text": "وتركنا عليهما في الآخرين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 120,
      "text": "سلام على موسى وهارون",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 121,
      "text": "إنا كذلك نجزي المحسنين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 122,
      "text": "إنهما من عبادنا المؤمنين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 123,
      "text": "وإن إلياس لمن المرسلين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 124,
      "text": "إذ قال لقومه ألا تتقون",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 125,
      "text": "أتدعون بعلا وتذرون أحسن الخالقين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 126,
      "text": "الله ربكم ورب آبائكم الأولين",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 127,
      "text": "فكذبوه فإنهم لمحضرون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 128,
      "text": "إلا عباد الله المخلصين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 129,
      "text": "وتركنا عليه في الآخرين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 130,
      "text": "سلام على إل ياسين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 131,
      "text": "إنا كذلك نجزي المحسنين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 132,
      "text": "إنه من عبادنا المؤمنين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 133,
      "text": "وإن لوطا لمن المرسلين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 134,
      "text": "إذ نجيناه وأهله أجمعين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 135,
      "text": "إلا عجوزا في الغابرين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 136,
      "text": "ثم دمرنا الآخرين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 137,
      "text": "وإنكم لتمرون عليهم مصبحين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 138,
      "text": "وبالليل ۗ أفلا تعقلون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 139,
      "text": "وإن يونس لمن المرسلين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 140,
      "text": "إذ أبق إلى الفلك المشحون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 141,
      "text": "فساهم فكان من المدحضين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 142,
      "text": "فالتقمه الحوت وهو مليم",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 143,
      "text": "فلولا أنه كان من المسبحين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 144,
      "text": "للبث في بطنه إلى يوم يبعثون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 145,
      "text": "۞ فنبذناه بالعراء وهو سقيم",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 146,
      "text": "وأنبتنا عليه شجرة من يقطين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 147,
      "text": "وأرسلناه إلى مائة ألف أو يزيدون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 148,
      "text": "فآمنوا فمتعناهم إلى حين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 149,
      "text": "فاستفتهم ألربك البنات ولهم البنون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 150,
      "text": "أم خلقنا الملائكة إناثا وهم شاهدون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 151,
      "text": "ألا إنهم من إفكهم ليقولون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 152,
      "text": "ولد الله وإنهم لكاذبون",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 153,
      "text": "أصطفى البنات على البنين",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 154,
      "text": "ما لكم كيف تحكمون",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 155,
      "text": "أفلا تذكرون",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 156,
      "text": "أم لكم سلطان مبين",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 157,
      "text": "فأتوا بكتابكم إن كنتم صادقين",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 158,
      "text": "وجعلوا بينه وبين الجنة نسبا ۚ ولقد علمت الجنة إنهم لمحضرون",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 159,
      "text": "سبحان الله عما يصفون",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 160,
      "text": "إلا عباد الله المخلصين",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 161,
      "text": "فإنكم وما تعبدون",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 162,
      "text": "ما أنتم عليه بفاتنين",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 163,
      "text": "إلا من هو صال الجحيم",
      "juz": 23,
      "manzil": 6,
      "page": 452,
