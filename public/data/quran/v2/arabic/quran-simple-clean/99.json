{
  "number": 99,
  "name": "سُورَةُ الزَّلۡزَلَةِ",
  "englishName": "Az-Zalzala",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم إذا زلزلت الأرض زلزالها",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وأخرجت الأرض أثقالها",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وقال الإنسان ما لها",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "يومئذ تحدث أخبارها",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بأن ربك أوحى لها",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يومئذ يصدر الناس أشتاتا ليروا أعمالهم",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فمن يعمل مثقال ذرة خيرا يره",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ومن يعمل مثقال ذرة شرا يره",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}