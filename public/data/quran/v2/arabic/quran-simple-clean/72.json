{
  "number": 72,
  "name": "سُورَةُ الجِنِّ",
  "englishName": "Al-Jinn",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم قل أوحي إلي أنه استمع نفر من الجن فقالوا إنا سمعنا قرآنا عجبا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 2,
      "text": "يهدي إلى الرشد فآمنا به ۖ ولن نشرك بربنا أحدا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وأنه تعالى جد ربنا ما اتخذ صاحبة ولا ولدا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وأنه كان يقول سفيهنا على الله شططا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وأنا ظننا أن لن تقول الإنس والجن على الله كذبا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وأنه كان رجال من الإنس يعوذون برجال من الجن فزادوهم رهقا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وأنهم ظنوا كما ظننتم أن لن يبعث الله أحدا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وأنا لمسنا السماء فوجدناها ملئت حرسا شديدا وشهبا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وأنا كنا نقعد منها مقاعد للسمع ۖ فمن يستمع الآن يجد له شهابا رصدا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وأنا لا ندري أشر أريد بمن في الأرض أم أراد بهم ربهم رشدا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وأنا منا الصالحون ومنا دون ذلك ۖ كنا طرائق قددا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وأنا ظننا أن لن نعجز الله في الأرض ولن نعجزه هربا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وأنا لما سمعنا الهدى آمنا به ۖ فمن يؤمن بربه فلا يخاف بخسا ولا رهقا",
      "juz": 29,
      "manzil": 7,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وأنا منا المسلمون ومنا القاسطون ۖ فمن أسلم فأولئك تحروا رشدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وأما القاسطون فكانوا لجهنم حطبا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وأن لو استقاموا على الطريقة لأسقيناهم ماء غدقا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 17,
      "text": "لنفتنهم فيه ۚ ومن يعرض عن ذكر ربه يسلكه عذابا صعدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وأن المساجد لله فلا تدعوا مع الله أحدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وأنه لما قام عبد الله يدعوه كادوا يكونون عليه لبدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 20,
      "text": "قل إنما أدعو ربي ولا أشرك به أحدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 21,
      "text": "قل إني لا أملك لكم ضرا ولا رشدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 22,
      "text": "قل إني لن يجيرني من الله أحد ولن أجد من دونه ملتحدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 23,
      "text": "إلا بلاغا من الله ورسالاته ۚ ومن يعص الله ورسوله فإن له نار جهنم خالدين فيها أبدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 24,
      "text": "حتى إذا رأوا ما يوعدون فسيعلمون من أضعف ناصرا وأقل عددا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 25,
      "text": "قل إن أدري أقريب ما توعدون أم يجعل له ربي أمدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 26,
      "text": "عالم الغيب فلا يظهر على غيبه أحدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إلا من ارتضى من رسول فإنه يسلك من بين يديه ومن خلفه رصدا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 28,
      "text": "ليعلم أن قد أبلغوا رسالات ربهم وأحاط بما لديهم وأحصى كل شيء عددا",
      "juz": 29,
      "manzil": 7,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    }
  ]
}