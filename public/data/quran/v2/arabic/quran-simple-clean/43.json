{
  "number": 43,
  "name": "سُورَةُ الزُّخۡرُفِ",
  "englishName": "Az-Zukhruf",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم حم",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 2,
      "text": "والكتاب المبين",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 3,
      "text": "إنا جعلناه قرآنا عربيا لعلكم تعقلون",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وإنه في أم الكتاب لدينا لعلي حكيم",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 5,
      "text": "أفنضرب عنكم الذكر صفحا أن كنتم قوما مسرفين",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وكم أرسلنا من نبي في الأولين",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وما يأتيهم من نبي إلا كانوا به يستهزئون",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فأهلكنا أشد منهم بطشا ومضى مثل الأولين",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 9,
      "text": "ولئن سألتهم من خلق السماوات والأرض ليقولن خلقهن العزيز العليم",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 10,
      "text": "الذي جعل لكم الأرض مهدا وجعل لكم فيها سبلا لعلكم تهتدون",
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 11,
      "text": "والذي نزل من السماء ماء بقدر فأنشرنا به بلدة ميتا ۚ كذلك تخرجون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 12,
      "text": "والذي خلق الأزواج كلها وجعل لكم من الفلك والأنعام ما تركبون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 13,
      "text": "لتستووا على ظهوره ثم تذكروا نعمة ربكم إذا استويتم عليه وتقولوا سبحان الذي سخر لنا هذا وما كنا له مقرنين",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وإنا إلى ربنا لمنقلبون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وجعلوا له من عباده جزءا ۚ إن الإنسان لكفور مبين",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 16,
      "text": "أم اتخذ مما يخلق بنات وأصفاكم بالبنين",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وإذا بشر أحدهم بما ضرب للرحمن مثلا ظل وجهه مسودا وهو كظيم",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 18,
      "text": "أومن ينشأ في الحلية وهو في الخصام غير مبين",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وجعلوا الملائكة الذين هم عباد الرحمن إناثا ۚ أشهدوا خلقهم ۚ ستكتب شهادتهم ويسألون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وقالوا لو شاء الرحمن ما عبدناهم ۗ ما لهم بذلك من علم ۖ إن هم إلا يخرصون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 21,
      "text": "أم آتيناهم كتابا من قبله فهم به مستمسكون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 22,
      "text": "بل قالوا إنا وجدنا آباءنا على أمة وإنا على آثارهم مهتدون",
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وكذلك ما أرسلنا من قبلك في قرية من نذير إلا قال مترفوها إنا وجدنا آباءنا على أمة وإنا على آثارهم مقتدون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 24,
      "text": "۞ قال أولو جئتكم بأهدى مما وجدتم عليه آباءكم ۖ قالوا إنا بما أرسلتم به كافرون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 425,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 25,
      "text": "فانتقمنا منهم ۖ فانظر كيف كان عاقبة المكذبين",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 425,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وإذ قال إبراهيم لأبيه وقومه إنني براء مما تعبدون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 27,
      "text": "إلا الذي فطرني فإنه سيهدين",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وجعلها كلمة باقية في عقبه لعلهم يرجعون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 29,
      "text": "بل متعت هؤلاء وآباءهم حتى جاءهم الحق ورسول مبين",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 30,
      "text": "ولما جاءهم الحق قالوا هذا سحر وإنا به كافرون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وقالوا لولا نزل هذا القرآن على رجل من القريتين عظيم",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 32,
      "text": "أهم يقسمون رحمت ربك ۚ نحن قسمنا بينهم معيشتهم في الحياة الدنيا ۚ ورفعنا بعضهم فوق بعض درجات ليتخذ بعضهم بعضا سخريا ۗ ورحمت ربك خير مما يجمعون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 33,
      "text": "ولولا أن يكون الناس أمة واحدة لجعلنا لمن يكفر بالرحمن لبيوتهم سقفا من فضة ومعارج عليها يظهرون",
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 34,
      "text": "ولبيوتهم أبوابا وسررا عليها يتكئون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 35,
      "text": "وزخرفا ۚ وإن كل ذلك لما متاع الحياة الدنيا ۚ والآخرة عند ربك للمتقين",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 36,
      "text": "ومن يعش عن ذكر الرحمن نقيض له شيطانا فهو له قرين",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وإنهم ليصدونهم عن السبيل ويحسبون أنهم مهتدون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 38,
      "text": "حتى إذا جاءنا قال يا ليت بيني وبينك بعد المشرقين فبئس القرين",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ولن ينفعكم اليوم إذ ظلمتم أنكم في العذاب مشتركون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 40,
      "text": "أفأنت تسمع الصم أو تهدي العمي ومن كان في ضلال مبين",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 41,
      "text": "فإما نذهبن بك فإنا منهم منتقمون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 42,
      "text": "أو نرينك الذي وعدناهم فإنا عليهم مقتدرون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 43,
      "text": "فاستمسك بالذي أوحي إليك ۖ إنك على صراط مستقيم",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وإنه لذكر لك ولقومك ۖ وسوف تسألون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 45,
      "text": "واسأل من أرسلنا من قبلك من رسلنا أجعلنا من دون الرحمن آلهة يعبدون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 46,
      "text": "ولقد أرسلنا موسى بآياتنا إلى فرعون وملئه فقال إني رسول رب العالمين",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 47,
      "text": "فلما جاءهم بآياتنا إذا هم منها يضحكون",
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وما نريهم من آية إلا هي أكبر من أختها ۖ وأخذناهم بالعذاب لعلهم يرجعون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وقالوا يا أيه الساحر ادع لنا ربك بما عهد عندك إننا لمهتدون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 50,
      "text": "فلما كشفنا عنهم العذاب إذا هم ينكثون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ونادى فرعون في قومه قال يا قوم أليس لي ملك مصر وهذه الأنهار تجري من تحتي ۖ أفلا تبصرون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 52,
      "text": "أم أنا خير من هذا الذي هو مهين ولا يكاد يبين",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فلولا ألقي عليه أسورة من ذهب أو جاء معه الملائكة مقترنين",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فاستخف قومه فأطاعوه ۚ إنهم كانوا قوما فاسقين",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فلما آسفونا انتقمنا منهم فأغرقناهم أجمعين",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 56,
      "text": "فجعلناهم سلفا ومثلا للآخرين",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 57,
      "text": "۞ ولما ضرب ابن مريم مثلا إذا قومك منه يصدون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 58,
      "text": "وقالوا أآلهتنا خير أم هو ۚ ما ضربوه لك إلا جدلا ۚ بل هم قوم خصمون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 59,
      "text": "إن هو إلا عبد أنعمنا عليه وجعلناه مثلا لبني إسرائيل",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 60,
      "text": "ولو نشاء لجعلنا منكم ملائكة في الأرض يخلفون",
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 61,
      "text": "وإنه لعلم للساعة فلا تمترن بها واتبعون ۚ هذا صراط مستقيم",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 62,
      "text": "ولا يصدنكم الشيطان ۖ إنه لكم عدو مبين",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 63,
      "text": "ولما جاء عيسى بالبينات قال قد جئتكم بالحكمة ولأبين لكم بعض الذي تختلفون فيه ۖ فاتقوا الله وأطيعون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 64,
      "text": "إن الله هو ربي وربكم فاعبدوه ۚ هذا صراط مستقيم",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 65,
      "text": "فاختلف الأحزاب من بينهم ۖ فويل للذين ظلموا من عذاب يوم أليم",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 66,
      "text": "هل ينظرون إلا الساعة أن تأتيهم بغتة وهم لا يشعرون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 67,
      "text": "الأخلاء يومئذ بعضهم لبعض عدو إلا المتقين",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 68,
      "text": "يا عباد لا خوف عليكم اليوم ولا أنتم تحزنون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 69,
      "text": "الذين آمنوا بآياتنا وكانوا مسلمين",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 70,
      "text": "ادخلوا الجنة أنتم وأزواجكم تحبرون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 71,
      "text": "يطاف عليهم بصحاف من ذهب وأكواب ۖ وفيها ما تشتهيه الأنفس وتلذ الأعين ۖ وأنتم فيها خالدون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 72,
      "text": "وتلك الجنة التي أورثتموها بما كنتم تعملون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 73,
      "text": "لكم فيها فاكهة كثيرة منها تأكلون",
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 74,
      "text": "إن المجرمين في عذاب جهنم خالدون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 75,
      "text": "لا يفتر عنهم وهم فيه مبلسون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وما ظلمناهم ولكن كانوا هم الظالمين",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 77,
      "text": "ونادوا يا مالك ليقض علينا ربك ۖ قال إنكم ماكثون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 78,
      "text": "لقد جئناكم بالحق ولكن أكثركم للحق كارهون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 79,
      "text": "أم أبرموا أمرا فإنا مبرمون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 80,
      "text": "أم يحسبون أنا لا نسمع سرهم ونجواهم ۚ بلى ورسلنا لديهم يكتبون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 81,
      "text": "قل إن كان للرحمن ولد فأنا أول العابدين",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 82,
      "text": "سبحان رب السماوات والأرض رب العرش عما يصفون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 83,
      "text": "فذرهم يخوضوا ويلعبوا حتى يلاقوا يومهم الذي يوعدون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 84,
      "text": "وهو الذي في السماء إله وفي الأرض إله ۚ وهو الحكيم العليم",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 85,
      "text": "وتبارك الذي له ملك السماوات والأرض وما بينهما وعنده علم الساعة وإليه ترجعون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 86,
      "text": "ولا يملك الذين يدعون من دونه الشفاعة إلا من شهد بالحق وهم يعلمون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 87,
      "text": "ولئن سألتهم من خلقهم ليقولن الله ۖ فأنى يؤفكون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 88,
      "text": "وقيله يا رب إن هؤلاء قوم لا يؤمنون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 89,
      "text": "فاصفح عنهم وقل سلام ۚ فسوف يعلمون",
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    }
  ]
}