{
  "number": 32,
  "name": "سُورَةُ السَّجۡدَةِ",
  "englishName": "As-Sajda",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم الم",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 2,
      "text": "تنزيل الكتاب لا ريب فيه من رب العالمين",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 3,
      "text": "أم يقولون افتراه ۚ بل هو الحق من ربك لتنذر قوما ما أتاهم من نذير من قبلك لعلهم يهتدون",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 4,
      "text": "الله الذي خلق السماوات والأرض وما بينهما في ستة أيام ثم استوى على العرش ۖ ما لكم من دونه من ولي ولا شفيع ۚ أفلا تتذكرون",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 5,
      "text": "يدبر الأمر من السماء إلى الأرض ثم يعرج إليه في يوم كان مقداره ألف سنة مما تعدون",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ذلك عالم الغيب والشهادة العزيز الرحيم",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 7,
      "text": "الذي أحسن كل شيء خلقه ۖ وبدأ خلق الإنسان من طين",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ثم جعل نسله من سلالة من ماء مهين",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 9,
      "text": "ثم سواه ونفخ فيه من روحه ۖ وجعل لكم السمع والأبصار والأفئدة ۚ قليلا ما تشكرون",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وقالوا أإذا ضللنا في الأرض أإنا لفي خلق جديد ۚ بل هم بلقاء ربهم كافرون",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 165,
      "sajda": false
    },
    {
      "number": 11,
      "text": "۞ قل يتوفاكم ملك الموت الذي وكل بكم ثم إلى ربكم ترجعون",
      "juz": 21,
      "manzil": 5,
      "page": 415,
      "ruku": 358,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 12,
      "text": "ولو ترى إذ المجرمون ناكسو رءوسهم عند ربهم ربنا أبصرنا وسمعنا فارجعنا نعمل صالحا إنا موقنون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ولو شئنا لآتينا كل نفس هداها ولكن حق القول مني لأملأن جهنم من الجنة والناس أجمعين",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فذوقوا بما نسيتم لقاء يومكم هذا إنا نسيناكم ۖ وذوقوا عذاب الخلد بما كنتم تعملون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 15,
      "text": "إنما يؤمن بآياتنا الذين إذا ذكروا بها خروا سجدا وسبحوا بحمد ربهم وهم لا يستكبرون ۩",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": {
        "id": 10,
        "recommended": false,
        "obligatory": true
      }
    },
    {
      "number": 16,
      "text": "تتجافى جنوبهم عن المضاجع يدعون ربهم خوفا وطمعا ومما رزقناهم ينفقون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فلا تعلم نفس ما أخفي لهم من قرة أعين جزاء بما كانوا يعملون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 18,
      "text": "أفمن كان مؤمنا كمن كان فاسقا ۚ لا يستوون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 19,
      "text": "أما الذين آمنوا وعملوا الصالحات فلهم جنات المأوى نزلا بما كانوا يعملون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وأما الذين فسقوا فمأواهم النار ۖ كلما أرادوا أن يخرجوا منها أعيدوا فيها وقيل لهم ذوقوا عذاب النار الذي كنتم به تكذبون",
      "juz": 21,
      "manzil": 5,
      "page": 416,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 21,
      "text": "ولنذيقنهم من العذاب الأدنى دون العذاب الأكبر لعلهم يرجعون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ومن أظلم ممن ذكر بآيات ربه ثم أعرض عنها ۚ إنا من المجرمين منتقمون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 359,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 23,
      "text": "ولقد آتينا موسى الكتاب فلا تكن في مرية من لقائه ۖ وجعلناه هدى لبني إسرائيل",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وجعلنا منهم أئمة يهدون بأمرنا لما صبروا ۖ وكانوا بآياتنا يوقنون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 25,
      "text": "إن ربك هو يفصل بينهم يوم القيامة فيما كانوا فيه يختلفون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 26,
      "text": "أولم يهد لهم كم أهلكنا من قبلهم من القرون يمشون في مساكنهم ۚ إن في ذلك لآيات ۖ أفلا يسمعون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 27,
      "text": "أولم يروا أنا نسوق الماء إلى الأرض الجرز فنخرج به زرعا تأكل منه أنعامهم وأنفسهم ۖ أفلا يبصرون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 28,
      "text": "ويقولون متى هذا الفتح إن كنتم صادقين",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قل يوم الفتح لا ينفع الذين كفروا إيمانهم ولا هم ينظرون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فأعرض عنهم وانتظر إنهم منتظرون",
      "juz": 21,
      "manzil": 5,
      "page": 417,
      "ruku": 360,
      "hizbQuarter": 166,
      "sajda": false
    }
  ]
}