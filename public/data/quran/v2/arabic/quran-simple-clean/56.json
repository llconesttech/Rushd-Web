{
  "number": 56,
  "name": "سُورَةُ الوَاقِعَةِ",
  "englishName": "Al-Waaqia",
  "ayahs": [
    {
      "number": 1,
      "text": "بسم الله الرحمن الرحيم إذا وقعت الواقعة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ليس لوقعتها كاذبة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 3,
      "text": "خافضة رافعة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إذا رجت الأرض رجا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وبست الجبال بسا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فكانت هباء منبثا",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وكنتم أزواجا ثلاثة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فأصحاب الميمنة ما أصحاب الميمنة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وأصحاب المشأمة ما أصحاب المشأمة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 10,
      "text": "والسابقون السابقون",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 11,
      "text": "أولئك المقربون",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 12,
      "text": "في جنات النعيم",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 13,
      "text": "ثلة من الأولين",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وقليل من الآخرين",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 15,
      "text": "على سرر موضونة",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 16,
      "text": "متكئين عليها متقابلين",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 17,
      "text": "يطوف عليهم ولدان مخلدون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 18,
      "text": "بأكواب وأباريق وكأس من معين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 19,
      "text": "لا يصدعون عنها ولا ينزفون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وفاكهة مما يتخيرون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 21,
      "text": "ولحم طير مما يشتهون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وحور عين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 23,
      "text": "كأمثال اللؤلؤ المكنون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 24,
      "text": "جزاء بما كانوا يعملون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 25,
      "text": "لا يسمعون فيها لغوا ولا تأثيما",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 26,
      "text": "إلا قيلا سلاما سلاما",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وأصحاب اليمين ما أصحاب اليمين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 28,
      "text": "في سدر مخضود",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وطلح منضود",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وظل ممدود",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 31,
      "text": "وماء مسكوب",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وفاكهة كثيرة",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 33,
      "text": "لا مقطوعة ولا ممنوعة",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وفرش مرفوعة",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 35,
      "text": "إنا أنشأناهن إنشاء",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فجعلناهن أبكارا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 37,
      "text": "عربا أترابا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 38,
      "text": "لأصحاب اليمين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 39,
      "text": "ثلة من الأولين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 40,
      "text": "وثلة من الآخرين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وأصحاب الشمال ما أصحاب الشمال",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 42,
      "text": "في سموم وحميم",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 43,
      "text": "وظل من يحموم",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 44,
      "text": "لا بارد ولا كريم",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 45,
      "text": "إنهم كانوا قبل ذلك مترفين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 46,
      "text": "وكانوا يصرون على الحنث العظيم",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 47,
      "text": "وكانوا يقولون أئذا متنا وكنا ترابا وعظاما أإنا لمبعوثون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 48,
      "text": "أوآباؤنا الأولون",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 49,
      "text": "قل إن الأولين والآخرين",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 50,
      "text": "لمجموعون إلى ميقات يوم معلوم",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ثم إنكم أيها الضالون المكذبون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 52,
      "text": "لآكلون من شجر من زقوم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 53,
      "text": "فمالئون منها البطون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 54,
      "text": "فشاربون عليه من الحميم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 55,
      "text": "فشاربون شرب الهيم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 56,
      "text": "هذا نزلهم يوم الدين",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 57,
      "text": "نحن خلقناكم فلولا تصدقون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 58,
      "text": "أفرأيتم ما تمنون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 59,
      "text": "أأنتم تخلقونه أم نحن الخالقون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 60,
      "text": "نحن قدرنا بينكم الموت وما نحن بمسبوقين",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 61,
      "text": "على أن نبدل أمثالكم وننشئكم في ما لا تعلمون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 62,
      "text": "ولقد علمتم النشأة الأولى فلولا تذكرون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 63,
      "text": "أفرأيتم ما تحرثون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 64,
      "text": "أأنتم تزرعونه أم نحن الزارعون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 65,
      "text": "لو نشاء لجعلناه حطاما فظلتم تفكهون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 66,
      "text": "إنا لمغرمون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 67,
      "text": "بل نحن محرومون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 68,
      "text": "أفرأيتم الماء الذي تشربون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 69,
      "text": "أأنتم أنزلتموه من المزن أم نحن المنزلون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 70,
      "text": "لو نشاء جعلناه أجاجا فلولا تشكرون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 71,
      "text": "أفرأيتم النار التي تورون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 72,
      "text": "أأنتم أنشأتم شجرتها أم نحن المنشئون",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 73,
      "text": "نحن جعلناها تذكرة ومتاعا للمقوين",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 74,
      "text": "فسبح باسم ربك العظيم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 75,
      "text": "۞ فلا أقسم بمواقع النجوم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 76,
      "text": "وإنه لقسم لو تعلمون عظيم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 77,
      "text": "إنه لقرآن كريم",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 78,
      "text": "في كتاب مكنون",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 79,
      "text": "لا يمسه إلا المطهرون",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 80,
      "text": "تنزيل من رب العالمين",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 81,
      "text": "أفبهذا الحديث أنتم مدهنون",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 82,
      "text": "وتجعلون رزقكم أنكم تكذبون",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 83,
      "text": "فلولا إذا بلغت الحلقوم",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 84,
      "text": "وأنتم حينئذ تنظرون",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
