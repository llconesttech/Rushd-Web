{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالسَّمَاءِ وَالطَّارِقِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَمَا أَدْرَاكَ مَا الطَّارِقُ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "النَّجْمُ الثَّاقِبُ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَلْيَنظُرِ الْإِنسَانُ مِمَّ خُلِقَ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "خُلِقَ مِن مَّاءٍ دَافِقٍ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "يَخْرُجُ مِن بَيْنِ الصُّلْبِ وَالتَّرَائِبِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "يَوْمَ تُبْلَى السَّرَائِرُ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَمَا لَهُ مِن قُوَّةٍ وَلَا نَاصِرٍ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَالْأَرْضِ ذَاتِ الصَّدْعِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إِنَّهُ لَقَوْلٌ فَصْلٌ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَمَا هُوَ بِالْهَزْلِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَأَكِيدُ كَيْدًا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}