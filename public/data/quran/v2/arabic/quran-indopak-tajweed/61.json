{
  "number": 61,
  "name": "سُورَةُ الصَّفِّ",
  "englishName": "As-Saff",
  "ayahs": [
    {
      "number": 1,
      "text": "سَبَّحَ لِلّٰهِ مَا فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَمَا فِی [h:44[ا]لْاَرْضِ ۚ وَهُوَ [h:7045[ا]لْعَزِیْزُ [h:681[ا]لْحَكِ[p[ی]ْمُ ۟",
      "juz": 28,
      "page": 551,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 2,
      "text": "یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] لِمَ تَقُوْلُوْنَ مَا لَا تَفْعَلُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 551,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 3,
      "text": "كَبُرَ مَ[q:11151[ق]ْتًا عِ[f:264[ن]ْ[f:264[د]َ [h:408[ا]للّٰهِ اَ[f:1330[ن]ْ [f:1330[ت]َقُوْلُوْ[s[ا] مَا لَا تَفْعَلُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 551,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِ[g[ن]َّ [h:80[ا]للّٰهَ یُحِبُّ [h:13562[ا]لَّذِیْنَ یُقَاتِلُوْنَ فِیْ سَبِیْلِهٖ صَفًّ[f:13563[ا] [f:13563[ك]َاَ[g[ن]َّهُ[c:13564[م]ْ [c:13564[ب]ُنْیَا[a:13565[ن]ٌ [a:13565[م]َّرْصُ[p[و]ْصٌ ۟",
      "juz": 28,
      "page": 551,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَاِذْ قَالَ مُوْسٰی لِقَوْمِهٖ یٰقَوْمِ لِمَ تُؤْذُوْنَنِیْ وَقَ[d:13566[د]ْ تَّعْلَمُوْنَ اَ[g[ن]ِّیْ رَسُوْلُ [h:3306[ا]للّٰهِ اِلَیْكُمْ ؕ فَلَ[g[م]َّا زَاغُ[o[و]ْۤ[s[ا] اَزَاغَ [h:13567[ا]للّٰهُ قُلُوْبَهُمْ ؕ وَ[h:72[ا]للّٰهُ لَا یَهْدِی [h:1504[ا]لْقَوْمَ [h:3860[ا]لْفٰسِقِ[p[ی]ْنَ ۟",
      "juz": 28,
      "page": 551,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَاِذْ قَالَ عِیْسَی [h:1916[ا][q:1917[ب]ْنُ مَرْیَمَ یٰبَنِ[o[ی]ْۤ اِسْرَ[o[ا]ٓءِیْلَ اِ[g[ن]ِّیْ رَسُوْلُ [h:3306[ا]للّٰهِ اِلَیْكُ[w:13568[م]ْ [w:13568[م]ُّصَدِّ[u:206[ق]ً[u:206[ا] [u:206[ل]ِّمَا بَیْنَ یَدَیَّ مِنَ [h:1941[ا][l[ل]تَّوْرٰىةِ وَمُبَشِّرً[i:13569[ا] [i:13569[ب]ِرَسُوْ[a:13570[ل]ٍ [a:13570[ی]َّاْتِیْ مِ[i:692[ن]ْ [i:692[ب]َعْدِی [h:13571[ا]سْمُهٗۤ اَحْمَدُ ؕ فَلَ[g[م]َّا جَ[o[ا]ٓءَهُ[c:13572[م]ْ [c:13572[ب]ِ[h:484[ا]لْبَیِّنٰتِ قَالُوْ[s[ا] هٰذَا سِحْ[a:3879[ر]ٌ [a:3879[م]ُّبِ[p[ی]ْنٌ ۟",
      "juz": 28,
      "page": 552,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَمَنْ اَظْلَمُ مِ[g[م]َّنِ [h:3980[ا]فْتَرٰی عَلَی [h:412[ا]للّٰهِ [h:2035[ا]لْكَذِبَ وَهُوَ یُ[q:13573[د]ْعٰۤ[o[ی] اِلَی [h:13574[ا]لْاِسْلَ[h:72[ا]مِ ؕ وَال[l[ل]ّٰهُ لَا [p[ی]َهْدِی الْقَوْمَ الظّٰلِمِیْنَ ۟ۚ",
      "juz": 28,
      "page": 552,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یُرِیْدُوْنَ لِیُ[q:13575[ط]ْفِـُٔوْ[s[ا] نُوْرَ [h:5554[ا]للّٰهِ بِاَفْوَ[n[ا]هِهِمْ ؕ وَ[h:72[ا]للّٰهُ مُتِ[g[م]ُّ نُوْرِهٖ وَلَوْ كَرِهَ [h:5556[ا]لْكٰفِرُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 552,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 9,
      "text": "هُوَ [h:153[ا]لَّذِ[o[ی]ْۤ اَرْسَلَ رَسُوْلَهٗ بِ[h:54[ا]لْهُدٰی وَدِیْنِ [h:5557[ا]لْحَقِّ لِیُظْهِرَهٗ عَلَی [h:5558[ا][l[ل]دِّیْنِ كُلِّهٖ وَلَوْ كَرِهَ [h:5559[ا]لْمُشْرِكُ[p[و]ْنَ ۟۠",
      "juz": 28,
      "page": 552,
      "ruku": 484,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 10,
      "text": "یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] هَلْ اَدُلُّكُمْ عَلٰی تِجَارَ[f:13576[ة]ٍ [f:13576[ت]ُ[f:13577[ن]ْ[f:13577[ج]ِیْكُ[w:13578[م]ْ [w:13578[م]ِّنْ عَذَابٍ اَلِ[p[ی]ْمٍ ۟",
      "juz": 28,
      "page": 552,
      "ruku": 485,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 11,
      "text": "تُؤْمِنُوْنَ بِ[h:32[ا]للّٰهِ وَرَسُوْلِهٖ وَتُجَاهِدُوْنَ فِیْ سَبِیْلِ [h:788[ا]للّٰهِ بِاَمْوَ[n[ا]لِكُمْ وَاَ[f:2485[ن]ْ[f:2485[ف]ُسِكُمْ ؕ ذٰلِكُمْ خَیْ[u:263[ر]ٌ [u:263[ل]َّكُمْ اِ[f:109[ن]ْ [f:109[ك]ُ[f:100[ن]ْ[f:100[ت]ُمْ تَعْلَمُ[p[و]ْنَ ۟ۙ",
      "juz": 28,
      "page": 552,
      "ruku": 485,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 12,
      "text": "یَغْفِرْ لَكُمْ ذُنُوْبَكُمْ وَیُ[q:13579[د]ْخِلْكُمْ جَ[g[ن]ّٰ[f:119[ت]ٍ [f:119[ت]َ[q:120[ج]ْرِیْ مِ[f:121[ن]ْ [f:121[ت]َحْتِهَا [h:122[ا]لْاَنْهٰرُ وَمَسٰكِنَ طَیِّبَ[f:5719[ة]ً [f:5719[ف]ِیْ جَ[g[ن]ّٰتِ عَ[q:5720[د]ْ[f:13580[ن]ٍ ؕ [f:13580[ذ]ٰلِكَ [h:3911[ا]لْفَوْزُ [h:2633[ا]لْعَظِ[p[ی]ْمُ ۟ۙ",
      "juz": 28,
      "page": 552,
      "ruku": 485,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاُخْرٰی تُحِبُّوْنَهَا ؕ نَصْ[a:13581[ر]ٌ [a:13581[م]ِّنَ [h:321[ا]للّٰهِ وَفَتْ[f:13582[ح]ٌ [f:13582[ق]َرِیْ[a:13583[ب]ٌ ؕ [a:13583[و]َبَشِّرِ [h:1257[ا]لْمُؤْمِنِ[p[ی]ْنَ ۟",
      "juz": 28,
      "page": 552,
      "ruku": 485,
      "hizbQuarter": 220,
      "sajda": false
    },
    {
      "number": 14,
      "text": "یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] كُوْنُ[o[و]ْۤ[s[ا] اَ[f:13584[ن]ْ[f:13584[ص]َارَ [h:13585[ا]للّٰهِ كَمَا قَالَ عِیْسَی [h:1916[ا][q:1917[ب]ْنُ مَرْیَمَ لِلْحَوَارِیّٖنَ مَنْ اَ[f:1949[ن]ْ[f:1949[ص]َارِ[o[ی]ْۤ اِلَی [h:1637[ا]للّٰهِ ؕ قَالَ [h:1950[ا]لْحَوَارِیُّوْنَ نَحْنُ اَ[f:1951[ن]ْ[f:1951[ص]َارُ [h:1952[ا]للّٰهِ فَاٰمَنَ[d:13586[ت]ْ طَّ[o[ا]ٓىِٕفَ[a:2015[ة]ٌ [a:2015[م]ِّ[i:1385[ن]ْ [i:1385[ب]َنِ[o[ی]ْۤ اِسْرَ[o[ا]ٓءِیْلَ وَكَفَرَ[d:13587[ت]ْ طَّ[o[ا]ٓىِٕفَ[f:13588[ة]ٌ ۚ [f:13588[ف]َاَیَّ[q:13589[د]ْنَا [h:13590[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] عَلٰی عَدُوِّهِمْ فَاَصْبَحُوْ[s[ا] ظٰهِرِ[p[ی]ْنَ ۟۠",
      "juz": 28,
      "page": 552,
      "ruku": 485,
