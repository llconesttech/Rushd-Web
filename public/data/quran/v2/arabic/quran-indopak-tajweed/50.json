  "ayahs": [
    {
      "number": 1,
      "text": "[m[ق]ٓ ۚ۫ وَ[h:5855[ا]لْقُرْاٰنِ [h:12775[ا]لْمَجِ[p[ی]ْ[q[د]ِ ۟ۚ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 2,
      "text": "بَلْ عَجِبُ[o[و]ْۤ[s[ا] اَ[f:11468[ن]ْ [f:11468[ج]َ[o[ا]ٓءَهُ[w:11469[م]ْ [w:11469[م]ُّ[f:6895[ن]ْ[f:6895[ذ]ِ[a:11470[ر]ٌ [a:11470[م]ِّنْهُمْ فَقَالَ [h:12776[ا]لْكٰفِرُوْنَ هٰذَا شَیْءٌ عَجِ[p[ی]ْ[q[ب]ٌ ۟ۚ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ءَاِذَا مِتْنَا وَكُنَّا تُرَا[i:12779[ب]ًا ۚ ذٰلِكَ رَجْعٌ بَعِیْدٌ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4,
      "text": "قَ[q:298[د]ْ عَلِمْنَا مَا تَ[f:12780[ن]ْ[f:12780[ق]ُصُ [h:12781[ا]لْاَرْضُ مِنْهُمْ ۚ وَعِ[f:12782[ن]ْ[f:12782[د]َنَا كِتٰبٌ حَفِ[p[ی]ْظٌ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بَلْ كَذَّبُوْ[s[ا] بِ[h:370[ا]لْحَقِّ لَ[g[م]َّا جَ[o[ا]ٓءَهُمْ فَهُمْ فِ[o[ی]ْۤ اَمْ[a:12783[ر]ٍ [a:12783[م]َّرِ[p[ی]ْ[q[ج]ٍ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اَفَلَمْ یَ[f:5165[ن]ْ[f:5165[ظ]ُرُ[o[و]ْۤ[s[ا] اِلَی [h:157[ا][l[ل]سَّمَ[o[ا]ٓءِ فَوْقَهُمْ كَیْفَ بَنَیْنٰهَا وَزَیَّ[g[ن]ّٰهَا وَمَا لَهَا مِ[f:12784[ن]ْ [f:12784[ف]ُرُ[p[و]ْ[q[ج]ٍ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَ[h:9999[ا]لْاَرْضَ مَدَ[q:7217[د]ْنٰهَا وَاَلْقَیْنَا فِیْهَا رَوَ[n[ا]سِیَ وَاَ[i:7218[ن]ْۢ[i:7218[ب]َتْنَا فِیْهَا مِ[f:843[ن]ْ [f:843[ك]ُلِّ زَوْ[i:8922[ج]ٍ [i:8922[ب]َهِ[p[ی]ْ[q[ج]ٍ ۟ۙ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 8,
      "text": "تَ[q:12785[ب]ْصِرَ[a:12786[ة]ً [a:12786[و]َّذِكْرٰی لِكُلِّ عَ[q:10938[ب]ْ[a:10939[د]ٍ [a:10939[م]ُّنِ[p[ی]ْ[q[ب]ٍ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَنَزَّلْنَا مِنَ [h:64[ا][l[ل]سَّمَ[o[ا]ٓءِ مَ[o[ا]ٓ[a:12787[ء]ً [a:12787[م]ُّبٰرَ[f:12788[ك]ً[f:12788[ا] [f:12788[ف]َاَ[i:9998[ن]ْۢ[i:9998[ب]َتْنَا بِهٖ جَ[g[ن]ّٰ[a:12789[ت]ٍ [a:12789[و]َّحَبَّ [h:12790[ا]لْحَصِ[p[ی]ْ[q[د]ِ ۟ۙ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَ[h:9999[ا][l[ل][g[ن]َّخْلَ بٰسِقٰ[u:12791[ت]ٍ [u:12791[ل]َّهَا طَلْ[a:12792[ع]ٌ [a:12792[ن]َّضِ[p[ی]ْ[q[د]ٌ ۟ۙ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 11,
      "text": "رِّزْ[u:12793[ق]ً[u:12793[ا] [u:12793[ل]ِّلْعِبَادِ ۙ وَاَحْیَیْنَا بِهٖ بَلْدَ[a:9661[ة]ً [a:9661[م]َّیْ[f:12206[ت]ً[f:12206[ا] ؕ [f:12206[ك]َذٰلِكَ [h:12794[ا]لْخُرُ[p[و]ْ[q[ج]ُ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 12,
      "text": "كَذَّبَتْ قَ[q:8394[ب]ْلَهُمْ قَوْمُ نُوْ[a:12795[ح]ٍ [a:12795[و]َّاَصْحٰبُ [h:12796[ا][l[ل]رَّسِّ وَثَمُ[p[و]ْ[q[د]ُ ۟ۙ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَعَا[a:11490[د]ٌ [a:11490[و]َّفِرْعَوْنُ وَاِخْوَ[n[ا]نُ لُ[p[و]ْ[q[ط]ٍ ۟ۙ",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّاَصْحٰبُ [h:12797[ا]لْاَیْكَةِ وَقَوْمُ تُبَّ[f:12798[ع]ٍ ؕ [f:12798[ك]ُ[f:12799[ل]ٌّ [f:12799[ك]َذَّبَ [h:11495[ا][l[ل]رُّسُلَ فَحَقَّ وَعِ[p[ی]ْ[q[د]ِ ۟",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اَفَعَیِیْنَا بِ[h:12800[ا]لْخَلْقِ [h:12801[ا]لْاَوَّلِ ؕ بَلْ هُمْ فِیْ لَ[q:12802[ب]ْ[a:12803[س]ٍ [a:12803[م]ِّنْ خَلْ[f:6887[ق]ٍ [f:6887[ج]َدِ[p[ی]ْ[q[د]ٍ ۟۠",
      "juz": 26,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَلَقَ[q:341[د]ْ خَلَ[q:5159[ق]ْنَا [h:7228[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ وَنَعْلَمُ مَ[h:12805[ا] تُوَسْوِسُ بِهٖ نَفْسُهٗ ۖۚ وَنَحْنُ اَقْرَبُ اِلَیْهِ مِنْ حَبْلِ الْوَرِیْدِ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِذْ یَتَلَقَّی [h:12806[ا]لْمُتَلَقِّیٰنِ عَنِ [h:7424[ا]لْیَمِیْنِ وَعَنِ [h:12807[ا][l[ل]شِّمَالِ قَعِ[p[ی]ْ[q[د]ٌ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 18,
      "text": "مَا یَلْفِظُ مِ[f:12808[ن]ْ [f:12808[ق]َوْلٍ اِلَّا لَدَیْهِ رَقِیْبٌ عَتِ[p[ی]ْ[q[د]ٌ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَجَ[o[ا]ٓءَتْ سَكْرَةُ [h:12809[ا]لْمَوْتِ بِ[h:370[ا]لْحَقِّ ؕ ذٰلِكَ مَا كُ[f:735[ن]ْ[f:735[ت]َ مِنْهُ تَحِ[p[ی]ْ[q[د]ُ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَنُفِخَ فِی [h:4185[ا][l[ل]صُّوْرِ ؕ ذٰلِكَ یَوْمُ [h:12810[ا]لْوَعِ[p[ی]ْ[q[د]ِ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَجَ[o[ا]ٓءَتْ كُلُّ نَفْ[a:12811[س]ٍ [a:12811[م]َّعَهَا سَ[o[ا]ٓىِٕ[a:12812[ق]ٌ [a:12812[و]َّشَهِ[p[ی]ْ[q[د]ٌ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لَقَ[q:2389[د]ْ كُ[f:735[ن]ْ[f:735[ت]َ فِیْ غَفْلَ[a:8863[ة]ٍ [a:8863[م]ِّنْ هٰذَا فَكَشَفْنَا عَ[f:626[ن]ْ[f:626[ك]َ غِطَ[o[ا]ٓءَكَ فَبَصَرُكَ [h:12813[ا]لْیَوْمَ حَدِ[p[ی]ْ[q[د]ٌ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَقَالَ قَرِیْنُهٗ هٰذَا مَا لَدَیَّ عَتِ[p[ی]ْ[q[د]ٌ ۟ؕ",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اَلْقِیَا فِیْ جَهَ[g[ن]َّمَ كُلَّ كَفَّارٍ عَنِ[p[ی]ْ[q[د]ٍ ۟ۙ",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 25,
      "text": "مَّ[g[ن]َّا[u:12814[ع]ٍ [u:12814[ل]ِّلْخَیْرِ مُعْتَ[a:12815[د]ٍ [a:12815[م]ُّرِ[p[ی]ْ[q[ب]ِ ۟ۙ",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 26,
      "text": "لَّذِیْ جَعَلَ مَعَ [h:3975[ا]للّٰهِ اِلٰهًا اٰخَرَ فَاَلْقِیٰهُ فِی [h:10442[ا]لْعَذَابِ [h:12816[ا][l[ل]شَّدِ[p[ی]ْ[q[د]ِ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 27,
      "text": "قَالَ قَرِیْنُهٗ رَبَّنَا مَ[o[ا]ۤ اَ[q:12817[ط]ْغَیْتُهٗ وَلٰكِ[f:2006[ن]ْ [f:2006[ك]َانَ فِیْ ضَلٰ[i:7043[ل]ٍۢ [i:7043[ب]َعِ[p[ی]ْ[q[د]ٍ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قَالَ لَا تَخْتَصِمُوْ[s[ا] لَدَیَّ وَقَ[q:387[د]ْ قَدَّمْتُ اِلَیْكُ[c:12818[م]ْ [c:12818[ب]ِ[h:12819[ا]لْوَعِ[p[ی]ْ[q[د]ِ ۟",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 29,
      "text": "مَا یُبَدَّلُ [h:12820[ا]لْقَوْلُ لَدَیَّ وَمَ[o[ا]ۤ اَنَا بِظَلَّا[u:2466[م]ٍ [u:2466[ل]ِّلْعَبِ[p[ی]ْ[q[د]ِ ۟۠",
      "juz": 26,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 30,
