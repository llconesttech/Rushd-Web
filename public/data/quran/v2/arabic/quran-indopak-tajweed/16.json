  "name": "سُورَةُ النَّحۡلِ",
  "englishName": "An-Nahl",
  "ayahs": [
    {
      "number": 1,
      "text": "اَتٰۤ[o[ی] اَمْرُ [h:2832[ا]للّٰهِ فَلَا تَسْتَعْجِلُوْهُ ؕ سُ[q:608[ب]ْحٰنَهٗ وَتَعٰلٰی عَ[g[م]َّا یُشْرِكُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 267,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 2,
      "text": "یُنَزِّلُ [h:7321[ا]لْمَلٰٓىِٕكَةَ بِ[h:7322[ا][l[ل]رُّوْحِ مِنْ اَمْرِهٖ عَلٰی مَ[a:476[ن]ْ [a:476[ی]َّشَ[o[ا]ٓءُ مِنْ عِبَادِهٖۤ اَنْ اَ[f:7323[ن]ْ[f:7323[ذ]ِرُ[o[و]ْۤ[s[ا] اَ[g[ن]َّهٗ لَ[o[ا]ۤ اِلٰهَ اِلَّ[o[ا]ۤ اَنَا فَ[h:210[ا]تَّقُ[p[و]ْنِ ۟",
      "juz": 14,
      "page": 267,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 3,
      "text": "خَلَقَ [h:3913[ا][l[ل]سَّمٰوٰتِ وَ[h:1474[ا]لْاَرْضَ بِ[h:370[ا]لْحَقِّ ؕ تَعٰلٰی عَ[g[م]َّا یُشْرِكُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 267,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 4,
      "text": "خَلَقَ [h:7324[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ [a:7328[م]ِنْ نُّطْفَةٍ فَاِذَا هُوَ خَصِیْمٌ مُّبِیْنٌ ۟",
      "juz": 14,
      "page": 267,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَ[h:9999[ا]لْاَنْعَامَ خَلَقَهَا ۚ لَكُمْ فِیْهَا دِفْءٌ [p[و]َّمَنَافِعُ وَمِنْهَا تَاْكُلُوْنَ ۪۟",
      "juz": 14,
      "page": 267,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَلَكُمْ فِیْهَا جَمَالٌ حِیْنَ تُرِیْحُوْنَ وَحِیْنَ تَسْرَحُ[p[و]ْنَ ۪۟",
      "juz": 14,
      "page": 267,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَتَحْمِلُ اَثْقَالَكُمْ اِلٰی بَلَ[u:7330[د]ٍ [u:7330[ل]َّمْ تَكُوْنُوْ[s[ا] بٰلِغِیْهِ اِلَّا بِشِقِّ [h:7331[ا]لْاَ[f:7332[ن]ْ[f:7332[ف]ُسِ ؕ اِ[g[ن]َّ رَبَّكُمْ لَرَءُوْ[u:744[ف]ٌ [u:744[ر]َّحِ[p[ی]ْمٌ ۟ۙ",
      "juz": 14,
      "page": 268,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَّ[h:9999[ا]لْخَیْلَ وَ[h:7333[ا]لْبِغَالَ وَ[h:7334[ا]لْحَمِیْرَ لِتَرْكَبُوْهَا وَزِیْنَ[a:7335[ة]ً ؕ [a:7335[و]َیَخْلُقُ مَا لَا تَعْلَمُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 268,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَعَلَی [h:2207[ا]للّٰهِ قَصْدُ [h:7336[ا][l[ل]سَّبِیْلِ وَمِنْهَا جَ[o[ا]ٓىِٕ[a:7337[ر]ٌ ؕ [a:7337[و]َلَوْ شَ[o[ا]ٓءَ لَهَدٰ[p[ى]كُمْ اَجْمَعِیْنَ ۟۠",
      "juz": 14,
      "page": 268,
      "ruku": 224,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 10,
      "text": "هُوَ [h:153[ا]لَّذِ[o[ی]ْۤ اَ[f:471[ن]ْ[f:471[ز]َلَ مِنَ [h:64[ا][l[ل]سَّمَ[o[ا]ٓءِ مَ[o[ا]ٓ[u:7338[ء]ً [u:7338[ل]َّكُ[w:6234[م]ْ [w:6234[م]ِّنْهُ شَرَا[a:7339[ب]ٌ [a:7339[و]َّمِنْهُ شَجَ[f:7340[ر]ٌ [f:7340[ف]ِیْهِ تُسِیْمُ[p[و]ْنَ ۟",
      "juz": 14,
      "page": 268,
      "ruku": 225,
      "hizbQuarter": 107,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یُ[i:7341[ن]ْۢ[i:7341[ب]ِتُ لَكُ[c:2000[م]ْ [c:2000[ب]ِهِ [h:7342[ا][l[ل]زَّرْعَ وَ[h:4290[ا][l[ل]زَّیْتُوْنَ وَ[h:7343[ا][l[ل][g[ن]َّخِیْلَ وَ[h:7344[ا]لْاَعْنَ[h:1571[ا]بَ وَمِ[g[ن]ْ كُلِّ ا[u:7345[ل]ثَّ[a:6018[م]َرٰتِ ؕ اِنَّ فِیْ ذٰلِكَ لَاٰیَةً لِّقَوْمٍ یَّتَفَكَّرُوْنَ ۟",
