      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَشَاهِ[a:14404[د]ٍ [a:14404[و]َّمَشْهُ[p[و]ْ[q[د]ٍ ۟ؕ",
      "juz": 30,
