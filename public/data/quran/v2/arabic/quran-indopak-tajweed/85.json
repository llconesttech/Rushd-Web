  "ayahs": [
    {
      "number": 1,
      "text": "وَ[h:9999[ا][l[ل]سَّمَ[o[ا]ٓءِ ذَاتِ [h:14402[ا]لْبُرُ[p[و]ْ[q[ج]ِ ۟ۙ",
