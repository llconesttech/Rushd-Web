      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ا[l[ل][g[ن]َّارِ ذَاتِ [h:14406[ا]لْوَقُ[p[و]ْ[q[د]ِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِذْ هُمْ عَلَیْهَا قُعُ[p[و]ْ[q[د]ٌ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّهُمْ عَلٰی مَا یَفْعَلُوْنَ بِ[h:3560[ا]لْمُؤْمِنِیْنَ شُهُ[p[و]ْ[q[د]ٌ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَمَا نَقَمُوْ[s[ا] مِنْهُمْ اِلَّ[o[ا]ۤ اَ[a:386[ن]ْ [a:386[ی]ُّؤْمِنُوْ[s[ا] بِ[h:32[ا]للّٰهِ [h:14407[ا]لْعَزِیْزِ [h:7037[ا]لْحَمِ[p[ی]ْ[q[د]ِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "الَّذِیْ لَهٗ مُلْكُ [h:555[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ ؕ وَ[h:72[ا]للّٰهُ عَلٰی كُلِّ شَ[f:3903[ی]ْ[f:3903[ء]ٍ [f:3903[ش]َهِ[p[ی]ْ[q[د]ٌ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اِ[g[ن]َّ [h:24[ا]لَّذِیْنَ فَتَنُو[s[ا] [h:14408[ا]لْمُؤْمِنِیْنَ وَ[h:5718[ا]لْمُؤْمِنٰتِ ثُ[g[م]َّ لَمْ یَتُوْبُوْ[s[ا] فَلَهُمْ عَذَابُ جَهَ[g[ن]َّمَ وَلَهُمْ عَذَابُ [h:14409[ا]لْحَرِ[p[ی]ْ[q[ق]ِ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اِ[g[ن]َّ [h:24[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] وَعَمِلُو[s[ا] [h:118[ا][l[ل]صّٰلِحٰتِ لَهُمْ جَ[g[ن]ّٰ[f:1786[ت]ٌ [f:1786[ت]َ[q:120[ج]ْرِیْ مِ[f:121[ن]ْ [f:121[ت]َحْتِهَا [h:122[ا]لْاَنْهٰرُ ؔؕ۬ ذٰلِكَ [h:3911[ا]لْفَوْزُ [h:14410[ا]لْكَبِ[p[ی]ْرُ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِ[g[ن]َّ بَ[q:14411[ط]ْشَ رَبِّكَ لَشَدِ[p[ی]ْ[q[د]ٌ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِ[g[ن]َّهٗ هُوَ یُ[q:10320[ب]ْدِئُ وَیُعِ[p[ی]ْ[q[د]ُ ۟ۚ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَهُوَ [h:6227[ا]لْغَفُوْرُ [h:14412[ا]لْوَدُ[p[و]ْ[q[د]ُ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "ذُو [h:11807[ا]لْعَرْشِ [h:14413[ا]لْمَجِ[p[ی]ْ[q[د]ُ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَعَّا[u:6506[ل]ٌ [u:6506[ل]ِّمَا یُرِ[p[ی]ْ[q[د]ُ ۟ؕ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "هَلْ اَتٰىكَ حَ[q[د]ِیْثُ الْجُنُوْدِ ۟ۙ",
      "juz": 30,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
