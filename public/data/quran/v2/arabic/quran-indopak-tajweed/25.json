{
  "number": 25,
  "name": "سُورَةُ الفُرۡقَانِ",
  "englishName": "Al-Furqaan",
  "ayahs": [
    {
      "number": 1,
      "text": "تَبٰرَكَ [h:9556[ا]لَّذِیْ نَزَّلَ [h:9557[ا]لْفُرْقَانَ عَلٰی عَ[q:7944[ب]ْدِهٖ لِیَكُوْنَ لِلْعٰلَمِیْنَ نَذِیْرَا ۟ۙ",
      "juz": 18,
      "page": 359,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 2,
      "text": "لَّذِیْ لَهٗ مُلْكُ [h:555[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ وَلَمْ یَتَّخِذْ وَلَ[a:7939[د]ً[a:7939[ا] [a:7939[و]َّلَمْ یَكُ[u:2596[ن]ْ [u:2596[ل]َّهٗ شَرِیْ[f:7940[ك]ٌ [f:7940[ف]ِی [h:7941[ا]لْمُلْكِ وَخَلَقَ كُلَّ شَ[f:9558[ی]ْ[f:9558[ء]ٍ [f:9558[ف]َقَدَّرَهٗ تَ[q:9559[ق]ْدِیْرًا ۟",
      "juz": 18,
      "page": 359,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَ[h:9999[ا]تَّخَذُوْ[s[ا] مِ[f:3136[ن]ْ [f:3136[د]ُوْنِهٖۤ اٰلِهَ[u:9560[ة]ً [u:9560[ل]َّا یَخْلُقُوْنَ شَیْـًٔ[a:5188[ا] [a:5188[و]َّهُمْ یُخْلَقُوْنَ وَلَا یَمْلِكُوْنَ لِاَ[f:6929[ن]ْ[f:6929[ف]ُسِهِمْ ضَرًّ[a:3719[ا] [a:3719[و]َّلَا نَفْ[a:5175[ع]ً[a:5175[ا] [a:5175[و]َّلَا یَمْلِكُوْنَ مَوْ[a:9561[ت]ً[a:9561[ا] [a:9561[و]َّلَا حَیٰ[s[و][a:9562[ة]ً [a:9562[و]َّلَا نُشُوْرًا ۟",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَقَالَ [h:614[ا]لَّذِیْنَ كَفَرُ[o[و]ْۤ[s[ا] اِنْ هٰذَ[o[ا]ۤ اِلَّ[o[ا]ۤ اِفْكُ فْتَرٰىهُ وَاَعَانَهٗ عَلَیْهِ قَوْمٌ اٰخَرُوْنَ ۛۚ فَقَ[q:563[د]ْ جَ[o[ا]ٓءُوْ ظُلْ[a:9564[م]ً[a:9564[ا] [a:9564[و]َّزُوْرًا ۟ۚۛ",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَقَالُ[o[و]ْۤ[s[ا] اَسَ[h:3993[ا]طِیْرُ [h:9565[ا]لْاَوَّلِیْنَ اكْتَتَبَهَا فَهِیَ تُمْلٰی عَلَیْهِ بُكْرَةً وَّاَصِیْلًا ۟",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 6,
      "text": "قُلْ اَ[f:3300[ن]ْ[f:3300[ز]َلَهُ [h:9567[ا]لَّذِیْ یَعْلَمُ [h:8444[ا][l[ل]سِّرَّ فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ ؕ اِ[g[ن]َّهٗ كَانَ غَفُوْ[u:2701[ر]ً[u:2701[ا] [u:2701[ر]َّحِیْمًا ۟",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَقَالُوْ[s[ا] مَالِ هٰذَا [h:9568[ا][l[ل]رَّسُوْلِ یَاْكُلُ [h:9569[ا][l[ل]طَّعَامَ وَیَمْشِیْ فِی [h:9570[ا]لْاَسْوَاقِ ؕ لَوْلَ[o[ا]ۤ اُ[f:17[ن]ْ[f:17[ز]ِلَ اِلَیْهِ مَلَ[f:9571[ك]ٌ [f:9571[ف]َیَكُوْنَ مَعَهٗ نَذِیْرًا ۟ۙ",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اَوْ یُلْقٰۤ[o[ی] اِلَیْهِ كَ[f:6264[ن]ْ[f:6264[ز]ٌ اَوْ تَكُوْنُ لَهٗ جَ[g[ن]َّ[a:9572[ة]ٌ [a:9572[ی]َّاْكُلُ مِنْهَا ؕ وَقَالَ [h:9573[ا][l[ل]ظّٰلِمُوْنَ اِ[f:4511[ن]ْ [f:4511[ت]َتَّبِعُوْنَ اِلَّا رَجُلً[a:7781[ا] [a:7781[م]َّسْحُوْرًا ۟",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اُ[f:2837[ن]ْ[f:2837[ظ]ُرْ كَیْفَ ضَرَبُوْ[s[ا] لَكَ [h:7782[ا]لْاَمْثَ[s[ا]لَ فَضَلُّوْا فَلَا یَسْتَطِیْعُوْنَ سَبِیْلًا ۟۠",
      "juz": 18,
      "page": 360,
      "ruku": 310,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 10,
      "text": "تَبٰرَكَ [h:9556[ا]لَّذِ[o[ی]ْۤ اِ[f:364[ن]ْ [f:364[ش]َ[o[ا]ٓءَ جَعَلَ لَكَ خَیْ[a:8082[ر]ً[a:8082[ا] [a:8082[م]ِّ[f:2624[ن]ْ [f:2624[ذ]ٰلِكَ جَ[g[ن]ّٰ[f:119[ت]ٍ [f:119[ت]َ[q:120[ج]ْرِیْ مِ[f:121[ن]ْ [f:121[ت]َحْتِهَا [h:122[ا]لْاَنْهٰرُ ۙ وَیَ[q:9574[ج]ْعَلْ لَّكَ قُصُوْرًا ۟",
      "juz": 18,
      "page": 360,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 11,
      "text": "بَلْ كَذَّبُوْ[s[ا] بِ[h:9575[ا][l[ل]سَّاعَةِ ۫ وَاَعْتَ[q:2782[د]ْنَا لِمَ[f:9576[ن]ْ [f:9576[ك]َذَّبَ بِ[h:9575[ا][l[ل]سَّاعَةِ سَعِیْرًا ۟ۚ",
      "juz": 18,
      "page": 360,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِذَا رَاَتْهُ[w:9577[م]ْ [w:9577[م]ِّ[a:9578[ن]ْ [a:9578[م]َّكَا[i:9579[ن]ٍ [i:9579[ب]َعِیْ[f:9580[د]ٍ [f:9580[س]َمِعُوْ[s[ا] لَهَا تَغَیُّ[a:9581[ظ]ً[a:9581[ا] [a:9581[و]َّزَفِیْرًا ۟",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاِذَ[o[ا]ۤ اُلْقُوْ[s[ا] مِنْهَا مَكَا[f:9582[ن]ً[f:9582[ا] [f:9582[ض]َیِّ[a:9583[ق]ً[a:9583[ا] [a:9583[م]ُّقَرَّنِیْنَ دَعَوْ[s[ا] هُنَالِكَ ثُبُوْرًا ۟ؕ",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 14,
      "text": "لَا تَ[q:7934[د]ْعُو[s[ا] [h:9584[ا]لْیَوْمَ ثُبُوْ[a:9585[ر]ً[a:9585[ا] [a:9585[و]َّ[n[ا]حِ[a:9586[د]ً[a:9586[ا] [a:9586[و]َّ[h:105[ا][q:105[د]ْعُوْ[s[ا] ثُبُوْ[f:9587[ر]ً[f:9587[ا] [f:9587[ك]َثِیْرًا ۟",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 15,
      "text": "قُلْ اَذٰلِكَ خَیْرٌ اَمْ جَ[g[ن]َّةُ [h:9588[ا]لْخُلْدِ [h:9589[ا]لَّتِیْ وُعِدَ [h:7004[ا]لْمُتَّقُوْنَ ؕ كَانَتْ لَهُمْ جَزَ[o[ا]ٓ[a:9590[ء]ً [a:9590[و]َّمَصِیْرًا ۟",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 16,
      "text": "لَهُمْ فِیْهَا مَا یَشَ[o[ا]ٓءُوْنَ خٰلِدِیْنَ ؕ كَانَ عَلٰی رَبِّكَ وَعْ[a:9591[د]ً[a:9591[ا] [a:9591[م]َّسْـُٔوْلًا ۟",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَیَوْمَ یَحْشُرُهُمْ وَمَا یَعْبُدُوْنَ مِ[f:107[ن]ْ [f:107[د]ُوْنِ [h:108[ا]للّٰهِ فَیَقُوْلُ ءَاَ[f:716[ن]ْ[f:716[ت]ُمْ اَضْلَلْتُمْ عِبَادِیْ هٰۤؤُلَ[o[ا]ٓءِ اَمْ هُمْ ضَلُّو[s[ا] [h:9592[ا][l[ل]سَّبِ[p[ی]ْلَ ۟ؕ",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قَالُوْ[s[ا] سُ[q:168[ب]ْحٰنَكَ مَا كَانَ یَ[i:8425[ن]ْۢ[i:8425[ب]َغِیْ لَنَ[o[ا]ۤ اَ[a:8693[ن]ْ [a:8693[ن]َّتَّخِذَ مِ[f:7543[ن]ْ [f:7543[د]ُوْنِكَ مِنْ اَوْلِیَ[o[ا]ٓءَ وَلٰكِ[a:9593[ن]ْ [a:9593[م]َّتَّعْتَهُمْ وَاٰبَ[o[ا]ٓءَهُمْ حَتّٰی نَسُو[s[ا] [h:9594[ا][l[ل]ذِّكْرَ ۚ وَكَانُوْ[s[ا] قَوْمً[i:9595[ا] [i:9595[ب]ُوْرًا ۟",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَقَ[q:563[د]ْ كَذَّبُوْكُ[c:9596[م]ْ [c:9596[ب]ِمَا تَقُوْلُوْنَ ۙ فَمَا تَسْتَطِیْعُوْنَ صَرْ[a:9597[ف]ً[a:9597[ا] [a:9597[و]َّلَا نَصْ[a:9598[ر]ً[a:9598[ا] ۚ [a:9598[و]َمَ[a:9599[ن]ْ [a:9599[ی]َّظْلِ[w:9600[م]ْ [w:9600[م]ِّ[f:344[ن]ْ[f:344[ك]ُمْ نُذِ[q:8980[ق]ْهُ عَذَا[f:9601[ب]ً[f:9601[ا] [f:9601[ك]َبِیْرًا ۟",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَمَ[o[ا]ۤ اَرْسَلْنَا قَ[q:7849[ب]ْلَكَ مِنَ [h:4822[ا]لْمُرْسَلِیْنَ اِلَّ[o[ا]ۤ اِ[g[ن]َّهُمْ لَیَاْكُلُوْنَ [h:9602[ا][l[ل]طَّعَامَ وَیَمْشُوْنَ فِی [h:9570[ا]لْاَسْوَاقِ ؕ وَجَعَلْنَا بَعْضَكُمْ لِبَعْ[f:9603[ض]ٍ [f:9603[ف]ِتْنَةً ؕ اَتَصْبِرُوْنَ ۚ وَكَانَ رَبُّكَ بَصِیْرًا ۟۠",
      "juz": 18,
      "page": 361,
      "ruku": 311,
      "hizbQuarter": 144,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَقَالَ [h:614[ا]لَّذِیْنَ لَا یَرْجُوْنَ لِقَ[o[ا]ٓءَنَا لَوْلَ[o[ا]ۤ اُ[f:17[ن]ْ[f:17[ز]ِلَ عَلَیْنَا [h:9604[ا]لْمَلٰٓىِٕكَةُ اَوْ نَرٰی رَبَّنَا ؕ لَقَدِ [h:9605[ا]سْتَكْبَرُوْ[s[ا] فِ[o[ی]ْۤ اَ[f:568[ن]ْ[f:568[ف]ُسِهِمْ وَعَتَوْ عُتُوًّ[f:9606[ا] [f:9606[ك]َبِیْرًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 22,
      "text": "یَوْمَ یَرَوْنَ [h:9607[ا]لْمَلٰٓىِٕكَةَ لَا بُشْرٰی یَوْمَىِٕ[u:9608[ذ]ٍ [u:9608[ل]ِّلْمُ[q:9609[ج]ْرِمِیْنَ وَیَقُوْلُوْنَ حِ[q:9610[ج]ْ[a:9611[ر]ً[a:9611[ا] [a:9611[م]َّحْجُوْرًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَقَدِمْنَ[o[ا]ۤ اِلٰی مَا عَمِلُوْ[s[ا] مِنْ عَمَ[f:9612[ل]ٍ [f:9612[ف]َجَعَلْنٰهُ هَبَ[o[ا]ٓ[a:9613[ء]ً [a:9613[م]َّ[f:9614[ن]ْ[f:9614[ث]ُوْرًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اَصْحٰبُ [h:415[ا]لْجَ[g[ن]َّةِ یَوْمَىِٕذٍ خَیْ[a:9615[ر]ٌ [a:9615[م]ُّسْتَقَرًّ[a:9616[ا] [a:9616[و]َّاَحْسَنُ مَقِیْلًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَیَوْمَ تَشَقَّقُ [h:9617[ا][l[ل]سَّمَ[o[ا]ٓءُ بِ[h:9618[ا]لْغَمَ[h:9619[ا]مِ وَ[f:7929[ن]ُ[f:7929[ز]ِّلَ الْمَلٰٓىِٕكَةُ تَنْزِیْلًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 26,
      "text": "اَلْمُلْكُ یَوْمَىِٕذِ لْحَقُّ لِلرَّحْمٰنِ ؕ وَكَانَ یَوْمًا عَلَی [h:468[ا]لْكٰفِرِیْنَ عَسِیْرًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَیَوْمَ یَعَضُّ [h:9620[ا][l[ل]ظَّالِمُ عَلٰی یَدَیْهِ یَقُوْلُ یٰلَیْتَنِی [h:9621[ا]تَّخَذْتُ مَعَ [h:9622[ا][l[ل]رَّسُوْلِ سَبِیْلًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 28,
      "text": "یٰوَیْلَتٰی لَیْتَنِیْ لَمْ اَتَّخِذْ فُلَانًا خَلِیْلًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 29,
      "text": "لَقَ[q:2389[د]ْ اَضَلَّنِیْ عَنِ [h:9623[ا][l[ل]ذِّكْرِ بَعْدَ اِذْ جَ[o[ا]ٓءَنِیْ ؕ وَكَانَ [h:7732[ا][l[ل]شَّیْطٰنُ لِلْاِ[f:6549[ن]ْ[f:6549[س]َانِ خَذُوْلًا ۟",
      "juz": 19,
      "page": 362,
      "ruku": 312,
      "hizbQuarter": 145,
      "sajda": false
    },
    {
      "number": 30,
