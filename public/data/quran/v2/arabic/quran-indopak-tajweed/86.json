{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "ayahs": [
    {
      "number": 1,
      "text": "وَ[h:9999[ا][l[ل]سَّمَ[o[ا]ٓءِ وَ[h:14418[ا][l[ل]طَّارِ[q[ق]ِ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَمَ[o[ا]ۤ اَ[q:13858[د]ْرٰىكَ مَا [h:14419[ا][l[ل]طَّارِ[q[ق]ُ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ا[l[ل][g[ن]َّ[q:14420[ج]ْمُ [h:14421[ا][l[ل]ثَّاقِ[q[ب]ُ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِ[f:8426[ن]ْ [f:8426[ك]ُلُّ نَفْ[u:14422[س]ٍ [u:14422[ل]َّ[g[م]َّا عَلَیْهَا حَافِظٌ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَلْیَ[f:14313[ن]ْ[f:14313[ظ]ُرِ [h:14314[ا]لْاِ[f:2732[ن]ْ[f:2732[س]َانُ مِ[g[م]َّ خُلِ[q[ق]َ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "خُلِقَ مِ[a:840[ن]ْ [a:840[م]َّ[o[ا]ٓ[f:14423[ء]ٍ [f:14423[د]َافِ[q[ق]ٍ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "یَّخْرُجُ مِ[i:4612[ن]ْ [i:4612[ب]َیْنِ [h:14424[ا][l[ل]صُّلْبِ وَ[h:14425[ا][l[ل]تَّرَ[o[ا]ٓىِٕ[q[ب]ِ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِ[g[ن]َّهٗ عَلٰی رَ[q:14426[ج]ْعِهٖ لَقَادِرٌ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یَوْمَ تُ[q:14427[ب]ْلَی [h:14428[ا][l[ل]سَّرَ[o[ا]ٓىِٕرُ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَمَا لَهٗ مِ[f:5395[ن]ْ [f:5395[ق]ُوَّ[a:14429[ة]ٍ [a:14429[و]َّلَا نَاصِرٍ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَ[h:9999[ا][l[ل]سَّمَ[o[ا]ٓءِ ذَاتِ [h:14430[ا][l[ل]رَّ[q:14431[ج]ْعِ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَ[h:9999[ا]لْاَرْضِ ذَاتِ [h:14432[ا][l[ل]صَّ[q:14433[د]ْعِ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِ[g[ن]َّهٗ لَقَوْ[f:14434[ل]ٌ [f:14434[ف]َصْلٌ ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّمَا هُوَ بِ[h:14435[ا]لْهَزْلِ ۟ؕ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِ[g[ن]َّهُمْ یَكِیْدُوْنَ كَیْدًا ۟ۙ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَّاَكِیْدُ كَیْدًا ۟ۚۖ",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَمَهِّلِ [h:14436[ا]لْكٰفِرِیْنَ اَمْهِلْهُمْ رُوَیْدًا ۟۠",
      "juz": 30,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
