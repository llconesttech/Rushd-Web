{
  "number": 70,
  "name": "سُورَةُ المَعَارِجِ",
  "englishName": "Al-Ma'aarij",
  "ayahs": [
    {
      "number": 1,
      "text": "سَاَلَ سَ[o[ا]ٓىِٕ[i:13912[ل]ٌۢ [i:13912[ب]ِعَذَا[a:13913[ب]ٍ [a:13913[و]َّاقِعٍ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 2,
      "text": "لِّلْكٰفِرِیْنَ لَیْسَ لَهٗ دَافِعٌ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 3,
      "text": "مِّنَ [h:321[ا]للّٰهِ ذِی [h:13914[ا]لْمَعَارِ[q[ج]ِ ۟ؕ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَعْرُجُ [h:13915[ا]لْمَلٰٓىِٕكَةُ وَ[h:13916[ا][l[ل]رُّوْحُ اِلَیْهِ فِیْ یَوْ[f:10625[م]ٍ [f:10625[ك]َانَ مِ[q:10626[ق]ْدَارُهٗ خَمْسِیْنَ اَلْفَ سَنَةٍ ۟ۚ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَ[h:9999[ا]صْبِرْ صَ[q:1427[ب]ْ[f:13917[ر]ً[f:13917[ا] [f:13917[ج]َمِیْلًا ۟",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اِ[g[ن]َّهُمْ یَرَوْنَهٗ بَعِیْدًا ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّنَرٰىهُ قَرِیْبًا ۟ؕ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یَوْمَ تَكُوْنُ [h:13918[ا][l[ل]سَّمَ[o[ا]ٓءُ كَ[h:8046[ا]لْمُهْلِ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَتَكُوْنُ [h:13919[ا]لْجِبَالُ كَ[h:13920[ا]لْعِهْنِ ۟ۙ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَا یَسْـَٔلُ حَمِیْمٌ حَمِیْمًا ۟ۚۖ",
      "juz": 29,
      "page": 568,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یُّبَصَّرُوْنَهُمْ ؕ یَوَدُّ [h:13921[ا]لْمُ[q:13922[ج]ْرِمُ لَوْ یَفْتَدِیْ مِنْ عَذَابِ یَوْمِىِٕ[i:13923[ذ]ٍ [i:13923[ب]ِبَنِ[p[ی]ْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَصَاحِبَتِهٖ وَاَخِیْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَفَصِیْلَتِهِ [h:13924[ا]لَّتِیْ تُـْٔوِ[p[ی]ْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَمَ[f:3453[ن]ْ [f:3453[ف]ِی [h:44[ا]لْاَرْضِ جَمِیْ[f:155[ع]ً[f:155[ا] ۙ [f:155[ث]ُ[g[م]َّ یُ[f:13925[ن]ْ[f:13925[ج]ِ[p[ی]ْهِ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلَّ[o[ا] ؕ اِ[g[ن]َّهَا لَظٰی ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 16,
      "text": "نَزَّاعَ[u:13926[ة]ً [u:13926[ل]ِّلشَّوٰی ۟ۚۖ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 17,
      "text": "تَ[q:7934[د]ْعُوْ[s[ا] مَنْ اَ[q:13927[د]ْبَرَ وَتَوَلّٰی ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَجَمَعَ فَاَوْعٰی ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اِ[g[ن]َّ [h:7137[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ خُلِقَ هَلُوْعًا ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اِذَا مَسَّهُ [h:7875[ا][l[ل]شَّرُّ جَزُوْعًا ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَّاِذَا مَسَّهُ [h:13928[ا]لْخَیْرُ مَنُوْعًا ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اِلَّا [h:13929[ا]لْمُصَلِّ[p[ی]ْنَ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 23,
      "text": "الَّذِیْنَ هُمْ عَلٰی صَلَاتِهِمْ دَ[o[ا]ٓىِٕمُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَ[h:9999[ا]لَّذِیْنَ فِ[o[ی]ْۤ اَمْوَ[n[ا]لِهِمْ حَ[a:13930[ق]ٌّ [a:13930[م]َّعْلُ[p[و]ْمٌ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 25,
      "text": "لِّلسَّ[o[ا]ٓىِٕلِ وَ[h:12851[ا]لْمَحْرُ[p[و]ْمِ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَ[h:9999[ا]لَّذِیْنَ یُصَدِّقُوْنَ بِیَوْمِ [h:13931[ا][l[ل]دِّ[p[ی]ْنِ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَ[h:9999[ا]لَّذِیْنَ هُ[w:9232[م]ْ [w:9232[م]ِّنْ عَذَابِ رَبِّهِ[w:9233[م]ْ [w:9233[م]ُّشْفِقُ[p[و]ْنَ ۟ۚ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اِ[g[ن]َّ عَذَابَ رَبِّهِمْ غَیْرُ مَاْمُ[p[و]ْنٍ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَ[h:9999[ا]لَّذِیْنَ هُمْ لِفُرُوْجِهِمْ حٰفِظُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اِلَّا عَلٰۤ[o[ی] اَزْوَ[n[ا]جِهِمْ اَوْ مَا مَلَكَتْ اَیْمَا[g[ن]ُهُمْ فَاِنَّهُمْ غَیْرُ مَلُوْمِیْنَ ۟ۚ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَمَنِ [h:9139[ا][q:9140[ب]ْتَغٰی وَرَ[o[ا]ٓءَ ذٰلِكَ فَاُ[s[و]لٰٓىِٕكَ هُمُ [h:9141[ا]لْعٰدُ[p[و]ْنَ ۟ۚ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَ[h:9999[ا]لَّذِیْنَ هُمْ لِاَمٰنٰتِهِمْ وَعَهْدِهِمْ رٰعُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 33,
      "text": "وَ[h:9999[ا]لَّذِیْنَ هُ[c:13932[م]ْ [c:13932[ب]ِشَهٰدٰتِهِمْ قَ[o[ا]ٓىِٕمُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَ[h:9999[ا]لَّذِیْنَ هُمْ عَلٰی صَلَاتِهِمْ یُحَافِظُ[p[و]ْنَ ۟ؕ",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اُ[s[و]لٰٓىِٕكَ فِیْ جَ[g[ن]ّٰ[a:13933[ت]ٍ [a:13933[م]ُّكْرَمُ[p[و]ْنَ ۟ؕ۠",
      "juz": 29,
      "page": 569,
      "ruku": 502,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 36,
      "text": "فَمَالِ [h:13934[ا]لَّذِیْنَ كَفَرُوْ[s[ا] قِبَلَكَ مُهْطِعِ[p[ی]ْنَ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 37,
      "text": "عَنِ [h:7424[ا]لْیَمِیْنِ وَعَنِ [h:12807[ا][l[ل]شِّمَالِ عِزِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اَیَ[q:13935[ط]ْمَعُ كُلُّ [h:12916[ا]مْرِئٍ [a:9346[م]ِّنْهُمْ اَ[a:13936[ن]ْ [a:13936[ی]ُّ[q:13937[د]ْخَلَ جَ[g[ن]َّةَ نَعِ[p[ی]ْمٍ ۟ۙ",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 39,
      "text": "كَلَّ[o[ا] ؕ اِ[g[ن]َّا خَلَ[q:11326[ق]ْنٰهُ[w:13938[م]ْ [w:13938[م]ِّ[g[م]َّا یَعْلَمُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 569,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 40,
      "text": "فَلَ[o[ا]ۤ اُ[q:13287[ق]ْسِمُ بِرَبِّ [h:13939[ا]لْمَشٰرِقِ وَ[h:13940[ا]لْمَغٰرِبِ اِ[g[ن]َّا لَقٰدِرُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 41,
      "text": "عَلٰۤ[o[ی] اَ[a:13265[ن]ْ [a:13265[ن]ُّبَدِّلَ خَیْ[a:12743[ر]ً[a:12743[ا] [a:12743[م]ِّنْهُمْ ۙ وَمَا نَحْنُ بِمَسْبُوْقِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 42,
      "text": "فَذَرْهُمْ یَخُوْضُوْ[s[ا] وَیَلْعَبُوْ[s[ا] حَتّٰی یُلٰقُوْ[s[ا] یَوْمَهُمُ [h:12321[ا]لَّذِیْ یُوْعَدُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 43,
      "text": "یَوْمَ یَخْرُجُوْنَ مِنَ [h:11261[ا]لْاَ[q:11262[ج]ْدَاثِ سِرَا[f:13941[ع]ً[f:13941[ا] [f:13941[ك]َاَ[g[ن]َّهُمْ اِلٰی نُصُ[a:13942[ب]ٍ [a:13942[ی]ُّوْفِضُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 44,
      "text": "خَاشِعَةً اَبْصَارُهُمْ [f:13943[ت]َرْهَقُهُمْ ذِلَّةٌ ؕ ذٰلِكَ [s[ا]لْیَوْمُ الَّذِیْ كَانُوْا یُوْعَدُوْنَ ۟۠",
      "juz": 29,
      "page": 570,
      "ruku": 503,
      "hizbQuarter": 228,
      "sajda": false
    }
  ]
