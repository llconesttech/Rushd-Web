  "ayahs": [
    {
      "number": 1,
      "text": "اِ[q:5169[ق]ْتَرَبَ لِل[g[ن]َّاسِ حِسَابُهُمْ وَهُمْ فِیْ غَفْلَ[a:8666[ة]ٍ [a:8666[م]ُّعْرِضُ[p[و]ْنَ ۟ۚ",
      "juz": 17,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَا یَاْتِیْهِ[w:7205[م]ْ [w:7205[م]ِّ[f:8667[ن]ْ [f:8667[ذ]ِكْ[a:8668[ر]ٍ [a:8668[م]ِّ[u:22[ن]ْ [u:22[ر]َّبِّهِ[w:8669[م]ْ [w:8669[م]ُّحْدَثٍ اِلَّا [h:8670[ا]سْتَمَعُوْهُ وَهُمْ یَلْعَبُ[p[و]ْنَ ۟ۙ",
      "juz": 17,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لَاهِیَ[f:8671[ة]ً [f:8671[ق]ُلُوْبُهُمْ ؕ وَاَسَرُّو[s[ا] [h:8518[ا][l[ل][g[ن]َّ[q:8519[ج]ْوَی ۖۗ [h:8672[ا]لَّذِیْنَ ظَلَمُوْ[s[ا] ۖۗ هَلْ هٰذَ[o[ا]ۤ اِلَّا بَشَ[a:7069[ر]ٌ [a:7069[م]ِّثْلُكُمْ ۚ اَفَتَاْتُوْنَ [h:8673[ا][l[ل]سِّحْرَ وَاَ[f:98[ن]ْ[f:98[ت]ُمْ تُ[q:8674[ب]ْصِرُ[p[و]ْنَ ۟",
      "juz": 17,
      "page": 322,
      "ruku": 278,
      "hizbQuarter": 129,
      "sajda": false
    },
    {
      "number": 4,
