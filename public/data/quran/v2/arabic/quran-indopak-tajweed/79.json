      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَّ[h:9999[ا][l[ل][g[ن]ّٰشِطٰتِ نَشْطًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّ[h:9999[ا][l[ل]سّٰبِحٰتِ سَ[q:14030[ب]ْحًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَ[h:9999[ا][l[ل]سّٰبِقٰتِ سَ[q:14265[ب]ْقًا ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَ[h:9999[ا]لْمُدَبِّرٰتِ اَمْرًا ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَوْمَ تَرْجُفُ [h:14266[ا][l[ل]رَّاجِفَةُ ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تَتْبَعُهَا [h:14267[ا][l[ل]رَّادِفَةُ ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "قُلُوْ[a:14268[ب]ٌ [a:14268[ی]َّوْمَىِٕ[a:14269[ذ]ٍ [a:14269[و]َّاجِفَةٌ ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اَ[q:14270[ب]ْصَارُهَا خَاشِعَةٌ ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "یَقُوْلُوْنَ ءَاِنَّ[h:14271[ا] لَمَرْدُوْدُوْنَ فِی الْحَافِرَةِ ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "ءَاِذَا كُ[a:14272[ن]َّا عِظَامًا نَّخِرَةً ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "قَالُوْ[s[ا] تِلْكَ اِ[f:14273[ذ]ً[f:14273[ا] [f:14273[ك]َرَّةٌ خَاسِرَةٌ ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَاِ[g[ن]َّمَا هِیَ زَ[q:11332[ج]ْرَ[a:11333[ة]ٌ [a:11333[و]َّ[n[ا]حِدَةٌ ۟ۙ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَاِذَا هُ[c:14274[م]ْ [c:14274[ب]ِ[h:14275[ا][l[ل]سَّاهِرَةِ ۟ؕ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ مُوْسٰی ۟ۘ",
      "juz": 30,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اِذْ نَادٰىهُ رَبُّهٗ بِ[h:8451[ا]لْوَادِ [h:8452[ا]لْمُقَدَّسِ طُوًی ۟ۚ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِذْهَ[q:7814[ب]ْ اِلٰی فِرْعَوْنَ اِ[g[ن]َّهٗ طَغٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَقُلْ هَلْ لَّكَ اِلٰۤ[o[ی] اَ[f:14276[ن]ْ [f:14276[ت]َزَكّٰی ۟ۙ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَاَهْدِیَكَ اِلٰی رَبِّكَ فَتَخْشٰی ۟ۚ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَاَرٰىهُ [h:14278[ا]لْاٰیَةَ الْكُبْرٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَكَذَّبَ وَعَصٰی ۟ؗۖ",
      "juz": 30,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ثُ[g[م]َّ اَ[q:13927[د]ْبَرَ یَسْعٰی ۟ؗۖ",
