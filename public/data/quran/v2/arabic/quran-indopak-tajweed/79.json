{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "ayahs": [
    {
      "number": 1,
      "text": "وَ[h:9999[ا][l[ل][g[ن]ّٰزِعٰتِ غَرْقًا ۟ۙ",
