{
  "number": 76,
  "name": "سُورَةُ الإِنسَانِ",
  "englishName": "Al-Insaan",
  "ayahs": [
    {
      "number": 1,
      "text": "هَلْ اَتٰی عَلَی [h:7873[ا]لْاِ[f:7874[ن]ْ[f:7874[س]َ[h:14154[ا][f:14155[ن]ِ حِیْنٌ [a:14156[م]ِّنَ الدَّهْرِ لَمْ یَكُنْ شَیْـًٔا مَّذْكُوْرًا ۟",
      "juz": 29,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اِ[g[ن]َّا خَلَ[q:5159[ق]ْنَا [h:7228[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َا[a:14157[ن]َ مِنْ نُّطْفَةٍ [i:2864[ا]َمْشَاجٍ ۖۗ نَّبْتَلِیْهِ فَجَعَلْنٰهُ سَمِیْعًا بَصِیْرًا ۟",
      "juz": 29,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اِ[g[ن]َّا هَدَیْنٰهُ [h:14159[ا][l[ل]سَّبِیْلَ اِ[g[م]َّا شَاكِ[a:14160[ر]ً[a:14160[ا] [a:14160[و]َّاِ[g[م]َّا كَفُوْرًا ۟",
      "juz": 29,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِ[g[ن]َّ[o[ا]ۤ اَعْتَ[q:2660[د]ْنَا لِلْكٰفِرِیْنَ سَلٰسِلَ[s[ا]ۡ وَاَغْلٰلً[a:14161[ا] [a:14161[و]َّسَعِیْرًا ۟",
      "juz": 29,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اِ[g[ن]َّ [h:14162[ا]لْاَ[q:14163[ب]ْرَارَ یَشْرَبُوْنَ مِ[f:14164[ن]ْ [f:14164[ك]َاْ[f:14165[س]ٍ [f:14165[ك]َانَ مِزَاجُهَا كَافُوْرًا ۟ۚ",
      "juz": 29,
      "page": 578,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "عَیْ[a:14166[ن]ً[a:14166[ا] [a:14166[ی]َّشْرَبُ بِهَا عِبَادُ [h:14167[ا]للّٰهِ یُفَجِّرُوْنَهَا تَفْجِیْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
      "text": "یُوْفُوْنَ بِ[h:14168[ا][l[ل][g[ن]َّذْرِ وَیَخَافُوْنَ یَوْ[f:14169[م]ً[f:14169[ا] [f:14169[ك]َانَ شَرُّهٗ مُسْتَطِیْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَیُ[q:14170[ط]ْعِمُوْنَ [h:14171[ا][l[ل]طَّعَامَ عَلٰی حُبِّهٖ مِسْكِیْ[a:14172[ن]ً[a:14172[ا] [a:14172[و]َّیَتِیْ[a:14173[م]ً[a:14173[ا] [a:14173[و]َّاَسِیْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اِ[g[ن]َّمَا نُ[q:14174[ط]ْعِمُكُمْ لِوَ[q:14175[ج]ْهِ [h:14176[ا]للّٰهِ لَا نُرِیْدُ مِ[f:344[ن]ْ[f:344[ك]ُمْ جَزَ[o[ا]ٓ[a:14177[ء]ً [a:14177[و]َّلَا شُكُوْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اِ[g[ن]َّا نَخَافُ مِ[u:10191[ن]ْ [u:10191[ر]َّبِّنَا یَوْمًا عَبُوْ[f:14178[س]ً[f:14178[ا] [f:14178[ق]َمْطَرِیْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَوَقٰىهُمُ [h:14179[ا]للّٰهُ شَرَّ ذٰلِكَ [h:14180[ا]لْیَوْمِ وَلَقّٰىهُمْ نَضْرَ[a:14181[ة]ً [a:14181[و]َّسُرُوْرًا ۟ۚ",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَجَزٰىهُ[c:14182[م]ْ [c:14182[ب]ِمَا صَبَرُوْ[s[ا] جَ[g[ن]َّ[a:14183[ة]ً [a:14183[و]َّحَرِیْرًا ۟ۙ",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 13,
      "text": "مُّتَّكِـِٕیْنَ فِیْهَا عَلَی [h:8058[ا]لْاَرَ[o[ا]ٓىِٕكِ ۚ لَا یَرَوْنَ فِیْهَا شَمْ[a:14184[س]ً[a:14184[ا] [a:14184[و]َّلَا زَمْهَرِیْرًا ۟ۚ",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَدَانِیَةً عَلَیْهِمْ ظِلٰلُهَا وَذُلِّلَتْ قُطُوْفُهَا تَذْلِیْلًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَیُطَافُ عَلَیْهِ[c:14185[م]ْ [c:14185[ب]ِاٰنِیَ[a:14186[ة]ٍ [a:14186[م]ِّ[f:12249[ن]ْ [f:12249[ف]ِضَّ[a:14187[ة]ٍ [a:14187[و]َّاَكْوَا[f:14188[ب]ٍ [f:14188[ك]َانَتْ قَوَارِیْرَاۡؔ ۟ۙ",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 16,
      "text": "قَوَارِیْرَ[s[ا]ۡؔ مِ[f:12249[ن]ْ [f:12249[ف]ِضَّ[f:14189[ة]ٍ [f:14189[ق]َدَّرُوْهَا تَ[q:9559[ق]ْدِیْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَیُسْقَوْنَ فِیْهَا كَاْ[f:14190[س]ً[f:14190[ا] [f:14190[ك]َانَ مِزَاجُهَا زَ[f:14191[ن]ْ[f:14191[ج]َبِیْلًا ۟ۚ",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 18,
      "text": "عَیْ[f:14192[ن]ً[f:14192[ا] [f:14192[ف]ِیْهَا تُسَ[g[م]ّٰی سَلْسَبِیْلًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَیَطُوْفُ عَلَیْهِمْ وِلْدَ[n[ا][a:13218[ن]ٌ [a:13218[م]ُّخَلَّدُوْنَ ۚ اِذَا رَاَیْتَهُمْ حَسِ[q:14193[ب]ْتَهُمْ لُؤْلُ[a:14194[ؤ]ً[a:14194[ا] [a:14194[م]َّ[f:9614[ن]ْ[f:9614[ث]ُوْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَاِذَا رَاَیْتَ ثَ[g[م]َّ رَاَیْتَ نَعِیْ[a:14195[م]ً[a:14195[ا] [a:14195[و]َّمُلْ[f:14196[ك]ً[f:14196[ا] [f:14196[ك]َبِیْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 21,
      "text": "عٰلِیَهُمْ ثِیَابُ سُ[f:8054[ن]ْ[f:8054[د]ُسٍ خُضْ[a:14197[ر]ٌ [a:14197[و]َّاِسْتَ[q:14198[ب]ْرَ[a:14199[ق]ٌ ؗ [a:14199[و]َّحُلُّ[o[و]ْۤ[s[ا] اَسَاوِرَ مِ[f:12249[ن]ْ [f:12249[ف]ِضَّ[a:14200[ة]ٍ ۚ [a:14200[و]َسَقٰىهُمْ رَبُّهُمْ شَرَا[f:14201[ب]ً[f:14201[ا] [f:14201[ط]َهُوْرًا ۟",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اِ[g[ن]َّ هٰذَا كَانَ لَكُمْ جَزَ[o[ا]ٓ[a:14202[ء]ً [a:14202[و]َّكَانَ سَعْیُكُ[w:14203[م]ْ [w:14203[م]َّشْكُوْرًا ۟۠",
      "juz": 29,
      "page": 579,
      "ruku": 514,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِ[g[ن]َّا نَحْنُ نَزَّلْنَا عَلَیْكَ [h:8434[ا]لْقُرْاٰنَ تَ[f:7929[ن]ْ[f:7929[ز]ِیْلًا ۟ۚ",
      "juz": 29,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَ[h:9999[ا]صْبِرْ لِحُكْمِ رَبِّكَ وَلَا تُطِعْ مِنْهُمْ اٰثِمًا اَوْ كَفُوْرًا ۟ۚ",
      "juz": 29,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَ[h:9999[ا]ذْكُرِ [h:14032[ا]سْمَ رَبِّكَ بُكْرَ[a:9566[ة]ً [a:9566[و]َّاَصِیْلًا ۟ۖۚ",
      "juz": 29,
      "page": 579,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَمِنَ [h:7856[ا]لَّیْلِ فَ[h:14204[ا]سْجُ[q:14204[د]ْ لَهٗ وَسَبِّحْهُ لَیْلً[f:14205[ا] [f:14205[ط]َوِیْلًا ۟",
      "juz": 29,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اِ[g[ن]َّ هٰۤؤُلَ[o[ا]ٓءِ یُحِبُّوْنَ [h:14206[ا]لْعَاجِلَةَ وَیَذَرُوْنَ وَرَ[o[ا]ٓءَهُمْ یَوْ[f:14207[م]ً[f:14207[ا] [f:14207[ث]َقِیْلًا ۟",
      "juz": 29,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 28,
      "text": "نَحْنُ خَلَ[q:11326[ق]ْنٰهُمْ وَشَدَ[q:11504[د]ْنَ[o[ا]ۤ اَسْرَهُمْ ۚ وَاِذَا شِئْنَا بَدَّلْنَ[o[ا]ۤ اَمْثَالَهُمْ تَبْدِیْلًا ۟",
      "juz": 29,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اِ[g[ن]َّ هٰذِهٖ تَذْكِرَ[f:14055[ة]ٌ ۚ [f:14055[ف]َمَ[f:8041[ن]ْ [f:8041[ش]َ[o[ا]ٓءَ [h:14056[ا]تَّخَذَ اِلٰی رَبِّهٖ سَبِیْلًا ۟",
      "juz": 29,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 30,
      "text": "وَمَا تَشَ[o[ا]ٓءُوْنَ اِلَّ[o[ا]ۤ اَ[a:4201[ن]ْ [a:4201[ی]َّشَ[o[ا]ٓءَ [h:4334[ا]للّٰهُ ؕ اِ[g[ن]َّ [h:80[ا]للّٰهَ كَانَ عَلِیْمًا حَكِیْمًا ۟ۗۖ",
      "juz": 29,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 31,
      "text": "یُّ[q:8947[د]ْخِلُ مَ[a:476[ن]ْ [a:476[ی]َّشَ[o[ا]ٓءُ فِیْ رَحْمَتِهٖ ؕ وَ[h:14208[ا][l[ل]ظّٰلِمِیْنَ اَعَدَّ لَهُمْ عَذَابًا اَلِیْمًا ۟۠",
      "juz": 29,
      "page": 580,
      "ruku": 515,
      "hizbQuarter": 232,
      "sajda": false
    }
  ]
}