{
  "number": 68,
  "name": "سُورَةُ القَلَمِ",
  "englishName": "Al-Qalam",
  "ayahs": [
    {
      "number": 1,
      "text": "[m[ن]ٓ وَ[h:13811[ا]لْقَلَمِ وَمَا یَسْطُرُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَ[o[ا]ۤ اَ[f:169[ن]ْ[f:169[ت]َ بِنِعْمَةِ رَبِّكَ بِمَ[q:13812[ج]ْنُ[p[و]ْنٍ ۟ۚ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِ[g[ن]َّ لَكَ لَاَ[q:4918[ج]ْرًا غَیْرَ مَمْنُ[p[و]ْنٍ ۟ۚ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاِ[g[ن]َّكَ لَعَلٰی خُلُقٍ عَظِ[p[ی]ْمٍ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَسَتُ[q:13813[ب]ْصِرُ وَیُ[q:13814[ب]ْصِرُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بِاَیِّىكُمُ [h:13815[ا]لْمَفْتُ[p[و]ْنُ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِ[g[ن]َّ رَبَّكَ هُوَ اَعْلَمُ بِمَ[f:7639[ن]ْ [f:7639[ض]َلَّ عَ[f:4361[ن]ْ [f:4361[س]َبِیْلِهٖ ۪ وَهُوَ اَعْلَمُ بِ[h:4362[ا]لْمُهْتَدِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَلَا تُطِعِ [h:13816[ا]لْمُكَذِّبِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَدُّوْ[s[ا] لَوْ تُ[q:13817[د]ْهِنُ فَیُ[q:13818[د]ْهِنُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَلَا تُطِعْ كُلَّ حَلَّا[a:13819[ف]ٍ [a:13819[م]َّهِ[p[ی]ْنٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 11,
      "text": "هَ[g[م]َّا[a:13820[ز]ٍ [a:13820[م]َّشَّ[o[ا]ٓ[i:13821[ء]ٍ [i:13821[ب]ِنَمِ[p[ی]ْمٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 12,
      "text": "مَّ[g[ن]َّا[u:12814[ع]ٍ [u:12814[ل]ِّلْخَیْرِ مُعْتَدٍ اَثِ[p[ی]ْمٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 13,
      "text": "عُتُلٍّ [i:13822[ب]َعْدَ ذٰلِكَ زَنِ[p[ی]ْمٍ ۟ۙ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اَ[f:13823[ن]ْ [f:13823[ك]َانَ ذَا مَا[a:9231[ل]ٍ [a:9231[و]َّبَنِ[p[ی]ْنَ ۟ؕ",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِذَا تُتْلٰی عَلَیْهِ اٰیٰتُنَا قَالَ اَسَ[h:3993[ا]طِ[p[ی]ْرُ الْاَوَّلِیْنَ ۟",
      "juz": 29,
      "page": 564,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 16,
      "text": "سَنَسِمُهٗ عَلَی [h:13824[ا]لْخُرْطُ[p[و]ْمِ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِ[g[ن]َّا بَلَوْنٰهُمْ كَمَا بَلَوْنَ[o[ا]ۤ اَصْحٰبَ [h:4715[ا]لْجَ[g[ن]َّةِ ۚ اِذْ اَ[q:3631[ق]ْسَمُوْ[s[ا] لَیَصْرِمُ[g[ن]َّهَا مُصْبِحِ[p[ی]ْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَلَا یَسْتَثْنُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَطَافَ عَلَیْهَا طَ[o[ا]ٓىِٕ[a:13825[ف]ٌ [a:13825[م]ِّ[u:770[ن]ْ [u:770[ر]َّبِّكَ وَهُمْ نَ[o[ا]ٓىِٕمُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَاَصْبَحَتْ كَ[h:13826[ا][l[ل]صَّرِ[p[ی]ْمِ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَتَنَادَوْ[s[ا] مُصْبِحِ[p[ی]ْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اَنِ [h:13827[ا]غْدُوْ[s[ا] عَلٰی حَرْثِكُمْ اِ[f:109[ن]ْ [f:109[ك]ُ[f:100[ن]ْ[f:100[ت]ُمْ صٰرِمِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 23,
      "text": "فَ[h:9999[ا][f:13828[ن]ْ[f:13828[ط]َلَقُوْ[s[ا] وَهُمْ یَتَخَافَتُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اَ[u:4908[ن]ْ [u:4908[ل]َّا یَ[q:13829[د]ْخُلَ[g[ن]َّهَا [h:13830[ا]لْیَوْمَ عَلَیْكُ[w:13831[م]ْ [w:13831[م]ِّسْكِ[p[ی]ْنٌ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَّغَدَوْ[s[ا] عَلٰی حَرْ[f:13832[د]ٍ [f:13832[ق]ٰدِرِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 26,
      "text": "فَلَ[g[م]َّا رَاَوْهَا قَالُ[o[و]ْۤ[s[ا] اِ[g[ن]َّا لَضَ[m[ا]ٓلُّ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 27,
      "text": "بَلْ نَحْنُ مَحْرُوْمُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 28,
      "text": "قَالَ اَوْسَطُهُمْ اَلَمْ اَقُلْ لَّكُمْ لَوْلَا تُسَبِّحُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 29,
      "text": "قَالُوْ[s[ا] سُ[q:7648[ب]ْحٰنَ رَبِّنَ[o[ا]ۤ اِ[g[ن]َّا كُ[g[ن]َّا ظٰلِمِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 30,
      "text": "فَاَ[q:11367[ق]ْبَلَ بَعْضُهُمْ عَلٰی بَعْ[a:13833[ض]ٍ [a:13833[ی]َّتَلَاوَمُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 31,
      "text": "قَالُوْ[s[ا] یٰوَیْلَنَ[o[ا]ۤ اِ[g[ن]َّا كُ[g[ن]َّا طٰغِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 32,
      "text": "عَسٰی رَبُّنَ[o[ا]ۤ اَ[a:13834[ن]ْ [a:13834[ی]ُّ[q:13835[ب]ْدِلَنَا خَیْ[a:8075[ر]ً[a:8075[ا] [a:8075[م]ِّنْهَ[o[ا]ۤ اِ[g[ن]َّ[o[ا]ۤ اِلٰی رَبِّنَا رٰغِبُ[p[و]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَذٰلِكَ [h:13836[ا]لْعَذَابُ ؕ وَلَعَذَابُ [h:7000[ا]لْاٰخِرَةِ اَكْبَرُ ۘ لَوْ كَانُوْ[s[ا] یَعْلَمُ[p[و]ْنَ ۟۠",
      "juz": 29,
      "page": 565,
      "ruku": 498,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اِ[g[ن]َّ لِلْمُتَّقِیْنَ عِ[f:264[ن]ْ[f:264[د]َ رَبِّهِمْ جَ[g[ن]ّٰتِ [h:3680[ا][l[ل][g[ن]َّعِ[p[ی]ْمِ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اَفَنَ[q:13837[ج]ْعَلُ [h:13838[ا]لْمُسْلِمِیْنَ كَ[h:13839[ا]لْمُ[q:13839[ج]ْرِمِ[p[ی]ْنَ ۟ؕ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 36,
      "text": "مَا لَكُمْ ۥ كَیْفَ تَحْكُمُ[p[و]ْنَ ۟ۚ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اَمْ لَكُمْ كِتٰ[f:13840[ب]ٌ [f:13840[ف]ِیْهِ تَ[q:2051[د]ْرُسُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 38,
