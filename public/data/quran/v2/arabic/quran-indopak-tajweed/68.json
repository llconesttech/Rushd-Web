      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اَمْ لَكُمْ اَیْمَانٌ عَلَیْنَ[h:1965[ا] بَالِغَةٌ اِلٰی یَ[p[و]ْمِ الْقِیٰمَةِ ۙ اِنَّ لَكُمْ لَمَا تَحْكُمُوْنَ ۟ۚ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 40,
      "text": "سَلْهُمْ اَیُّهُ[c:13841[م]ْ [c:13841[ب]ِذٰلِكَ زَعِ[p[ی]ْمٌ ۟ۚۛ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اَمْ لَهُمْ شُرَكَ[o[ا]ٓءُ ۛۚ فَلْیَاْتُوْ[s[ا] بِشُرَكَ[o[ا]ٓىِٕهِمْ اِ[f:5674[ن]ْ [f:5674[ك]َانُوْ[s[ا] صٰدِقِ[p[ی]ْنَ ۟",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 42,
      "text": "یَوْمَ یُكْشَفُ عَ[f:13842[ن]ْ [f:13842[س]َا[a:13843[ق]ٍ [a:13843[و]َّیُ[q:13844[د]ْعَوْنَ اِلَی [h:13845[ا][l[ل]سُّجُوْدِ فَلَا یَسْتَطِیْعُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 565,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 43,
      "text": "خَاشِعَةً اَبْصَارُهُمْ [a:13846[ت]َرْهَقُهُمْ ذِلَّةٌ ؕ وَقَ[q:1815[د]ْ كَانُوْا یُدْعَوْنَ اِلَی السُّجُوْدِ وَهُمْ سٰلِمُوْنَ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَذَرْنِیْ وَمَ[a:13847[ن]ْ [a:13847[ی]ُّكَذِّبُ بِهٰذَا [h:7957[ا]لْحَدِیْثِ ؕ سَنَسْتَ[q:5160[د]ْرِجُهُ[w:5161[م]ْ [w:5161[م]ِّنْ حَیْثُ لَا یَعْلَمُ[p[و]ْنَ ۟ۙ",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 45,
      "text": "وَاُمْلِیْ لَهُمْ ؕ اِ[g[ن]َّ كَیْدِیْ مَتِ[p[ی]ْنٌ ۟",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 46,
      "text": "اَمْ تَسْـَٔلُهُمْ اَ[q:2795[ج]ْ[f:12943[ر]ً[f:12943[ا] [f:12943[ف]َهُ[w:12944[م]ْ [w:12944[م]ِّ[a:12945[ن]ْ [a:12945[م]َّغْرَ[a:12946[م]ٍ [a:12946[م]ُّثْقَلُ[p[و]ْنَ ۟ۚ",
      "juz": 29,
      "page": 566,
      "ruku": 499,
      "hizbQuarter": 226,
      "sajda": false
    },
    {
      "number": 47,
      "text": "اَمْ عِ[f:3212[ن]ْ[f:3212[د]َهُمُ [h:12947[ا]لْغَیْبُ فَهُمْ یَكْتُبُ[p[و]ْنَ ۟",
