{
  "number": 72,
  "name": "سُورَةُ الجِنِّ",
  "englishName": "Al-Jinn",
  "ayahs": [
    {
      "number": 1,
      "text": "قُلْ اُوْحِیَ اِلَیَّ اَ[g[ن]َّهُ [h:13977[ا]سْتَمَعَ نَفَ[a:13978[ر]ٌ [a:13978[م]ِّنَ [h:4673[ا]لْجِ[g[ن]ِّ فَقَالُ[o[و]ْۤ[s[ا] اِ[g[ن]َّا سَمِعْنَا قُرْاٰنًا عَجَبًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 2,
      "text": "یَّهْدِ[o[ی]ْۤ اِلَی [h:13979[ا][l[ل]رُّشْدِ فَاٰمَ[g[ن]َّا بِهٖ ؕ وَلَ[a:13980[ن]ْ [a:13980[ن]ُّشْرِكَ بِرَبِّنَ[o[ا]ۤ اَحَدًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّاَ[g[ن]َّهٗ تَعٰلٰی جَدُّ رَبِّنَا مَا [h:9277[ا]تَّخَذَ صَاحِبَةً وَّلَا وَلَدًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَّاَ[g[ن]َّهٗ كَانَ یَقُوْلُ سَفِیْهُنَا عَلَی [h:412[ا]للّٰهِ شَطَطًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَّاَ[g[ن]َّا ظَنَ[g[ن]َّ[o[ا]ۤ اَ[u:8842[ن]ْ [u:8842[ل]َّ[f:13982[ن]ْ [f:13982[ت]َقُوْلَ [h:13983[ا]لْاِ[f:7884[ن]ْ[f:7884[س]ُ وَ[h:7885[ا]لْجِ[g[ن]ُّ عَلَی [h:412[ا]للّٰهِ كَذِبًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَّاَ[g[ن]َّهٗ كَانَ رِجَا[a:13984[ل]ٌ [a:13984[م]ِّنَ [h:4410[ا]لْاِ[f:4338[ن]ْ[f:4338[س]ِ یَعُوْذُوْنَ بِرِجَا[a:13985[ل]ٍ [a:13985[م]ِّنَ [h:4673[ا]لْجِ[g[ن]ِّ فَزَادُوْهُمْ رَهَقًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَّاَ[g[ن]َّهُمْ ظَ[g[ن]ُّوْ[s[ا] كَمَا ظَنَ[f:12002[ن]ْ[f:12002[ت]ُمْ اَ[u:8842[ن]ْ [u:8842[ل]َّ[a:11863[ن]ْ [a:11863[ی]َّ[q:4140[ب]ْعَثَ [h:11864[ا]للّٰهُ اَحَدًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَّاَ[g[ن]َّا لَمَسْنَا [h:13986[ا][l[ل]سَّمَ[o[ا]ٓءَ فَوَجَ[q:13987[د]ْنٰهَا مُلِئَتْ حَرَ[f:13988[س]ً[f:13988[ا] [f:13988[ش]َدِیْ[a:13989[د]ً[a:13989[ا] [a:13989[و]َّشُهُبًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَّاَ[g[ن]َّا كُ[g[ن]َّا نَ[q:13990[ق]ْعُدُ مِنْهَا مَقَاعِدَ لِلسَّمْعِ ؕ فَمَ[a:13991[ن]ْ [a:13991[ی]َّسْتَمِعِ [h:13992[ا]لْاٰنَ یَجِ[q:1067[د]ْ لَهٗ شِهَا[u:13993[ب]ً[u:13993[ا] [u:13993[ر]َّصَدًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَّاَ[g[ن]َّا لَا نَ[q:12439[د]ْرِ[o[ی]ْۤ اَشَرٌّ اُرِیْدَ بِمَ[f:7793[ن]ْ [f:7793[ف]ِی [h:44[ا]لْاَرْضِ اَمْ اَرَادَ بِهِمْ رَبُّهُمْ رَشَدًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَّاَ[g[ن]َّا مِ[g[ن]َّا [h:13994[ا][l[ل]صّٰلِحُوْنَ وَمِ[g[ن]َّا دُوْنَ ذٰلِكَ ؕ كُ[g[ن]َّا طَرَ[o[ا]ٓىِٕقَ قِدَدًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَّاَ[g[ن]َّا ظَنَ[g[ن]َّ[o[ا]ۤ اَ[u:8842[ن]ْ [u:8842[ل]َّ[a:13995[ن]ْ [a:13995[ن]ُّعْجِزَ [h:13996[ا]للّٰهَ فِی [h:44[ا]لْاَرْضِ وَلَ[a:13997[ن]ْ [a:13997[ن]ُّعْجِزَهٗ هَرَبًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَّاَ[g[ن]َّا لَ[g[م]َّا سَمِعْنَا [h:13998[ا]لْهُدٰۤ[o[ی] اٰمَ[g[ن]َّا بِهٖ ؕ فَمَ[a:13999[ن]ْ [a:13999[ی]ُّؤْمِ[i:14000[ن]ْ [i:14000[ب]ِرَبِّهٖ فَلَا یَخَافُ بَخْ[a:14001[س]ً[a:14001[ا] [a:14001[و]َّلَا رَهَقًا ۟ۙ",
      "juz": 29,
      "page": 572,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّاَ[g[ن]َّا مِ[g[ن]َّا [h:14002[ا]لْمُسْلِمُوْنَ وَمِ[g[ن]َّا [h:14003[ا]لْقٰسِطُوْنَ ؕ فَمَنْ اَسْلَمَ فَاُ[s[و]لٰٓىِٕكَ تَحَرَّوْ[s[ا] رَشَدًا ۟",
      "juz": 29,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَاَ[g[م]َّا [h:14004[ا]لْقٰسِطُوْنَ فَكَانُوْ[s[ا] لِجَهَ[g[ن]َّمَ حَطَبًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَّاَنْ لَّوِ [h:14005[ا]سْتَقَ[s[ا]مُوْ[h:14006[ا] عَ[l[ل]َی الطَّرِیْقَةِ لَاَسْقَیْنٰهُمْ مَّآءً غَدَقًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 17,
      "text": "لِّنَفْتِنَهُمْ فِیْهِ ؕ وَمَ[a:14008[ن]ْ [a:14008[ی]ُّعْرِضْ عَ[f:3773[ن]ْ [f:3773[ذ]ِكْرِ رَبِّهٖ یَسْلُكْهُ عَذَا[f:14009[ب]ً[f:14009[ا] [f:14009[ص]َعَدًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَّاَ[g[ن]َّ [h:14010[ا]لْمَسٰجِدَ لِلّٰهِ فَلَا تَ[q:7934[د]ْعُوْ[s[ا] مَعَ [h:3975[ا]للّٰهِ اَحَدًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَّاَ[g[ن]َّهٗ لَ[g[م]َّا قَامَ عَ[q:8319[ب]ْدُ [h:8320[ا]للّٰهِ یَ[q:14011[د]ْعُوْهُ كَادُوْ[s[ا] یَكُوْنُوْنَ عَلَیْهِ لِبَدًا ۟ؕ۠",
      "juz": 29,
      "page": 573,
      "ruku": 506,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 20,
      "text": "قُلْ اِ[g[ن]َّمَ[o[ا]ۤ اَ[q:6847[د]ْعُوْ[s[ا] رَبِّیْ وَلَ[o[ا]ۤ اُشْرِكُ بِهٖۤ اَحَدًا ۟",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 21,
      "text": "قُلْ اِ[g[ن]ِّیْ لَ[o[ا]ۤ اَمْلِكُ لَكُمْ ضَرًّ[a:3719[ا] [a:3719[و]َّلَا رَشَدًا ۟",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 22,
      "text": "قُلْ اِ[g[ن]ِّیْ لَ[a:14012[ن]ْ [a:14012[ی]ُّجِیْرَنِیْ مِنَ [h:321[ا]للّٰهِ اَحَ[a:14013[د]ٌ ۙ۬ [a:14013[و]َّلَنْ اَجِدَ مِ[f:3136[ن]ْ [f:3136[د]ُوْنِهٖ مُلْتَحَدًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِلَّا بَلٰ[a:14014[غ]ً[a:14014[ا] [a:14014[م]ِّنَ [h:321[ا]للّٰهِ وَرِسٰلٰتِهٖ ؕ وَمَ[a:2634[ن]ْ [a:2634[ی]َّعْصِ [h:2635[ا]للّٰهَ وَرَسُوْلَهٗ فَاِ[g[ن]َّ لَهٗ نَارَ جَهَ[g[ن]َّمَ خٰلِدِیْنَ فِیْهَ[o[ا]ۤ اَبَدًا ۟ؕ",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 24,
      "text": "حَتّٰۤ[o[ی] اِذَا رَاَوْ[s[ا] مَا یُوْعَدُوْنَ فَسَیَعْلَمُوْنَ مَنْ اَضْعَفُ نَاصِ[a:14015[ر]ً[a:14015[ا] [a:14015[و]َّاَقَلُّ عَدَدًا ۟",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 25,
      "text": "قُلْ اِنْ اَ[q:8888[د]ْرِ[o[ی]ْۤ اَقَرِیْ[a:14016[ب]ٌ [a:14016[م]َّا تُوْعَدُوْنَ اَمْ یَ[q:4388[ج]ْعَلُ لَهٗ رَبِّ[o[ی]ْۤ اَمَدًا ۟",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 26,
      "text": "عٰلِمُ [h:4186[ا]لْغَیْبِ فَلَا یُظْهِرُ عَلٰی غَیْبِهٖۤ اَحَدًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اِلَّا مَنِ [h:14017[ا]رْتَضٰی مِ[u:2882[ن]ْ [u:2882[ر]َّسُوْ[f:14018[ل]ٍ [f:14018[ف]َاِ[g[ن]َّهٗ یَسْلُكُ مِ[i:4612[ن]ْ [i:4612[ب]َیْنِ یَدَیْهِ وَمِنْ خَلْفِهٖ رَصَدًا ۟ۙ",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    },
    {
      "number": 28,
      "text": "لِّیَعْلَمَ اَ[f:3885[ن]ْ [f:3885[ق]َ[q:298[د]ْ اَ[q:14019[ب]ْلَغُوْ[s[ا] رِسٰلٰتِ رَبِّهِمْ وَاَحَاطَ بِمَا لَدَیْهِمْ وَاَحْصٰی كُلَّ شَیْءٍ عَدَدًا ۟۠",
      "juz": 29,
      "page": 573,
      "ruku": 507,
      "hizbQuarter": 229,
      "sajda": false
    }
  ]
}