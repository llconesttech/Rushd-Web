{
  "number": 63,
  "name": "سُورَةُ المُنَافِقُونَ",
  "englishName": "Al-Munaafiqoon",
  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا جَ[o[ا]ٓءَكَ [h:13611[ا]لْمُنٰفِقُوْنَ قَالُوْ[s[ا] نَشْهَدُ اِ[g[ن]َّكَ لَرَسُوْلُ [h:13612[ا]للّٰهِ ۘ وَ[h:72[ا]للّٰهُ یَعْلَمُ اِ[g[ن]َّكَ لَرَسُوْلُهٗ ؕ وَ[h:72[ا]للّٰهُ یَشْهَدُ اِ[g[ن]َّ [h:3224[ا]لْمُنٰفِقِیْنَ لَكٰذِبُ[p[و]ْنَ ۟ۚ",
      "juz": 28,
      "page": 554,
      "ruku": 488,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اِتَّخَذُ[o[و]ْۤ[s[ا] اَیْمَانَهُمْ جُ[g[ن]َّ[f:13424[ة]ً [f:13424[ف]َصَدُّوْ[s[ا] عَ[f:1203[ن]ْ [f:1203[س]َبِیْلِ [h:788[ا]للّٰهِ ؕ اِ[g[ن]َّهُمْ سَ[o[ا]ٓءَ مَا كَانُوْ[s[ا] یَعْمَلُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 554,
      "ruku": 488,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ذٰلِكَ بِاَ[g[ن]َّهُمْ اٰمَنُوْ[s[ا] ثُ[g[م]َّ كَفَرُوْ[s[ا] فَطُبِعَ عَلٰی قُلُوْبِهِمْ فَهُمْ لَا یَفْقَهُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 554,
      "ruku": 488,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاِذَا رَاَیْتَهُمْ تُعْجِبُكَ اَ[q:13613[ج]ْسَامُهُمْ ؕ وَاِ[a:13614[ن]ْ [a:13614[ی]َّقُوْلُوْ[s[ا] تَسْمَعْ لِقَوْلِهِمْ ؕ كَاَ[g[ن]َّهُمْ خُشُ[a:13615[ب]ٌ [a:13615[م]ُّسَ[g[ن]َّدَ[a:13616[ة]ٌ ؕ [a:13616[ی]َحْسَبُوْنَ كُلَّ صَیْحَةٍ عَلَیْهِمْ ؕ هُمُ [h:13617[ا]لْعَدُوُّ فَ[h:13618[ا]حْذَرْهُمْ ؕ قٰتَلَهُمُ [h:5548[ا]للّٰهُ ؗ اَ[g[ن]ّٰی یُؤْفَكُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 554,
      "ruku": 488,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَاِذَا قِیْلَ لَهُمْ تَعَالَوْ[s[ا] یَسْتَغْفِرْ لَكُمْ رَسُوْلُ [h:3306[ا]للّٰهِ لَوَّوْ[s[ا] رُءُوْسَهُمْ وَرَاَیْتَهُمْ یَصُدُّوْنَ وَهُ[w:7366[م]ْ [w:7366[م]ُّسْتَكْبِرُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 555,
      "ruku": 488,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 6,
      "text": "سَوَ[o[ا]ٓءٌ عَلَیْهِمْ اَسْتَغْفَرْتَ لَهُمْ اَمْ لَمْ تَسْتَغْفِرْ لَهُمْ ؕ لَ[a:13619[ن]ْ [a:13619[ی]َّغْفِرَ [h:5741[ا]للّٰهُ لَهُمْ ؕ اِ[g[ن]َّ [h:80[ا]للّٰهَ لَا یَهْدِی [h:1504[ا]لْقَوْمَ [h:3860[ا]لْفٰسِقِ[p[ی]ْنَ ۟",
      "juz": 28,
      "page": 555,
      "ruku": 488,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 7,
      "text": "هُمُ [h:12679[ا]لَّذِیْنَ یَقُوْلُوْنَ لَا تُ[f:1607[ن]ْ[f:1607[ف]ِقُوْ[s[ا] عَلٰی مَنْ عِ[f:264[ن]ْ[f:264[د]َ رَسُوْلِ [h:5744[ا]للّٰهِ حَتّٰی یَ[f:13620[ن]ْ[f:13620[ف]َضُّوْ[s[ا] ؕ وَلِلّٰهِ خَزَ[o[ا]ٓىِٕنُ [h:13621[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ وَلٰكِ[g[ن]َّ [h:13622[ا]لْمُنٰفِقِیْنَ لَا یَفْقَهُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 555,
      "ruku": 488,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یَقُوْلُوْنَ لَىِٕ[u:13623[ن]ْ [u:13623[ر]َّجَعْنَ[o[ا]ۤ اِلَی [h:8006[ا]لْمَدِیْنَةِ لَیُخْرِجَ[g[ن]َّ [h:13624[ا]لْاَعَزُّ مِنْهَا [h:13625[ا]لْاَذَلَّ ؕ وَلِلّٰهِ [h:13626[ا]لْعِزَّةُ وَلِرَسُوْلِهٖ وَلِلْمُؤْمِنِیْنَ وَلٰكِ[g[ن]َّ [h:13622[ا]لْمُنٰفِقِیْنَ لَا یَعْلَمُ[p[و]ْنَ ۟۠",
      "juz": 28,
      "page": 555,
      "ruku": 488,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ اٰمَنُوْ[s[ا] لَا تُلْهِكُمْ اَمْوَ[n[ا]لُكُمْ وَلَ[o[ا]ۤ اَوْلَ[h:3774[ا]دُكُمْ عَنْ ذِكْرِ اللّٰهِ ۚ [p[و]َمَنْ یَّفْعَلْ ذٰلِكَ فَاُولٰٓىِٕكَ هُمُ الْخٰسِرُوْنَ ۟",
      "juz": 28,
      "page": 555,
      "ruku": 489,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاَ[f:1045[ن]ْ[f:1045[ف]ِقُوْ[s[ا] مِ[a:10464[ن]ْ [a:10464[م]َّا رَزَ[q:275[ق]ْنٰكُ[w:1461[م]ْ [w:1461[م]ِّ[f:1346[ن]ْ [f:1346[ق]َ[q:1347[ب]ْلِ اَ[a:1462[ن]ْ [a:1462[ی]َّاْتِیَ اَحَدَكُمُ [h:942[ا]لْمَوْتُ فَیَقُوْلَ رَبِّ لَوْلَ[o[ا]ۤ اَخَّرْتَنِ[o[ی]ْۤ اِلٰۤ[o[ی] اَجَ[f:2940[ل]ٍ [f:2940[ق]َرِیْ[f:13627[ب]ٍ ۙ [f:13627[ف]َاَصَّدَّقَ وَاَكُ[a:6642[ن]ْ [a:6642[م]ِّنَ [h:1889[ا][l[ل]صّٰلِحِ[p[ی]ْنَ ۟",
      "juz": 28,
      "page": 555,
      "ruku": 489,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَلَ[a:13628[ن]ْ [a:13628[ی]ُّؤَخِّرَ [h:13629[ا]للّٰهُ نَفْسًا اِذَا جَ[o[ا]ٓءَ اَجَلُهَا ؕ وَ[h:72[ا]للّٰهُ خَبِیْ[i:2335[ر]ٌ [i:2335[ب]ِمَا تَعْمَلُ[p[و]ْنَ ۟۠",
      "juz": 28,
      "page": 555,
      "ruku": 489,
      "hizbQuarter": 222,
      "sajda": false
    }
  ]
