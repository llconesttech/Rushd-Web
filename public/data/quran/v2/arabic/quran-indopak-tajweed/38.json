      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَعَجِبُ[o[و]ْۤ[s[ا] اَ[f:11468[ن]ْ [f:11468[ج]َ[o[ا]ٓءَهُ[w:11469[م]ْ [w:11469[م]ُّ[f:6895[ن]ْ[f:6895[ذ]ِ[a:11470[ر]ٌ [a:11470[م]ِّنْهُمْ ؗ وَقَالَ [h:11471[ا]لْكٰفِرُوْنَ هٰذَا سٰحِ[f:11472[ر]ٌ [f:11472[ك]َذَّ[p[ا][q[ب]ٌ ۟ۖۚ",
      "juz": 23,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اَجَعَلَ [h:11473[ا]لْاٰلِهَةَ اِلٰ[a:693[ه]ً[a:693[ا] [a:693[و]َّ[n[ا]حِدًا ۖۚ اِ[g[ن]َّ هٰذَا لَشَیْءٌ عُجَ[p[ا][q[ب]ٌ ۟",
      "juz": 23,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَ[h:9999[ا][f:11474[ن]ْ[f:11474[ط]َلَقَ [h:11475[ا]لْمَلَاُ مِنْهُمْ اَنِ [h:11476[ا]مْشُوْ[s[ا] وَ[h:11477[ا]صْبِرُوْ[s[ا] عَلٰۤ[o[ی] اٰلِهَتِكُمْ ۖۚ اِ[g[ن]َّ هٰذَا لَشَ[a:11478[ی]ْ[a:11478[ء]ٌ [a:11478[ی]ُّرَ[p[ا][q[د]ُ ۟ۖۚ",
      "juz": 23,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 7,
      "text": "مَا سَمِعْنَا بِهٰذَا فِی [h:11479[ا]لْمِلَّةِ [h:11480[ا]لْاٰخِرَةِ ۖۚ اِنْ هٰذَ[o[ا]ۤ اِلَّا [h:11481[ا]خْتِلَا[q[ق]ٌ ۟ۖۚ",
      "juz": 23,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ءَ[h:7197[ا]ُ[i:4086[ن]ْزِلَ عَلَیْهِ ال[f:11483[ذ]ِّكْرُ [g[م]ِنْ بَیْنِنَ[s[ا] ؕ [q[ب]َلْ هُمْ فِیْ شَكٍّ مِّنْ ذِكْرِیْ ۚ بَلْ لَّمَّا یَذُوْقُوْا عَذَابِ ۟ؕ",
      "juz": 23,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اَمْ عِ[f:5060[ن]ْ[f:5060[د]َهُمْ خَزَ[o[ا]ٓىِٕنُ رَحْمَةِ رَبِّكَ [h:11484[ا]لْعَزِیْزِ [h:11485[ا]لْوَهَّ[p[ا][q[ب]ِ ۟ۚ",
      "juz": 23,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اَمْ لَهُ[w:11486[م]ْ [w:11486[م]ُّلْكُ [h:555[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ وَمَا بَیْنَهُمَا ۫ فَلْیَرْتَقُوْ[s[ا] فِی [h:11487[ا]لْاَسْبَا[q[ب]ِ ۟",
