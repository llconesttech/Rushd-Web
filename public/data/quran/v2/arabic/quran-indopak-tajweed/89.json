      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اَلَمْ تَرَ كَیْفَ فَعَلَ رَبُّكَ بِعَ[p[ا][q[د]ٍ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِرَمَ ذَاتِ [h:14472[ا]لْعِمَ[p[ا][q[د]ِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "الَّتِیْ لَمْ یُخْلَ[q:14473[ق]ْ مِثْلُهَا فِی [h:2519[ا]لْبِلَا[q[د]ِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَثَمُوْدَ [h:14474[ا]لَّذِیْنَ جَابُو[s[ا] [h:14475[ا][l[ل]صَّخْرَ بِ[h:8451[ا]لْوَ[p[ا][q[د]ِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَفِرْعَوْنَ ذِی [h:14476[ا]لْاَوْتَ[p[ا][q[د]ِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "الَّذِیْنَ طَغَوْ[s[ا] فِی [h:2519[ا]لْبِلَا[q[د]ِ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فَاَكْثَرُوْ[s[ا] فِیْهَا [h:14477[ا]لْفَسَ[p[ا][q[د]َ ۟",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَصَبَّ عَلَیْهِمْ رَبُّكَ سَوْطَ عَذَ[p[ا][q[ب]ٍ ۟ۚۙ",
      "juz": 30,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اِ[g[ن]َّ رَبَّكَ لَبِ[h:14478[ا]لْمِرْصَ[p[ا][q[د]ِ ۟ؕ",
