      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّ[h:9999[ا][l[ل]شَّفْعِ وَ[h:14469[ا]لْوَتْرِ ۟ۙ",
      "juz": 30,
