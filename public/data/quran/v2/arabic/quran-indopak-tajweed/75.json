      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلٰكِ[f:14146[ن]ْ [f:14146[ك]َذَّبَ وَتَوَلّٰى ۟ۙ",
      "juz": 29,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
