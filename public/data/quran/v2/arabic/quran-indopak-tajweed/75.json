{
  "number": 75,
  "name": "سُورَةُ القِيَامَةِ",
  "englishName": "Al-Qiyaama",
  "ayahs": [
    {
      "number": 1,
      "text": "لَ[o[ا]ۤ اُ[q:13287[ق]ْسِمُ بِیَوْمِ [h:14115[ا]لْقِیٰمَةِ ۟ۙ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلَ[o[ا]ۤ اُ[q:13287[ق]ْسِمُ بِ[h:3575[ا][l[ل][g[ن]َّفْسِ [h:14116[ا][l[ل]لَّوَّامَةِ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اَیَحْسَبُ [h:14117[ا]لْاِ[f:2732[ن]ْ[f:2732[س]َا[a:14118[ن]ُ اَلَّنْ نَّجْمَعَ عِظَامَهٗ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "بَلٰى قٰدِرِیْنَ عَلٰۤ[o[ی] اَ[a:14120[ن]ْ [a:14120[ن]ُّسَوِّیَ بَنَانَهٗ ۟",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بَلْ یُرِیْدُ [h:14121[ا]لْاِ[f:2732[ن]ْ[f:2732[س]َانُ لِیَفْجُرَ اَمَامَهٗ ۟ۚ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یَسْـَٔلُ اَیَّانَ یَوْمُ [h:14122[ا]لْقِیٰمَةِ ۟ؕ",
      "juz": 29,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
