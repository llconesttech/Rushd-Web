  "ayahs": [
    {
      "number": 1,
      "text": "اِذَا [h:14351[ا][l[ل]سَّمَ[o[ا]ٓءُ [h:14352[ا][f:14353[ن]ْ[f:14353[ف]َطَرَتْ ۟ۙ",
