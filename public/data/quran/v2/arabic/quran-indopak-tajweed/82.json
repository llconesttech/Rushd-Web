      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاِذَا [h:14354[ا]لْكَوَاكِبُ [h:14355[ا][f:14356[ن]ْ[f:14356[ت]َثَرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَاِذَا [h:14337[ا]لْبِحَارُ فُجِّرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَاِذَا [h:14357[ا]لْقُبُوْرُ بُعْثِرَتْ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلِمَتْ نَفْ[a:10644[س]ٌ [a:10644[م]َّا قَدَّمَتْ وَاَخَّرَتْ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 6,
      "text": "یٰۤاَیُّهَا [h:14358[ا]لْاِ[f:2732[ن]ْ[f:2732[س]َانُ مَ[h:14359[ا] غَرَّكَ بِرَبِّكَ الْكَرِیْمِ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 7,
      "text": "الَّذِیْ خَلَقَكَ فَسَوّٰىكَ فَعَدَلَكَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فِ[o[ی]ْۤ اَیِّ صُوْرَ[a:14360[ة]ٍ [a:14360[م]َّا شَ[o[ا]ٓءَ رَكَّبَكَ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كَلَّا بَلْ تُكَذِّبُوْنَ بِ[h:14361[ا][l[ل]دِّ[p[ی]ْنِ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاِ[g[ن]َّ عَلَیْكُمْ لَحٰفِظِ[p[ی]ْنَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كِرَا[f:14362[م]ً[f:14362[ا] [f:14362[ك]َاتِبِ[p[ی]ْنَ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 12,
      "text": "یَعْلَمُوْنَ مَا تَفْعَلُ[p[و]ْنَ ۟",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اِ[g[ن]َّ [h:14162[ا]لْاَ[q:14163[ب]ْرَارَ لَفِیْ نَعِ[p[ی]ْمٍ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَاِ[g[ن]َّ [h:14363[ا]لْفُجَّارَ لَفِیْ جَحِ[p[ی]ْمٍ ۟ۚۖ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 15,
      "text": "یَّصْلَوْنَهَا یَوْمَ [h:9781[ا][l[ل]دِّ[p[ی]ْنِ ۟",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَمَا هُمْ عَنْهَا بِغَ[o[ا]ٓىِٕبِ[p[ی]ْنَ ۟ؕ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَمَ[o[ا]ۤ اَ[q:13858[د]ْرٰىكَ مَ[h:11335[ا] [p[ی]َوْمُ الدِّیْنِ ۟ۙ",
      "juz": 30,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ثُ[g[م]َّ مَ[o[ا]ۤ اَ[q:13858[د]ْرٰىكَ مَ[h:11335[ا] [p[ی]َوْمُ الدِّیْنِ ۟ؕ",
