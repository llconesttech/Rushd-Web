{
  "number": 12,
  "name": "سُورَةُ يُوسُفَ",
  "englishName": "Yusuf",
  "ayahs": [
    {
      "number": 1,
      "text": "ا[m[ل]ٓرٰ ۫ تِلْكَ اٰیٰتُ [h:5932[ا]لْكِتٰبِ [h:6541[ا]لْمُبِ[p[ی]ْنِ ۟۫",
      "juz": 12,
      "page": 235,
      "ruku": 193,
      "hizbQuarter": 94,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اِ[g[ن]َّ[o[ا]ۤ اَ[f:4236[ن]ْ[f:4236[ز]َلْنٰهُ قُرْءٰنًا عَرَبِیًّ[u:6542[ا] [u:6542[ل]َّعَلَّكُمْ تَعْقِلُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 235,
      "ruku": 193,
      "hizbQuarter": 94,
      "sajda": false
    },
    {
      "number": 3,
      "text": "نَحْنُ نَقُصُّ عَلَیْكَ اَحْسَنَ [h:6543[ا]لْقَصَصِ بِمَ[o[ا]ۤ اَوْحَیْنَ[o[ا]ۤ اِلَیْكَ هٰذَا [h:6544[ا]لْقُرْاٰنَ ۖۗ وَاِ[f:6545[ن]ْ [f:6545[ك]ُ[f:735[ن]ْ[f:735[ت]َ مِ[f:1097[ن]ْ [f:1097[ق]َ[q:1098[ب]ْلِهٖ لَمِنَ [h:6546[ا]لْغٰفِلِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 235,
      "ruku": 193,
      "hizbQuarter": 94,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِذْ قَالَ یُوْسُفُ لِاَبِیْهِ یٰۤاَبَتِ اِ[g[ن]ِّیْ رَاَیْتُ اَحَدَ عَشَرَ كَوْكَ[a:6547[ب]ً[a:6547[ا] [a:6547[و]َّ[h:4264[ا][l[ل]شَّمْسَ وَ[h:4265[ا]لْقَمَرَ رَاَیْتُهُمْ لِیْ سٰجِدِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 235,
      "ruku": 193,
      "hizbQuarter": 94,
      "sajda": false
    },
    {
      "number": 5,
      "text": "قَالَ یٰبُنَیَّ لَا تَ[q:6548[ق]ْصُصْ رُءْیَاكَ عَلٰۤ[o[ی] اِخْوَتِكَ فَیَكِیْدُوْ[s[ا] لَكَ كَیْدًا ؕ اِ[g[ن]َّ [h:4634[ا][l[ل]شَّیْطٰنَ لِلْاِ[f:6549[ن]ْ[f:6549[س]َانِ عَدُ[a:873[و]ٌّ [a:873[م]ُّبِ[p[ی]ْنٌ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 193,
      "hizbQuarter": 94,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَكَذٰلِكَ یَ[q:6550[ج]ْتَبِیْكَ رَبُّكَ وَیُعَلِّمُكَ مِ[f:6551[ن]ْ [f:6551[ت]َاْوِیْلِ [h:6552[ا]لْاَحَادِیْثِ وَیُتِ[g[م]ُّ نِعْمَتَهٗ عَلَیْكَ وَعَلٰۤ[o[ی] اٰلِ یَعْقُوْبَ كَمَ[o[ا]ۤ اَتَ[g[م]َّهَا عَلٰۤ[o[ی] اَبَوَیْكَ مِ[f:128[ن]ْ [f:128[ق]َ[q:129[ب]ْلُ اِ[q:1860[ب]ْرٰهِیْمَ وَاِسْحٰقَ ؕ اِ[g[ن]َّ رَبَّكَ عَلِیْمٌ حَكِ[p[ی]ْمٌ ۟۠",
      "juz": 12,
      "page": 236,
      "ruku": 193,
      "hizbQuarter": 94,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لَقَ[q:2389[د]ْ كَانَ فِیْ یُوْسُفَ وَاِخْوَتِهٖۤ اٰیٰ[u:6553[ت]ٌ [u:6553[ل]ِّلسَّ[o[ا]ٓىِٕلِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِذْ قَالُوْ[s[ا] لَیُوْسُفُ وَاَخُوْهُ اَحَبُّ اِلٰۤ[o[ی] اَبِیْنَا مِ[g[ن]َّا وَنَحْنُ عُصْبَةٌ ؕ اِ[g[ن]َّ اَبَانَا لَفِیْ ضَلٰ[a:2393[ل]ٍ [a:2393[م]ُّبِ[p[ی]ْنِ ۟ۚۖ",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 9,
      "text": "[q:2891[ق]ْتُلُوْ[s[ا] یُوْسُفَ اَوِ [h:6554[ا][q:6555[ط]ْرَحُوْهُ اَرْ[a:6556[ض]ً[a:6556[ا] [a:6556[ی]َّخْلُ لَكُمْ وَ[q:603[ج]ْهُ اَبِیْكُمْ وَتَكُوْنُوْ[s[ا] مِ[i:254[ن]ْ [i:254[ب]َعْدِهٖ قَوْ[f:6557[م]ً[f:6557[ا] [f:6557[ص]ٰلِحِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 10,
      "text": "قَالَ قَ[o[ا]ٓىِٕ[a:6558[ل]ٌ [a:6558[م]ِّنْهُمْ لَا تَ[q:2734[ق]ْتُلُوْ[s[ا] یُوْسُفَ وَاَلْقُوْهُ فِیْ غَیٰبَتِ [h:6559[ا]لْجُبِّ یَلْتَقِ[q:6560[ط]ْهُ بَعْضُ [h:6561[ا][l[ل]سَّیَّارَةِ اِ[f:109[ن]ْ [f:109[ك]ُ[f:100[ن]ْ[f:100[ت]ُمْ فٰعِلِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 11,
      "text": "قَالُوْ[s[ا] یٰۤاَبَانَا مَا لَكَ لَا تَاْمَ[g[ن]َّا عَلٰی یُوْسُفَ وَاِ[g[ن]َّا لَهٗ لَنٰصِحُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اَرْسِلْهُ مَعَنَا غَ[a:6562[د]ً[a:6562[ا] [a:6562[ی]َّرْتَعْ وَیَلْعَ[q:6563[ب]ْ وَاِ[g[ن]َّا لَهٗ لَحٰفِظُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 13,
      "text": "قَالَ اِ[g[ن]ِّیْ لَیَحْزُنُنِ[o[ی]ْۤ اَ[f:6564[ن]ْ [f:6564[ت]َذْهَبُوْ[s[ا] بِهٖ وَاَخَافُ اَ[a:6565[ن]ْ [a:6565[ی]َّاْكُلَهُ [h:6566[ا][l[ل]ذِّئْبُ وَاَ[f:98[ن]ْ[f:98[ت]ُمْ عَنْهُ غٰفِلُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 14,
      "text": "قَالُوْ[s[ا] لَىِٕنْ اَكَلَهُ [h:6567[ا][l[ل]ذِّئْبُ وَنَحْنُ عُصْبَةٌ اِ[g[ن]َّ[o[ا]ۤ اِ[u:4868[ذ]ً[u:4868[ا] [u:4868[ل]َّخٰسِرُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 236,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَلَ[g[م]َّا ذَهَبُوْ[s[ا] بِهٖ وَاَ[q:6568[ج]ْمَعُ[o[و]ْۤ[s[ا] اَ[a:6569[ن]ْ [a:6569[ی]َّ[q:6570[ج]ْعَلُوْهُ فِیْ غَیٰبَتِ [h:6559[ا]لْجُبِّ ۚ وَاَوْحَیْنَ[o[ا]ۤ اِلَیْهِ لَتُنَبِّئَ[g[ن]َّهُ[c:6571[م]ْ [c:6571[ب]ِاَمْرِهِمْ هٰذَا وَهُمْ لَا یَشْعُرُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 237,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَجَ[o[ا]ٓءُ[o[و]ْۤ اَبَاهُمْ عِشَ[o[ا]ٓ[a:6572[ء]ً [a:6572[ی]َّ[q:6573[ب]ْكُ[p[و]ْنَ ۟ؕ",
      "juz": 12,
      "page": 237,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 17,
      "text": "قَالُوْ[s[ا] یٰۤاَبَانَ[o[ا]ۤ اِ[g[ن]َّا ذَهَ[q:6574[ب]ْنَا نَسْتَبِقُ وَتَرَكْنَا یُوْسُفَ عِ[f:264[ن]ْ[f:264[د]َ مَتَاعِنَا فَ[h:6575[ا]َكَ[l[ل]َهُ [o[ا][u:6576[ل]ذِّئْبُ ۚ وَمَاۤ اَنْتَ بِمُؤْمِنٍ لَّنَا وَلَوْ كُنَّا صٰدِقِیْنَ ۟",
      "juz": 12,
      "page": 237,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَجَ[o[ا]ٓءُوْ عَلٰی قَمِیْصِهٖ بِدَ[f:6577[م]ٍ [f:6577[ك]َذِ[f:6578[ب]ٍ ؕ [f:6578[ق]َالَ بَلْ سَوَّلَتْ لَكُمْ اَ[f:6579[ن]ْ[f:6579[ف]ُسُكُمْ اَمْ[f:6580[ر]ً[f:6580[ا] ؕ [f:6580[ف]َصَ[q:6581[ب]ْ[f:6582[ر]ٌ [f:6582[ج]َمِیْ[a:6583[ل]ٌ ؕ [a:6583[و]َ[h:72[ا]للّٰهُ [h:6584[ا]لْمُسْتَعَانُ عَلٰی مَا تَصِفُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 237,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَجَ[o[ا]ٓءَتْ سَیَّارَ[f:6585[ة]ٌ [f:6585[ف]َاَرْسَلُوْ[s[ا] وَارِدَهُمْ فَاَ[q:6586[د]ْلٰی دَلْوَهٗ ؕ قَالَ یٰبُشْرٰی هٰذَا غُلٰ[a:6587[م]ٌ ؕ [a:6587[و]َاَسَرُّوْهُ بِضَ[h:72[ا]عَةً ؕ وَاللّٰهُ عَلِیْمٌۢ بِمَا یَعْمَلُوْنَ ۟",
      "juz": 12,
      "page": 237,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَشَرَوْهُ بِثَمَ[i:6589[ن]ٍ [i:6589[ب]َخْ[f:6590[س]ٍ [f:6590[د]َرَ[n[ا]هِمَ مَعْدُوْدَ[a:6591[ة]ٍ ۚ [a:6591[و]َكَانُوْ[s[ا] فِیْهِ مِنَ [h:6592[ا][l[ل]زَّ[n[ا]هِدِ[p[ی]ْنَ ۟۠",
      "juz": 12,
      "page": 237,
      "ruku": 194,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَقَالَ [h:6593[ا]لَّذِی [h:6594[ا]شْتَرٰىهُ مِ[a:6595[ن]ْ [a:6595[م]ِّصْرَ لِ[h:6596[ا]مْرَاَتِهٖۤ اَكْرِمِیْ مَثْوٰ[o[ى]هُ عَسٰۤی [o[ا]َنْ یَّنْفَعَنَ[a:6599[ا]ۤ [n[ا]َوْ نَتَّخِذَهٗ وَلَدًا ؕ وَكَذٰلِكَ مَكَّنَّا لِ[o[ی]ُوْسُفَ فِی ا[l[ل]ْاَرْضِ ؗ [p[و]َلِنُعَلِّمَهٗ مِنْ تَاْوِیْلِ الْاَحَادِیْثِ ؕ وَاللّٰهُ غَالِبٌ عَلٰۤی اَمْرِهٖ وَلٰكِنَّ اَكْثَرَ النَّاسِ لَا یَعْلَمُوْنَ ۟",
      "juz": 12,
      "page": 237,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَلَ[g[م]َّا بَلَغَ اَشُدَّهٗۤ اٰتَیْنٰهُ حُكْ[a:6600[م]ً[a:6600[ا] [a:6600[و]َّعِلْ[a:6601[م]ً[a:6601[ا] ؕ [a:6601[و]َكَذٰلِكَ نَ[q:4214[ج]ْزِی [h:4215[ا]لْمُحْسِنِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 237,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 23,
      "text": "وَرَ[n[ا]وَدَتْهُ [h:6602[ا]لَّتِیْ هُوَ فِیْ بَیْتِهَا عَ[a:5890[ن]ْ [a:5890[ن]َّفْسِهٖ وَغَلَّقَتِ [h:6603[ا]لْاَ[q:6604[ب]ْوَ[n[ا]بَ وَقَالَتْ هَیْتَ لَكَ ؕ قَالَ مَعَاذَ [h:6605[ا]للّٰهِ اِ[g[ن]َّهٗ رَبِّ[o[ی]ْۤ اَحْسَنَ مَثْوَایَ ؕ اِ[g[ن]َّهٗ لَا یُفْلِحُ [h:3981[ا][l[ل]ظّٰلِمُ[p[و]ْنَ ۟",
      "juz": 12,
      "page": 238,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَلَقَ[q:341[د]ْ هَ[g[م]َّتْ بِهٖ ۚ وَهَ[g[م]َّ بِهَا لَوْلَ[o[ا]ۤ اَ[u:6606[ن]ْ [u:6606[ر]َّاٰ بُرْهَ[n[ا]نَ رَبِّهٖ ؕ كَذٰ[l[ل]ِكَ لِ[g[ن]َصْرِفَ عَنْهُ السُّوْٓءَ وَالْفَحْشَآءَ ؕ اِنَّهٗ مِنْ عِبَادِنَا الْمُخْلَصِیْنَ ۟",
      "juz": 12,
      "page": 238,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَ[h:9999[ا]سْتَبَقَا [h:6610[ا]لْبَابَ وَقَدَّتْ قَمِیْصَهٗ مِ[f:6611[ن]ْ [f:6611[د]ُبُ[a:6612[ر]ٍ [a:6612[و]َّاَلْفَیَا سَیِّدَهَا لَدَا [h:6613[ا]لْبَابِ ؕ قَالَتْ مَا جَزَ[o[ا]ٓءُ مَنْ اَرَادَ بِاَهْلِكَ سُ[o[و]ْٓءًا اِلَّ[o[ا]ۤ اَ[a:6614[ن]ْ [a:6614[ی]ُّسْجَنَ اَوْ عَذَابٌ اَلِ[p[ی]ْمٌ ۟",
      "juz": 12,
      "page": 238,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 26,
      "text": "قَالَ هِیَ رَ[n[ا]وَدَتْنِیْ عَ[a:6615[ن]ْ [a:6615[ن]َّفْسِیْ وَشَهِدَ شَاهِ[a:6616[د]ٌ [a:6616[م]ِّنْ اَهْلِهَ[o[ا] ۚ اِ[f:2594[ن]ْ [f:2594[ك]َانَ قَمِیْصُهٗ قُدَّ مِ[f:6617[ن]ْ [f:6617[ق]ُبُ[f:6618[ل]ٍ [f:6618[ف]َصَدَقَتْ وَهُوَ مِنَ [h:4785[ا]لْكٰذِبِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 238,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَاِ[f:1649[ن]ْ [f:1649[ك]َانَ قَمِیْصُهٗ قُدَّ مِ[f:6611[ن]ْ [f:6611[د]ُبُ[f:6619[ر]ٍ [f:6619[ف]َكَذَبَتْ وَهُوَ مِنَ [h:4793[ا][l[ل]صّٰدِقِ[p[ی]ْنَ ۟",
      "juz": 12,
      "page": 238,
      "ruku": 195,
      "hizbQuarter": 95,
      "sajda": false
    },
    {
      "number": 28,
      "text": "فَلَ[g[م]َّا رَاٰ قَمِیْصَهٗ قُدَّ مِ[f:6611[ن]ْ [f:6611[د]ُبُ[f:6620[ر]ٍ [f:6620[ق]َالَ اِ[g[ن]َّهٗ مِ[f:6621[ن]ْ [f:6621[ك]َیْدِكُ[g[ن]َّ ؕ اِ[g[ن]َّ كَیْدَكُ[g[ن]َّ عَظِ[p[ی]ْمٌ ۟",
      "juz": 12,
