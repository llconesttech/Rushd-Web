{
  "number": 64,
  "name": "سُورَةُ التَّغَابُنِ",
  "englishName": "At-Taghaabun",
  "ayahs": [
    {
      "number": 1,
      "text": "یُسَبِّحُ لِلّٰهِ مَا فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَمَا فِی [h:44[ا]لْاَرْضِ ۚ لَهُ [h:1397[ا]لْمُلْكُ وَلَهُ [h:10443[ا]لْحَمْدُ ؗ وَهُوَ عَلٰی كُلِّ شَ[f:81[ی]ْ[f:81[ء]ٍ [f:81[ق]َدِ[p[ی]ْرٌ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 2,
      "text": "هُوَ [h:153[ا]لَّذِیْ خَلَقَكُمْ فَمِ[f:12615[ن]ْ[f:12615[ك]ُمْ كَافِ[a:13630[ر]ٌ [a:13630[و]َّمِ[f:2325[ن]ْ[f:2325[ك]ُ[w:13631[م]ْ [w:13631[م]ُّؤْمِ[a:13632[ن]ٌ ؕ [a:13632[و]َ[h:72[ا]للّٰهُ بِمَا تَعْمَلُوْنَ بَصِ[p[ی]ْرٌ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 3,
      "text": "خَلَقَ [h:3913[ا][l[ل]سَّمٰوٰتِ وَ[h:1474[ا]لْاَرْضَ بِ[h:370[ا]لْحَقِّ وَصَوَّرَكُمْ فَاَحْسَنَ صُوَرَكُمْ ۚ وَاِلَیْهِ [h:3461[ا]لْمَصِ[p[ی]ْرُ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 4,
      "text": "یَعْلَمُ مَا فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَ[h:177[ا]لْاَرْضِ وَیَعْلَمُ مَا تُسِرُّوْنَ وَمَا تُعْلِنُوْنَ ؕ وَ[h:72[ا]للّٰهُ عَلِیْ[i:2196[م]ٌۢ [i:2196[ب]ِذَاتِ [h:2197[ا][l[ل]صُّدُ[p[و]ْرِ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اَلَمْ یَاْتِكُمْ نَبَؤُ[s[ا] [h:7058[ا]لَّذِیْنَ كَفَرُوْ[s[ا] مِ[f:128[ن]ْ [f:128[ق]َ[q:129[ب]ْلُ ؗ فَذَاقُوْ[s[ا] وَبَالَ اَمْرِهِمْ وَلَهُمْ عَذَابٌ اَلِ[p[ی]ْمٌ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ذٰلِكَ بِاَ[g[ن]َّهٗ كَانَتْ تَّاْتِیْهِمْ رُسُلُهُ[c:4900[م]ْ [c:4900[ب]ِ[h:484[ا]لْبَیِّنٰتِ فَقَالُ[o[و]ْۤ[s[ا] اَبَشَ[a:13633[ر]ٌ [a:13633[ی]َّهْدُوْنَنَا ؗ فَكَفَرُوْ[s[ا] وَتَوَلَّوْ[s[ا] وَّ[h:13634[ا]سْتَغْنَی [h:13635[ا]للّٰهُ ؕ وَ[h:72[ا]للّٰهُ غَنِیٌّ حَمِ[p[ی]ْ[q[د]ٌ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 7,
      "text": "زَعَمَ [h:13636[ا]لَّذِیْنَ كَفَرُ[o[و]ْۤ[s[ا] اَ[u:8842[ن]ْ [u:8842[ل]َّ[a:13637[ن]ْ [a:13637[ی]ُّ[q:13638[ب]ْعَثُوْ[s[ا] ؕ قُلْ بَلٰی وَرَبِّیْ لَتُ[q:13639[ب]ْعَثُ[g[ن]َّ ثُ[g[م]َّ لَتُنَبَّؤُ[g[ن]َّ بِمَا عَمِلْتُمْ ؕ وَذٰلِكَ عَلَی [h:412[ا]للّٰهِ یَسِ[p[ی]ْرٌ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَاٰمِنُوْ[s[ا] بِ[h:32[ا]للّٰهِ وَرَسُوْلِهٖ وَ[h:13640[ا][l[ل][g[ن]ُّوْرِ [h:13641[ا]لَّذِ[o[ی]ْۤ اَ[f:515[ن]ْ[f:515[ز]َلْنَا ؕ وَ[h:72[ا]للّٰهُ بِمَا تَعْمَلُوْنَ خَبِ[p[ی]ْرٌ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یَوْمَ یَ[q:12425[ج]ْمَعُكُمْ لِیَوْمِ [h:13642[ا]لْجَمْعِ ذٰلِكَ یَوْمُ [h:13643[ا][l[ل]تَّغَابُنِ ؕ وَمَ[a:13644[ن]ْ [a:13644[ی]ُّؤْمِ[i:12652[ن]ْ [i:12652[ب]ِ[h:32[ا]للّٰهِ وَیَعْمَلْ صَ[a:13645[ا]لِحًا یُّكَفِّرْ عَ[f:121[ن]ْهُ سَیِّاٰتِهٖ وَیُ[f:5811[د]ْخِلْهُ جَنّٰتٍ تَجْرِ[p[ی]ْ مِنْ تَحْتِهَا الْاَنْهٰرُ خٰلِدِیْنَ فِیْهَاۤ اَبَدًا ؕ ذٰلِكَ الْفَوْزُ الْعَظِیْمُ ۟",
      "juz": 28,
      "page": 556,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَ[h:9999[ا]لَّذِیْنَ كَفَرُوْ[s[ا] وَكَذَّبُوْ[s[ا] بِاٰیٰتِنَ[o[ا]ۤ اُ[s[و]لٰٓىِٕكَ اَصْحٰبُ [h:201[ا][l[ل][g[ن]َّارِ خٰلِدِیْنَ فِیْهَا ؕ وَبِئْسَ [h:665[ا]لْمَصِ[p[ی]ْرُ ۟۠",
      "juz": 28,
      "page": 557,
      "ruku": 490,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 11,
      "text": "مَ[o[ا]ۤ اَصَابَ مِ[a:12140[ن]ْ [a:12140[م]ُّصِیْبَةٍ اِلَّا بِاِذْنِ [h:509[ا]للّٰهِ ؕ وَمَ[a:13644[ن]ْ [a:13644[ی]ُّؤْمِ[i:12652[ن]ْ [i:12652[ب]ِ[h:32[ا]للّٰهِ یَهْدِ قَلْبَهٗ ؕ وَ[h:72[ا]للّٰهُ بِكُلِّ شَیْءٍ عَلِ[p[ی]ْمٌ ۟",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَاَطِیْعُو[s[ا] [h:2238[ا]للّٰهَ وَاَطِیْعُو[s[ا] [h:2865[ا][l[ل]رَّسُوْلَ ۚ فَاِ[f:3779[ن]ْ [f:3779[ت]َوَلَّیْتُمْ فَاِ[g[ن]َّمَا عَلٰی رَسُوْلِنَا [h:3780[ا]لْبَلٰغُ [h:3781[ا]لْمُبِ[p[ی]ْنُ ۟",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اَللّٰهُ لَ[o[ا]ۤ اِلٰهَ اِلَّا هُوَ ؕ وَعَلَی [h:2207[ا]للّٰهِ فَلْیَتَوَكَّلِ [h:2208[ا]لْمُؤْمِنُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 14,
      "text": "یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ اٰمَنُ[o[و]ْۤ[s[ا] اِ[g[ن]َّ مِنْ اَزْوَ[n[ا]جِكُمْ وَاَوْلَ[u:13647[ا]دِكُمْ عَدُوًّا لَّكُمْ فَ[s[ا]حْذَرُوْهُمْ ۚ وَاِنْ تَعْفُوْا وَتَصْفَحُوْا وَتَغْفِرُوْا فَاِنَّ اللّٰهَ غَفُوْرٌ رَّحِیْمٌ ۟",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِ[g[ن]َّمَ[o[ا]ۤ اَمْوَ[n[ا]لُكُمْ وَاَوْلَادُكُمْ فِتْنَ[a:13650[ة]ٌ ؕ [a:13650[و]َ[h:72[ا]للّٰهُ عِ[f:719[ن]ْ[f:719[د]َهٗۤ اَ[q:2423[ج]ْرٌ عَظِ[p[ی]ْمٌ ۟",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَ[h:9999[ا]تَّقُو[s[ا] [h:1945[ا]للّٰهَ مَا [h:5393[ا]سْتَطَعْتُمْ وَ[h:486[ا]سْمَعُوْ[s[ا] وَاَطِیْعُوْ[s[ا] وَاَ[f:1045[ن]ْ[f:1045[ف]ِقُوْ[s[ا] خَیْ[u:13651[ر]ً[u:13651[ا] [u:13651[ل]ِّاَ[f:13652[ن]ْ[f:13652[ف]ُسِكُمْ ؕ وَمَ[a:13470[ن]ْ [a:13470[ی]ُّوْقَ شُحَّ نَفْسِهٖ فَاُ[s[و]لٰٓىِٕكَ هُمُ [h:23[ا]لْمُفْلِحُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اِ[f:13653[ن]ْ [f:13653[ت]ُ[q:13654[ق]ْرِضُو[s[ا] [h:13655[ا]للّٰهَ قَرْضًا حَسَ[a:13656[ن]ً[a:13656[ا] [a:13656[ی]ُّضٰعِفْهُ لَكُمْ وَیَغْفِرْ لَكُمْ ؕ وَ[h:72[ا]للّٰهُ شَكُوْرٌ حَلِ[p[ی]ْمٌ ۟ۙ",
      "juz": 28,
      "page": 557,
      "ruku": 491,
      "hizbQuarter": 222,
      "sajda": false
    },
    {
      "number": 18,
      "text": "عٰلِمُ [h:4186[ا]لْغَیْبِ وَ[h:4187[ا][l[ل]شَّهَ[h:10627[ا]دَةِ [h:681[ا]لْعَزِ[p[ی]ْزُ الْحَكِیْمُ ۟۠",
      "juz": 28,
