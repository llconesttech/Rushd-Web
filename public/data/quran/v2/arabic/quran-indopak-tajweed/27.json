      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 3,
      "text": "الَّذِیْنَ یُقِیْمُوْنَ [h:3638[ا][l[ل]صَّلٰ[s[و]ةَ وَیُؤْتُوْنَ [h:3639[ا][l[ل]زَّكٰ[s[و]ةَ وَهُ[c:4710[م]ْ [c:4710[ب]ِ[h:447[ا]لْاٰخِرَةِ هُمْ یُوْقِنُ[p[و]ْنَ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اِ[g[ن]َّ [h:24[ا]لَّذِیْنَ لَا یُؤْمِنُوْنَ بِ[h:447[ا]لْاٰخِرَةِ زَیَّ[g[ن]َّا لَهُمْ اَعْمَالَهُمْ فَهُمْ یَعْمَهُ[p[و]ْنَ ۟ؕ",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اُ[s[و]لٰٓىِٕكَ [h:51[ا]لَّذِیْنَ لَهُمْ سُ[o[و]ْٓءُ [h:9886[ا]لْعَذَابِ وَهُمْ فِی [h:536[ا]لْاٰخِرَةِ هُمُ [h:6285[ا]لْاَخْسَرُ[p[و]ْنَ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَاِ[g[ن]َّكَ لَتُلَقَّی [h:9887[ا]لْقُرْاٰنَ مِ[u:6233[ن]ْ [u:6233[ل]َّدُنْ حَكِیْمٍ عَلِ[p[ی]ْمٍ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِذْ قَالَ مُوْسٰی لِاَهْلِهٖۤ اِ[g[ن]ِّ[o[ی]ْۤ اٰنَسْتُ نَا[f:9888[ر]ً[f:9888[ا] ؕ [f:9888[س]َاٰتِیْكُ[w:9889[م]ْ [w:9889[م]ِّنْهَا بِخَبَرٍ اَوْ اٰتِیْكُ[c:9890[م]ْ [c:9890[ب]ِشِهَا[f:9891[ب]ٍ [f:9891[ق]َبَ[u:9892[س]ٍ [u:9892[ل]َّعَلَّكُمْ تَصْطَلُ[p[و]ْنَ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَلَ[g[م]َّا جَ[o[ا]ٓءَهَا نُوْدِیَ اَ[i:9893[ن]ْ [i:9893[ب]ُوْرِكَ مَ[f:2074[ن]ْ [f:2074[ف]ِی [h:4674[ا][l[ل][g[ن]َّارِ وَمَنْ حَوْلَهَا ؕ وَسُ[q:6849[ب]ْحٰنَ [h:6850[ا]للّٰهِ رَبِّ [h:4[ا]لْعٰلَمِ[p[ی]ْنَ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یٰمُوْسٰۤ[o[ی] اِ[g[ن]َّهٗۤ اَنَا [h:8455[ا]للّٰهُ [h:9894[ا]لْعَزِیْزُ [h:681[ا]لْحَكِ[p[ی]ْمُ ۟ۙ",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَاَلْقِ عَصَاكَ ؕ فَلَ[g[م]َّا رَاٰهَا تَهْتَزُّ كَاَ[g[ن]َّهَا جَ[m[ا]ٓ[a:9895[ن]ٌّ وَّلّٰی مُ[q:9896[د]ْبِ[a:9897[ر]ً[a:9897[ا] [a:9897[و]َّلَمْ یُعَقِّ[q:9898[ب]ْ ؕ یٰمُوْسٰی لَا تَخَفْ ۫ اِ[g[ن]ِّیْ لَا یَخَافُ لَدَیَّ [h:9899[ا]لْمُرْسَلُ[p[و]ْنَ ۟ۗۖ",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اِلَّا مَ[f:8211[ن]ْ [f:8211[ظ]َلَمَ ثُ[g[م]َّ بَدَّلَ حُسْنً[i:9900[ا] [i:9900[ب]َعْدَ سُ[o[و]ْٓ[f:9901[ء]ٍ [f:9901[ف]َاِ[g[ن]ِّیْ غَفُوْ[u:890[ر]ٌ [u:890[ر]َّحِ[p[ی]ْمٌ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَاَ[q:9902[د]ْخِلْ یَدَكَ فِیْ جَیْبِكَ تَخْرُ[q:8461[ج]ْ بَیْضَ[o[ا]ٓءَ مِنْ غَیْرِ سُ[o[و]ْٓ[f:9903[ء]ٍ ۫ [f:9903[ف]ِیْ تِسْعِ اٰیٰتٍ اِلٰی فِرْعَوْنَ وَقَوْمِهٖ ؕ اِ[g[ن]َّهُمْ كَانُوْ[s[ا] قَوْ[f:5652[م]ً[f:5652[ا] [f:5652[ف]ٰسِقِ[p[ی]ْنَ ۟",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَلَ[g[م]َّا جَ[o[ا]ٓءَتْهُمْ اٰیٰتُنَا مُ[q:7692[ب]ْصِرَ[f:9904[ة]ً [f:9904[ق]َالُوْ[s[ا] هٰذَا سِحْ[a:3879[ر]ٌ [a:3879[م]ُّبِ[p[ی]ْنٌ ۟ۚ",
      "juz": 19,
      "page": 377,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَجَحَدُوْ[s[ا] بِهَا وَ[h:9905[ا]سْتَیْقَنَتْهَ[o[ا]ۤ اَ[f:2341[ن]ْ[f:2341[ف]ُسُهُمْ ظُلْ[a:9906[م]ً[a:9906[ا] [a:9906[و]َّعُلُوًّ[f:9907[ا] ؕ [f:9907[ف]َ[h:1513[ا][f:1513[ن]ْ[f:1513[ظ]ُرْ كَیْفَ كَانَ عَ[h:4852[ا]قِبَةُ الْمُفْسِدِیْنَ ۟۠",
      "juz": 19,
      "page": 378,
      "ruku": 327,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَقَ[q:341[د]ْ اٰتَیْنَا دَاوٗدَ وَسُلَیْمٰنَ عِلْ[a:9908[م]ً[a:9908[ا] ۚ [a:9908[و]َقَالَا [h:9909[ا]لْحَمْدُ لِلّٰهِ [h:3912[ا]لَّذِیْ فَضَّلَنَا عَلٰی كَثِیْ[a:3126[ر]ٍ [a:3126[م]ِّنْ عِبَادِهِ [h:9910[ا]لْمُؤْمِنِ[p[ی]ْنَ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَوَرِثَ سُلَیْمٰنُ دَاوٗدَ وَقَالَ یٰۤاَیُّهَا [h:82[ا][l[ل][g[ن]َّاسُ عُلِّمْنَا مَ[f:9911[ن]ْ[f:9911[ط]ِقَ [h:9912[ا][l[ل]طَّیْرِ وَاُوْتِیْنَا مِ[f:843[ن]ْ [f:843[ك]ُلِّ شَیْءٍ ؕ اِ[g[ن]َّ هٰذَا لَهُوَ [h:9913[ا]لْفَضْلُ [h:9914[ا]لْمُبِ[p[ی]ْنُ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَحُشِرَ لِسُلَیْمٰنَ جُنُوْدُهٗ مِنَ [h:4673[ا]لْجِ[g[ن]ِّ وَ[h:4418[ا]لْاِ[f:4418[ن]ْ[f:4418[س]ِ وَ[h:9915[ا][l[ل]طَّیْرِ فَهُمْ یُوْزَعُ[p[و]ْنَ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 18,
      "text": "حَتّٰۤ[o[ی] اِذَ[o[ا]ۤ اَتَوْ[s[ا] عَلٰی وَادِ [h:9916[ا][l[ل][g[ن]َّمْلِ ۙ قَالَتْ نَمْلَ[a:9917[ة]ٌ [a:9917[ی]ّٰۤاَیُّهَا [h:9918[ا][l[ل][g[ن]َّمْلُ [h:9919[ا][q:278[د]ْخُلُوْ[s[ا] مَسٰكِنَكُمْ ۚ لَا یَحْطِمَ[g[ن]َّكُمْ سُلَیْمٰنُ وَجُنُوْدُهٗ ۙ وَهُمْ لَا یَشْعُرُ[p[و]ْنَ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَتَبَسَّمَ ضَاحِ[a:9920[ك]ً[a:9920[ا] [a:9920[م]ِّ[f:9921[ن]ْ [f:9921[ق]َوْلِهَا وَقَالَ رَبِّ اَوْزِعْنِ[o[ی]ْۤ اَنْ اَشْكُرَ نِعْمَتَكَ [h:9922[ا]لَّتِ[o[ی]ْۤ اَنْعَمْتَ عَلَیَّ وَعَلٰی وَ[n[ا]لِدَیَّ وَاَنْ اَعْمَلَ صَ[f:9923[ا]لِحًا تَرْضٰ[p[ى]هُ وَاَدْخِلْنِیْ بِرَحْمَتِكَ فِیْ عِبَادِكَ الصّٰلِحِیْنَ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَتَفَقَّدَ [h:9926[ا][l[ل]طَّیْرَ فَقَالَ مَا لِیَ لَ[o[ا]ۤ اَرَی [h:9927[ا]لْهُ[q:9928[د]ْهُدَ ۖؗ اَمْ كَانَ مِنَ [h:9929[ا]لْغَ[o[ا]ٓىِٕبِ[p[ی]ْنَ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 21,
      "text": "لَاُعَذِّبَ[g[ن]َّهٗ عَذَا[f:1966[ب]ً[f:1966[ا] [f:1966[ش]َدِیْدًا اَوْ لَاَ[s[ا]َذْبَحَ[g[ن]َّهٗۤ اَوْ لَیَاْتِیَ[g[ن]ِّیْ بِسُلْطٰ[a:7067[ن]ٍ [a:7067[م]ُّبِ[p[ی]ْنٍ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 22,
      "text": "فَمَكَثَ غَیْرَ بَعِیْ[f:9930[د]ٍ [f:9930[ف]َقَالَ اَحَطْتُّ بِمَا لَمْ تُحِ[q:8160[ط]ْ بِهٖ وَجِئْتُكَ مِ[f:9931[ن]ْ [f:9931[س]َبَ[i:9932[ا]ٍۭ [i:9932[ب]ِنَبَ[a:9933[ا]ٍ [a:9933[ی]َّقِ[p[ی]ْنٍ ۟",
      "juz": 19,
      "page": 378,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِ[g[ن]ِّیْ وَجَ[d:9934[د]ْتُّ [h:9935[ا]مْرَاَ[f:9936[ة]ً [f:9936[ت]َمْلِكُهُمْ وَاُوْتِیَتْ مِ[f:843[ن]ْ [f:843[ك]ُلِّ شَ[a:9937[ی]ْ[a:9937[ء]ٍ [a:9937[و]َّلَهَا عَرْشٌ عَظِ[p[ی]ْمٌ ۟",
      "juz": 19,
      "page": 379,
      "ruku": 328,
      "hizbQuarter": 151,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَجَ[d:9938[د]ْتُّهَا وَقَوْمَهَا یَسْجُدُوْنَ لِلشَّمْسِ مِ[f:107[ن]ْ [f:107[د]ُوْنِ [h:108[ا]للّٰهِ وَزَیَّنَ لَهُمُ [h:4046[ا][l[ل]شَّیْطٰنُ اَعْمَالَهُمْ فَصَدَّهُمْ عَنِ [h:6998[ا][l[ل]سَّبِیْلِ فَهُمْ لَا یَهْتَدُ[p[و]ْنَ ۟ۙ",
