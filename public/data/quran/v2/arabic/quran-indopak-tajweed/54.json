  "ayahs": [
    {
      "number": 1,
      "text": "اِ[q:13032[ق]ْتَرَبَتِ [h:13033[ا][l[ل]سَّاعَةُ وَ[h:13034[ا][f:13034[ن]ْ[f:13034[ش]َقَّ [h:13035[ا]لْقَمَرُ ۟",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاِ[a:3990[ن]ْ [a:3990[ی]َّرَوْ[s[ا] اٰیَ[a:13036[ة]ً [a:13036[ی]ُّعْرِضُوْ[s[ا] وَیَقُوْلُوْ[s[ا] سِحْ[a:13037[ر]ٌ [a:13037[م]ُّسْتَمِرٌّ ۟",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَكَذَّبُوْ[s[ا] وَ[h:2433[ا]تَّبَعُ[o[و]ْۤ[s[ا] اَهْوَ[o[ا]ٓءَهُمْ وَكُلُّ اَمْ[a:13038[ر]ٍ [a:13038[م]ُّسْتَقِرٌّ ۟",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَلَقَ[q:341[د]ْ جَ[o[ا]ٓءَهُ[w:13039[م]ْ [w:13039[م]ِّنَ [h:13040[ا]لْاَ[i:13041[ن]ْۢ[i:13041[ب]َ[o[ا]ٓءِ مَا فِیْهِ مُزْدَجَرٌ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 5,
      "text": "حِكْمَ[i:13042[ة]ٌ [i:13042[ب]َا[l[ل]ِغَةٌ فَمَا تُغْنِ النُّذُرُ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 6,
      "text": "فَتَوَلَّ عَنْهُمْ ۘ یَوْمَ یَ[q:9312[د]ْعُ [h:13045[ا][l[ل]دَّاعِ اِلٰی شَ[a:13046[ی]ْ[a:13046[ء]ٍ [a:13046[ن]ُّكُرٍ ۟ۙ",
      "juz": 27,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 7,
      "text": "خُشَّعًا اَ[q:4718[ب]ْصَارُهُمْ یَخْرُجُوْنَ مِنَ [h:11261[ا]لْاَ[q:11262[ج]ْدَاثِ كَاَ[g[ن]َّهُمْ جَرَا[a:13047[د]ٌ [a:13047[م]ُّ[f:13048[ن]ْ[f:13048[ت]َشِرٌ ۟ۙ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 8,
      "text": "مُّهْطِعِیْنَ اِلَی [h:13049[ا][l[ل]دَّاعِ ؕ یَقُوْلُ [h:13050[ا]لْكٰفِرُوْنَ هٰذَا یَوْمٌ عَسِرٌ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كَذَّبَتْ قَ[q:8394[ب]ْلَهُمْ قَوْمُ نُوْ[f:13051[ح]ٍ [f:13051[ف]َكَذَّبُوْ[s[ا] عَ[q:11499[ب]ْدَنَا وَقَالُوْ[s[ا] مَ[q:12342[ج]ْنُوْ[a:13052[ن]ٌ [a:13052[و]َّ[h:13053[ا]زْدُجِرَ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَدَعَا رَبَّهٗۤ اَ[g[ن]ِّیْ مَغْلُوْ[f:13054[ب]ٌ [f:13054[ف]َ[h:13055[ا][f:13055[ن]ْ[f:13055[ت]َصِرْ ۟",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَفَتَحْنَ[o[ا]ۤ اَ[q:4047[ب]ْوَ[n[ا]بَ [h:13056[ا][l[ل]سَّمَ[o[ا]ٓءِ بِمَ[o[ا]ٓ[a:13057[ء]ٍ [a:13057[م]ُّنْهَمِرٍ ۟ؗۖ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَّفَجَّرْنَا [h:13058[ا]لْاَرْضَ عُیُوْ[f:13059[ن]ً[f:13059[ا] [f:13059[ف]َ[h:13060[ا]لْتَقَی [h:13061[ا]لْمَ[o[ا]ٓءُ عَلٰۤ[o[ی] اَمْ[f:13062[ر]ٍ [f:13062[ق]َ[q:298[د]ْ قُدِرَ ۟ۚ",
      "juz": 27,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَحَمَلْنٰهُ عَلٰی ذَاتِ اَلْوَ[n[ا][a:13063[ح]ٍ [a:13063[و]َّدُسُرٍ ۟ۙ",
