{
  "number": 17,
  "name": "سُورَةُ الإِسۡرَاءِ",
  "englishName": "Al-Israa",
  "ayahs": [
    {
      "number": 1,
      "text": "سُ[q:7648[ب]ْحٰنَ [h:7649[ا]لَّذِ[o[ی]ْۤ اَسْرٰی بِعَ[q:7650[ب]ْدِهٖ لَیْلً[a:7651[ا] [a:7651[م]ِّنَ [h:7652[ا]لْمَسْجِدِ [h:752[ا]لْحَرَامِ اِلَی [h:7653[ا]لْمَسْجِدِ [h:7654[ا]لْاَ[q:7655[ق]ْصَا [h:7656[ا]لَّذِیْ بٰرَكْنَا حَوْلَهٗ لِنُرِیَهٗ مِنْ اٰیٰتِنَ[o[ا] ؕ اِ[g[ن]َّهٗ هُوَ [h:3721[ا][l[ل]سَّمِیْعُ [h:7657[ا]لْبَصِ[p[ی]ْرُ ۟",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَاٰتَیْنَا مُوْسَی [h:257[ا]لْكِتٰبَ وَجَعَلْنٰهُ هُ[u:7658[د]ً[u:7658[ی] [u:7658[ل]ِّبَنِ[o[ی]ْۤ اِسْرَ[o[ا]ٓءِیْلَ اَلَّا تَتَّخِذُوْ[s[ا] مِ[f:7659[ن]ْ [f:7659[د]ُوْنِیْ وَكِیْلًا ۟ؕ",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ذُرِّیَّةَ مَنْ حَمَلْنَا مَعَ نُوْحٍ ؕ اِ[g[ن]َّهٗ كَانَ عَ[q:3317[ب]ْ[f:7660[د]ً[f:7660[ا] [f:7660[ش]َكُوْرًا ۟",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَقَضَیْنَ[o[ا]ۤ اِلٰی بَنِ[o[ی]ْۤ اِسْرَ[o[ا]ٓءِیْلَ فِی [h:818[ا]لْكِتٰبِ لَتُفْسِدُ[g[ن]َّ فِی [h:44[ا]لْاَرْضِ مَرَّتَیْنِ وَلَتَعْلُ[g[ن]َّ عُلُوًّ[f:7661[ا] [f:7661[ك]َبِیْرًا ۟",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَاِذَا جَ[o[ا]ٓءَ وَعْدُ اُوْلٰىهُمَا بَعَثْنَا عَلَیْكُمْ عِبَا[u:7662[د]ً[u:7662[ا] [u:7662[ل]َّنَ[o[ا]ۤ اُ[s[و]لِیْ بَاْ[f:7663[س]ٍ [f:7663[ش]َدِیْ[f:7664[د]ٍ [f:7664[ف]َجَاسُوْ[s[ا] خِلٰلَ [h:7665[ا][l[ل]دِّیَارِؕ وَكَانَ وَعْ[a:7666[د]ً[a:7666[ا] [a:7666[م]َّفْعُوْلًا ۟",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ثُ[g[م]َّ رَدَ[q:7667[د]ْنَا لَكُمُ [h:7668[ا]لْكَرَّةَ عَلَیْهِمْ وَاَمْدَ[q:7669[د]ْنٰكُ[c:7670[م]ْ [c:7670[ب]ِاَمْوَ[n[ا][a:7671[ل]ٍ [a:7671[و]َّبَنِیْنَ وَجَعَلْنٰكُمْ اَكْثَرَ نَفِیْرًا ۟",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِنْ اَحْسَ[f:7672[ن]ْ[f:7672[ت]ُمْ اَحْسَ[f:7672[ن]ْ[f:7672[ت]ُمْ لِاَ[f:574[ن]ْ[f:574[ف]ُسِكُمْ ۫ وَاِنْ اَسَاْتُمْ فَلَهَا ؕ فَاِذَا جَ[o[ا]ٓءَ وَعْدُ [h:7673[ا]لْاٰخِرَةِ لِیَسُوْٓءٗ[s[ا] وُجُوْهَكُمْ وَلِیَ[q:7674[د]ْخُلُو[s[ا] [h:7675[ا]لْمَسْجِدَ كَمَا دَخَلُوْهُ اَوَّلَ مَرَّ[a:7676[ة]ٍ [a:7676[و]َّلِیُتَبِّرُوْ[s[ا] مَا عَلَوْ[s[ا] تَتْبِیْرًا ۟",
      "juz": 15,
      "page": 282,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 8,
      "text": "عَسٰی رَبُّكُمْ اَ[a:7677[ن]ْ [a:7677[ی]َّرْحَمَكُمْ ۚ وَاِنْ عُ[d:7678[د]ْتُّمْ عُ[q:4860[د]ْنَا ۘ وَجَعَلْنَا جَهَ[g[ن]َّمَ لِلْكٰفِرِیْنَ حَصِیْرًا ۟",
      "juz": 15,
      "page": 283,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اِ[g[ن]َّ هٰذَا [h:6544[ا]لْقُرْاٰنَ یَهْدِیْ لِلَّتِیْ هِیَ اَ[q:7679[ق]ْوَمُ وَیُبَشِّرُ [h:7680[ا]لْمُؤْمِنِیْنَ [h:7681[ا]لَّذِیْنَ یَعْمَلُوْنَ [h:7682[ا][l[ل]صّٰلِحٰتِ اَ[g[ن]َّ لَهُمْ اَ[q:2795[ج]ْ[f:7683[ر]ً[f:7683[ا] [f:7683[ك]َبِیْرًا ۟ۙ",
      "juz": 15,
      "page": 283,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَّاَ[g[ن]َّ [h:7684[ا]لَّذِیْنَ لَا یُؤْمِنُوْنَ بِ[h:447[ا]لْاٰخِرَةِ اَعْتَ[q:2660[د]ْنَا لَهُمْ عَذَابًا اَلِیْمًا ۟۠",
      "juz": 15,
      "page": 283,
      "ruku": 240,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَیَ[q:7685[د]ْعُ [h:7686[ا]لْاِ[f:2732[ن]ْ[f:2732[س]َ[h:7687[ا]نُ بِ[h:5966[ا]لشَّرِّ دُعَآءَهٗ بِ[h:7688[ا]لْخَیْرِ ؕ وَكَانَ الْاِنْسَانُ عَجُوْلًا ۟",
      "juz": 15,
      "page": 283,
      "ruku": 241,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَجَعَلْنَا [h:7689[ا]لَّیْلَ وَ[h:6117[ا][l[ل][g[ن]َّهَارَ اٰیَتَیْنِ فَمَحَوْنَ[o[ا]ۤ اٰیَةَ [h:7690[ا]لَّیْلِ وَجَعَلْنَ[o[ا]ۤ اٰیَةَ [h:7691[ا][l[ل][g[ن]َّهَارِ مُ[q:7692[ب]ْصِرَ[u:7693[ة]ً [u:7693[ل]ِّتَ[q:7694[ب]ْتَغُوْ[s[ا] فَضْلً[a:1088[ا] [a:1088[م]ِّ[u:243[ن]ْ [u:243[ر]َّبِّكُمْ وَلِتَعْلَمُوْ[s[ا] عَدَدَ [h:5950[ا][l[ل]سِّنِیْنَ وَ[h:5951[ا]لْحِسَابَ ؕ وَكُلَّ شَ[f:7695[ی]ْ[f:7695[ء]ٍ [f:7695[ف]َصَّلْنٰهُ تَفْصِیْلًا ۟",
      "juz": 15,
      "page": 283,
      "ruku": 241,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَكُلَّ اِ[f:7696[ن]ْ[f:7696[س]َانٍ اَلْزَمْنٰهُ طٰٓىِٕرَهٗ فِ[a:7697[ی]ْ عُ[f:7698[ن]ُقِهٖ ؕ وَنُخْرِجُ لَهٗ یَوْمَ الْقِیٰمَةِ كِتٰبًا یَّلْقٰىهُ مَنْشُوْرًا ۟",
      "juz": 15,
      "page": 283,
      "ruku": 241,
      "hizbQuarter": 113,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اِ[q:7699[ق]ْرَاْ كِتٰبَكَ ؕ كَفٰی بِنَفْسِكَ [h:7700[ا]لْیَوْمَ عَلَیْكَ حَسِیْبًا ۟ؕ",
