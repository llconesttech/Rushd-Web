  "ayahs": [
    {
      "number": 1,
      "text": "اِ[q:7699[ق]ْرَاْ بِ[h:13285[ا]سْمِ رَبِّكَ [h:14559[ا]لَّذِیْ خَلَ[q[ق]َ ۟ۚ",
      "juz": 30,
