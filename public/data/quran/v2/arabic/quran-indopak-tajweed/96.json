      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "خَلَقَ [h:7324[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ مِنْ عَلَ[q[ق]ٍ ۟ۚ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اِ[q:7699[ق]ْرَاْ وَرَبُّكَ [h:14560[ا]لْاَكْرَمُ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "الَّذِیْ عَلَّمَ بِ[h:14561[ا]لْقَلَمِ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلَّمَ [h:14562[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ مَا لَمْ یَعْلَمْ ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "كَلَّ[o[ا]ۤ اِ[g[ن]َّ [h:7137[ا]لْاِ[f:5969[ن]ْ[f:5969[س]َانَ لَیَ[q:14563[ط]ْغٰۤی ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اَ[u:14564[ن]ْ [u:14564[ر]َّاٰهُ [h:14565[ا]سْتَغْنٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اِ[g[ن]َّ اِلٰی رَبِّكَ [h:14566[ا][l[ل]رُّ[q:14567[ج]ْعٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اَرَءَیْتَ [h:14568[ا]لَّذِیْ یَنْهٰی ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 10,
      "text": "عَ[q:3317[ب]ْدًا اِذَا صَلّٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اَرَءَیْتَ اِ[f:2594[ن]ْ [f:2594[ك]َانَ عَلَی [h:4019[ا]لْهُدٰۤی ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اَوْ اَمَرَ بِ[h:14569[ا][l[ل]تَّ[q:14569[ق]ْوٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اَرَءَیْتَ اِ[f:14570[ن]ْ [f:14570[ك]َذَّبَ وَتَوَلّٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اَلَمْ یَعْلَ[c:14571[م]ْ [c:14571[ب]ِاَ[g[ن]َّ [h:897[ا]للّٰهَ یَرٰی ۟ؕ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلَّا لَىِٕ[u:4195[ن]ْ [u:4195[ل]َّمْ یَ[f:10891[ن]ْ[f:10891[ت]َهِ ۙ۬ لَنَسْفَعً[i:14572[ا] [i:14572[ب]ِ[h:14573[ا][l[ل][g[ن]َّاصِیَةِ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 16,
      "text": "نَاصِیَ[f:14574[ة]ٍ [f:14574[ك]َاذِبَةٍ خَاطِئَةٍ ۟ۚ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَلْیَ[q:14575[د]ْعُ نَادِیَهٗ ۟ۙ",
      "juz": 30,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
