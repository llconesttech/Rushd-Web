{
  "number": 20,
  "name": "سُورَةُ طه",
  "englishName": "Taa-Haa",
  "ayahs": [
    {
      "number": 1,
      "text": "طٰهٰ ۟ۚ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَ[o[ا]ۤ اَ[f:515[ن]ْ[f:515[ز]َلْنَا عَلَیْكَ [h:8434[ا]لْقُرْاٰنَ لِتَشْقٰۤی ۟ۙ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اِلَّا تَذْكِرَ[u:8435[ة]ً [u:8435[ل]ِّمَ[a:8436[ن]ْ [a:8436[ی]َّخْشٰی ۟ۙ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَ[f:7929[ن]ْ[f:7929[ز]ِیْلً[a:8437[ا] [a:8437[م]ِّ[g[م]َّنْ خَلَقَ [h:8438[ا]لْاَرْضَ وَ[h:8439[ا][l[ل]سَّمٰوٰتِ [h:8440[ا]لْعُلٰی ۟ؕ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اَ[l[ل]رَّحْمٰنُ عَلَی [h:4740[ا]لْعَرْشِ [h:8441[ا]سْتَوٰی ۟",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 6,
      "text": "لَهٗ مَا فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَمَا فِی [h:44[ا]لْاَرْضِ وَمَا بَیْنَهُمَا وَمَا تَحْتَ [h:8442[ا][l[ل]ثَّرٰی ۟",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَاِ[f:8443[ن]ْ [f:8443[ت]َ[q:7936[ج]ْهَرْ بِ[h:7118[ا]لْقَوْلِ فَاِ[g[ن]َّهٗ یَعْلَمُ [h:8444[ا][l[ل]سِّرَّ وَاَخْفٰی ۟",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اَللّٰهُ لَ[o[ا]ۤ اِلٰهَ اِلَّا هُوَ ؕ لَهُ [h:8445[ا]لْاَسْمَ[o[ا]ٓءُ [h:5156[ا]لْحُسْنٰی ۟",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَهَلْ اَتٰىكَ حَدِیْثُ مُوْسٰی ۟ۘ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اِذْ رَاٰ نَا[f:8446[ر]ً[f:8446[ا] [f:8446[ف]َقَالَ لِاَهْلِهِ [h:8447[ا]مْكُثُ[o[و]ْۤ[s[ا] اِ[g[ن]ِّ[o[ی]ْۤ اٰنَسْتُ نَا[u:8448[ر]ً[u:8448[ا] [u:8448[ل]َّعَلِّ[o[ی]ْۤ اٰتِیْكُ[w:8449[م]ْ [w:8449[م]ِّنْهَا بِقَبَسٍ اَوْ اَجِدُ عَلَی [h:896[ا][l[ل][g[ن]َّارِ هُدًی ۟",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 11,
      "text": "فَلَ[g[م]َّ[o[ا]ۤ اَتٰىهَا نُوْدِیَ یٰمُوْسٰی ۟ؕ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اِ[g[ن]ِّ[o[ی]ْۤ اَنَا رَبُّكَ فَ[h:8450[ا]خْلَعْ نَعْلَیْكَ ۚ اِ[g[ن]َّكَ بِ[h:8451[ا]لْوَادِ [h:8452[ا]لْمُقَدَّسِ طُوًی ۟ؕ",
      "juz": 16,
      "page": 312,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وَاَنَا [h:8453[ا]خْتَرْتُكَ فَ[h:8454[ا]سْتَمِعْ لِمَا یُوْحٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اِ[g[ن]َّنِ[o[ی]ْۤ اَنَا [h:8455[ا]للّٰهُ لَ[o[ا]ۤ اِلٰهَ اِلَّ[o[ا]ۤ اَنَا فَ[h:8456[ا]عْبُ[q:8456[د]ْنِیْ ۙ وَاَقِمِ [h:6518[ا][l[ل]صَّلٰ[s[و]ةَ لِذِكْرِیْ ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اِ[g[ن]َّ [h:8457[ا][l[ل]سَّاعَةَ اٰتِیَةٌ اَكَادُ اُخْفِیْهَا لِتُ[q:8458[ج]ْزٰی كُلُّ نَفْ[i:6995[س]ٍ [i:6995[ب]ِمَا تَسْعٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 16,
      "text": "فَلَا یَصُدَّ[g[ن]َّكَ عَنْهَا مَ[u:6065[ن]ْ [u:6065[ل]َّا یُؤْمِنُ بِهَا وَ[h:3160[ا]تَّبَعَ هَوٰىهُ فَتَرْدٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَمَا تِلْكَ بِیَمِیْنِكَ یٰمُوْسٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 18,
      "text": "قَالَ هِیَ عَصَایَ ۚ اَتَوَكَّؤُ[s[ا] عَلَیْهَا وَاَهُشُّ بِهَا عَلٰی غَنَمِیْ وَلِیَ فِیْهَا مَاٰرِبُ اُخْرٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 19,
      "text": "قَالَ اَلْقِهَا یٰمُوْسٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 20,
      "text": "فَاَلْقٰى[f:8459[ه]َا فَاِذَا هِیَ حَیَّةٌ تَسْعٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 21,
      "text": "قَالَ خُذْهَا وَلَا تَخَفْ ۥ سَنُعِیْدُهَا سِیْرَتَهَا [h:8460[ا]لْاُوْلٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وَ[h:9999[ا]ضْمُمْ یَدَكَ اِلٰی جَنَاحِكَ تَخْرُ[q:8461[ج]ْ بَیْضَ[o[ا]ٓءَ مِنْ غَیْرِ سُ[o[و]ْٓءٍ اٰیَةً اُخْرٰی ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 23,
      "text": "لِنُرِیَكَ مِنْ اٰیٰتِنَا [h:8462[ا]لْكُ[q:8463[ب]ْرٰی ۟ۚ",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اِذْهَ[q:7814[ب]ْ اِلٰی فِرْعَوْنَ اِ[g[ن]َّهٗ طَغٰی ۟۠",
      "juz": 16,
      "page": 313,
      "ruku": 270,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 25,
      "text": "قَالَ رَبِّ [h:8464[ا]شْرَحْ لِیْ صَ[q:8465[د]ْرِیْ ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَیَسِّرْ لِ[o[ی]ْۤ اَمْرِیْ ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَ[h:9999[ا]حْلُلْ عُ[q:8466[ق]ْدَ[a:8467[ة]ً [a:8467[م]ِّ[u:8468[ن]ْ [u:8468[ل]ِّسَانِیْ ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 28,
      "text": "یَفْقَهُوْ[s[ا] قَوْلِیْ ۪۟",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَ[h:9999[ا][q:2930[ج]ْعَلْ لِّیْ وَزِیْ[a:8469[ر]ً[a:8469[ا] [a:8469[م]ِّنْ اَهْلِیْ ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 30,
      "text": "هٰرُوْنَ اَخِی ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اشْدُ[q:8470[د]ْ بِهٖۤ اَزْرِیْ ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَاَشْرِكْهُ فِ[o[ی]ْۤ اَمْرِیْ ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 33,
      "text": "كَیْ نُسَبِّحَكَ كَثِیْرًا ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَّنَذْكُرَكَ كَثِیْرًا ۟ؕ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اِ[g[ن]َّكَ كُ[f:735[ن]ْ[f:735[ت]َ بِنَا بَصِیْرًا ۟",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 36,
      "text": "قَالَ قَ[q:298[د]ْ اُوْتِیْتَ سُؤْلَكَ یٰمُوْسٰی ۟",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وَلَقَ[q:341[د]ْ مَنَ[g[ن]َّا عَلَیْكَ مَرَّةً اُخْرٰۤی ۟ۙ",
      "juz": 16,
      "page": 313,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اِذْ اَوْحَیْنَ[o[ا]ۤ اِلٰۤ[o[ی] اُ[g[م]ِّكَ مَا یُوْحٰۤی ۟ۙ",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اَنِ [h:8471[ا][q:8472[ق]ْذِفِیْهِ فِی [h:8473[ا][l[ل]تَّابُوْتِ فَ[h:8474[ا][q:8474[ق]ْذِفِیْهِ فِی [h:4971[ا]لْیَ[g[م]ِّ فَلْیُلْقِهِ [h:8475[ا]لْیَ[g[م]ُّ بِ[h:8476[ا][l[ل]سَّاحِلِ یَاْخُذْهُ عَدُ[u:8477[و]ٌّ [u:8477[ل]ِّیْ وَعَدُ[u:8478[و]ٌّ [u:8478[ل]َّهٗ ؕ وَاَلْقَیْتُ عَلَیْكَ مَحَبَّ[a:8479[ة]ً [a:8479[م]ِّ[g[ن]ِّیْ ۚ۬ وَلِتُصْنَعَ عَلٰی عَیْنِیْ ۟ۘ",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اِذْ تَمْشِ[o[ی]ْۤ اُخْتُكَ فَتَقُوْلُ هَلْ اَدُلُّكُمْ عَلٰی مَ[a:8480[ن]ْ [a:8480[ی]َّكْفُلُهٗ ؕ فَرَجَعْنٰكَ اِلٰۤ[o[ی] اُ[g[م]ِّكَ كَیْ تَقَرَّ عَیْنُهَا وَلَا تَحْزَنَ ؕ۬ وَقَتَلْتَ نَفْ[f:8481[س]ً[f:8481[ا] [f:8481[ف]َنَجَّیْنٰكَ مِنَ [h:8482[ا]لْغَ[g[م]ِّ وَفَتَ[g[ن]ّٰكَ فُتُوْ[f:8483[ن]ً[f:8483[ا] ۫۬ [f:8483[ف]َلَبِثْتَ سِنِیْنَ فِ[o[ی]ْۤ اَهْلِ مَ[q:4842[د]ْیَنَ ۙ۬ ثُ[g[م]َّ جِئْتَ عَلٰی قَدَ[a:8484[ر]ٍ [a:8484[ی]ّٰمُوْسٰی ۟",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَ[h:9999[ا]صْطَنَعْتُكَ لِنَفْسِیْ ۟ۚ",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 42,
      "text": "اِذْهَ[q:7814[ب]ْ اَ[f:169[ن]ْ[f:169[ت]َ وَاَخُوْكَ بِاٰیٰتِیْ وَلَا تَنِیَا فِیْ ذِكْرِیْ ۟ۚ",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اِذْهَبَ[o[ا]ۤ اِلٰی فِرْعَوْنَ اِ[g[ن]َّهٗ طَغٰی ۟ۚۖ",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 44,
      "text": "فَقُوْلَا لَهٗ قَوْلً[u:8485[ا] [u:8485[ل]َّیِّ[u:8486[ن]ً[u:8486[ا] [u:8486[ل]َّعَلَّهٗ یَتَذَكَّرُ اَوْ یَخْشٰی ۟",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 45,
      "text": "قَالَا رَبَّنَ[o[ا]ۤ اِ[g[ن]َّنَا نَخَافُ اَ[a:8487[ن]ْ [a:8487[ی]َّفْرُطَ عَلَیْنَ[o[ا]ۤ اَوْ اَ[a:8488[ن]ْ [a:8488[ی]َّ[q:8489[ط]ْغٰی ۟",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 46,
      "text": "قَالَ لَا تَخَافَ[o[ا]ۤ اِ[g[ن]َّنِیْ مَعَكُمَ[o[ا]ۤ اَسْمَعُ وَاَرٰی ۟",
      "juz": 16,
      "page": 314,
      "ruku": 271,
      "hizbQuarter": 125,
      "sajda": false
    },
    {
      "number": 47,
