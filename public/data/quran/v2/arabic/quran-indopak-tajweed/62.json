{
  "number": 62,
  "name": "سُورَةُ الجُمُعَةِ",
  "englishName": "Al-Jumu'a",
  "ayahs": [
    {
      "number": 1,
      "text": "یُسَبِّحُ لِلّٰهِ مَا فِی [h:609[ا][l[ل]سَّمٰوٰتِ وَمَا فِی [h:44[ا]لْاَرْضِ [h:13591[ا]لْمَلِكِ [h:13592[ا]لْقُدُّوْسِ [h:13593[ا]لْعَزِیْزِ [h:2229[ا]لْحَكِ[p[ی]ْمِ ۟",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 2,
      "text": "هُوَ [h:153[ا]لَّذِیْ بَعَثَ فِی [h:2033[ا]لْاُ[g[م]ِّیّٖنَ رَسُوْلً[a:677[ا] [a:677[م]ِّنْهُمْ یَتْلُوْ[s[ا] عَلَیْهِمْ اٰیٰتِهٖ وَیُزَكِّیْهِمْ وَیُعَلِّمُهُمُ [h:678[ا]لْكِتٰبَ وَ[h:679[ا]لْحِكْمَةَ ۗ وَاِ[f:2392[ن]ْ [f:2392[ك]َانُوْ[s[ا] مِ[f:128[ن]ْ [f:128[ق]َ[q:129[ب]ْلُ لَفِیْ ضَلٰ[a:2393[ل]ٍ [a:2393[م]ُّبِ[p[ی]ْنٍ ۟ۙ",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَّاٰخَرِیْنَ مِنْهُمْ لَ[g[م]َّا یَلْحَقُوْ[s[ا] بِهِمْ ؕ وَهُوَ [h:7045[ا]لْعَزِیْزُ [h:681[ا]لْحَكِ[p[ی]ْمُ ۟",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ذٰلِكَ فَضْلُ [h:337[ا]للّٰهِ یُؤْتِیْهِ مَ[a:476[ن]ْ [a:476[ی]َّشَ[o[ا]ٓءُ ؕ وَ[h:72[ا]للّٰهُ ذُو [h:550[ا]لْفَضْلِ [h:551[ا]لْعَظِ[p[ی]ْمِ ۟",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 5,
      "text": "مَثَلُ [h:1179[ا]لَّذِیْنَ حُ[g[م]ِّلُو[s[ا] [h:13594[ا][l[ل]تَّوْرٰىةَ ثُمَّ لَمْ یَحْمِلُوْهَ[h:4053[ا] كَمَثَلِ [h:72[ا]لْحِمَارِ یَحْمِ[l[ل]ُ اَسْفَارًا ؕ بِئْسَ مَثَلُ الْقَوْمِ الَّذِیْنَ كَذَّبُوْا بِاٰیٰتِ اللّٰهِ ؕ وَاللّٰهُ لَا یَهْدِی الْقَوْمَ الظّٰلِمِیْنَ ۟",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 6,
      "text": "قُلْ یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ هَادُ[o[و]ْۤ[s[ا] اِ[f:13597[ن]ْ [f:13597[ز]َعَمْتُمْ اَ[g[ن]َّكُمْ اَوْلِیَ[o[ا]ٓءُ لِلّٰهِ مِ[f:107[ن]ْ [f:107[د]ُوْنِ [h:493[ا][l[ل][g[ن]َّاسِ فَتَمَ[g[ن]َّوُ[s[ا] [h:494[ا]لْمَوْتَ اِ[f:109[ن]ْ [f:109[ك]ُ[f:100[ن]ْ[f:100[ت]ُمْ صٰدِقِ[p[ی]ْنَ ۟",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَلَا یَتَمَ[g[ن]َّوْنَهٗۤ اَبَدً[i:496[ا] [i:496[ب]ِمَا قَدَّمَتْ اَیْدِیْهِمْ ؕ وَ[h:72[ا]للّٰهُ عَلِیْ[i:497[م]ٌۢ [i:497[ب]ِ[h:498[ا][l[ل]ظّٰلِمِ[p[ی]ْنَ ۟",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 8,
      "text": "قُلْ اِ[g[ن]َّ [h:13598[ا]لْمَوْتَ [h:13599[ا]لَّذِیْ تَفِرُّوْنَ مِنْهُ فَاِ[g[ن]َّهٗ مُلٰقِیْكُمْ ثُ[g[م]َّ تُرَدُّوْنَ اِلٰی عٰلِمِ [h:5788[ا]لْغَیْبِ وَ[h:4187[ا][l[ل]شَّهَادَةِ فَیُنَبِّئُكُ[c:3605[م]ْ [c:3605[ب]ِمَا كُ[f:100[ن]ْ[f:100[ت]ُمْ تَعْمَلُ[p[و]ْنَ ۟۠",
      "juz": 28,
      "page": 553,
      "ruku": 486,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 9,
      "text": "یٰۤاَیُّهَا [h:541[ا]لَّذِیْنَ اٰمَنُ[o[و]ْۤ[s[ا] اِذَا نُوْدِیَ لِلصَّلٰ[s[و]ةِ مِ[a:13600[ن]ْ [a:13600[ی]َّوْمِ [h:13601[ا]لْجُمُعَةِ فَ[h:13602[ا]سْعَوْ[s[ا] اِلٰی ذِكْرِ [h:3774[ا]للّٰهِ وَذَرُو[s[ا] [h:13603[ا]لْبَیْعَ ؕ ذٰلِكُمْ خَیْ[u:263[ر]ٌ [u:263[ل]َّكُمْ اِ[f:109[ن]ْ [f:109[ك]ُ[f:100[ن]ْ[f:100[ت]ُمْ تَعْلَمُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 554,
      "ruku": 487,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَاِذَا قُضِیَتِ [h:13604[ا][l[ل]صَّلٰ[s[و]ةُ فَ[h:10874[ا][f:10874[ن]ْ[f:10874[ت]َشِرُوْ[s[ا] فِی [h:44[ا]لْاَرْضِ وَ[h:988[ا][q:988[ب]ْتَغُوْ[s[ا] مِ[f:6655[ن]ْ [f:6655[ف]َضْلِ [h:6656[ا]للّٰهِ وَ[h:335[ا]ذْكُرُو[s[ا] [h:1113[ا]للّٰهَ كَثِیْ[u:5368[ر]ً[u:5368[ا] [u:5368[ل]َّعَلَّكُمْ تُفْلِحُ[p[و]ْنَ ۟",
      "juz": 28,
      "page": 554,
      "ruku": 487,
      "hizbQuarter": 221,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَاِذَا رَاَوْ[s[ا] تِجَارَةً اَ[o[و]ْ لَهْوَ[o[ا] [f:264[ن]ْفَضُّوْۤ[h:13609[ا] [h:13610[ا]ِ[l[ل]َیْهَ[h:72[ا] وَتَرَكُوْكَ قَآ[p[ى]ِٕمًا ؕ قُلْ مَا عِنْدَ اللّٰهِ خَیْرٌ مِّنَ اللَّهْوِ وَمِنَ التِّجَارَةِ ؕ وَاللّٰهُ خَیْرُ الرّٰزِقِیْنَ ۟۠",
      "juz": 28,
      "page": 554,
      "ruku": 487,
      "hizbQuarter": 221,
      "sajda": false
    }
  ]
}