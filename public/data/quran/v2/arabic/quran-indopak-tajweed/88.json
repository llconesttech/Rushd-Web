{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "ayahs": [
    {
      "number": 1,
      "text": "هَلْ اَتٰىكَ حَدِیْثُ الْغَاشِیَةِ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وُجُوْ[a:14135[ه]ٌ [a:14135[ی]َّوْمَىِٕذٍ خَاشِعَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "عَامِلَ[a:14452[ة]ٌ [a:14452[ن]َّاصِبَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَصْلٰی نَارًا حَامِیَةً ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تُسْقٰی مِنْ عَیْنٍ اٰنِیَةٍ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "لَیْسَ لَهُمْ طَعَامٌ اِلَّا مِ[f:14453[ن]ْ [f:14453[ض]َرِ[p[ی]ْعٍ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "لَّا یُسْمِنُ وَلَا یُغْنِیْ مِ[f:14454[ن]ْ [f:14454[ج]ُ[p[و]ْعٍ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وُجُوْ[a:14135[ه]ٌ [a:14135[ی]َّوْمَىِٕ[a:14455[ذ]ٍ [a:14455[ن]َّاعِمَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "لِّسَعْیِهَا رَاضِیَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فِیْ جَ[g[ن]َّةٍ عَالِیَةٍ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "لَّا تَسْمَعُ فِیْهَا لَاغِیَةً ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "فِیْهَا عَیْ[f:14456[ن]ٌ [f:14456[ج]َارِیَةٌ ۟ۘ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فِیْهَا سُرُ[a:14457[ر]ٌ [a:14457[م]َّرْفُوْعَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَّاَكْوَا[a:14458[ب]ٌ [a:14458[م]َّوْضُوْعَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَّنَمَارِقُ مَصْفُوْفَةٌ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَّزَرَابِیُّ مَ[q:14459[ب]ْثُوْثَةٌ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اَفَلَا یَ[f:1141[ن]ْ[f:1141[ظ]ُرُوْنَ اِلَی [h:14460[ا]لْاِبِلِ كَیْفَ خُلِقَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "وَاِلَی [h:14461[ا][l[ل]سَّمَ[o[ا]ٓءِ كَیْفَ رُفِعَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وَاِلَی [h:14462[ا]لْجِبَالِ كَیْفَ نُصِبَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "وَاِلَی [h:14463[ا]لْاَرْضِ كَیْفَ سُطِحَتْ ۟ۥ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَذَكِّرْ ؕ۫ اِ[g[ن]َّمَ[o[ا]ۤ اَ[f:169[ن]ْ[f:169[ت]َ مُذَكِّرٌ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "لَسْتَ عَلَیْهِ[c:14464[م]ْ [c:14464[ب]ِمُصَۜیْطِرٍ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اِلَّا مَ[f:12995[ن]ْ [f:12995[ت]َوَلّٰی وَكَفَرَ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "فَیُعَذِّبُهُ [h:14465[ا]للّٰهُ [h:14466[ا]لْعَذَابَ [h:14467[ا]لْاَكْبَرَ ۟ؕ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اِ[g[ن]َّ اِلَیْنَ[o[ا]ۤ اِیَابَهُمْ ۟ۙ",
      "juz": 30,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ثُ[g[م]َّ اِ[g[ن]َّ عَلَیْنَا حِسَابَهُمْ ۟۠",
      "juz": 30,
      "page": 592,
      "ruku": 530,
