{
  "number": 75,
  "name": "سُورَةُ القِيَامَةِ",
  "englishName": "Al-Qiyaama",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَٰمَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 3,
      "text": "أَيَحْسَبُ ٱلْإِنسَٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 4,
      "text": "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 5,
      "text": "بَلْ يُرِيدُ ٱلْإِنسَٰنُ لِيَفْجُرَ أَمَامَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَٰمَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 7,
      "text": "فَإِذَا بَرِقَ ٱلْبَصَرُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَخَسَفَ ٱلْقَمَرُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 10,
      "text": "يَقُولُ ٱلْإِنسَٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَلَّا لَا وَزَرَ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 13,
      "text": "يُنَبَّؤُا۟ ٱلْإِنسَٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 14,
      "text": "بَلِ ٱلْإِنسَٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 16,
      "text": "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 17,
      "text": "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فَإِذَا قَرَأْنَٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 19,
      "text": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ",
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 20,
      "text": "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وَتَذَرُونَ ٱلْءَاخِرَةَ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 22,
      "text": "وُجُوهٌۭ يَوْمَئِذٍۢ نَّاضِرَةٌ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 23,
      "text": "إِلَىٰ رَبِّهَا نَاظِرَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وَوُجُوهٌۭ يَوْمَئِذٍۭ بَاسِرَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 26,
      "text": "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 27,
      "text": "وَقِيلَ مَنْ ۜ رَاقٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 28,
      "text": "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 30,
      "text": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 512,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 31,
      "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 32,
      "text": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 33,
      "text": "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 34,
      "text": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 36,
      "text": "أَيَحْسَبُ ٱلْإِنسَٰنُ أَن يُتْرَكَ سُدًى",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 37,
      "text": "أَلَمْ يَكُ نُطْفَةًۭ مِّن مَّنِىٍّۢ يُمْنَىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 38,
      "text": "ثُمَّ كَانَ عَلَقَةًۭ فَخَلَقَ فَسَوَّىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 39,
      "text": "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    },
    {
      "number": 40,
      "text": "أَلَيْسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحْۦِىَ ٱلْمَوْتَىٰ",
      "juz": 29,
      "manzil": 7,
      "page": 578,
      "ruku": 513,
      "hizbQuarter": 231,
      "sajda": false
    }
  ]
}