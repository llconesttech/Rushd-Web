{
  "number": 100,
  "name": "سُورَةُ العَادِيَاتِ",
  "englishName": "Al-Aadiyaat",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلْعَٰدِيَٰتِ ضَبْحًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَٱلْمُورِيَٰتِ قَدْحًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "فَٱلْمُغِيرَٰتِ صُبْحًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَأَثَرْنَ بِهِۦ نَقْعًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَوَسَطْنَ بِهِۦ جَمْعًا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "إِنَّ ٱلْإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍۢ لَّخَبِيرٌۢ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}