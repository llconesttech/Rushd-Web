{
  "number": 97,
  "name": "سُورَةُ القَدۡرِ",
  "englishName": "Al-Qadr",
  "ayahs": [
    {
      "number": 1,
      "text": "بِّسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِنَّآ أَنزَلْنَٰهُ فِى لَيْلَةِ ٱلْقَدْرِ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَمَآ أَدْرَىٰكَ مَا لَيْلَةُ ٱلْقَدْرِ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لَيْلَةُ ٱلْقَدْرِ خَيْرٌۭ مِّنْ أَلْفِ شَهْرٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "تَنَزَّلُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "سَلَٰمٌ هِىَ حَتَّىٰ مَطْلَعِ ٱلْفَجْرِ",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
