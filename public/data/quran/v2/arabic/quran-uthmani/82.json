{
  "number": 82,
  "name": "سُورَةُ الانفِطَارِ",
  "englishName": "Al-Infitaar",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلِمَتْ نَفْسٌۭ مَّا قَدَّمَتْ وَأَخَّرَتْ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 6,
      "text": "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 7,
      "text": "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فِىٓ أَىِّ صُورَةٍۢ مَّا شَآءَ رَكَّبَكَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 9,
      "text": "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَإِنَّ عَلَيْكُمْ لَحَٰفِظِينَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كِرَامًۭا كَٰتِبِينَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 12,
      "text": "يَعْلَمُونَ مَا تَفْعَلُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 15,
      "text": "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 19,
      "text": "يَوْمَ لَا تَمْلِكُ نَفْسٌۭ لِّنَفْسٍۢ شَيْـًۭٔا ۖ وَٱلْأَمْرُ يَوْمَئِذٍۢ لِّلَّهِ",
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 524,
      "hizbQuarter": 235,
      "sajda": false
    }
  ]
}