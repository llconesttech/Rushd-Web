{
  "number": 104,
  "name": "سُورَةُ الهُمَزَةِ",
  "englishName": "Al-Humaza",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَيْلٌۭ لِّكُلِّ هُمَزَةٍۢ لُّمَزَةٍ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ٱلَّذِى جَمَعَ مَالًۭا وَعَدَّدَهُۥ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلْحُطَمَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "نَارُ ٱللَّهِ ٱلْمُوقَدَةُ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "فِى عَمَدٍۢ مُّمَدَّدَةٍۭ",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}