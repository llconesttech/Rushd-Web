{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ٱلنَّجْمُ ٱلثَّاقِبُ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَمَا هُوَ بِٱلْهَزْلِ",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَأَكِيدُ كَيْدًۭا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}