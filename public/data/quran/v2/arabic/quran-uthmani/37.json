      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 180,
      "text": "سُبْحَٰنَ رَبِّكَ رَبِّ ٱلْعِزَّةِ عَمَّا يَصِفُونَ",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 181,
