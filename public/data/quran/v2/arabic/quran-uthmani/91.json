{
  "number": 91,
  "name": "سُورَةُ الشَّمۡسِ",
  "englishName": "Ash-Shams",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلشَّمْسِ وَضُحَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَنَفْسٍۢ وَمَا سَوَّىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "وَلَا يَخَافُ عُقْبَٰهَا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}