{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 10,
      "text": "عَبْدًا إِذَا صَلَّىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 11,
      "text": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 12,
      "text": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 13,
      "text": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 14,
      "text": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 15,
      "text": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 16,
      "text": "نَاصِيَةٍۢ كَٰذِبَةٍ خَاطِئَةٍۢ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 17,
      "text": "فَلْيَدْعُ نَادِيَهُۥ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 18,
      "text": "سَنَدْعُ ٱلزَّبَانِيَةَ",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 19,
      "text": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
        "id": 15,
        "recommended": false,
        "obligatory": true
      }
    }
  ]
}