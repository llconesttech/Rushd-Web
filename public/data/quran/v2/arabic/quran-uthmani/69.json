{
  "number": 69,
  "name": "سُورَةُ الحَاقَّةِ",
  "englishName": "Al-Haaqqa",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱلْحَآقَّةُ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 2,
      "text": "مَا ٱلْحَآقَّةُ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 4,
      "text": "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 5,
      "text": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وَأَمَّا عَادٌۭ فَأُهْلِكُوا۟ بِرِيحٍۢ صَرْصَرٍ عَاتِيَةٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 7,
      "text": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍۢ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومًۭا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 8,
      "text": "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 566,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 9,
      "text": "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَٰتُ بِٱلْخَاطِئَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 10,
      "text": "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةًۭ رَّابِيَةً",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 11,
      "text": "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَٰكُمْ فِى ٱلْجَارِيَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 12,
      "text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةًۭ وَتَعِيَهَآ أُذُنٌۭ وَٰعِيَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌۭ وَٰحِدَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 14,
      "text": "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةًۭ وَٰحِدَةًۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 15,
      "text": "فَيَوْمَئِذٍۢ وَقَعَتِ ٱلْوَاقِعَةُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 16,
      "text": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍۢ وَاهِيَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍۢ ثَمَٰنِيَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 18,
      "text": "يَوْمَئِذٍۢ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 19,
      "text": "فَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَٰبِيَهْ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 20,
      "text": "إِنِّى ظَنَنتُ أَنِّى مُلَٰقٍ حِسَابِيَهْ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 21,
      "text": "فَهُوَ فِى عِيشَةٍۢ رَّاضِيَةٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 22,
      "text": "فِى جَنَّةٍ عَالِيَةٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 23,
      "text": "قُطُوفُهَا دَانِيَةٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 24,
      "text": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيْتَنِى لَمْ أُوتَ كِتَٰبِيَهْ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 26,
      "text": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 27,
      "text": "يَٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 28,
      "text": "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 29,
      "text": "هَلَكَ عَنِّى سُلْطَٰنِيَهْ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 30,
      "text": "خُذُوهُ فَغُلُّوهُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 31,
      "text": "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 32,
      "text": "ثُمَّ فِى سِلْسِلَةٍۢ ذَرْعُهَا سَبْعُونَ ذِرَاعًۭا فَٱسْلُكُوهُ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 33,
      "text": "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 34,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "juz": 29,
      "manzil": 7,
      "page": 567,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 35,
      "text": "فَلَيْسَ لَهُ ٱلْيَوْمَ هَٰهُنَا حَمِيمٌۭ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 36,
      "text": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 37,
      "text": "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَٰطِـُٔونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 500,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 38,
      "text": "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 39,
      "text": "وَمَا لَا تُبْصِرُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 40,
      "text": "إِنَّهُۥ لَقَوْلُ رَسُولٍۢ كَرِيمٍۢ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 41,
      "text": "وَمَا هُوَ بِقَوْلِ شَاعِرٍۢ ۚ قَلِيلًۭا مَّا تُؤْمِنُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 42,
      "text": "وَلَا بِقَوْلِ كَاهِنٍۢ ۚ قَلِيلًۭا مَّا تَذَكَّرُونَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 43,
      "text": "تَنزِيلٌۭ مِّن رَّبِّ ٱلْعَٰلَمِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 45,
      "text": "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 46,
      "text": "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 47,
      "text": "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَٰجِزِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 48,
      "text": "وَإِنَّهُۥ لَتَذْكِرَةٌۭ لِّلْمُتَّقِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 49,
      "text": "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 50,
      "text": "وَإِنَّهُۥ لَحَسْرَةٌ عَلَى ٱلْكَٰفِرِينَ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 51,
      "text": "وَإِنَّهُۥ لَحَقُّ ٱلْيَقِينِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    },
    {
      "number": 52,
      "text": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ",
      "juz": 29,
      "manzil": 7,
      "page": 568,
      "ruku": 501,
      "hizbQuarter": 227,
      "sajda": false
    }
  ]
}