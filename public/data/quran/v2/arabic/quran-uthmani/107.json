{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "ayahs": [
    {
      "number": 1,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَرَءَيْتَ ٱلَّذِى يُكَذِّبُ بِٱلدِّينِ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "فَذَٰلِكَ ٱلَّذِى يَدُعُّ ٱلْيَتِيمَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فَوَيْلٌۭ لِّلْمُصَلِّينَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ٱلَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ٱلَّذِينَ هُمْ يُرَآءُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وَيَمْنَعُونَ ٱلْمَاعُونَ",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}