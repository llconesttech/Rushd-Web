{
  "number": 36,
  "name": "سُورَةُ يسٓ",
  "englishName": "Yaseen",
  "englishNameTranslation": "Yaseen",
  "revelationType": "Meccan",
  "numberOfAyahs": 83,
  "ayahs": [
    {
      "number": 3706,
      "text": "yâ-sîn.",
      "numberInSurah": 1,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3707,
      "text": "velḳur'âni-lḥakîm.",
      "numberInSurah": 2,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3708,
      "text": "inneke lemine-lmürselîn.",
      "numberInSurah": 3,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3709,
      "text": "`alâ ṣirâṭim müsteḳîm.",
      "numberInSurah": 4,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3710,
      "text": "tenzîle-l`azîzi-rraḥîm.",
      "numberInSurah": 5,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3711,
      "text": "litünẕira ḳavmem mâ ünẕira âbâühüm fehüm gâfilûn.",
      "numberInSurah": 6,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3712,
      "text": "leḳad ḥaḳḳa-lḳavlü `alâ ekŝerihim fehüm lâ yü'minûn.",
      "numberInSurah": 7,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3713,
      "text": "innâ ce`alnâ fî a`nâḳihim aglâlen fehiye ile-l'eẕḳâni fehüm muḳmeḥûn.",
      "numberInSurah": 8,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3714,
      "text": "vece`alnâ mim beyni eydîhim seddev vemin ḫalfihim sedden feagşeynâhüm fehüm lâ yübṣirûn.",
      "numberInSurah": 9,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3715,
      "text": "vesevâün `aleyhim eenẕertehüm em lem tünẕirhüm lâ yü'minûn.",
      "numberInSurah": 10,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3716,
      "text": "innemâ tünẕiru meni-ttebe`a-ẕẕikra veḫaşiye-rraḥmâne bilgayb. febeşşirhü bimagfirativ veecrin kerîm.",
      "numberInSurah": 11,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3717,
      "text": "innâ naḥnü nuḥyi-lmevtâ venektübü mâ ḳaddemû veâŝârahüm. vekülle şey'in aḥṣaynâhü fî imâmim mübîn.",
      "numberInSurah": 12,
      "juz": 22,
      "manzil": 5,
      "page": 440,
      "ruku": 381,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3718,
      "text": "vaḍrib lehüm meŝelen aṣḥâbe-lḳaryeh. iẕ câehe-lmürselûn.",
      "numberInSurah": 13,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3719,
      "text": "iẕ erselnâ ileyhimü-ŝneyni fekeẕẕebûhümâ fe`azzeznâ biŝâliŝin feḳâlû innâ ileyküm mürselûn.",
      "numberInSurah": 14,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3720,
      "text": "ḳâlû mâ entüm illâ beşerum miŝlünâ vemâ enzele-rraḥmânü min şey'in in entüm illâ tekẕibûn.",
      "numberInSurah": 15,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3721,
      "text": "ḳâlû rabbünâ ya`lemü innâ ileyküm lemürselûn.",
      "numberInSurah": 16,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3722,
      "text": "vemâ `aleynâ ille-lbelâgu-lmübîn.",
      "numberInSurah": 17,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3723,
      "text": "ḳâlû innâ teṭayyernâ biküm. leil lem tentehû lenercümenneküm veleyemessenneküm minnâ `aẕâbün elîm.",
      "numberInSurah": 18,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3724,
      "text": "ḳâlû ṭâiruküm me`aküm. ein ẕükkirtüm. bel entüm ḳavmüm müsrifûn.",
      "numberInSurah": 19,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3725,
      "text": "vecâe min aḳṣe-lmedîneti racülüy yes`â ḳâle yâ ḳavmi-ttebi`ü-lmürselîn.",
      "numberInSurah": 20,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3726,
      "text": "ittebi`û mel lâ yes'elüküm ecrav vehüm mühtedûn.",
      "numberInSurah": 21,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3727,
      "text": "vemâ liye lâ a`büdü-lleẕî feṭaranî veileyhi türce`ûn.",
      "numberInSurah": 22,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3728,
      "text": "eetteḫiẕü min dûnihî âliheten iy yüridni-rraḥmânü biḍurril lâ tugni `annî şefâ`atühüm şey'ev velâ yünḳiẕûn.",
      "numberInSurah": 23,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3729,
      "text": "innî iẕel lefî ḍalâlim mübîn.",
      "numberInSurah": 24,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3730,
      "text": "innî âmentü birabbiküm fesme`ûn.",
      "numberInSurah": 25,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3731,
      "text": "ḳîle-dḫuli-lcenneh. ḳâle yâ leyte ḳavmî ya`lemûn.",
      "numberInSurah": 26,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3732,
      "text": "bimâ gafera lî rabbî vece`alenî mine-lmükramîn.",
      "numberInSurah": 27,
      "juz": 22,
      "manzil": 5,
      "page": 441,
      "ruku": 382,
      "hizbQuarter": 176,
      "sajda": false
    },
    {
      "number": 3733,
      "text": "vemâ enzelnâ `alâ ḳavmihî mim ba`dihî min cündim mine-ssemâi vemâ künnâ münzilîn.",
      "numberInSurah": 28,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3734,
      "text": "in kânet illâ ṣayḥatev vâḥideten feiẕâ hüm ḫâmidûn.",
      "numberInSurah": 29,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3735,
      "text": "yâ ḥasraten `ale-l`ibâd. mâ ye'tîhim mir rasûlin illâ kânû bihî yestehziûn.",
      "numberInSurah": 30,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3736,
      "text": "elem yerav kem ehleknâ ḳablehüm mine-lḳurûni ennehüm ileyhim lâ yerci`ûn.",
      "numberInSurah": 31,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3737,
      "text": "vein küllül lemmâ cemî`ul ledeynâ muḥḍarûn.",
      "numberInSurah": 32,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 382,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3738,
      "text": "veâyetül lehümü-l'arḍu-lmeyteh. aḥyeynâhâ veaḫracnâ minhâ ḥabben feminhü ye'külûn.",
      "numberInSurah": 33,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3739,
      "text": "vece`alnâ fîhâ cennâtim min neḫîliv vea`nâbiv vefeccernâ fîhâ mine-l`uyûn.",
      "numberInSurah": 34,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3740,
      "text": "liye'külû min ŝemerihî vemâ `amilethü eydîhim. efelâ yeşkürûn.",
      "numberInSurah": 35,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3741,
      "text": "sübḥâne-lleẕî ḫaleḳa-l'ezvâce küllehâ mimmâ tümbitü-l'arḍu vemin enfüsihim vemimmâ lâ ya`lemûn.",
      "numberInSurah": 36,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3742,
      "text": "veâyetül lehümü-lleyl. nesleḫu minhü-nnehâra feiẕâ hüm mużlimûn.",
      "numberInSurah": 37,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3743,
      "text": "veşşemsü tecrî limüsteḳarril lehâ. ẕâlike taḳdîru-l`azîzi-l`alîm.",
      "numberInSurah": 38,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3744,
      "text": "velḳamera ḳaddernâhü menâzile ḥattâ `âde kel`urcûni-lḳadîm.",
      "numberInSurah": 39,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3745,
      "text": "le-şşemsü yembegî lehâ en tüdrike-lḳamera vele-lleylü sâbiḳu-nnehâr. veküllün fî felekiy yesbeḥûn.",
      "numberInSurah": 40,
      "juz": 23,
      "manzil": 5,
      "page": 442,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3746,
      "text": "veâyetül lehüm ennâ ḥamelnâ ẕürriyyetehüm fi-lfülki-lmeşḥûn.",
      "numberInSurah": 41,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3747,
      "text": "veḫalaḳnâ lehüm mim miŝlihî mâ yerkebûn.",
      "numberInSurah": 42,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3748,
      "text": "vein neşe' nugriḳhüm felâ ṣarîḫa lehüm velâ hüm yünḳaẕûn.",
      "numberInSurah": 43,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3749,
      "text": "illâ raḥmetem minnâ vemetâ`an ilâ ḥîn.",
      "numberInSurah": 44,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3750,
      "text": "veiẕâ ḳîle lehümü-tteḳû mâ beyne eydîküm vemâ ḫalfeküm le`alleküm türḥamûn.",
      "numberInSurah": 45,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3751,
      "text": "vemâ te'tîhim min âyetim min âyâti rabbihim illâ kânû `anhâ mü`riḍîn.",
      "numberInSurah": 46,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3752,
      "text": "veiẕâ ḳîle lehüm enfiḳû mimmâ razeḳakümü-llâhü ḳâle-lleẕîne keferû lilleẕîne âmenû enuṭ`imü mel lev yeşâü-llâhü aṭ`ameh. in entüm illâ fî ḍalâlim mübîn.",
      "numberInSurah": 47,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3753,
      "text": "veyeḳûlûne metâ hâẕe-lva`dü in küntüm ṣâdiḳîn.",
      "numberInSurah": 48,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3754,
      "text": "mâ yenżurûne illâ ṣayḥatev vâḥideten te'ḫuẕühüm vehüm yeḫiṣṣimûn.",
      "numberInSurah": 49,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3755,
      "text": "felâ yesteṭî`ûne tevṣiyetev velâ ilâ ehlihim yerci`ûn.",
      "numberInSurah": 50,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 383,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3756,
      "text": "venüfiḫa fi-ṣṣûri feiẕâ hüm mine-l'ecdâŝi ilâ rabbihim yensilûn.",
      "numberInSurah": 51,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3757,
      "text": "ḳâlû yâ veylenâ mem be`aŝenâ mim merḳadinâ. hâẕâ mâ ve`ade-rraḥmânü veṣadeḳa-lmürselûn.",
      "numberInSurah": 52,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3758,
      "text": "in kânet illâ ṣayḥatev vâḥideten feiẕâ hüm cemî`ul ledeynâ muḥḍarûn.",
      "numberInSurah": 53,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3759,
      "text": "felyevme lâ tużlemü nefsün şey'ev velâ tüczevne illâ mâ küntüm ta`melûn.",
      "numberInSurah": 54,
      "juz": 23,
      "manzil": 5,
      "page": 443,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3760,
      "text": "inne aṣḥâbe-lcennehi-lyevme fî şügulin fâkihûn.",
      "numberInSurah": 55,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3761,
      "text": "hüm veezvâcühüm fî żilâlin `ale-l'erâiki müttekiûn.",
      "numberInSurah": 56,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3762,
      "text": "lehüm fîhâ fâkihetüv velehüm mâ yedde`ûn.",
      "numberInSurah": 57,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3763,
      "text": "selâmün ḳavlem mir rabbir raḥîm.",
      "numberInSurah": 58,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3764,
      "text": "vemtâzü-lyevme eyyühe-lmücrimûn.",
      "numberInSurah": 59,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 177,
      "sajda": false
    },
    {
      "number": 3765,
      "text": "elem a`hed ileyküm yâ benî âdeme el lâ ta`büdü-şşeyṭân. innehû leküm `adüvvüm mübîn.",
      "numberInSurah": 60,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3766,
      "text": "veeni-`büdûnî. hâẕâ ṣirâṭum müsteḳîm.",
      "numberInSurah": 61,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3767,
      "text": "veleḳad eḍalle minküm cibillen keŝîrâ. efelem tekûnû ta`ḳilûn.",
      "numberInSurah": 62,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3768,
      "text": "hâẕihî cehennemü-lletî küntüm tû`adûn.",
      "numberInSurah": 63,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3769,
      "text": "iṣlevhe-lyevme bimâ küntüm tekfürûn.",
      "numberInSurah": 64,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3770,
      "text": "elyevme naḫtimü `alâ efvâhihim vetükellimünâ eydîhim veteşhedü ercülühüm bimâ kânû yeksibûn.",
      "numberInSurah": 65,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3771,
      "text": "velev neşâü leṭamesnâ `alâ a`yünihim festebeḳu-ṣṣirâṭa feennâ yübṣirûn.",
      "numberInSurah": 66,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3772,
      "text": "velev neşâü lemesaḫnâhüm `alâ mekânetihim feme-steṭâ`û müḍiyyev velâ yerci`ûn.",
      "numberInSurah": 67,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 384,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3773,
      "text": "vemen nü`ammirhü nünekkishü fi-lḫalḳ. efelâ ya`ḳilûn.",
      "numberInSurah": 68,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3774,
      "text": "vemâ `allemnâhü-şşi`ra vemâ yembegî leh. in hüve illâ ẕikruv veḳur'ânüm mübîn.",
      "numberInSurah": 69,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3775,
      "text": "liyünẕira men kâne ḥayyev veyeḥiḳḳa-lḳavlü `ale-lkâfirîn.",
      "numberInSurah": 70,
      "juz": 23,
      "manzil": 5,
      "page": 444,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3776,
      "text": "evelem yerav ennâ ḫalaḳnâ lehüm mimmâ `amilet eydînâ en`âmen fehüm lehâ mâlikûn.",
      "numberInSurah": 71,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3777,
      "text": "veẕellelnâhâ lehüm feminhâ rakûbühüm veminhâ ye'külûn.",
      "numberInSurah": 72,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3778,
      "text": "velehüm fîhâ menâfi`u vemeşârib. efelâ yeşkürûn.",
      "numberInSurah": 73,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3779,
      "text": "vetteḫaẕû min dûni-llâhi âlihetel le`allehüm yünṣarûn.",
      "numberInSurah": 74,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3780,
      "text": "lâ yesteṭî`ûne naṣrahüm vehüm lehüm cündüm muḥḍarûn.",
      "numberInSurah": 75,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3781,
      "text": "felâ yaḥzünke ḳavlühüm. innâ na`lemü mâ yüsirrûne vemâ yü`linûn.",
      "numberInSurah": 76,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3782,
      "text": "evelem yera-l'insânü ennâ ḫalaḳnâhü min nuṭfetin feiẕâ hüve ḫaṣîmüm mübîn.",
      "numberInSurah": 77,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3783,
      "text": "veḍarabe lenâ meŝelev venesiye ḫalḳah. ḳâle mey yuḥyi-l`iżâme vehiye ramîm.",
      "numberInSurah": 78,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3784,
      "text": "ḳul yuḥyîhe-lleẕî enşeehâ evvele merrah. vehüve bikülli ḫalḳin `alîm.",
      "numberInSurah": 79,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3785,
      "text": "elleẕî ce`ale leküm mine-şşeceri-l'aḫḍari nâran feiẕâ entüm minhü tûḳidûn.",
      "numberInSurah": 80,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3786,
      "text": "eveleyse-lleẕî ḫaleḳa-ssemâvâti vel'arḍa biḳâdirin `alâ ey yaḫlüḳa miŝlehüm. belâ vehüve-lḫallâḳu-l`alîm.",
      "numberInSurah": 81,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3787,
      "text": "innemâ emruhû iẕâ erâde şey'en ey yeḳûle lehû kün feyekûn.",
      "numberInSurah": 82,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3788,
      "text": "fesübḥâne-lleẕî biyedihî melekûtü külli şey'iv veileyhi türce`ûn.",
      "numberInSurah": 83,
      "juz": 23,
      "manzil": 5,
      "page": 445,
      "ruku": 385,
      "hizbQuarter": 178,
      "sajda": false
    }
