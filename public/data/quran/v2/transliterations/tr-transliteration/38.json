{
  "number": 38,
  "name": "سُورَةُ صٓ",
  "englishName": "Saad",
  "englishNameTranslation": "The letter Saad",
  "revelationType": "Meccan",
  "numberOfAyahs": 88,
  "ayahs": [
    {
      "number": 3971,
      "text": "ṣâd. velḳur'âni ẕi-ẕẕikr.",
      "numberInSurah": 1,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3972,
      "text": "beli-lleẕîne keferû fî `izzetiv veşiḳâḳ.",
      "numberInSurah": 2,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3973,
      "text": "kem ehleknâ min ḳablihim min ḳarnin fenâdev velâte ḥîne menâṣ.",
      "numberInSurah": 3,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3974,
      "text": "ve`acibû en câehüm münẕirum minhüm. veḳâle-lkâfirûne hâẕâ sâḥirun keẕẕâb.",
      "numberInSurah": 4,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3975,
      "text": "ece`ale-l'âlihete ilâhev vâḥidâ. inne hâẕâ leşey'ün `ucâb.",
      "numberInSurah": 5,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3976,
      "text": "venṭaleḳa-lmeleü minhüm eni-mşû vaṣbirû `alâ âlihetiküm. inne hâẕâ leşey'üy yürâd.",
      "numberInSurah": 6,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3977,
      "text": "mâ semi`nâ bihâẕâ fi-lmilleti-l'âḫirah. in hâẕâ ille-ḫtilâḳ.",
      "numberInSurah": 7,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3978,
      "text": "eünzile `aleyhi-ẕẕikru mim beyninâ. bel hüm fî şekkim min ẕikrî. bel lemmâ yeẕûḳû `aẕâb.",
      "numberInSurah": 8,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3979,
      "text": "em `indehüm ḫazâinü raḥmeti rabbike-l`azîzi-lvehhâb.",
      "numberInSurah": 9,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3980,
      "text": "em lehüm mülkü-ssemâvâti vel'arḍi vemâ beynehümâ. felyerteḳû fi-l'esbâb.",
      "numberInSurah": 10,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3981,
      "text": "cündüm mâ hünâlike mehzûmüm mine-l'aḥzâb.",
      "numberInSurah": 11,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3982,
      "text": "keẕẕebet ḳablehüm ḳavmü nûḥiv ve`âdüv vefir`avnü ẕü-l'evtâd.",
      "numberInSurah": 12,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3983,
      "text": "veŝemûdü veḳavmü lûṭiv veaṣḥâbü-l'eykeh. ülâike-l'aḥzâb.",
      "numberInSurah": 13,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3984,
      "text": "in küllün illâ keẕẕebe-rrusüle feḥaḳḳa `iḳâb.",
      "numberInSurah": 14,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 391,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3985,
      "text": "vemâ yenżuru hâülâi illâ ṣayḥatev vâḥidetem mâ lehâ min fevâḳ.",
      "numberInSurah": 15,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 392,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3986,
      "text": "veḳâlû rabbenâ `accil lenâ ḳiṭṭanâ ḳable yevmi-lḥisâb.",
      "numberInSurah": 16,
      "juz": 23,
      "manzil": 6,
      "page": 453,
      "ruku": 392,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3987,
      "text": "iṣbir `alâ mâ yeḳûlûne veẕkür `abdenâ dâvûde ẕe-l'eyd. innehû evvâb.",
      "numberInSurah": 17,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3988,
      "text": "innâ seḫḫarne-lcibâle me`ahû yüsebbiḥne bil`aşiyyi vel'işrâḳ.",
      "numberInSurah": 18,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3989,
      "text": "veṭṭayra maḥşûrah. küllül lehû evvâb.",
      "numberInSurah": 19,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3990,
      "text": "veşedednâ mülkehû veâteynâhü-lḥikmete vefaṣle-lḫiṭâb.",
      "numberInSurah": 20,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3991,
      "text": "vehel etâke nebeü-lḫaṣm. iẕ tesevveru-lmiḥrâb.",
      "numberInSurah": 21,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3992,
      "text": "iẕ deḫalû `alâ dâvûde fefezi`a minhüm ḳâlû lâ teḫaf. ḫaṣmâni begâ ba`ḍunâ `alâ ba`ḍin faḥküm beynenâ bilḥaḳḳi velâ tüşṭiṭ vehdinâ ilâ sevâi-ṣṣirâṭ.",
      "numberInSurah": 22,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3993,
      "text": "inne hâẕâ eḫî lehû tis`uv vetis`ûne na`cetev veliye na`cetüv vâḥidetün feḳâle ekfilnîhâ ve`azzenî fi-lḫiṭâb.",
      "numberInSurah": 23,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3994,
      "text": "ḳâle leḳad żalemeke bisüâli na`cetike ilâ ni`âcih. veinne keŝîram mine-lḫuleṭâi leyebgî ba`ḍuhüm `alâ ba`ḍin ille-lleẕîne âmenû ve`amilu-ṣṣâliḥâti veḳalîlüm mâ hüm. veżanne dâvûdü ennemâ fetennâhü festagfera rabbehû veḫarra râki`av veenâb.",
      "numberInSurah": 24,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 182,
      "sajda": {
        "id": 11,
        "recommended": true,
        "obligatory": false
      }
    },
    {
      "number": 3995,
      "text": "fegafernâ lehû ẕâlik. veinne lehû `indenâ lezülfâ veḥusne meâb.",
      "numberInSurah": 25,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3996,
      "text": "yâ dâvûdü innâ ce`alnâke ḫalîfeten fi-l'arḍi faḥküm beyne-nnâsi bilḥaḳḳi velâ tettebi`i-lhevâ feyüḍilleke `an sebîli-llâh. inne-lleẕîne yeḍillûne `an sebîli-llâhi lehüm `aẕâbün şedîdüm bimâ nesû yevme-lḥisâb.",
      "numberInSurah": 26,
      "juz": 23,
      "manzil": 6,
      "page": 454,
      "ruku": 392,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3997,
      "text": "vemâ ḫalaḳne-ssemâe vel'arḍa vemâ beynehümâ bâṭilâ. ẕâlike żannü-lleẕîne keferû. feveylül lilleẕîne keferû mine-nnâr.",
      "numberInSurah": 27,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3998,
      "text": "em nec`alü-lleẕîne âmenû ve`amilu-ṣṣâliḥâti kelmüfsidîne fi-l'arḍ. em nec`alü-lmütteḳîne kelfüccâr.",
      "numberInSurah": 28,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 3999,
      "text": "kitâbün enzelnâhü ileyke mübârakül liyeddebberû âyâtihî veliyeteẕekkera ülü-l'elbâb.",
      "numberInSurah": 29,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4000,
      "text": "vevehebnâ lidâvûde süleymân. ni`me-l`abd. innehû evvâb.",
      "numberInSurah": 30,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4001,
      "text": "iẕ `uriḍa `aleyhi bil`aşiyyi-ṣṣâfinâtü-lciyâd.",
      "numberInSurah": 31,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4002,
      "text": "feḳâle innî aḥbebtü ḥubbe-lḫayri `an ẕikri rabbî. ḥattâ tevârat bilḥicâb.",
      "numberInSurah": 32,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4003,
      "text": "ruddûhâ `aleyy. feṭafiḳa mesḥam bissûḳi vel'a`nâḳ.",
      "numberInSurah": 33,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4004,
      "text": "veleḳad fetennâ süleymâne veelḳaynâ `alâ kürsiyyihî ceseden ŝümme enâb.",
      "numberInSurah": 34,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4005,
      "text": "ḳâle rabbi-gfir lî veheb lî mülkel lâ yembegî lieḥadim mim ba`dî. inneke ente-lvehhâb.",
      "numberInSurah": 35,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4006,
      "text": "feseḫḫarnâ lehü-rrîḥa tecrî biemrih ruḫâen ḥayŝü eṣâb.",
      "numberInSurah": 36,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4007,
      "text": "veşşeyâṭîne külle bennâiv vegavvâṣ.",
      "numberInSurah": 37,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4008,
      "text": "veâḫarîne müḳarranîne fi-l'aṣfâd.",
      "numberInSurah": 38,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4009,
      "text": "hâẕâ `aṭâünâ femnün ev emsik bigayri ḥisâb.",
      "numberInSurah": 39,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4010,
      "text": "veinne lehû `indenâ lezülfâ veḥusne meâb.",
      "numberInSurah": 40,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 393,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4011,
      "text": "veẕkür `abdenâ eyyûb. iẕ nâdâ rabbehû ennî messeniye-şşeyṭânü binuṣbiv ve`aẕâb.",
      "numberInSurah": 41,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4012,
      "text": "ürkuḍ biriclik. hâẕâ mugteselüm bâridüv veşerâb.",
      "numberInSurah": 42,
      "juz": 23,
      "manzil": 6,
      "page": 455,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4013,
      "text": "vevehebnâ lehû ehlehû vemiŝlehüm me`ahüm raḥmetem minnâ veẕikrâ liüli-l'elbâb.",
      "numberInSurah": 43,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4014,
      "text": "veḫuẕ biyedike ḍigŝen faḍrib bihî velâ taḥneŝ. innâ vecednâhü ṣâbirâ. ni`me-l`abd. innehû evvâb.",
      "numberInSurah": 44,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4015,
      "text": "veẕkür `ibâdenâ ibrâhîme veisḥâḳa veya`ḳûbe üli-l'eydî vel'ebṣâr.",
      "numberInSurah": 45,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4016,
      "text": "innâ aḫlaṣnâhüm biḫâliṣatin ẕikra-ddâr.",
      "numberInSurah": 46,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4017,
      "text": "veinnehüm `indenâ lemine-lmuṣṭafeyne-l'aḫyâr.",
      "numberInSurah": 47,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4018,
      "text": "veẕkür ismâ`île velyese`a veẕe-lkifl. veküllüm mine-l'aḫyâr.",
      "numberInSurah": 48,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4019,
      "text": "hâẕâ ẕikr. veinne lilmütteḳîne leḥusne meâb.",
      "numberInSurah": 49,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4020,
      "text": "cennâti `adnim müfetteḥatel lehümü-l'ebvâb.",
      "numberInSurah": 50,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4021,
      "text": "müttekiîne fîhâ yed`ûne fîhâ bifâkihetin keŝîrativ veşerâb.",
      "numberInSurah": 51,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 182,
      "sajda": false
    },
    {
      "number": 4022,
      "text": "ve`indehüm ḳâṣirâtu-ṭṭarfi etrâb.",
      "numberInSurah": 52,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4023,
      "text": "hâẕâ mâ tû`adûne liyevmi-lḥisâb.",
      "numberInSurah": 53,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4024,
      "text": "inne hâẕâ lerizḳunâ mâ lehû min nefâd.",
      "numberInSurah": 54,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4025,
      "text": "hâẕâ. veinne liṭṭâgîne leşerra meâb.",
      "numberInSurah": 55,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4026,
      "text": "cehennem. yaṣlevnehâ. febi'se-lmihâd.",
      "numberInSurah": 56,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4027,
      "text": "hâẕâ felyeẕûḳûhü ḥamîmüv vegassâḳ.",
      "numberInSurah": 57,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4028,
      "text": "veâḫaru min şeklihî ezvâc.",
      "numberInSurah": 58,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4029,
      "text": "hâẕâ fevcüm muḳteḥimüm me`aküm. lâ merḥabem bihim. innehüm ṣâlü-nnâr.",
      "numberInSurah": 59,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4030,
      "text": "ḳâlû bel entüm. lâ merḥabem biküm. entüm ḳaddemtümûhü lenâ. febi'se-lḳarâr.",
      "numberInSurah": 60,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4031,
      "text": "ḳâlû rabbenâ men ḳaddeme lenâ hâẕâ fezidhü `aẕâben ḍi`fen fi-nnâr.",
      "numberInSurah": 61,
      "juz": 23,
      "manzil": 6,
      "page": 456,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4032,
      "text": "veḳâlû mâ lenâ lâ nerâ ricâlen künnâ ne`uddühüm mine-l'eşrâr.",
      "numberInSurah": 62,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4033,
      "text": "etteḫaẕnâhüm siḫriyyen em zâgat `anhümü-l'ebṣâr.",
      "numberInSurah": 63,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4034,
      "text": "inne ẕâlike leḥaḳḳun teḫâṣumü ehli-nnâr.",
      "numberInSurah": 64,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 394,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4035,
      "text": "ḳul innemâ ene münẕir. vemâ min ilâhin ille-llâhü-lvâḥidü-lḳahhâr.",
      "numberInSurah": 65,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4036,
      "text": "rabbü-ssemâvâti vel'arḍi vemâ beynehüme-l`azîzü-lgaffâr.",
      "numberInSurah": 66,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4037,
      "text": "ḳul hüve nebeün `ażîm.",
      "numberInSurah": 67,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4038,
      "text": "entüm `anhü mü`riḍûn.",
      "numberInSurah": 68,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4039,
      "text": "mâ kâne liye min `ilmim bilmelei-l'a`lâ iẕ yaḫteṣimûn.",
      "numberInSurah": 69,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4040,
      "text": "iy yûḥâ ileyye illâ ennemâ ene neẕîrum mübîn.",
      "numberInSurah": 70,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4041,
      "text": "iẕ ḳâle rabbüke lilmelâiketi innî ḫâliḳum beşeram min ṭîn.",
      "numberInSurah": 71,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4042,
      "text": "feiẕâ sevveytühû venefaḫtü fîhi mir rûḥî feḳa`û lehû sâcidîn.",
      "numberInSurah": 72,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4043,
      "text": "fesecede-lmelâiketü küllühüm ecme`ûn.",
      "numberInSurah": 73,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4044,
      "text": "illâ iblîs. istekbera vekâne mine-lkâfirîn.",
      "numberInSurah": 74,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4045,
      "text": "ḳâle yâ iblîsü mâ mene`ake en tescüde limâ ḫalaḳtü biyedeyy. estekberte em künte mine-l`âlîn.",
      "numberInSurah": 75,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4046,
      "text": "ḳâle ene ḫayrum minh. ḫalaḳtenî min nâriv veḫalaḳtehû min ṭîn.",
      "numberInSurah": 76,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4047,
      "text": "ḳâle faḫruc minhâ feinneke racîm.",
      "numberInSurah": 77,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4048,
      "text": "veinne `aleyke la`netî ilâ yevmi-ddîn.",
      "numberInSurah": 78,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4049,
      "text": "ḳâle rabbi feenżirnî ilâ yevmi yüb`aŝûn.",
      "numberInSurah": 79,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4050,
      "text": "ḳâle feinneke mine-lmünżarîn.",
      "numberInSurah": 80,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4051,
      "text": "ilâ yevmi-lvaḳti-lma`lûm.",
      "numberInSurah": 81,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4052,
      "text": "ḳâle febi`izzetike leugviyennehüm ecme`în.",
      "numberInSurah": 82,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4053,
      "text": "illâ `ibâdeke minhümü-lmuḫleṣîn.",
      "numberInSurah": 83,
      "juz": 23,
      "manzil": 6,
      "page": 457,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4054,
      "text": "ḳâle felḥaḳḳ. velḥaḳḳa eḳûl.",
      "numberInSurah": 84,
      "juz": 23,
      "manzil": 6,
      "page": 458,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4055,
      "text": "leemleenne cehenneme minke vemimmen tebi`ake minhüm ecme`în.",
      "numberInSurah": 85,
      "juz": 23,
      "manzil": 6,
      "page": 458,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4056,
      "text": "ḳul mâ es'elüküm `aleyhi min ecriv vemâ ene mine-lmütekellifîn.",
      "numberInSurah": 86,
      "juz": 23,
      "manzil": 6,
      "page": 458,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4057,
      "text": "in hüve illâ ẕikrul lil`âlemîn.",
      "numberInSurah": 87,
      "juz": 23,
      "manzil": 6,
      "page": 458,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    },
    {
      "number": 4058,
      "text": "veleta`lemünne nebeehû ba`de ḥîn.",
      "numberInSurah": 88,
      "juz": 23,
      "manzil": 6,
      "page": 458,
      "ruku": 395,
      "hizbQuarter": 183,
      "sajda": false
    }
