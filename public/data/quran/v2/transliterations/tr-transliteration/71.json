{
  "number": 71,
  "name": "سُورَةُ نُوحٍ",
  "englishName": "Nooh",
  "englishNameTranslation": "Noah",
  "revelationType": "Meccan",
  "numberOfAyahs": 28,
  "ayahs": [
    {
      "number": 5420,
      "text": "innâ erselnâ nûḥan ilâ ḳavmihî en enẕir ḳavmeke min ḳabli ey ye'tiyehüm `aẕâbün elîm.",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5421,
      "text": "ḳâle yâ ḳavmi innî leküm neẕîrum mübîn.",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5422,
      "text": "eni-`büdü-llâhe vetteḳûhü veeṭî`ûn.",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5423,
      "text": "yagfir leküm min ẕünûbiküm veyüeḫḫirküm ilâ ecelim müsemmâ. inne ecele-llâhi iẕâ câe lâ yü'eḫḫar. lev küntüm ta`lemûn.",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5424,
      "text": "ḳâle rabbi innî de`avtü ḳavmî leylev venehârâ.",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5425,
      "text": "felem yezidhüm dü`âî illâ firârâ.",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5426,
      "text": "veinnî küllemâ de`avtühüm litagfira lehüm ce`alû eṣâbi`ahüm fî âẕânihim vestagşev ŝiyâbehüm veeṣarru vestekberü-stikbârâ.",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5427,
      "text": "ŝümme innî de`avtühüm cihârâ.",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5428,
      "text": "ŝümme innî a`lentü lehüm veesrartü lehüm isrârâ.",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5429,
      "text": "feḳultü-stagfirû rabbeküm innehû kâne gaffârâ.",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 570,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5430,
      "text": "yürsili-ssemâe `aleyküm midrârâ.",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5431,
      "text": "veyümdidküm biemvâliv vebenîne veyec`al leküm cennâtiv veyec`al leküm enhârâ.",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5432,
      "text": "mâ leküm lâ tercûne lillâhi veḳârâ.",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5433,
      "text": "veḳad ḫaleḳaküm aṭvârâ.",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5434,
      "text": "elem terav keyfe ḫaleḳa-llâhü seb`a semâvâtin ṭibâḳâ.",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5435,
      "text": "vece`ale-lḳamera fîhinne nûrav vece`ale-şşemse sirâcâ.",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5436,
      "text": "vellâhü embeteküm mine-l'arḍi nebâtâ.",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5437,
      "text": "ŝümme yü`îdüküm fîhâ veyuḫricüküm iḫrâcâ.",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5438,
      "text": "vellâhü ce`ale lekümü-l'arḍa bisâṭâ.",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5439,
      "text": "liteslükû minhâ sübülen ficâcâ.",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 504,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5440,
      "text": "ḳâle nûḥur rabbi innehüm `aṣavnî vettebe`û mel lem yezidhü mâlühû veveledühû illâ ḫasârâ.",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5441,
      "text": "vemekerû mekran kübbârâ.",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5442,
      "text": "veḳâlû lâ teẕerunne âliheteküm velâ teẕerunne veddev velâ süvâ`av velâ yegûŝe veye`ûḳa venesrâ.",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5443,
      "text": "veḳad eḍallû keŝîrâ. velâ tezidi-żżâlimîne illâ ḍalâlâ.",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5444,
      "text": "mimmâ ḫaṭîâtihim ugriḳû feüdḫilû nâran felem yecidû lehüm min dûni-llâhi enṣârâ.",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5445,
      "text": "veḳâle nûḥur rabbi lâ teẕer `ale-l'arḍi mine-lkâfirîne deyyârâ.",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5446,
      "text": "inneke in teẕerhüm yüḍillû `ibâdeke velâ yelidû illâ fâciran keffârâ.",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    },
    {
      "number": 5447,
      "text": "rabbi-gfir lî velivâlideyye velimen deḫale beytiye mü'minev velilmü'minîne velmü'minât. velâ tezidi-żżâlimîne illâ tebârâ.",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 571,
      "ruku": 505,
      "hizbQuarter": 228,
      "sajda": false
    }
  ],
