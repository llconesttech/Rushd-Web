{
  "number": 19,
  "name": "سُورَةُ مَرۡيَمَ",
  "englishName": "Maryam",
  "englishNameTranslation": "Mary",
  "revelationType": "Meccan",
  "numberOfAyahs": 98,
  "ayahs": [
    {
      "number": 2251,
      "text": "kâf-hâ-yâ-`ayn-ṣâd.",
      "numberInSurah": 1,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2252,
      "text": "ẕikru raḥmeti rabbike `abdehû zekeriyyâ.",
      "numberInSurah": 2,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2253,
      "text": "iẕ nâdâ rabbehû nidâen ḫafiyyâ.",
      "numberInSurah": 3,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2254,
      "text": "ḳâle rabbi innî vehene-l`ażmü minnî veşte`ale-rra'sü şeybev velem eküm bidü`âike rabbi şeḳiyyâ.",
      "numberInSurah": 4,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2255,
      "text": "veinnî ḫiftü-lmevâliye miv verâî vekâneti-mraetî `âḳiran feheb lî mil ledünke veliyyâ.",
      "numberInSurah": 5,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2256,
      "text": "yeriŝünî veyeriŝü min âli ya`ḳûb. vec`alhü rabbi raḍiyyâ.",
      "numberInSurah": 6,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2257,
      "text": "yâ zekeriyyâ innâ nübeşşiruke bigulâmin-smühû yaḥyâ lem nec`al lehû min ḳablü semiyyâ.",
      "numberInSurah": 7,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2258,
      "text": "ḳâle rabbi ennâ yekûnü lî gulâmüv vekâneti-mraetî `âḳirav veḳad belagtü mine-lkiberi `itiyyâ.",
      "numberInSurah": 8,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2259,
      "text": "ḳâle keẕâlik. ḳâle rabbüke hüve `aleyye heyyinüv veḳad ḫalaḳtüke min ḳablü velem tekü şey'â.",
      "numberInSurah": 9,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2260,
      "text": "ḳâle rabbi-c`al lî âyeh. ḳâle âyetüke ellâ tükellime-nnâse ŝelâŝe leyâlin seviyyâ.",
      "numberInSurah": 10,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2261,
      "text": "feḫarace `alâ ḳavmihî mine-lmiḥrâbi feevḥâ ileyhim en sebbiḥû bükratev ve`aşiyyâ.",
      "numberInSurah": 11,
      "juz": 16,
      "manzil": 4,
      "page": 305,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2262,
      "text": "yâ yaḥyâ ḫuẕi-lkitâbe biḳuvveh. veâteynâhü-lḥukme ṣabiyyâ.",
      "numberInSurah": 12,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2263,
      "text": "veḥanânem mil ledünnâ vezekâh. vekâne teḳiyyâ.",
      "numberInSurah": 13,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2264,
      "text": "veberram bivâlideyhi velem yekün cebbâran `aṣiyyâ.",
      "numberInSurah": 14,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2265,
      "text": "veselâmün `aleyhi yevme vulide veyevme yemûtü veyevme yüb`aŝü ḥayyâ.",
      "numberInSurah": 15,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 264,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2266,
      "text": "veẕkür fi-lkitâbi meryem. iẕi-ntebeẕet min ehlihâ mekânen şerḳiyyâ.",
      "numberInSurah": 16,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2267,
      "text": "fetteḫaẕet min dûnihim ḥicâben feerselnâ ileyhâ rûḥanâ fetemeŝŝele lehâ beşeran seviyyâ.",
      "numberInSurah": 17,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2268,
      "text": "ḳâlet innî e`ûẕü birraḥmâni minke in künte teḳiyyâ.",
      "numberInSurah": 18,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2269,
      "text": "ḳâle innemâ ene rasûlü rabbik. liehebe leki gulâmen zekiyyâ.",
      "numberInSurah": 19,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2270,
      "text": "ḳâlet ennâ yekûnü lî gulâmüv velem yemsesnî beşeruv velem ekü begiyyâ.",
      "numberInSurah": 20,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2271,
      "text": "ḳâle keẕâlik. ḳâle rabbüki hüve `aleyye heyyin. velinec`alehû âyetel linnâsi veraḥmetem minnâ. vekâne emram maḳḍiyyâ.",
      "numberInSurah": 21,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 122,
      "sajda": false
    },
    {
      "number": 2272,
      "text": "feḥamelethü fentebeẕet bihî mekânen ḳaṣiyyâ.",
      "numberInSurah": 22,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2273,
      "text": "feecâehe-lmeḫâḍu ilâ ciẕ`i-nnaḫleh. ḳâlet yâ leytenî mittü ḳable hâẕâ veküntü nesyem mensiyyâ.",
      "numberInSurah": 23,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2274,
      "text": "fenâdâhâ min taḥtihâ ellâ taḥzenî ḳad ce`ale rabbüki taḥteki seriyyâ.",
      "numberInSurah": 24,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2275,
      "text": "vehüzzî ileyki biciẕ`i-nnaḫleti tüṣâḳiṭ `aleyki ruṭaben ceniyyâ.",
      "numberInSurah": 25,
      "juz": 16,
      "manzil": 4,
      "page": 306,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2276,
      "text": "fekülî veşrabî veḳarrî `aynâ. feimmâ terayinne mine-lbeşeri eḥaden feḳûlî innî neẕertü lirraḥmâni ṣavmen felen ükellime-lyevme insiyyâ.",
      "numberInSurah": 26,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2277,
      "text": "feetet bihî ḳavmehâ taḥmilüh. ḳâlû yâ meryemü leḳad ci'ti şey'en feriyyâ.",
      "numberInSurah": 27,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2278,
      "text": "yâ uḫte hârûne mâ kâne ebûki-mrae sev'iv vemâ kânet ümmüki begiyyâ.",
      "numberInSurah": 28,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2279,
      "text": "feeşârat ileyh. ḳâlû keyfe nükellimü men kâne fi-lmehdi ṣabiyyâ.",
      "numberInSurah": 29,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2280,
      "text": "ḳâle innî `abdü-llâh. âtâniye-lkitâbe vece`alenî nebiyyâ.",
      "numberInSurah": 30,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2281,
      "text": "vece`alenî mübâraken eyne mâ künt. veevṣânî biṣṣalâti vezzekâti mâ dümtü ḥayyâ.",
      "numberInSurah": 31,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2282,
      "text": "veberram bivâlidetî. velem yec`alnî cebbâran şeḳiyyâ.",
      "numberInSurah": 32,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2283,
      "text": "vesselâmü `aleyye yevme vulittü veyevme emûtü veyevme üb`aŝü ḥayyâ.",
      "numberInSurah": 33,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2284,
      "text": "ẕâlike `îse-bnü meryem. ḳavle-lḥaḳḳi-lleẕî fîhi yemterûn.",
      "numberInSurah": 34,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2285,
      "text": "mâ kâne lillâhi ey yetteḫiẕe miv veledin sübḥâneh. iẕâ ḳaḍâ emran feinnemâ yeḳûlü lehû kün feyekûn.",
      "numberInSurah": 35,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2286,
      "text": "veinne-llâhe rabbî verabbüküm fa`büdûh. hâẕâ ṣirâṭum müsteḳîm.",
      "numberInSurah": 36,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2287,
      "text": "faḫtelefe-l'aḥzâbü mim beynihim. feveylül lilleẕîne keferû mim meşhedi yevmin `ażîm.",
      "numberInSurah": 37,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2288,
      "text": "esmi` bihim veebṣir yevme ye'tûnenâ lâkini-żżâlimûne-lyevme fî ḍalâlim mübîn.",
      "numberInSurah": 38,
      "juz": 16,
      "manzil": 4,
      "page": 307,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2289,
      "text": "veenẕirhüm yevme-lḥasrati iẕ ḳuḍiye-l'emr. vehüm fî gafletiv vehüm lâ yü'minûn.",
      "numberInSurah": 39,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2290,
      "text": "innâ naḥnü neriŝü-l'arḍa vemen `aleyhâ veileynâ yürce`ûn.",
      "numberInSurah": 40,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 265,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2291,
      "text": "veẕkür fi-lkitâbi ibrâhîm. innehû kâne ṣiddîḳan nebiyyâ.",
      "numberInSurah": 41,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2292,
      "text": "iẕ ḳâle liebîhi yâ ebeti lime ta`büdü mâ lâ yesme`u velâ yübṣiru velâ yugnî `anke şey'â.",
      "numberInSurah": 42,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2293,
      "text": "yâ ebeti innî ḳad câenî mine-l`ilmi mâ lem ye'tike fettebi`nî ehdike ṣirâṭan seviyyâ.",
      "numberInSurah": 43,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2294,
      "text": "yâ ebeti lâ ta`büdi-şşeyṭân. inne-şşeyṭâne kâne lirraḥmâni `aṣiyyâ.",
      "numberInSurah": 44,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2295,
      "text": "yâ ebeti innî eḫâfü ey yemesseke `aẕâbüm mine-rraḥmâni fetekûne lişşeyṭâni veliyyâ.",
      "numberInSurah": 45,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2296,
      "text": "ḳâle erâgibün ente `an âlihetî yâ ibrâhîm. leil lem tentehi leercümenneke vehcürnî meliyyâ.",
      "numberInSurah": 46,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2297,
      "text": "ḳâle selâmün `aleyk. seestagfiru leke rabbî. innehû kâne bî ḥafiyyâ.",
      "numberInSurah": 47,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2298,
      "text": "vea`tezilüküm vemâ ted`ûne min dûni-llâhi veed`û rabbî. `asâ ellâ ekûne bidü`âi rabbî şeḳiyyâ.",
      "numberInSurah": 48,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2299,
      "text": "felemme-`tezelehüm vemâ ya`büdûne min dûni-llâhi vehebnâ lehû isḥâḳa veya`ḳûb. veküllen ce`alnâ nebiyyâ.",
      "numberInSurah": 49,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2300,
      "text": "vevehebnâ lehüm mir raḥmetinâ vece`alnâ lehüm lisâne ṣidḳin `aliyyâ.",
      "numberInSurah": 50,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 266,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2301,
      "text": "veẕkür fi-lkitâbi mûsâ. innehû kâne muḫleṣav vekâne rasûlen nebiyyâ.",
      "numberInSurah": 51,
      "juz": 16,
      "manzil": 4,
      "page": 308,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2302,
      "text": "venâdeynâhü min cânibi-ṭṭûri-l'eymeni veḳarrabnâhü neciyyâ.",
      "numberInSurah": 52,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2303,
      "text": "vevehebnâ lehû mir raḥmetinâ eḫâhü hârûne nebiyyâ.",
      "numberInSurah": 53,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2304,
      "text": "veẕkür fi-lkitâbi ismâ`îl. innehû kâne ṣâdiḳa-lva`di vekâne rasûlen nebiyyâ.",
      "numberInSurah": 54,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2305,
      "text": "vekâne ye'müru ehlehû biṣṣalâti vezzekâh. vekâne `inde rabbihî merḍiyyâ.",
      "numberInSurah": 55,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2306,
      "text": "veẕkür fi-lkitâbi idrîs. innehû kâne ṣiddîḳan nebiyyâ.",
      "numberInSurah": 56,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2307,
      "text": "verafa`nâhü mekânen `aliyyâ.",
      "numberInSurah": 57,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": false
    },
    {
      "number": 2308,
      "text": "ülâike-lleẕîne en`ame-llâhü `aleyhim mine-nnebiyyîne min ẕürriyyeti âdeme vemimmen ḥamelnâ me`a nûḥ. vemin ẕürriyyeti ibrâhîme veisrâîle vemimmen hedeynâ vectebeynâ. iẕâ tütlâ `aleyhim âyâtü-rraḥmâni ḫarrû süccedev vebükiyyâ.",
      "numberInSurah": 58,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 123,
      "sajda": {
        "id": 5,
        "recommended": true,
        "obligatory": false
      }
    },
    {
      "number": 2309,
      "text": "feḫalefe mim ba`dihim ḫalfün eḍâ`u-ṣṣalâte vettebe`ü-şşehevâti fesevfe yelḳavne gayyâ.",
      "numberInSurah": 59,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2310,
      "text": "illâ men tâbe veâmene ve`amile ṣâliḥan feülâike yedḫulûne-lcennete velâ yużlemûne şey'â.",
      "numberInSurah": 60,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2311,
      "text": "cennâti `adnini-lletî ve`ade-rraḥmânü `ibâdehû bilgayb. innehû kâne va`dühû me'tiyyâ.",
      "numberInSurah": 61,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2312,
      "text": "lâ yesme`ûne fîhâ lagven illâ selâmâ. velehüm rizḳuhüm fîhâ bükratev ve`aşiyyâ.",
      "numberInSurah": 62,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2313,
      "text": "tilke-lcennetü-lletî nûriŝü min `ibâdinâ men kâne teḳiyyâ.",
      "numberInSurah": 63,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2314,
      "text": "vemâ netenezzelü illâ biemri rabbik. lehû mâ beyne eydînâ vemâ ḫalfenâ vemâ beyne ẕâlik. vemâ kâne rabbüke nesiyyâ.",
      "numberInSurah": 64,
      "juz": 16,
      "manzil": 4,
      "page": 309,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2315,
      "text": "rabbü-ssemâvâti vel'arḍi vemâ beynehümâ fa`büdhü vaṣṭabir li`ibâdetih. hel ta`lemü lehû semiyyâ.",
      "numberInSurah": 65,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 267,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2316,
      "text": "veyeḳûlü-l'insânü eiẕâ mâ mittü lesevfe uḫracü ḥayyâ.",
      "numberInSurah": 66,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2317,
      "text": "evelâ yeẕküru-l'insânü ennâ ḫalaḳnâhü min ḳablü velem yekü şey'â.",
      "numberInSurah": 67,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2318,
      "text": "feverabbike lenaḥşürannehüm veşşeyâṭîne ŝümme lenuḥḍirannehüm ḥavle cehenneme ciŝiyyâ.",
      "numberInSurah": 68,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2319,
      "text": "ŝümme lenenzi`anne min külli şî`atin eyyühüm eşeddü `ale-rraḥmâni `itiyyâ.",
      "numberInSurah": 69,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2320,
      "text": "ŝümme lenaḥnü a`lemü billeẕîne hüm evlâ bihâ ṣiliyyâ.",
      "numberInSurah": 70,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2321,
      "text": "veim minküm illâ vâridühâ. kâne `alâ rabbike ḥatmem maḳḍiyyâ.",
      "numberInSurah": 71,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2322,
      "text": "ŝümme nünecci-lleẕîne-tteḳav veneẕeru-żżâlimîne fîhâ ciŝiyyâ.",
      "numberInSurah": 72,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2323,
      "text": "veiẕâ tütlâ `aleyhim âyâtünâ beyyinâtin ḳâle-lleẕîne keferû lilleẕîne âmenû eyyü-lferîḳayni ḫayrum meḳâmev veaḥsenü nediyyâ.",
      "numberInSurah": 73,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2324,
      "text": "vekem ehleknâ ḳablehüm min ḳarnin hüm aḥsenü eŝâŝev veri'yâ.",
      "numberInSurah": 74,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2325,
      "text": "ḳul men kâne fi-ḍḍalâleti felyemdüd lehü-rraḥmânü meddâ. ḥattâ iẕâ raev mâ yû`adûne imme-l`aẕâbe veimme-ssâ`ah. feseya`lemûne men hüve şerrum mekânev veaḍ`afü cündâ.",
      "numberInSurah": 75,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2326,
      "text": "veyezîdü-llâhü-lleẕîne-htedev hüden. velbâḳiyâtu-ṣṣâliḥâtü ḫayrun `inde rabbike ŝevâbev veḫayrum meraddâ.",
      "numberInSurah": 76,
      "juz": 16,
      "manzil": 4,
      "page": 310,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2327,
      "text": "eferaeyte-lleẕî kefera biâyâtinâ veḳâle leûteyenne mâlev veveledâ.",
      "numberInSurah": 77,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2328,
      "text": "eṭṭale`a-lgaybe emi-tteḫaẕe `inde-rraḥmâni `ahdâ.",
      "numberInSurah": 78,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2329,
      "text": "kellâ. senektübü mâ yeḳûlü venemüddü lehû mine-l`aẕâbi meddâ.",
      "numberInSurah": 79,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2330,
      "text": "veneriŝühû mâ yeḳûlü veye'tînâ ferdâ.",
      "numberInSurah": 80,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2331,
      "text": "vetteḫaẕû min dûni-llâhi âlihetel liyekûnû lehüm `izzâ.",
      "numberInSurah": 81,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2332,
      "text": "kellâ. seyekfürûne bi`ibâdetihim veyekûnûne `aleyhim ḍiddâ.",
      "numberInSurah": 82,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 268,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2333,
      "text": "elem tera ennâ erselne-şşeyâṭîne `ale-lkâfirîne teüzzühüm ezzâ.",
      "numberInSurah": 83,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2334,
      "text": "felâ ta`cel `aleyhim. innemâ ne`uddü lehüm `addâ.",
      "numberInSurah": 84,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2335,
      "text": "yevme naḥşüru-lmütteḳîne ile-rraḥmâni vefdâ.",
      "numberInSurah": 85,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2336,
      "text": "venesûḳu-lmücrimîne ilâ cehenneme virdâ.",
      "numberInSurah": 86,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2337,
      "text": "lâ yemlikûne-şşefâ`ate illâ meni-tteḫaẕe `inde-rraḥmâni `ahdâ.",
      "numberInSurah": 87,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2338,
      "text": "veḳâlü-tteḫaẕe-rraḥmânü veledâ.",
      "numberInSurah": 88,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2339,
      "text": "leḳad ci'tüm şey'en iddâ.",
      "numberInSurah": 89,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2340,
      "text": "tekâdü-ssemâvâtü yetefeṭṭarne minhü vetenşeḳḳu-l'arḍu veteḫirru-lcibâlü heddâ.",
      "numberInSurah": 90,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2341,
      "text": "en de`av lirraḥmâni veledâ.",
      "numberInSurah": 91,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2342,
      "text": "vemâ yembegî lirraḥmâni ey yetteḫiẕe veledâ.",
      "numberInSurah": 92,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2343,
      "text": "in küllü men fi-ssemâvâti vel'arḍi illâ âti-rraḥmâni `abdâ.",
      "numberInSurah": 93,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2344,
      "text": "leḳad aḥṣâhüm ve`addehüm `addâ.",
      "numberInSurah": 94,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2345,
      "text": "veküllühüm âtîhi yevme-lḳiyâmeti ferdâ.",
      "numberInSurah": 95,
      "juz": 16,
      "manzil": 4,
      "page": 311,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2346,
      "text": "inne-lleẕîne âmenû ve`amilu-ṣṣâliḥâti seyec`alü lehümü-rraḥmânü vuddâ.",
      "numberInSurah": 96,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2347,
      "text": "feinnemâ yessernâhü bilisânike litübeşşira bihi-lmütteḳîne vetünẕira bihî ḳavmel lüddâ.",
      "numberInSurah": 97,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    },
    {
      "number": 2348,
      "text": "vekem ehleknâ ḳablehüm min ḳarn. hel tüḥissü minhüm min eḥadin ev tesme`u lehüm rikzâ.",
      "numberInSurah": 98,
      "juz": 16,
      "manzil": 4,
      "page": 312,
      "ruku": 269,
      "hizbQuarter": 124,
      "sajda": false
    }
  ],
