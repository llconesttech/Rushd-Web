{
  "number": 53,
  "name": "سُورَةُ النَّجۡمِ",
  "englishName": "An-Najm",
  "englishNameTranslation": "The Star",
  "revelationType": "Meccan",
  "numberOfAyahs": 62,
  "ayahs": [
    {
      "number": 4785,
      "text": "vennecmi iẕâ hevâ.",
      "numberInSurah": 1,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4786,
      "text": "mâ ḍalle ṣâḥibüküm vemâ gavâ.",
      "numberInSurah": 2,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4787,
      "text": "vemâ yenṭiḳu `ani-lhevâ.",
      "numberInSurah": 3,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4788,
      "text": "in hüve illâ vaḥyüy yûḥâ.",
      "numberInSurah": 4,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4789,
      "text": "`allemehû şedîdü-lḳuvâ.",
      "numberInSurah": 5,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4790,
      "text": "ẕû mirrah. festevâ.",
      "numberInSurah": 6,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4791,
      "text": "vehüve bil'üfüḳi-l'a`lâ.",
      "numberInSurah": 7,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4792,
      "text": "ŝümme denâ fetedellâ.",
      "numberInSurah": 8,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4793,
      "text": "fekâne ḳâbe ḳavseyni ev ednâ.",
      "numberInSurah": 9,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4794,
      "text": "feevḥâ ilâ `abdihî mâ evḥâ.",
      "numberInSurah": 10,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4795,
      "text": "mâ keẕebe-lfüâdü mâ raâ.",
      "numberInSurah": 11,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4796,
      "text": "efetümârûnehû `alâ mâ yerâ.",
      "numberInSurah": 12,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4797,
      "text": "veleḳad raâhü nezleten uḫrâ.",
      "numberInSurah": 13,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4798,
      "text": "`inde sidrati-lmüntehâ.",
      "numberInSurah": 14,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4799,
      "text": "`indehâ cennetü-lme'vâ.",
      "numberInSurah": 15,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4800,
      "text": "iẕ yagşe-ssidrate mâ yagşâ.",
      "numberInSurah": 16,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4801,
      "text": "mâ zâga-lbeṣaru vemâ ṭagâ.",
      "numberInSurah": 17,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4802,
      "text": "leḳad raâ min âyâti rabbihi-lkübrâ.",
      "numberInSurah": 18,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4803,
      "text": "eferaeytümü-llâte vel`uzzâ.",
      "numberInSurah": 19,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4804,
      "text": "vemenâte-ŝŝâliŝete-l'uḫrâ.",
      "numberInSurah": 20,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4805,
      "text": "elekümü-ẕẕekeru velehü-l'ünŝâ.",
      "numberInSurah": 21,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4806,
      "text": "tilke iẕen ḳismetün ḍîzâ.",
      "numberInSurah": 22,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4807,
      "text": "in hiye illâ esmâün semmeytümûhâ entüm veâbâüküm mâ enzele-llâhü bihâ min sülṭân. iy yettebi`ûne ille-żżanne vemâ tehve-l'enfüs. veleḳad câehüm mir rabbihimü-lhüdâ.",
      "numberInSurah": 23,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4808,
      "text": "em lil'insâni mâ temennâ.",
      "numberInSurah": 24,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4809,
      "text": "felillâhi-l'âḫiratü vel'ûlâ.",
      "numberInSurah": 25,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 460,
      "hizbQuarter": 210,
      "sajda": false
    },
    {
      "number": 4810,
      "text": "vekem mim melekin fi-ssemâvâti lâ tugnî şefâ`atühüm şey'en illâ mim ba`di ey ye'ẕene-llâhü limey yeşâü veyerḍâ.",
      "numberInSurah": 26,
      "juz": 27,
      "manzil": 7,
      "page": 526,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4811,
      "text": "inne-lleẕîne lâ yü'minûne bil'âḫirati leyüsemmûne-lmelâikete tesmiyete-l'ünŝâ.",
      "numberInSurah": 27,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4812,
      "text": "vemâ lehüm bihî min `ilm. iy yettebi`ûne ille-żżanne. veinne-żżanne lâ yugnî mine-lḥaḳḳi şey'â.",
      "numberInSurah": 28,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4813,
      "text": "fea`riḍ `am men tevellâ `an ẕikrinâ velem yürid ille-lḥayâte-ddünyâ.",
      "numberInSurah": 29,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4814,
      "text": "ẕâlike mebleguhüm mine-l`ilm. inne rabbeke hüve a`lemü bimen ḍalle `an sebîlihî vehüve a`lemü bimeni-htedâ.",
      "numberInSurah": 30,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4815,
      "text": "velillâhi mâ fi-ssemâvâti vemâ fi-l'arḍi liyecziye-lleẕîne esâü bimâ `amilû veyecziye-lleẕîne aḥsenû bilḥusnâ.",
      "numberInSurah": 31,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4816,
      "text": "elleẕîne yectenibûne kebâira-l'iŝmi velfevâḥişe ille-llemem. inne rabbeke vâsi`u-lmagfirah. hüve a`lemü biküm iẕ enşeeküm mine-l'arḍi veiẕ entüm ecinnetün fî büṭûni ümmehâtiküm. felâ tüzekkû enfüseküm. hüve a`lemü bimeni-tteḳâ.",
      "numberInSurah": 32,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 461,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4817,
      "text": "eferaeyte-lleẕî tevellâ.",
      "numberInSurah": 33,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4818,
      "text": "vea`ṭâ ḳalîlev veekdâ.",
      "numberInSurah": 34,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4819,
      "text": "e`indehû `ilmü-lgaybi fehüve yerâ.",
      "numberInSurah": 35,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4820,
      "text": "em lem yünebbe' bimâ fî ṣuḥufi mûsâ.",
      "numberInSurah": 36,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4821,
      "text": "veibrâhime-lleẕî veffâ.",
      "numberInSurah": 37,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4822,
      "text": "ellâ teziru vâziratüv vizra uḫrâ.",
      "numberInSurah": 38,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4823,
      "text": "veel leyse lil'insâni illâ mâ se`â.",
      "numberInSurah": 39,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4824,
      "text": "veenne sa`yehû sevfe yürâ.",
      "numberInSurah": 40,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4825,
      "text": "ŝümme yüczâhü-lcezâe-l'evfâ.",
      "numberInSurah": 41,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4826,
      "text": "veenne ilâ rabbike-lmüntehâ.",
      "numberInSurah": 42,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4827,
      "text": "veennehû hüve aḍḥake veebkâ.",
      "numberInSurah": 43,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4828,
      "text": "veennehû hüve emâte veaḥyâ.",
      "numberInSurah": 44,
      "juz": 27,
      "manzil": 7,
      "page": 527,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4829,
      "text": "veennehû ḫaleḳa-zzevceyni-ẕẕekera vel'ünŝâ.",
      "numberInSurah": 45,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4830,
      "text": "min nuṭfetin iẕâ tümnâ.",
      "numberInSurah": 46,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4831,
      "text": "veenne `aleyhi-nneş'ete-l'uḫrâ.",
      "numberInSurah": 47,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4832,
      "text": "veennehû hüve agnâ veaḳnâ.",
      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4833,
      "text": "veennehû hüve rabbü-şşi`râ.",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4834,
      "text": "veennehû ehleke `âden-l'ûlâ.",
      "numberInSurah": 50,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4835,
      "text": "veŝemûde femâ ebḳâ.",
      "numberInSurah": 51,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4836,
      "text": "veḳavme nûḥim min ḳabl. innehüm kânû hüm ażleme veaṭgâ.",
      "numberInSurah": 52,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4837,
      "text": "velmü'tefikete ehvâ.",
      "numberInSurah": 53,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4838,
      "text": "fegaşşâhâ mâ gaşşâ.",
      "numberInSurah": 54,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4839,
      "text": "febieyyi âlâi rabbike tetemârâ.",
      "numberInSurah": 55,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4840,
      "text": "hâẕâ neẕîrum mine-nnüẕüri-l'ûlâ.",
      "numberInSurah": 56,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4841,
      "text": "ezifeti-l'âzifeh.",
      "numberInSurah": 57,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4842,
      "text": "leyse lehâ min dûni-llâhi kâşifeh.",
      "numberInSurah": 58,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4843,
      "text": "efemin hâẕe-lḥadîŝi ta`cebûn.",
      "numberInSurah": 59,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4844,
      "text": "vetaḍḥakûne velâ tebkûn.",
      "numberInSurah": 60,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4845,
      "text": "veentüm sâmidûn.",
      "numberInSurah": 61,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4846,
      "text": "fescüdû lillâhi va`büdû.",
      "numberInSurah": 62,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 462,
      "hizbQuarter": 211,
