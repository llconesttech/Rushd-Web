{
  "number": 50,
  "name": "سُورَةُ قٓ",
  "englishName": "Qaaf",
  "englishNameTranslation": "The letter Qaaf",
  "revelationType": "Meccan",
  "numberOfAyahs": 45,
  "ayahs": [
    {
      "number": 4631,
      "text": "ḳâf. velḳur'âni-lmecîd.",
      "numberInSurah": 1,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4632,
      "text": "bel `acibû en câehüm münẕirum minhüm feḳâle-lkâfirûne hâẕâ şey'ün `acîb.",
      "numberInSurah": 2,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4633,
      "text": "eiẕâ mitnâ vekünnâ türâbâ. ẕâlike rac`um be`îd.",
      "numberInSurah": 3,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4634,
      "text": "ḳad `alimnâ mâ tenḳuṣu-l'arḍu minhüm. ve`indenâ kitâbün ḥafîż.",
      "numberInSurah": 4,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4635,
      "text": "bel keẕẕebû bilḥaḳḳi lemmâ câehüm fehüm fî emrim merîc.",
      "numberInSurah": 5,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4636,
      "text": "efelem yenżurû ile-ssemâi fevḳahüm keyfe beneynâhâ vezeyyennâhâ vemâ lehâ min fürûc.",
      "numberInSurah": 6,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4637,
      "text": "vel'arḍa medednâhâ veelḳaynâ fîhâ ravâsiye veembetnâ fîhâ min külli zevcim behîc.",
      "numberInSurah": 7,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4638,
      "text": "tebṣiratev veẕikrâ likülli `abdim münîb.",
      "numberInSurah": 8,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4639,
      "text": "venezzelnâ mine-ssemâi mâem mübâraken feembetnâ bihî cennâtiv veḥabbe-lḥaṣîd.",
      "numberInSurah": 9,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4640,
      "text": "vennaḫle bâsiḳâtil lehâ ṭal`un neḍîd.",
      "numberInSurah": 10,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4641,
      "text": "rizḳal lil`ibâdi veaḥyeynâ bihî beldetem meytâ. keẕâlike-lḫurûc.",
      "numberInSurah": 11,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4642,
      "text": "keẕẕebet ḳablehüm ḳavmü nûḥiv veaṣḥâbü-rrassi veŝemûd.",
      "numberInSurah": 12,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4643,
      "text": "ve`âdüv vefir`avnü veiḫvânü lûṭ.",
      "numberInSurah": 13,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4644,
      "text": "veaṣḥâbü-l'eyketi veḳavmü tübba`. küllün keẕẕebe-rrusüle feḥaḳḳa ve`îd.",
      "numberInSurah": 14,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4645,
      "text": "efe`ayînâ bilḫalḳi-l'evvel. bel hüm fî lebsim min ḫalḳin cedîd.",
      "numberInSurah": 15,
      "juz": 26,
      "manzil": 7,
      "page": 518,
      "ruku": 452,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4646,
      "text": "veleḳad ḫalaḳne-l'insâne vena`lemü mâ tüvesvisü bihî nefsüh. venaḥnü aḳrabü ileyhi min ḥabli-lverîd.",
      "numberInSurah": 16,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4647,
      "text": "iẕ yeteleḳḳe-lmüteleḳḳiyâni `ani-lyemîni ve`ani-şşimâli ḳa`îd.",
      "numberInSurah": 17,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4648,
      "text": "mâ yelfiżu min ḳavlin illâ ledeyhi raḳîbün `atîd.",
      "numberInSurah": 18,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4649,
      "text": "vecâet sekratü-lmevt bilḥaḳḳ. ẕâlike mâ künte minhü teḥîd.",
      "numberInSurah": 19,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4650,
      "text": "venüfiḫa fi-ṣṣûr. ẕâlike yevmü-lve`îd.",
      "numberInSurah": 20,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4651,
      "text": "vecâet küllü nefsim me`ahâ sâiḳuv veşehîd.",
      "numberInSurah": 21,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4652,
      "text": "leḳad künte fî gafletim min hâẕâ fekeşefnâ `anke giṭâeke febeṣaruke-lyevme ḥadîd.",
      "numberInSurah": 22,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4653,
      "text": "veḳâle ḳarînühû hâẕâ mâ ledeyye `atîd.",
      "numberInSurah": 23,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4654,
      "text": "elḳiyâ fî cehenneme külle keffârin `anîd.",
      "numberInSurah": 24,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4655,
      "text": "mennâ`il lilḫayri mü`tedim mürîbün.",
      "numberInSurah": 25,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4656,
      "text": "elleẕî ce`ale me`a-llâhi ilâhen âḫara feelḳiyâhü fi-l`aẕâbi-şşedîd.",
      "numberInSurah": 26,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 207,
      "sajda": false
    },
    {
      "number": 4657,
      "text": "ḳâle ḳarînühû rabbenâ mâ aṭgaytühû velâkin kâne fî ḍalâlim be`îd.",
      "numberInSurah": 27,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4658,
      "text": "ḳâle lâ taḫteṣimû ledeyye veḳad ḳaddemtü ileyküm bilve`îd.",
      "numberInSurah": 28,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4659,
      "text": "mâ yübeddelü-lḳavlü ledeyye vemâ ene biżallâmil lil`abîd.",
      "numberInSurah": 29,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 453,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4660,
      "text": "yevme neḳûlü licehenneme heli-mtele'ti veteḳûlü hel mim mezîd.",
      "numberInSurah": 30,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4661,
      "text": "veüzlifeti-lcennetü lilmütteḳîne gayra be`îd.",
      "numberInSurah": 31,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4662,
      "text": "hâẕâ mâ tû`adûne likülli evvâbin ḥafîż.",
      "numberInSurah": 32,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4663,
      "text": "men ḫaşiye-rraḥmâne bilgaybi vecâe biḳalbim münîb.",
      "numberInSurah": 33,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4664,
      "text": "üdḫulûhâ biselâm. ẕâlike yevmü-lḫulûd.",
      "numberInSurah": 34,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4665,
      "text": "lehüm mâ yeşâûne fîhâ veledeynâ mezîd.",
      "numberInSurah": 35,
      "juz": 26,
      "manzil": 7,
      "page": 519,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4666,
      "text": "vekem ehleknâ ḳablehüm min ḳarnin hüm eşeddü minhüm baṭşen feneḳḳabû fi-lbilâd. hel mim meḥîṣ.",
      "numberInSurah": 36,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4667,
      "text": "inne fî ẕâlike leẕikrâ limen kâne lehû ḳalbün ev elḳa-ssem`a vehüve şehîd.",
      "numberInSurah": 37,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4668,
      "text": "veleḳad ḫalaḳne-ssemâvâti vel'arḍa vemâ beynehümâ fî sitteti eyyâm. vemâ messenâ mil lügûb.",
      "numberInSurah": 38,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4669,
      "text": "faṣbir `alâ mâ yeḳûlûne vesebbiḥ biḥamdi rabbike ḳable ṭulû`i-şşemsi veḳable-lgurûb.",
      "numberInSurah": 39,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4670,
      "text": "vemine-lleyli fesebbiḥhü veedbâra-ssücûd.",
      "numberInSurah": 40,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4671,
      "text": "vestemi` yevme yünâdi-lmünâdi mim mekânin ḳarîb.",
      "numberInSurah": 41,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4672,
      "text": "yevme yesme`ûne-ṣṣayḥate bilḥaḳḳ. ẕâlike yevmü-lḫurûc.",
      "numberInSurah": 42,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4673,
      "text": "innâ naḥnü nuḥyî venümîtü veileyne-lmeṣîr.",
      "numberInSurah": 43,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4674,
      "text": "yevme teşeḳḳaḳu-l'arḍu `anhüm sirâ`â. ẕâlike ḥaşrun `aleynâ yesîr.",
      "numberInSurah": 44,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    },
    {
      "number": 4675,
      "text": "naḥnü a`lemü bimâ yeḳûlûne vemâ ente `aleyhim bicebbârin feẕekkir bilḳur'âni mey yeḫâfü ve`îd.",
      "numberInSurah": 45,
      "juz": 26,
      "manzil": 7,
      "page": 520,
      "ruku": 454,
      "hizbQuarter": 208,
      "sajda": false
    }
  ],
