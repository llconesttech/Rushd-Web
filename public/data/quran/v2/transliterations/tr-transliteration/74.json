{
  "number": 74,
  "name": "سُورَةُ المُدَّثِّرِ",
  "englishName": "Al-Muddaththir",
  "englishNameTranslation": "The Cloaked One",
  "revelationType": "Meccan",
  "numberOfAyahs": 56,
  "ayahs": [
    {
      "number": 5496,
      "text": "yâ eyyühe-lmüddeŝŝir.",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5497,
      "text": "ḳum feenẕir.",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5498,
      "text": "verabbeke fekebbir.",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5499,
      "text": "veŝiyâbeke feṭahhir.",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5500,
      "text": "verrucze fehcür.",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5501,
      "text": "velâ temnün testekŝir.",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5502,
      "text": "velirabbike faṣbir.",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5503,
      "text": "feiẕâ nüḳira fi-nnâḳûr.",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5504,
      "text": "feẕâlike yevmeiẕiy yevmün `asîr.",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5505,
      "text": "`ale-lkâfirîne gayru yesîr.",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5506,
      "text": "ẕernî vemen ḫalaḳtü veḥîdâ.",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5507,
      "text": "vece`altü lehû mâlem memdûdâ.",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5508,
      "text": "vebenîne şühûdâ.",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5509,
      "text": "vemehhettü lehû temhîdâ.",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5510,
      "text": "ŝümme yaṭme`u en ezîd.",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5511,
      "text": "kellâ. innehû kâne liâyâtinâ `anîdâ.",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5512,
      "text": "seürhiḳuhû ṣa`ûdâ.",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 575,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5513,
      "text": "innehû fekkera veḳaddera.",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5514,
      "text": "feḳutile keyfe ḳaddera.",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5515,
      "text": "ŝümme ḳutile keyfe ḳaddera.",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5516,
      "text": "ŝümme neżara.",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5517,
      "text": "ŝümme `abese vebesera.",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5518,
      "text": "ŝümme edbera vestekbera.",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5519,
      "text": "feḳâle in hâẕâ illâ siḥruy yü'ŝer.",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5520,
      "text": "in hâẕâ illâ ḳavlü-lbeşer.",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5521,
      "text": "seuṣlîhi seḳara.",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5522,
      "text": "vemâ edrâke mâ seḳar.",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5523,
      "text": "lâ tübḳî velâ teẕer.",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5524,
      "text": "levvâḥatül lilbeşer.",
      "numberInSurah": 29,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5525,
      "text": "`aleyhâ tis`ate `aşer.",
      "numberInSurah": 30,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5526,
      "text": "vemâ ce`alnâ aṣḥâbe-nnâri illâ melâikeh. vemâ ce`alnâ `iddetehüm illâ fitnetel lilleẕîne keferû liyesteyḳine-lleẕîne ûtü-lkitâbe veyezdâde-lleẕîne âmenû îmânev velâ yertâbe-lleẕîne ûtü-lkitâbe velmü'minûne veliyeḳûle-lleẕîne fî ḳulûbihim meraḍuv velkâfirûne mâẕâ erâde-llâhü bihâẕâ meŝelâ. keẕâlike yüḍillü-llâhü mey yeşâü veyehdî mey yeşâ'. vemâ ya`lemü cünûde rabbike illâ hû. vemâ hiye illâ ẕikrâ lilbeşer.",
      "numberInSurah": 31,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 510,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5527,
      "text": "kellâ velḳamer.",
      "numberInSurah": 32,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5528,
      "text": "velleyli iẕ edbera.",
      "numberInSurah": 33,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5529,
      "text": "veṣṣubḥi iẕâ esfera.",
      "numberInSurah": 34,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5530,
      "text": "innehâ leiḥde-lküber.",
      "numberInSurah": 35,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5531,
      "text": "neẕîral lilbeşer.",
      "numberInSurah": 36,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5532,
      "text": "limen şâe minküm ey yeteḳaddeme ev yeteeḫḫar.",
      "numberInSurah": 37,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5533,
      "text": "küllü nefsim bimâ kesebet rahîneh.",
      "numberInSurah": 38,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5534,
      "text": "illâ aṣḥâbe-lyemîn.",
      "numberInSurah": 39,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5535,
      "text": "fî cennâtin. yetesâelûn.",
      "numberInSurah": 40,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5536,
      "text": "`ani-lmücrimîn.",
      "numberInSurah": 41,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5537,
      "text": "mâ selekeküm fî seḳara.",
      "numberInSurah": 42,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5538,
      "text": "ḳâlû lem nekü mine-lmüṣallîn.",
      "numberInSurah": 43,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5539,
      "text": "velem nekü nuṭ`imü-lmiskîn.",
      "numberInSurah": 44,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5540,
      "text": "vekünnâ neḫûḍu me`a-lḫâiḍîn.",
      "numberInSurah": 45,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5541,
      "text": "vekünnâ nükeẕẕibü biyevmi-ddîn.",
      "numberInSurah": 46,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5542,
      "text": "ḥattâ etâne-lyeḳîn.",
      "numberInSurah": 47,
      "juz": 29,
      "manzil": 7,
      "page": 576,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5543,
      "text": "femâ tenfe`uhüm şefâ`atü-şşâfi`în.",
      "numberInSurah": 48,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5544,
      "text": "femâ lehüm `ani-tteẕkirati mü`riḍîn.",
      "numberInSurah": 49,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5545,
      "text": "keennehüm ḥumürum müstenfirah.",
      "numberInSurah": 50,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5546,
      "text": "ferrat min ḳasverah.",
      "numberInSurah": 51,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5547,
      "text": "bel yürîdü küllü-mriim minhüm ey yü'tâ ṣuḥufem müneşşerah.",
      "numberInSurah": 52,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5548,
      "text": "kellâ. bel lâ yeḫâfûne-l'âḫirah.",
      "numberInSurah": 53,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5549,
      "text": "kellâ innehû teẕkirah.",
      "numberInSurah": 54,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5550,
      "text": "femen şâe ẕekerah.",
      "numberInSurah": 55,
      "juz": 29,
      "manzil": 7,
      "page": 577,
      "ruku": 511,
      "hizbQuarter": 230,
      "sajda": false
    },
    {
      "number": 5551,
      "text": "vemâ yeẕkürûne illâ ey yeşâe-llâh. hüve ehlü-ttaḳvâ veehlü-lmagfirah.",
      "numberInSurah": 56,
      "juz": 29,
