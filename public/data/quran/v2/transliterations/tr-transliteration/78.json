{
  "number": 78,
  "name": "سُورَةُ النَّبَإِ",
  "englishName": "An-Naba",
  "englishNameTranslation": "The Announcement",
  "revelationType": "Meccan",
  "numberOfAyahs": 40,
  "ayahs": [
    {
      "number": 5673,
      "text": "`amme yetesâelûn.",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5674,
      "text": "`ani-nnebei-l`ażîm.",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5675,
      "text": "elleẕî hüm fîhi muḫtelifûn.",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5676,
      "text": "kellâ seya`lemûn.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5677,
      "text": "ŝümme kellâ seya`lemûn.",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5678,
      "text": "elem nec`ali-l'arḍa mihâdâ.",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5679,
      "text": "velcibâle evtâdâ.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5680,
      "text": "veḫalaḳnâküm ezvâcâ.",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5681,
      "text": "vece`alnâ nevmeküm sübâtâ.",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5682,
      "text": "vece`alne-lleyle libâsâ.",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5683,
      "text": "vece`alne-nnehâra me`âşâ.",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5684,
      "text": "vebeneynâ fevḳaküm seb`an şidâdâ.",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5685,
      "text": "vece`alnâ sirâcev vehhâcâ.",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5686,
      "text": "veenzelnâ mine-lmü`ṣirâti mâen ŝeccâcâ.",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5687,
      "text": "linuḫrice bihî ḥabbev venebâtâ.",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5688,
      "text": "vecennâtin elfâfâ.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5689,
      "text": "inne yevme-lfaṣli kâne mîḳâtâ.",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5690,
      "text": "yevme yünfeḫu fi-ṣṣûri fete'tûne efvâcâ.",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5691,
      "text": "vefütiḥati-ssemâü fekânet ebvâbâ.",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5692,
      "text": "vesüyyirati-lcibâlü fekânet serâbâ.",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5693,
      "text": "inne cehenneme kânet mirṣâdâ.",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5694,
      "text": "liṭṭâgîne meâbâ.",
      "numberInSurah": 22,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5695,
      "text": "lâbiŝîne fîhâ aḥḳâbâ.",
      "numberInSurah": 23,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5696,
      "text": "lâ yeẕûḳûne fîhâ berdev velâ şerâbâ.",
      "numberInSurah": 24,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5697,
      "text": "illâ ḥamîmev vegassâḳâ.",
      "numberInSurah": 25,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5698,
      "text": "cezâev vifâḳâ.",
      "numberInSurah": 26,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5699,
      "text": "innehüm kânû lâ yercûne ḥisâbâ.",
      "numberInSurah": 27,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5700,
      "text": "vekeẕẕebû biâyâtinâ kiẕẕâbâ.",
      "numberInSurah": 28,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5701,
      "text": "vekülle şey'in aḥṣaynâhü kitâbâ.",
      "numberInSurah": 29,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5702,
      "text": "feẕûḳû felen nezîdeküm illâ `aẕâbâ.",
      "numberInSurah": 30,
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5703,
      "text": "inne lilmütteḳîne mefâzâ.",
      "numberInSurah": 31,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5704,
      "text": "ḥadâiḳa vea`nâbâ.",
      "numberInSurah": 32,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5705,
      "text": "vekevâ`ibe etrâbâ.",
      "numberInSurah": 33,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5706,
      "text": "veke'sen dihâḳâ.",
      "numberInSurah": 34,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5707,
      "text": "lâ yesme`ûne fîhâ lagvev velâ kiẕẕâbâ.",
      "numberInSurah": 35,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5708,
      "text": "cezâem mir rabbike `aṭâen ḥisâbâ.",
      "numberInSurah": 36,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5709,
      "text": "rabbi-ssemâvâti vel'arḍi vemâ beynehüme-rraḥmâni lâ yemlikûne minhü ḫiṭâbâ.",
      "numberInSurah": 37,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5710,
      "text": "yevme yeḳûmü-rrûḥu velmelâiketü ṣaffâ. lâ yetekellemûne illâ men eẕine lehü-rraḥmânü veḳâle ṣavâbâ.",
      "numberInSurah": 38,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5711,
      "text": "ẕâlike-lyevmü-lḥaḳḳ. femen şâe-tteḫaẕe ilâ rabbihî meâbâ.",
      "numberInSurah": 39,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5712,
      "text": "innâ enẕernâküm `aẕâben ḳarîbâ. yevme yenżuru-lmerü mâ ḳaddemet yedâhü veyeḳûlü-lkâfiru yâ leytenî küntü türâbâ.",
      "numberInSurah": 40,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    }
