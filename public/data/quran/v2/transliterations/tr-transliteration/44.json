{
  "number": 44,
  "name": "سُورَةُ الدُّخَانِ",
  "englishName": "Ad-Dukhaan",
  "englishNameTranslation": "The Smoke",
  "revelationType": "Meccan",
  "numberOfAyahs": 59,
  "ayahs": [
    {
      "number": 4415,
      "text": "ḥâ-mîm.",
      "numberInSurah": 1,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4416,
      "text": "velkitâbi-lmübîn.",
      "numberInSurah": 2,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4417,
      "text": "innâ enzelnâhü fî leyletim mübâraketin innâ künnâ münẕirîn.",
      "numberInSurah": 3,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4418,
      "text": "fîhâ yüfraḳu küllü emrin ḥakîm.",
      "numberInSurah": 4,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4419,
      "text": "emram min `indinâ. innâ künnâ mürsilîn.",
      "numberInSurah": 5,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4420,
      "text": "raḥmetem mir rabbik. innehû hüve-ssemî`u-l`alîm.",
      "numberInSurah": 6,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4421,
      "text": "rabbi-ssemâvâti vel'arḍi vemâ beynehümâ. in küntüm mûḳinîn.",
      "numberInSurah": 7,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4422,
      "text": "lâ ilâhe illâ hüve yuḥyî veyümît. rabbüküm verabbü âbâikümü-l'evvelîn.",
      "numberInSurah": 8,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4423,
      "text": "bel hüm fî şekkiy yel`abûn.",
      "numberInSurah": 9,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4424,
      "text": "ferteḳib yevme te'ti-ssemâü bidüḫânim mübîn.",
      "numberInSurah": 10,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4425,
      "text": "yagşe-nnâs. hâẕâ `aẕâbün elîm.",
      "numberInSurah": 11,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4426,
      "text": "rabbene-kşif `anne-l`aẕâbe innâ mü'minûn.",
      "numberInSurah": 12,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4427,
      "text": "ennâ lehümü-ẕẕikrâ veḳad câehüm rasûlüm mübîn.",
      "numberInSurah": 13,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4428,
      "text": "ŝümme tevellev `anhü veḳâlû mü`allemüm mecnûn.",
      "numberInSurah": 14,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4429,
      "text": "innâ kâşifü-l`aẕâbi ḳalîlen inneküm `âidûn.",
      "numberInSurah": 15,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4430,
      "text": "yevme nebṭişü-lbaṭşete-lkübrâ. innâ münteḳimûn.",
      "numberInSurah": 16,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4431,
      "text": "veleḳad fetennâ ḳablehüm ḳavme fir`avne vecâehüm rasûlün kerîm.",
      "numberInSurah": 17,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4432,
      "text": "en eddû ileyye `ibâde-llâh. innî leküm rasûlün emîn.",
      "numberInSurah": 18,
      "juz": 25,
      "manzil": 6,
      "page": 496,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4433,
      "text": "veel lâ ta`lû `ale-llâh. innî âtîküm bisülṭânim mübîn.",
      "numberInSurah": 19,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4434,
      "text": "veinnî `uẕtü birabbî verabbiküm en tercümûn.",
      "numberInSurah": 20,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4435,
      "text": "veil lem tü'minû lî fa`tezilûn.",
      "numberInSurah": 21,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4436,
      "text": "fede`â rabbehû enne hâülâi ḳavmüm mücrimûn.",
      "numberInSurah": 22,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4437,
      "text": "feesri bi`ibâdî leylen inneküm müttebe`ûn.",
      "numberInSurah": 23,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4438,
      "text": "vetruki-lbaḥra rahvâ. innehüm cündüm mugraḳûn.",
      "numberInSurah": 24,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4439,
      "text": "kem terakû min cennâtiv ve`uyûn.",
      "numberInSurah": 25,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4440,
      "text": "vezürû`iv vemeḳâmin kerîm.",
      "numberInSurah": 26,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4441,
      "text": "vena`metin kânû fîhâ fâkihîn.",
      "numberInSurah": 27,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4442,
      "text": "keẕâlik. veevraŝnâhâ ḳavmen âḫarîn.",
      "numberInSurah": 28,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4443,
      "text": "femâ beket `aleyhimü-ssemâü vel'arḍu vemâ kânû münżarîn.",
      "numberInSurah": 29,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 431,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4444,
      "text": "veleḳad necceynâ benî isrâîle mine-l`aẕâbi-lmühîn.",
      "numberInSurah": 30,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4445,
      "text": "min fir`avn. innehû kâne `âliyem mine-lmüsrifîn.",
      "numberInSurah": 31,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4446,
      "text": "veleḳadi-ḫternâhüm `alâ `ilmin `ale-l`âlemîn.",
      "numberInSurah": 32,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4447,
      "text": "veâteynâhüm mine-l'âyâti mâ fîhi belâüm mübîn.",
      "numberInSurah": 33,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4448,
      "text": "inne hâülâi leyeḳûlûn.",
      "numberInSurah": 34,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4449,
      "text": "in hiye illâ mevtetüne-l'ûlâ vemâ naḥnü bimünşerîn.",
      "numberInSurah": 35,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4450,
      "text": "fe'tû biâbâinâ in küntüm ṣâdiḳîn.",
      "numberInSurah": 36,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4451,
      "text": "ehüm ḫayrun em ḳavmü tübbe`iv velleẕîne min ḳablihim. ehleknâhüm. innehüm kânû mücrimîn.",
      "numberInSurah": 37,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4452,
      "text": "vemâ ḫalaḳne-ssemâvâti vel'arḍa vemâ beynehümâ lâ`ibîn.",
      "numberInSurah": 38,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4453,
      "text": "mâ ḫalaḳnâhümâ illâ bilḥaḳḳi velâkinne ekŝerahüm lâ ya`lemûn.",
      "numberInSurah": 39,
      "juz": 25,
      "manzil": 6,
      "page": 497,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4454,
      "text": "inne yevme-lfaṣli mîḳâtühüm ecme`în.",
      "numberInSurah": 40,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4455,
      "text": "yevme lâ yugnî mevlen `am mevlen şey'ev velâ hüm yünṣarûn.",
      "numberInSurah": 41,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4456,
      "text": "illâ mer raḥime-llâh. innehû hüve-l`azîzü-rraḥîm.",
      "numberInSurah": 42,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 432,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4457,
      "text": "inne şecerate-zzeḳḳûm.",
      "numberInSurah": 43,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4458,
      "text": "ṭa`âmü-l'eŝîm.",
      "numberInSurah": 44,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4459,
      "text": "kelmühl. yaglî fi-lbüṭûn.",
      "numberInSurah": 45,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4460,
      "text": "kegalyi-lḥamîm.",
      "numberInSurah": 46,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4461,
      "text": "ḫuẕûhü fa`tilûhü ilâ sevâi-lceḥîm.",
      "numberInSurah": 47,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4462,
      "text": "ŝümme ṣubbû fevḳa ra'sihî min `aẕâbi-lḥamîm.",
      "numberInSurah": 48,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4463,
      "text": "ẕuḳ. inneke ente-l`azîzü-lkerîm.",
      "numberInSurah": 49,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4464,
      "text": "inne hâẕâ mâ küntüm bihî temterûn.",
      "numberInSurah": 50,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4465,
      "text": "inne-lmütteḳîne fî meḳâmin emîn.",
      "numberInSurah": 51,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4466,
      "text": "fî cennâtiv ve`uyûn.",
      "numberInSurah": 52,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4467,
      "text": "yelbesûne min sündüsiv veistebraḳim müteḳâbilîn.",
      "numberInSurah": 53,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4468,
      "text": "keẕâlik. vezevvecnâhüm biḥûrin `în.",
      "numberInSurah": 54,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4469,
      "text": "yed`ûne fîhâ bikülli fâkihetin âminîn.",
      "numberInSurah": 55,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4470,
      "text": "lâ yeẕûḳûne fîhe-lmevte ille-lmevtete-l'ûlâ. veveḳâhüm `aẕâbe-lceḥîm.",
      "numberInSurah": 56,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4471,
      "text": "faḍlem mir rabbik. ẕâlike hüve-lfevzü-l`ażîm.",
      "numberInSurah": 57,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4472,
      "text": "feinnemâ yessernâhü bilisânike le`allehüm yeteẕekkerûn.",
      "numberInSurah": 58,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    },
    {
      "number": 4473,
      "text": "ferteḳib innehüm mürteḳibûn.",
      "numberInSurah": 59,
      "juz": 25,
      "manzil": 6,
      "page": 498,
      "ruku": 433,
      "hizbQuarter": 199,
      "sajda": false
    }
  ],
