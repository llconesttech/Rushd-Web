      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5776,
      "text": "min eyyi şey'in ḫaleḳah.",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5777,
      "text": "min nuṭfeh. ḫaleḳahû feḳadderah.",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5778,
