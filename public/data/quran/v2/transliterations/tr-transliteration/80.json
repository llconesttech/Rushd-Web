      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5784,
      "text": "ŝümme şaḳaḳne-l'arḍa şeḳḳâ.",
