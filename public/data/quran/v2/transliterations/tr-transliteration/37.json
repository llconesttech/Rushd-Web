{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "englishNameTranslation": "Those drawn up in Ranks",
  "revelationType": "Meccan",
  "numberOfAyahs": 182,
  "ayahs": [
    {
      "number": 3789,
      "text": "veṣṣâffâti ṣaffâ.",
      "numberInSurah": 1,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3790,
      "text": "fezzâcirâti zecrâ.",
      "numberInSurah": 2,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3791,
      "text": "fettâliyâti ẕikrâ.",
      "numberInSurah": 3,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3792,
      "text": "inne ilâheküm levâḥid.",
      "numberInSurah": 4,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3793,
      "text": "rabbü-ssemâvâti vel'arḍi vemâ beynehümâ verabbü-lmeşâriḳ.",
      "numberInSurah": 5,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3794,
      "text": "innâ zeyyenne-ssemâe-ddünyâ bizînetini-lkevâkib.",
      "numberInSurah": 6,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3795,
      "text": "veḥifżam min külli şeyṭânim mârid.",
      "numberInSurah": 7,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3796,
      "text": "lâ yessemme`ûne ile-lmelei-l'a`lâ veyuḳẕefûne min külli cânib.",
      "numberInSurah": 8,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3797,
      "text": "düḥûrav velehüm `aẕâbüv vâṣib.",
      "numberInSurah": 9,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3798,
      "text": "illâ men ḫaṭife-lḫaṭfete feetbe`ahû şihâbün ŝâḳib.",
      "numberInSurah": 10,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3799,
      "text": "festeftihim ehüm eşeddü ḫalḳan em men ḫalaḳnâ. innâ ḫalaḳnâhüm min ṭînil lâzib.",
      "numberInSurah": 11,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3800,
      "text": "bel `acibte veyesḫarûn.",
      "numberInSurah": 12,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3801,
      "text": "veiẕâ ẕükkirû lâ yeẕkürûn.",
      "numberInSurah": 13,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3802,
      "text": "veiẕâ raev âyetey yestesḫirûn.",
      "numberInSurah": 14,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3803,
      "text": "veḳâlû in hâẕâ illâ siḥrum mübîn.",
      "numberInSurah": 15,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3804,
      "text": "eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemeb`ûŝûn.",
      "numberInSurah": 16,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3805,
      "text": "eveâbâüne-l'evvelûn.",
      "numberInSurah": 17,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3806,
      "text": "ḳul ne`am veentüm dâḫirûn.",
      "numberInSurah": 18,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3807,
      "text": "feinnemâ hiye zecratüv vâḥidetün feiẕâ hüm yenżurûn.",
      "numberInSurah": 19,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3808,
      "text": "veḳâlû yâ veylenâ hâẕâ yevmü-ddîn.",
      "numberInSurah": 20,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3809,
      "text": "hâẕâ yevmü-lfaṣli-lleẕî küntüm bihî tükeẕẕibûn.",
      "numberInSurah": 21,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3810,
      "text": "uḥşürü-lleẕîne żalemû veezvâcehüm vemâ kânû ya`büdûn.",
      "numberInSurah": 22,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3811,
      "text": "min dûni-llâhi fehdûhüm ilâ ṣirâṭi-lceḥîm.",
      "numberInSurah": 23,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3812,
      "text": "veḳifûhüm innehüm mes'ûlûn.",
      "numberInSurah": 24,
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3813,
      "text": "mâ leküm lâ tenâṣarûn.",
      "numberInSurah": 25,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3814,
      "text": "bel hümü-lyevme müsteslimûn.",
      "numberInSurah": 26,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3815,
      "text": "veaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn.",
      "numberInSurah": 27,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3816,
      "text": "ḳâlû inneküm küntüm te'tûnenâ `ani-lyemîn.",
      "numberInSurah": 28,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3817,
      "text": "ḳâlû bel lem tekûnû mü'minîn.",
      "numberInSurah": 29,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3818,
      "text": "vemâ kâne lenâ `aleyküm min sülṭân. bel küntüm ḳavmen ṭâgîn.",
      "numberInSurah": 30,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3819,
      "text": "feḥaḳḳa `aleynâ ḳavlü rabbinâ. innâ leẕâiḳûn.",
      "numberInSurah": 31,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3820,
      "text": "feagveynâküm innâ künnâ gâvîn.",
      "numberInSurah": 32,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3821,
      "text": "feinnehüm yevmeiẕin fi-l`aẕâbi müşterikûn.",
      "numberInSurah": 33,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3822,
      "text": "innâ keẕâlike nef`alü bilmücrimîn.",
      "numberInSurah": 34,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3823,
      "text": "innehüm kânû iẕâ ḳîle lehüm lâ ilâhe ille-llâhü yestekbirûn.",
      "numberInSurah": 35,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3824,
      "text": "veyeḳûlûne einnâ letârikû âlihetinâ lişâ`irim mecnûn.",
      "numberInSurah": 36,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3825,
      "text": "bel câe bilḥaḳḳi veṣaddeḳa-lmürselîn.",
      "numberInSurah": 37,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3826,
      "text": "inneküm leẕâiḳu-l`aẕâbi-l'elîm.",
      "numberInSurah": 38,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3827,
      "text": "vemâ tüczevne illâ mâ küntüm ta`melûn.",
      "numberInSurah": 39,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3828,
      "text": "illâ `ibâde-llâhi-lmuḫleṣîn.",
      "numberInSurah": 40,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3829,
      "text": "ülâike lehüm rizḳum ma`lûm.",
      "numberInSurah": 41,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3830,
      "text": "fevâkih. vehüm mükramûn.",
      "numberInSurah": 42,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3831,
      "text": "fî cennâti-nne`îm.",
      "numberInSurah": 43,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3832,
      "text": "`alâ sürurim müteḳâbilîn.",
      "numberInSurah": 44,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3833,
      "text": "yüṭâfü `aleyhim bike'sim mim me`în.",
      "numberInSurah": 45,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3834,
      "text": "beyḍâe leẕẕetil lişşâribîn.",
      "numberInSurah": 46,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3835,
      "text": "lâ fîhâ gavlüv velâ hüm `anhâ yünzefûn.",
      "numberInSurah": 47,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3836,
      "text": "ve`indehüm ḳâṣirâtu-ṭṭarfi `în.",
      "numberInSurah": 48,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3837,
      "text": "keennehünne beyḍum meknûn.",
      "numberInSurah": 49,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3838,
      "text": "feaḳbele ba`ḍuhüm `alâ ba`ḍiy yetesâelûn.",
      "numberInSurah": 50,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3839,
      "text": "ḳâle ḳâilüm minhüm innî kâne lî ḳarîn.",
      "numberInSurah": 51,
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3840,
      "text": "yeḳûlü einneke lemine-lmüṣaddiḳîn.",
      "numberInSurah": 52,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3841,
      "text": "eiẕâ mitnâ vekünnâ türâbev ve`iżâmen einnâ lemedînûn.",
      "numberInSurah": 53,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3842,
      "text": "ḳâle hel entüm müṭṭali`ûn.",
      "numberInSurah": 54,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3843,
      "text": "feṭṭale`a feraâhü fî sevâi-lceḥîm.",
      "numberInSurah": 55,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3844,
      "text": "ḳâle tellâhi in kitte letürdîn.",
      "numberInSurah": 56,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3845,
      "text": "velevlâ ni`metü rabbî leküntü mine-lmuḥḍarîn.",
      "numberInSurah": 57,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3846,
      "text": "efemâ naḥnü bimeyyitîn.",
      "numberInSurah": 58,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3847,
      "text": "illâ mevtetene-l'ûlâ vemâ naḥnü bimü`aẕẕebîn.",
      "numberInSurah": 59,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3848,
      "text": "inne hâẕâ lehüve-lfevzü-l`ażîm.",
      "numberInSurah": 60,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3849,
      "text": "limiŝli hâẕâ felya`meli-l`âmilûn.",
      "numberInSurah": 61,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3850,
      "text": "eẕâlike ḫayrun nüzülen em şeceratü-zzeḳḳûm.",
      "numberInSurah": 62,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3851,
      "text": "innâ ce`alnâhâ fitnetel liżżâlimîn.",
      "numberInSurah": 63,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3852,
      "text": "innehâ şeceratün taḫrucü fî aṣli-lceḥîm.",
      "numberInSurah": 64,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3853,
      "text": "ṭal`uhâ keennehû ruûsü-şşeyâṭîn.",
      "numberInSurah": 65,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3854,
      "text": "feinnehüm leâkilûne minhâ femâliûne minhe-lbüṭûn.",
      "numberInSurah": 66,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3855,
      "text": "ŝümme inne lehüm `aleyhâ leşevbem min ḥamîm.",
      "numberInSurah": 67,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3856,
      "text": "ŝümme inne merci`ahüm leile-lceḥîm.",
      "numberInSurah": 68,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 3857,
      "text": "innehüm elfev âbâehüm ḍâllîn.",
      "numberInSurah": 69,
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
