      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 3930,
      "text": "felteḳamehü-lḥûtü vehüve mülîm.",
      "numberInSurah": 142,
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 3931,
      "text": "felevlâ ennehû kâne mine-lmüsebbiḥîn.",
      "numberInSurah": 143,
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 3932,
      "text": "lelebiŝe fî baṭnih ilâ yevmi yüb`aŝûn.",
      "numberInSurah": 144,
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 3933,
      "text": "fenebeẕnâhü bil`arâi vehüve seḳîm.",
      "numberInSurah": 145,
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3934,
      "text": "veembetnâ `aleyhi şeceratem miy yaḳṭîn.",
      "numberInSurah": 146,
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 3935,
      "text": "veerselnâhü ilâ mieti elfin ev yezîdûn.",
