{
  "number": 83,
  "name": "سُورَةُ المُطَفِّفِينَ",
  "englishName": "Al-Mutaffifin",
  "englishNameTranslation": "Defrauding",
  "revelationType": "Meccan",
  "numberOfAyahs": 36,
  "ayahs": [
    {
      "number": 5849,
      "text": "veylül lilmüṭaffifîn.",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5850,
      "text": "elleẕîne iẕe-ktâlû `ale-nnâsi yestevfûn.",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5851,
      "text": "veiẕâ kâlûhüm ev vezenûhüm yuḫsirûn.",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5852,
      "text": "elâ yeżunnü ülâike ennehüm meb`ûŝûn.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5853,
      "text": "liyevmin `ażîm.",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5854,
      "text": "yevme yeḳûmü-nnâsü lirabbi-l`âlemîn.",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 587,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5855,
      "text": "kellâ inne kitâbe-lfüccâri lefî siccîn.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5856,
      "text": "vemâ edrâke mâ siccîn.",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5857,
      "text": "kitâbüm merḳûm.",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5858,
      "text": "veylüy yevmeiẕil lilmükeẕẕibîn.",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5859,
      "text": "elleẕîne yükeẕẕibûne biyevmi-ddîn.",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5860,
      "text": "vemâ yükeẕẕibü bihî illâ küllü mü`tedin eŝîm.",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5861,
      "text": "iẕâ tütlâ `aleyhi âyâtünâ ḳâle esâṭîru-l'evvelîn.",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5862,
      "text": "kellâ bel râne `alâ ḳulûbihim mâ kânû yeksibûn.",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5863,
      "text": "kellâ innehüm `ar rabbihim yevmeiẕil lemaḥcûbûn.",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5864,
      "text": "ŝümme innehüm leṣâlü-lceḥîm.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5865,
      "text": "ŝümme yüḳâlü hâẕe-lleẕî küntüm bihî tükeẕẕibûn.",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5866,
      "text": "kellâ inne kitâbe-l'ebrâri lefî `illiyyîn.",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5867,
      "text": "vemâ edrâke mâ `illiyyûn.",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5868,
      "text": "kitâbüm merḳûm.",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5869,
      "text": "yeşhedühü-lmüḳarrabûn.",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5870,
      "text": "inne-l'ebrâra lefî ne`îm.",
      "numberInSurah": 22,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5871,
      "text": "`ale-l'erâiki yenżurûn.",
      "numberInSurah": 23,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5872,
      "text": "ta`rifü fî vucûhihim naḍrate-nne`îm.",
      "numberInSurah": 24,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5873,
      "text": "yüsḳavne mir raḥîḳim maḫtûm.",
      "numberInSurah": 25,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5874,
      "text": "ḫitâmühû misk. vefî ẕâlike felyetenâfesi-lmütenâfisûn.",
      "numberInSurah": 26,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5875,
      "text": "vemizâcühû min tesnîm.",
      "numberInSurah": 27,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5876,
      "text": "`ayney yeşrabü bihe-lmüḳarrabûn.",
      "numberInSurah": 28,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5877,
      "text": "inne-lleẕîne ecramû kânû mine-lleẕîne âmenû yaḍḥakûn.",
      "numberInSurah": 29,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5878,
      "text": "veiẕâ merrû bihim yetegâmezûn.",
      "numberInSurah": 30,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5879,
      "text": "veiẕe-nḳalebû ilâ ehlihimü-nḳalebû fekihîn.",
      "numberInSurah": 31,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5880,
      "text": "veiẕâ raevhüm ḳâlû inne hâülâi leḍâllûn.",
      "numberInSurah": 32,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5881,
      "text": "vemâ ürsilû `aleyhim ḥâfiżîn.",
      "numberInSurah": 33,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5882,
      "text": "felyevme-lleẕîne âmenû mine-lküffâri yaḍḥakûn.",
      "numberInSurah": 34,
      "juz": 30,
      "manzil": 7,
      "page": 588,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5883,
      "text": "`ale-l'erâiki yenżurûn.",
      "numberInSurah": 35,
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    },
    {
      "number": 5884,
      "text": "hel ŝüvvibe-lküffâru mâ kânû yef`alûn.",
      "numberInSurah": 36,
      "juz": 30,
      "manzil": 7,
      "page": 589,
      "ruku": 525,
      "hizbQuarter": 235,
      "sajda": false
    }
