{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "englishNameTranslation": "Those who drag forth",
  "revelationType": "Meccan",
  "numberOfAyahs": 46,
  "ayahs": [
    {
      "number": 5713,
      "text": "vennâzi`âti garḳâ.",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5714,
      "text": "vennâşiṭâti neşṭâ.",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5715,
      "text": "vessâbiḥâti sebḥâ.",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5716,
      "text": "fessâbiḳâti sebḳâ.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5717,
      "text": "felmüdebbirâti emrâ.",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5718,
      "text": "yevme tercüfü-rrâcifeh.",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5719,
      "text": "tetbe`uhe-rrâdifeh.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5720,
      "text": "ḳulûbüy yevmeiẕiv vâcifeh.",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5721,
      "text": "ebṣâruhâ ḫâşi`ah.",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5722,
      "text": "yeḳûlûne einnâ lemerdûdûne fi-lḥâfirah.",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5723,
      "text": "eiẕâ künnâ `iżâmen neḫirah.",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5724,
      "text": "ḳâlû tilke iẕen kerratün ḫâsirah.",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5725,
      "text": "feinnemâ hiye zecratüv vâḥideh.",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5726,
      "text": "feiẕâ hüm bissâhirah.",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5727,
      "text": "hel etâke ḥadîŝü mûsâ.",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5728,
      "text": "iẕ nâdâhü rabbühû bilvâdi-lmüḳaddesi ṭuvâ.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5729,
      "text": "iẕheb ilâ fir`avne innehû ṭagâ.",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5730,
      "text": "feḳul hel leke ilâ en tezekkâ.",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5731,
      "text": "veehdiyeke ilâ rabbike fetaḫşâ.",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5732,
      "text": "feerâhü-l'âyete-lkübrâ.",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5733,
      "text": "fekeẕẕebe ve`aṣâ.",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5734,
      "text": "ŝümme edbera yes`â.",
      "numberInSurah": 22,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5735,
      "text": "feḥaşera fenâdâ.",
      "numberInSurah": 23,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5736,
      "text": "feḳâle ene rabbükümü-l'a`lâ.",
      "numberInSurah": 24,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5737,
      "text": "feeḫaẕehü-llâhü nekâle-l'âḫirati vel'ûlâ.",
      "numberInSurah": 25,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5738,
      "text": "inne fî ẕâlike le`ibratel limey yaḫşâ.",
      "numberInSurah": 26,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5739,
      "text": "eentüm eşeddü ḫalḳan emi-ssemâü. benâhâ.",
      "numberInSurah": 27,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5740,
      "text": "rafe`a semkehâ fesevvâhâ.",
      "numberInSurah": 28,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5741,
      "text": "veagṭaşe leylehâ veaḫrace ḍuḥâhâ.",
      "numberInSurah": 29,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5742,
      "text": "vel'arḍa ba`de ẕâlike deḥâhâ.",
      "numberInSurah": 30,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5743,
      "text": "aḫrace minhâ mâehâ vemer`âhâ.",
      "numberInSurah": 31,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5744,
      "text": "velcibâle ersâhâ.",
      "numberInSurah": 32,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5745,
      "text": "metâ`al leküm velien`âmiküm.",
      "numberInSurah": 33,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5746,
      "text": "feiẕâ câeti-ṭṭâmmetü-lkübrâ.",
      "numberInSurah": 34,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5747,
      "text": "yevme yeteẕekkeru-l'insânü mâ se`â.",
      "numberInSurah": 35,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5748,
      "text": "vebürrizeti-lceḥîmü limey yerâ.",
      "numberInSurah": 36,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5749,
      "text": "feemmâ men ṭagâ.",
      "numberInSurah": 37,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5750,
      "text": "veâŝera-lḥayâte-ddünyâ.",
      "numberInSurah": 38,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5751,
      "text": "feinne-lceḥîme hiye-lme'vâ.",
      "numberInSurah": 39,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5752,
      "text": "veemmâ men ḫâfe meḳâme rabbihî venehe-nnefse `ani-lhevâ.",
      "numberInSurah": 40,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5753,
      "text": "feinne-lcennete hiye-lme'vâ.",
      "numberInSurah": 41,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5754,
      "text": "yes'elûneke `ani-ssâ`ati eyyâne mürsâhâ.",
      "numberInSurah": 42,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5755,
      "text": "fîme ente min ẕikrâhâ.",
      "numberInSurah": 43,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5756,
      "text": "ilâ rabbike müntehâhâ.",
      "numberInSurah": 44,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5757,
      "text": "innemâ ente münẕiru mey yaḫşâhâ.",
      "numberInSurah": 45,
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5758,
      "text": "keennehüm yevme yeravnehâ lem yelbeŝû illâ `aşiyyeten ev ḍuḥâhâ.",
