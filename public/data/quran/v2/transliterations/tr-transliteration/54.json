{
  "number": 54,
  "name": "سُورَةُ القَمَرِ",
  "englishName": "Al-Qamar",
  "englishNameTranslation": "The Moon",
  "revelationType": "Meccan",
  "numberOfAyahs": 55,
  "ayahs": [
    {
      "number": 4847,
      "text": "iḳterabeti-ssâ`atü venşeḳḳa-lḳamer.",
      "numberInSurah": 1,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4848,
      "text": "veiy yerav âyetey yü`riḍû veyeḳûlû siḥrum müstemirr.",
      "numberInSurah": 2,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4849,
      "text": "vekeẕẕebû vettebe`û ehvâehüm veküllü emrim müsteḳirr.",
      "numberInSurah": 3,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4850,
      "text": "veleḳad câehüm mine-l'embâi mâ fîhi müzdecer.",
      "numberInSurah": 4,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4851,
      "text": "ḥikmetüm bâligatün femâ tugni-nnüẕür.",
      "numberInSurah": 5,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4852,
      "text": "fetevelle `anhüm. yevme yed`u-ddâ`i ilâ şey'in nükür.",
      "numberInSurah": 6,
      "juz": 27,
      "manzil": 7,
      "page": 528,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4853,
      "text": "ḫuşşe`an ebṣâruhüm yaḫrucûne mine-l'ecdâŝi keennehüm cerâdüm münteşir.",
      "numberInSurah": 7,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4854,
      "text": "mühti`îne ile-ddâ`. yeḳûlü-lkâfirûne hâẕâ yevmün `asir.",
      "numberInSurah": 8,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 211,
      "sajda": false
    },
    {
      "number": 4855,
      "text": "keẕẕebet ḳablehüm ḳavmü nûḥin fekeẕẕebû `abdenâ veḳâlû mecnûnüv vezdücira.",
      "numberInSurah": 9,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4856,
      "text": "fede`â rabbehû ennî maglûbün fenteṣir.",
      "numberInSurah": 10,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4857,
      "text": "fefetaḥnâ ebvâbe-ssemâi bimâim münhemir.",
      "numberInSurah": 11,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4858,
      "text": "vefeccerne-l'arḍa `uyûnen felteḳe-lmâü `alâ emrin ḳad ḳudir.",
      "numberInSurah": 12,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4859,
      "text": "veḥamelnâhü `alâ ẕâti elvâḥiv vedüsür.",
      "numberInSurah": 13,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4860,
      "text": "tecrî bia`yüninâ. cezâel limen kâne küfira.",
      "numberInSurah": 14,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4861,
      "text": "veleḳat teraknâhâ âyeten fehel mim müddekir.",
      "numberInSurah": 15,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4862,
      "text": "fekeyfe kâne `aẕâbî venüẕür.",
      "numberInSurah": 16,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4863,
      "text": "veleḳad yesserne-lḳur'âne liẕẕikri fehel mim müddekir.",
      "numberInSurah": 17,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4864,
      "text": "keẕẕebet `âdün fekeyfe kâne `aẕâbî venüẕür.",
      "numberInSurah": 18,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4865,
      "text": "innâ erselnâ `aleyhim rîḥan ṣarṣaran fî yevmi naḥsim müstemirr.",
      "numberInSurah": 19,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4866,
      "text": "tenzi`u-nnâse keennehüm a`câzü naḫlim münḳa`ir.",
      "numberInSurah": 20,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4867,
      "text": "fekeyfe kâne `aẕâbî venüẕür.",
      "numberInSurah": 21,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4868,
      "text": "veleḳad yesserne-lḳur'âne liẕẕikri fehel mim müddekir.",
      "numberInSurah": 22,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 463,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4869,
      "text": "keẕẕebet ŝemûdü binnüẕür.",
      "numberInSurah": 23,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4870,
      "text": "feḳâlû ebeşeram minnâ vâḥiden nettebi`uhû innâ iẕel lefî ḍalâliv vesü`ur.",
      "numberInSurah": 24,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4871,
      "text": "eülḳiye-ẕẕikru `aleyhi mim beyninâ bel hüve keẕẕâbün eşir.",
      "numberInSurah": 25,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4872,
      "text": "seya`lemûne gadem meni-lkeẕẕâbü-l'eşir.",
      "numberInSurah": 26,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4873,
      "text": "innâ mürsilü-nnâḳati fitnetel lehüm ferteḳibhüm vaṣṭabir.",
      "numberInSurah": 27,
      "juz": 27,
      "manzil": 7,
      "page": 529,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4874,
      "text": "venebbi'hüm enne-lmâe ḳismetüm beynehüm. küllü şirbim muḥteḍar.",
      "numberInSurah": 28,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4875,
      "text": "fenâdev ṣâḥibehüm fete`âṭâ fe`aḳara.",
      "numberInSurah": 29,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4876,
      "text": "fekeyfe kâne `aẕâbî venüẕür.",
      "numberInSurah": 30,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4877,
      "text": "innâ erselnâ `aleyhim ṣayḥatev vâḥideten fekânû keheşîmi-lmuḥteżir.",
      "numberInSurah": 31,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4878,
      "text": "veleḳad yesserne-lḳur'âne liẕẕikri fehel mim müddekir.",
      "numberInSurah": 32,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4879,
      "text": "keẕẕebet ḳavmü lûṭim binnüẕür.",
      "numberInSurah": 33,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4880,
      "text": "innâ erselnâ `aleyhim ḥâṣiben illâ âle lûṭ. necceynâhüm biseḥar.",
      "numberInSurah": 34,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4881,
      "text": "ni`metem min `indinâ. keẕâlike neczî men şekera.",
      "numberInSurah": 35,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4882,
      "text": "veleḳad enẕerahüm baṭşetenâ fetemârav binnüẕür.",
      "numberInSurah": 36,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4883,
      "text": "veleḳad râvedûhü `an ḍayfihî feṭamesnâ a`yünehüm feẕûḳû `aẕâbî venüẕür.",
      "numberInSurah": 37,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4884,
      "text": "veleḳad ṣabbeḥahüm bükraten `aẕâbüm müsteḳirr.",
      "numberInSurah": 38,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4885,
      "text": "feẕûḳû `aẕâbî venüẕür.",
      "numberInSurah": 39,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4886,
      "text": "veleḳad yesserne-lḳur'âne liẕẕikri fehel mim müddekir.",
      "numberInSurah": 40,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 464,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4887,
      "text": "veleḳad câe âle fir`avne-nnüẕür.",
      "numberInSurah": 41,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4888,
      "text": "keẕẕebû biâyâtinâ küllihâ feeḫaẕnâhüm aḫẕe `azîzim muḳtedir.",
      "numberInSurah": 42,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4889,
      "text": "eküffâruküm ḫayrum min ülâiküm em leküm berâetün fi-zzübür.",
      "numberInSurah": 43,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4890,
      "text": "em yeḳûlûne naḥnü cemî`um münteṣir.",
      "numberInSurah": 44,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4891,
      "text": "seyühzemü-lcem`u veyüvellûne-ddübüra.",
      "numberInSurah": 45,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4892,
      "text": "beli-ssâ`atü mev`idühüm vessâ`atü edhâ veemerr.",
      "numberInSurah": 46,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4893,
      "text": "inne-lmücrimîne fî ḍalâliv vesü`ur.",
      "numberInSurah": 47,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4894,
      "text": "yevme yüsḥabûne fi-nnâri `alâ vucûhihim. ẕûḳû messe seḳara.",
      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4895,
      "text": "innâ külle şey'in ḫalaḳnâhü biḳader.",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 530,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4896,
      "text": "vemâ emrunâ illâ vâḥidetün kelemḥim bilbeṣar.",
      "numberInSurah": 50,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4897,
      "text": "veleḳad ehleknâ eşyâ`aküm fehel mim müddekir.",
      "numberInSurah": 51,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4898,
      "text": "veküllü şey'in fe`alûhü fi-zzübür.",
      "numberInSurah": 52,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4899,
      "text": "veküllü ṣagîriv vekebîrim müsteṭar.",
      "numberInSurah": 53,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4900,
      "text": "inne-lmütteḳîne fî cennâtiv veneher.",
      "numberInSurah": 54,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    },
    {
      "number": 4901,
      "text": "fî maḳ`adi ṣidḳin `inde melîkim muḳtedir.",
      "numberInSurah": 55,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 465,
      "hizbQuarter": 212,
      "sajda": false
    }
