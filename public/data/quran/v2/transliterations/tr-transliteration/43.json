{
  "number": 43,
  "name": "سُورَةُ الزُّخۡرُفِ",
  "englishName": "Az-Zukhruf",
  "englishNameTranslation": "Ornaments of gold",
  "revelationType": "Meccan",
  "numberOfAyahs": 89,
  "ayahs": [
    {
      "number": 4326,
      "text": "ḥâ-mîm.",
      "numberInSurah": 1,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4327,
      "text": "velkitâbi-lmübîn.",
      "numberInSurah": 2,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4328,
      "text": "innâ ce`alnâhü ḳur'ânen `arabiyyel le`alleküm ta`ḳilûn.",
      "numberInSurah": 3,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4329,
      "text": "veinnehû fî ümmi-lkitâbi ledeynâ le`aliyyün ḥakîm.",
      "numberInSurah": 4,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4330,
      "text": "efenaḍribü `ankümü-ẕẕikra ṣafḥan en küntüm ḳavmem müsrifîn.",
      "numberInSurah": 5,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4331,
      "text": "vekem erselnâ min nebiyyin fi-l'evvelîn.",
      "numberInSurah": 6,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4332,
      "text": "vemâ ye'tîhim min nebiyyin illâ kânû bihî yestehziûn.",
      "numberInSurah": 7,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4333,
      "text": "feehleknâ eşedde minhüm baṭşev vemeḍâ meŝelü-l'evvelîn.",
      "numberInSurah": 8,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4334,
      "text": "velein seeltehüm men ḫaleḳa-ssemâvâti vel'arḍa leyeḳûlünne ḫaleḳahünne-l`azîzü-l`alîm.",
      "numberInSurah": 9,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4335,
      "text": "elleẕî ce`ale lekümü-l'arḍa mehdev vece`ale leküm fîhâ sübülel le`alleküm tehtedûn.",
      "numberInSurah": 10,
      "juz": 25,
      "manzil": 6,
      "page": 489,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4336,
      "text": "velleẕî nezzele mine-ssemâi mâem biḳader. feenşernâ bihî beldetem meytâ. keẕâlike tuḫracûn.",
      "numberInSurah": 11,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4337,
      "text": "velleẕî ḫaleḳa-l'ezvâce küllehâ vece`ale leküm mine-lfülki vel'en`âmi mâ terkebûn.",
      "numberInSurah": 12,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4338,
      "text": "litestevû `alâ żuhûrihî ŝümme teẕkürû ni`mete rabbiküm iẕe-steveytüm `aleyhi veteḳûlû sübḥâne-lleẕî seḫḫara lenâ hâẕâ vemâ künnâ lehû muḳrinîn.",
      "numberInSurah": 13,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4339,
      "text": "veinnâ ilâ rabbinâ lemünḳalibûn.",
      "numberInSurah": 14,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4340,
      "text": "vece`alû lehû min `ibâdihî cüz'â. inne-l'insâne lekefûrum mübîn.",
      "numberInSurah": 15,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 424,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4341,
      "text": "emi-tteḫaẕe mimmâ yaḫlüḳu benâtiv veaṣfâküm bilbenîn.",
      "numberInSurah": 16,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4342,
      "text": "veiẕâ büşşira eḥadühüm bimâ ḍarabe lirraḥmâni meŝelen żalle vechühû müsveddev vehüve keżîm.",
      "numberInSurah": 17,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4343,
      "text": "evemey yüneşşeü fi-lḥilyeti vehüve fi-lḫiṣâmi gayru mübîn.",
      "numberInSurah": 18,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4344,
      "text": "vece`alü-lmelâikete-lleẕîne hüm `ibâdü-rraḥmâni inâŝâ. eşehidû ḫalḳahüm. setüktebü şehâdetühüm veyüs'elûn.",
      "numberInSurah": 19,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4345,
      "text": "veḳâlû lev şâe-rraḥmânü mâ `abednâhüm. mâ lehüm biẕâlike min `ilm. in hüm illâ yaḫruṣûn.",
      "numberInSurah": 20,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4346,
      "text": "em âteynâhüm kitâbem min ḳablihî fehüm bihî müstemsikûn.",
      "numberInSurah": 21,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4347,
      "text": "bel ḳâlû innâ vecednâ âbâenâ `alâ ümmetiv veinnâ `alâ âŝârihim mühtedûn.",
      "numberInSurah": 22,
      "juz": 25,
      "manzil": 6,
      "page": 490,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4348,
      "text": "vekeẕâlike mâ erselnâ min ḳablike fî ḳaryetim min neẕîrin illâ ḳâle mütrafûhâ innâ vecednâ âbâenâ `alâ ümmetiv veinnâ `alâ âŝârihim muḳtedûn.",
      "numberInSurah": 23,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 425,
      "hizbQuarter": 196,
      "sajda": false
    },
    {
      "number": 4349,
      "text": "ḳâle evelev ci'tüküm biehdâ mimmâ vecettüm `aleyhi âbâeküm. ḳâlû innâ bimâ ürsiltüm bihî kâfirûn.",
      "numberInSurah": 24,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 425,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4350,
      "text": "fenteḳamnâ minhüm fenżur keyfe kâne `âḳibetü-lmükeẕẕibîn.",
      "numberInSurah": 25,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 425,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4351,
      "text": "veiẕ ḳâle ibrâhîmü liebîhi veḳavmihî innenî berâüm mimmâ ta`büdûn.",
      "numberInSurah": 26,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4352,
      "text": "ille-lleẕî feṭaranî feinnehû seyehdîn.",
      "numberInSurah": 27,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4353,
      "text": "vece`alehâ kelimetem bâḳiyeten fî `aḳibihî le`allehüm yerci`ûn.",
      "numberInSurah": 28,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4354,
      "text": "bel metta`tü hâülâi veâbâehüm ḥattâ câehümü-lḥaḳḳu verasûlüm mübîn.",
      "numberInSurah": 29,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4355,
      "text": "velemmâ câehümü-lḥaḳḳu ḳâlû hâẕâ siḥruv veinnâ bihî kâfirûn.",
      "numberInSurah": 30,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4356,
      "text": "veḳâlû levlâ nüzzile hâẕe-lḳur'ânü `alâ racülim mine-lḳaryeteyni `ażîm.",
      "numberInSurah": 31,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4357,
      "text": "ehüm yaḳsimûne raḥmete rabbik. naḥnü ḳasemnâ beynehüm me`îşetehüm fi-lḥayâti-ddünyâ verafa`nâ ba`ḍahüm fevḳa ba`ḍin deracâtil liyetteḫiẕe ba`ḍuhüm ba`ḍan suḫriyyâ. veraḥmetü rabbike ḫayrum mimmâ yecme`ûn.",
      "numberInSurah": 32,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4358,
      "text": "velevlâ ey yekûne-nnâsü ümmetev vâḥidetel lece`alnâ limey yekfüru birraḥmâni libüyûtihim süḳufem min fiḍḍativ veme`ârice `aleyhâ yażherûn.",
      "numberInSurah": 33,
      "juz": 25,
      "manzil": 6,
      "page": 491,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4359,
      "text": "velibüyûtihim ebvâbev vesüruran `aleyhâ yettekiûn.",
      "numberInSurah": 34,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4360,
      "text": "vezuḫrufâ. vein küllü ẕâlike lemmâ metâ`u-lḥayâti-ddünyâ. vel'âḫiratü `inde rabbike lilmütteḳîn.",
      "numberInSurah": 35,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 426,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4361,
      "text": "vemey ya`şü `an ẕikri-rraḥmâni nüḳayyiḍ lehû şeyṭânen fehüve lehû ḳarîn.",
      "numberInSurah": 36,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4362,
      "text": "veinnehüm leyeṣuddûnehüm `ani-ssebîli veyaḥsebûne ennehüm mühtedûn.",
      "numberInSurah": 37,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4363,
      "text": "ḥattâ iẕâ câenâ ḳâle yâ leyte beynî vebeyneke bü`de-lmeşriḳayni febi'se-lḳarîn.",
      "numberInSurah": 38,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4364,
      "text": "veley yenfe`akümü-lyevme iż żalemtüm enneküm fi-l`aẕâbi müşterikûn.",
      "numberInSurah": 39,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4365,
      "text": "efeente tüsmi`u-ṣṣumme ev tehdi-l`umye vemen kâne fî ḍalâlim mübîn.",
      "numberInSurah": 40,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4366,
      "text": "feimmâ neẕhebenne bike feinnâ minhüm münteḳimûn.",
      "numberInSurah": 41,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4367,
      "text": "ev nüriyenneke-lleẕî ve`adnâhüm feinnâ `aleyhim muḳtedirûn.",
      "numberInSurah": 42,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4368,
      "text": "festemsik billeẕî ûḥiye ileyk. inneke `alâ ṣirâṭim müsteḳîm.",
      "numberInSurah": 43,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4369,
      "text": "veinnehû leẕikrul leke veliḳavmik. vesevfe tüs'elûn.",
      "numberInSurah": 44,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4370,
      "text": "ves'el men erselnâ min ḳablike mir rusülinâ. ece`alnâ min dûni-rraḥmâni âlihetey yü`bedûn.",
      "numberInSurah": 45,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 427,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4371,
      "text": "veleḳad erselnâ mûsâ biâyâtinâ ilâ fir`avne vemeleihî feḳâle innî rasûlü rabbi-l`âlemîn.",
      "numberInSurah": 46,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4372,
      "text": "felemmâ câehüm biâyâtinâ iẕâ hüm minhâ yaḍḥakûn.",
      "numberInSurah": 47,
      "juz": 25,
      "manzil": 6,
      "page": 492,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4373,
      "text": "vemâ nürîhim min âyetin illâ hiye ekberu min uḫtihâ. veeḫaẕnâhüm bil`aẕâbi le`allehüm yerci`ûn.",
      "numberInSurah": 48,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4374,
      "text": "veḳâlû yâ eyyühe-ssâḥiru-d`u lenâ rabbeke bimâ `ahide `indeke innenâ lemühtedûn.",
      "numberInSurah": 49,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4375,
      "text": "felemmâ keşefnâ `anhümü-l`aẕâbe iẕâ hüm yenküŝûn.",
      "numberInSurah": 50,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4376,
      "text": "venâdâ fir`avnü fî ḳavmihî ḳâle yâ ḳavmi eleyse lî mülkü miṣra vehâẕihi-l'enhâru tecrî min taḥtî. efelâ tübṣirûn.",
      "numberInSurah": 51,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4377,
      "text": "em ene ḫayrum min hâẕe-lleẕî hüve mehînüv velâ yekâdü yübîn.",
      "numberInSurah": 52,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4378,
      "text": "felevlâ ülḳiye `aleyhi esviratüm min ẕehebin ev câe me`ahü-lmelâiketü muḳterinîn.",
      "numberInSurah": 53,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4379,
      "text": "festeḫaffe ḳavmehû feeṭâ`ûh. innehüm kânû ḳavmen fâsiḳîn.",
      "numberInSurah": 54,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4380,
      "text": "felemmâ âsefûne-nteḳamnâ minhüm feagraḳnâhüm ecme`în.",
      "numberInSurah": 55,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4381,
      "text": "fece`alnâhüm selefev vemeŝelel lil'âḫirîn.",
      "numberInSurah": 56,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 428,
      "hizbQuarter": 197,
      "sajda": false
    },
    {
      "number": 4382,
      "text": "velemmâ ḍuribe-bnü meryeme meŝelen iẕâ ḳavmüke minhü yeṣiddûn.",
      "numberInSurah": 57,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4383,
      "text": "veḳâlû eâlihetünâ ḫayrun em hû. mâ ḍarabûhü leke illâ cedelâ. bel hüm ḳavmün ḫasimûn.",
      "numberInSurah": 58,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4384,
      "text": "in hüve illâ `abdün en`amnâ `aleyhi vece`alnâhü meŝelel libenî isrâîl.",
      "numberInSurah": 59,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4385,
      "text": "velev neşâü lece`alnâ minküm melâiketen fi-l'arḍi yaḫlüfûn.",
      "numberInSurah": 60,
      "juz": 25,
      "manzil": 6,
      "page": 493,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4386,
      "text": "veinnehû le`ilmül lissâ`ati felâ temterunne bihâ vettebi`ûn. hâẕâ ṣirâṭum müsteḳîm.",
      "numberInSurah": 61,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4387,
      "text": "velâ yeṣuddennekümü-şşeyṭân. innehû leküm `adüvvüm mübîn.",
      "numberInSurah": 62,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4388,
      "text": "velemmâ câe `îsâ bilbeyyinâti ḳâle ḳad ci'tüküm bilḥikmeti veliübeyyine leküm ba`ḍa-lleẕî taḫtelifûne fîh. fetteḳu-llâhe veeṭî`ûn.",
      "numberInSurah": 63,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4389,
      "text": "inne-llâhe hüve rabbî verabbüküm fa`büdûh. hâẕâ ṣirâṭum müsteḳîm.",
      "numberInSurah": 64,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4390,
      "text": "faḫtelefe-l'aḥzâbü mim beynihim. feveylül lilleẕîne żalemû min `aẕâbi yevmin elîm.",
      "numberInSurah": 65,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4391,
      "text": "hel yenżurûne ille-ssâ`ate en te'tiyehüm bagtetev vehüm lâ yeş`urûn.",
      "numberInSurah": 66,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4392,
      "text": "el'eḫillâü yevmeiẕim ba`ḍuhüm liba`ḍin `adüvvün ille-lmütteḳîn.",
      "numberInSurah": 67,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 429,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4393,
      "text": "yâ `ibâdi lâ ḫavfün `aleykümü-lyevme velâ entüm taḥzenûn.",
      "numberInSurah": 68,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4394,
      "text": "elleẕîne âmenû biâyâtinâ vekânû müslimîn.",
      "numberInSurah": 69,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4395,
      "text": "üdḫulü-lcennete entüm veezvâcüküm tuḥberûn.",
      "numberInSurah": 70,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4396,
      "text": "yüṭâfü `aleyhim biṣiḥâfim min ẕehebiv veekvâb. vefîhâ mâ teştehîhi-l'enfüsü veteleẕẕü-l'a`yün. veentüm fîhâ ḫâlidûn.",
      "numberInSurah": 71,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4397,
      "text": "vetilke-lcennetü-lletî ûriŝtümûhâ bimâ küntüm ta`melûn.",
      "numberInSurah": 72,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4398,
      "text": "leküm fîhâ fâkihetün keŝîratüm minhâ te'külûn.",
      "numberInSurah": 73,
      "juz": 25,
      "manzil": 6,
      "page": 494,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4399,
      "text": "inne-lmücrimîne fî `aẕâbi cehenneme ḫâlidûn.",
      "numberInSurah": 74,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4400,
      "text": "lâ yüfetteru `anhüm vehüm fîhi müblisûn.",
      "numberInSurah": 75,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4401,
      "text": "vemâ żalemnâhüm velâkin kânû hümu-żżâlimîn.",
      "numberInSurah": 76,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4402,
      "text": "venâdev yâ mâlikü liyaḳḍi `aleynâ rabbük. ḳâle inneküm mâkiŝûn.",
      "numberInSurah": 77,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4403,
      "text": "leḳad ci'nâküm bilḥaḳḳi velâkinne ekŝeraküm lilḥaḳḳi kârihûn.",
      "numberInSurah": 78,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4404,
      "text": "em ebramû emran feinnâ mübrimûn.",
      "numberInSurah": 79,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4405,
      "text": "em yaḥsebûne ennâ lâ nesme`u sirrahüm venecvâhüm. belâ verusülünâ ledeyhim yektübûn.",
      "numberInSurah": 80,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4406,
      "text": "ḳul in kâne lirraḥmâni veled. feenâ evvelü-l`âbidîn.",
      "numberInSurah": 81,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4407,
      "text": "sübḥâne rabbi-ssemâvâti vel'arḍi rabbi-l`arşi `ammâ yeṣifûn.",
      "numberInSurah": 82,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4408,
      "text": "feẕerhüm yeḫûḍû veyel`abû ḥattâ yülâḳû yevmehümü-lleẕî yû`adûn.",
      "numberInSurah": 83,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4409,
      "text": "vehüve-lleẕî fi-ssemâi ilâhüv vefi-l'arḍi ilâhün. vehüve-lḥakîmü-l`alîm.",
      "numberInSurah": 84,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4410,
      "text": "vetebârake-lleẕî lehû mülkü-ssemâvâti vel'arḍi vemâ beynehümâ. ve`indehû `ilmü-ssâ`ah. veileyhi türce`ûn.",
      "numberInSurah": 85,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4411,
      "text": "velâ yemlikü-lleẕîne yed`ûne min dûnihi-şşefâ`ate illâ men şehide bilḥaḳḳi vehüm ya`lemûn.",
      "numberInSurah": 86,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4412,
      "text": "velein seeltehüm men ḫaleḳahüm leyeḳûlünne-llâhü feennâ yü'fekûn.",
      "numberInSurah": 87,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4413,
      "text": "veḳîlihî yâ rabbi inne hâülâi ḳavmül lâ yü'minûn.",
      "numberInSurah": 88,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    },
    {
      "number": 4414,
      "text": "faṣfaḥ `anhüm veḳul selâm. fesevfe ya`lemûn.",
      "numberInSurah": 89,
      "juz": 25,
      "manzil": 6,
      "page": 495,
      "ruku": 430,
      "hizbQuarter": 198,
      "sajda": false
    }
