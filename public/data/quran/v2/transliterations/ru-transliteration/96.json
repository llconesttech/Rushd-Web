{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "englishNameTranslation": "The Clot",
  "revelationType": "Meccan",
  "numberOfAyahs": 19,
  "ayahs": [
    {
      "number": 6107,
      "text": "А қ ра ' Биӓсми Ра ббика А л-Лаҙӥ Ҳ ала қ а \r",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6108,
      "text": "Ҳ ала қ а А л-'И н с ӓ на Мин `Ала қ и н \r",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6109,
      "text": "А қ ра ' Ұа Ра ббука А л-'Ак ра му \r",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6110,
      "text": "Ал-Лаҙӥ `Аллама Бил- Қ алами \r",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6111,
      "text": "`Аллама А л-'И н с ӓ на Мӓ Ла м Йа`ла м \r",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6112,
      "text": "Калл ӓ 'И нн а А л-'И н с ӓ на Лайа ҭ ғ á \r",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6113,
      "text": "'А н Ра ' ӓ һу А ста ғ нá \r",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6114,
      "text": "'И нн а 'Илá Ра ббика А р- Ру җ `á \r",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6115,
      "text": "'А ра 'а й та А л-Лаҙӥ Йанһá \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6116,
      "text": "`А б дӓан 'Иҙӓ Ҫ аллá \r",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6117,
      "text": "'А ра 'а й та 'И н К ӓ на `Алá А л-Һудá \r",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6118,
      "text": "'Аұ 'Ама ра Бит-Та қ ұá \r",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6119,
      "text": "'А ра 'а й та 'И н Каҙҙаба Ұа Таұаллá \r",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6120,
      "text": "'Ала м Йа`ла м Би'а нн а А лл ӓһа Йарá \r",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6121,
      "text": "Каллӓ Ла'и н Ла м Йа н таһ и Ланасфа`ӓа н Би н -Нӓ ҫ ийати \r",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6122,
      "text": "Нӓ ҫ ийати н Кӓҙибатин Ҳ ӓ ҭ и'ати н \r",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6123,
      "text": "Фалйа д `у Нӓдийаһ \r",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6124,
      "text": "Сана д `у А з-Забӓнийата \r",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6125,
      "text": "Каллӓ Лӓ Ту ҭ и`һу Ұа А сҗу д Ұӓ қ та р и б \r",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
