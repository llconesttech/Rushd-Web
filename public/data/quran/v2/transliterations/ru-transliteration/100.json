{
  "number": 100,
  "name": "سُورَةُ العَادِيَاتِ",
  "englishName": "Al-Aadiyaat",
  "englishNameTranslation": "The Chargers",
  "revelationType": "Meccan",
  "numberOfAyahs": 11,
  "ayahs": [
    {
      "number": 6147,
      "text": "Ұа А л-`Ӓдий ӓ ти Đ а б хӓа н \r",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6148,
      "text": "Фӓлмӱ р ий ӓ ти Қ а д хӓа н \r",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6149,
      "text": "Фӓлму ғ ӥ р ӓ ти Ҫ у б хӓа н \r",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6150,
      "text": "Фа'аćарна Биһ и На қ `ӓа н \r",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6151,
      "text": "Фаұаса ҭ на Биһ и Җа м `ӓа н \r",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6152,
      "text": "'И нн а А л-'И н с ӓ на Ли ра ббиһ и Лакан ӱ д у н \r",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6153,
      "text": "Ұа 'И нн аһ у `Алá Ҙӓлика Лашаһ ӥ д у н \r",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6154,
      "text": "Ұа 'И нн аһ у Лихубби А л- Ҳ а й р и Лашад ӥ д у н \r",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6155,
      "text": "'Афалӓ Йа`ламу 'Иҙӓ Бу`ćи р а Мӓ Фӥ А л- Қ уб ӱ р и \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6156,
      "text": "Ұа Ху ҫ ҫ ила Мӓ Фӥ А ҫ - Ҫ уд ӱ р и \r",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6157,
      "text": "'И нн а Ра ббаһу м Биһи м Йаұма'иҙи н Ла ҳ аб ӥ р у н \r",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    }
  ],
