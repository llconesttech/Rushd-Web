{
  "number": 15,
  "name": "سُورَةُ الحِجۡرِ",
  "englishName": "Al-Hijr",
  "englishNameTranslation": "The Rock",
  "revelationType": "Meccan",
  "numberOfAyahs": 99,
  "ayahs": [
    {
      "number": 1803,
      "text": "'Алиф-Лӓ м - Рӓ ۚ Тилка 'Ӓй ӓ ту А л-Кит ӓ би Ұа Қ ур' ӓ ни н Муб ӥ ни н \r",
      "numberInSurah": 1,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1804,
      "text": "Ру бамӓ Йаұадду А л-Лаҙ ӥ на Кафарӱ Лаұ Кӓнӱ Муслим ӥ на \r",
      "numberInSurah": 2,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1805,
      "text": "Ҙарһу м Йа'кулӱ Ұа Йатаматта`ӱ Ұа Йулһиһиму А л-'Амалу ۖ Фаса ұ фа Йа`лам ӱ на \r",
      "numberInSurah": 3,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1806,
      "text": "Ұа М ӓ 'Аһлакнӓ Ми н Қ арйатин 'Иллӓ Ұа Лаһӓ Кит ӓ бу н Ма`л ӱ му н \r",
      "numberInSurah": 4,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1807,
      "text": "Мӓ Тасби қ у Мин 'У мм атин 'Аҗалаһӓ Ұа Мӓ Йаста' ҳ ир ӱ на \r",
      "numberInSurah": 5,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1808,
      "text": "Ұа Қ ӓлӱ Й ӓ 'Аййуһӓ А л-Лаҙӥ Нуззила `Ала й һи А ҙ-Ҙик ру 'И нн ака Лама җ н ӱ ну н \r",
      "numberInSurah": 6,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1809,
      "text": "Лаұ Мӓ Та'тӥнӓ Бил-Мал ӓ 'икати 'И н Ку н та Мина А ҫ - Ҫ ӓди қ ӥ на \r",
      "numberInSurah": 7,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1810,
      "text": "Мӓ Нуназзилу А л-Мал ӓ 'иката 'Иллӓ Бил-Ха қ қ и Ұа Мӓ Кӓн ӱ 'Иҙӓа н Му н žа р ӥ на \r",
      "numberInSurah": 8,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1811,
      "text": "'И нн ӓ Нахну Наззалнӓ А ҙ-Ҙик ра Ұа 'И нн ӓ Лаһ у Лахӓфиž ӱ на \r",
      "numberInSurah": 9,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1812,
      "text": "Ұа Ла қ а д 'Арсалнӓ Ми н Қ а б лика Фӥ Шийа`и А л-'Аұұал ӥ на \r",
      "numberInSurah": 10,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1813,
      "text": "Ұа Мӓ Йа'тӥһи м Ми н Ра с ӱ лин 'Иллӓ Кӓнӱ Биһ и Йастаһзи' ӱ на \r",
      "numberInSurah": 11,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1814,
      "text": "Каҙӓлика Наслукуһу Фӥ Қ ул ӱ би А л-Му җ р им ӥ на \r",
      "numberInSurah": 12,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1815,
      "text": "Лӓ Йу'умин ӱ на Биһ и ۖ Ұа Қ а д Ҳ алат Су нн ату А л-'Аұұал ӥ на \r",
      "numberInSurah": 13,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1816,
      "text": "Ұа Лаұ Фатахнӓ `Алайһи м Бӓбӓа н Мина А с-Сам ӓ 'и Фаžаллӱ Ф ӥ һи Йа` ру җ ӱ на \r",
      "numberInSurah": 14,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1817,
      "text": "Ла қ ӓл ӱ 'И нн амӓ Сукки ра т 'А б ҫ ӓ ру нӓ Бал Нахну Қ а ұ му н Масхӱр ӱ на \r",
      "numberInSurah": 15,
      "juz": 14,
      "manzil": 3,
      "page": 262,
      "ruku": 218,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1818,
      "text": "Ұа Ла қ а д Җа`алнӓ Фӥ А с-Сам ӓ 'и Бурӱҗӓа н Ұа Заййа нн ӓһӓ Лил нн ӓžи р ӥ на \r",
      "numberInSurah": 16,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1819,
      "text": "Ұа Хафиžнӓһӓ Ми н Кулли Шай ҭ ӓ ни н Ра җ ӥ ми н \r",
      "numberInSurah": 17,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1820,
      "text": "'Иллӓ Мани А ста ра қ а А с-Са м `а Фа'атба`аһу Шиһ ӓ бу н Муб ӥ ну н \r",
      "numberInSurah": 18,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1821,
      "text": "Ұа А л-'Арđа Мада д нӓһӓ Ұа 'Ал қ айнӓ Фӥһӓ Ра ұӓсийа Ұа 'А н батнӓ Фӥһӓ Ми н Кулли Ша й 'и н Маұз ӱ ни н \r",
      "numberInSurah": 19,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1822,
      "text": "Ұа Җа`алнӓ Лаку м Фӥһӓ Ма`ӓйиша Ұа Ма н Ласту м Лаһ у Би р ӓзи қ ӥ на \r",
      "numberInSurah": 20,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1823,
      "text": "Ұа 'И н Ми н Ша й 'ин 'Иллӓ `И н данӓ Ҳ аз ӓ 'инуһ у Ұа Мӓ Нуназзилуһ у~ 'Иллӓ Би қ ада р и н Ма`л ӱ ми н \r",
      "numberInSurah": 21,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1824,
      "text": "Ұа 'Арсалнӓ А р- Р ий ӓ ха Лаұӓ қ иха Фа'а н залнӓ Мина А с-Сам ӓ 'и М ӓ 'а н Фа'ас қ айнӓкум ӱ һу Ұа М ӓ 'А н ту м Лаһ у Би ҳ ӓзин ӥ на \r",
      "numberInSurah": 22,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1825,
      "text": "Ұа 'И нн ӓ Ланахну Нухйӥ Ұа Нум ӥ ту Ұа Нахну А л-Ұӓ р иć ӱ на \r",
      "numberInSurah": 23,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1826,
      "text": "Ұа Ла қ а д `Али м нӓ А л-Муста қ дим ӥ на Ми н ку м Ұа Ла қ а д `Али м нӓ А л-Муста' ҳ и р ӥ на \r",
      "numberInSurah": 24,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1827,
      "text": "Ұа 'И нн а Ра ббака Һуұа Йахшу ру һу м ۚ 'И нн аһу Хак ӥ мун `Ал ӥ му н \r",
      "numberInSurah": 25,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 219,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1828,
      "text": "Ұа Ла қ а д Ҳ ала қ нӓ А л-'И н с ӓ на Ми н Ҫ ал ҫ ӓ ли н Мин Хама'ии н Масн ӱ ни н \r",
      "numberInSurah": 26,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1829,
      "text": "Ұа А л-Җ ӓ нн а Ҳ ала қ н ӓ һу Ми н Қ а б лу Ми н Н ӓ р и А с-Сам ӱ ми \r",
      "numberInSurah": 27,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1830,
      "text": "Ұа 'Иҙ Қ ӓ ла Ра ббука Лилмал ӓ 'икати 'И нн ӥ Ҳ ӓли қ у н Баша рӓ а н Ми н Ҫ ал ҫ ӓ ли н Мин Хама'ии н Масн ӱ ни н \r",
      "numberInSurah": 28,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1831,
      "text": "Фа'иҙӓ Саұұайтуһ у Ұа Нафа ҳ ту Ф ӥ һи Ми н Рӱхӥ Фа қ а`ӱ Лаһ у Сӓҗид ӥ на \r",
      "numberInSurah": 29,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1832,
      "text": "Фасаҗада А л-Мал ӓ 'икату Куллуһу м 'А җ ма` ӱ на \r",
      "numberInSurah": 30,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1833,
      "text": "'Илл ӓ 'И б л ӥ са 'Аб á 'А н Йак ӱ на Ма`а А с-Сӓҗид ӥ на \r",
      "numberInSurah": 31,
      "juz": 14,
      "manzil": 3,
      "page": 263,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1834,
      "text": "Қ ӓ ла Й ӓ 'И б л ӥ су Мӓ Лака 'Аллӓ Так ӱ на Ма`а А с-Сӓҗид ӥ на \r",
      "numberInSurah": 32,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1835,
      "text": "Қ ӓ ла Ла м 'Аку н Ли'сҗуда Либаша р ин Ҳ ала қ таһ у Ми н Ҫ ал ҫ ӓ ли н Мин Хама'ии н Масн ӱ ни н \r",
      "numberInSurah": 33,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1836,
      "text": "Қ ӓ ла Фӓ ҳ ру җ Минһӓ Фа'и нн ака Ра җ ӥ му н \r",
      "numberInSurah": 34,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1837,
      "text": "Ұа 'И нн а `Ала й ка А л-Ла`ната 'Илá Йа ұ ми А д-Д ӥ ни \r",
      "numberInSurah": 35,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1838,
      "text": "Қ ӓ ла Ра бби Фа'а н žи р н ӥ 'Илá Йа ұ ми Йу б `аć ӱ на \r",
      "numberInSurah": 36,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1839,
      "text": "Қ ӓ ла Фа'и нн ака Мина А л-Му н žа р ӥ на \r",
      "numberInSurah": 37,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1840,
      "text": "'Илá Йа ұ ми А л-Ұа қ ти А л-Ма`л ӱ ми \r",
      "numberInSurah": 38,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1841,
      "text": "Қ ӓ ла Ра бби Бим ӓ 'А ғ ұайтанӥ Ла'узаййина нн а Лаһу м Фӥ А л-'Арđи Ұа Ла'у ғ ұийа нн аһу м 'А җ ма` ӥ на \r",
      "numberInSurah": 39,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1842,
      "text": "'Иллӓ `Ибӓдака Минһуму А л-Му ҳ ла ҫ ӥ на \r",
      "numberInSurah": 40,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1843,
      "text": "Қ ӓ ла Һӓҙӓ Ҫ и р ӓ ҭ ун `Ала й йа Муста қ ӥ му н \r",
      "numberInSurah": 41,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1844,
      "text": "'И нн а `Ибӓдӥ Ла й са Лака `Алайһи м Сул ҭ ӓ нун 'Иллӓ Мани А ттаба`ака Мина А л- Ғ ӓұ ӥ на \r",
      "numberInSurah": 42,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1845,
      "text": "Ұа 'И нн а Җаһа нн ама Ламаұ`идуһу м 'А җ ма` ӥ на \r",
      "numberInSurah": 43,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1846,
      "text": "Лаһӓ Са б `ату 'А б ұ ӓ би н Ликулли Б ӓ би н Минһу м Җуз'у н Ма қ с ӱ му н \r",
      "numberInSurah": 44,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 220,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1847,
      "text": "'И нн а А л-Мутта қ ӥ на Фӥ Җа нн ӓ ти н Ұа `Уй ӱ ни н \r",
      "numberInSurah": 45,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1848,
      "text": "А д ҳ улӱһӓ Бисал ӓ мин 'Ӓмин ӥ на \r",
      "numberInSurah": 46,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1849,
      "text": "Ұа Наза`нӓ Мӓ Фӥ Ҫ удӱ р иһи м Мин Ғ иллин 'И ҳ ұӓнӓан `Алá Су ру р и н Мута қ ӓбил ӥ на \r",
      "numberInSurah": 47,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1850,
      "text": "Лӓ Йамассуһу м Фӥһӓ На ҫ абу н Ұа Мӓ Һу м Минһӓ Биму ҳ ра җ ӥ на \r",
      "numberInSurah": 48,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1851,
      "text": "Набби' `Ибӓд ӥ 'А нн ӥ 'Анӓ А л- Ғ аф ӱ ру А р- Ра х ӥ му \r",
      "numberInSurah": 49,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 105,
      "sajda": false
    },
    {
      "number": 1852,
      "text": "Ұа 'А нн а `Аҙӓбӥ Һуұа А л-`Аҙ ӓ бу А л-'Ал ӥ му \r",
      "numberInSurah": 50,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1853,
      "text": "Ұа Набби'һу м `А н Đ а й фи 'И б рӓ һ ӥ ма \r",
      "numberInSurah": 51,
      "juz": 14,
      "manzil": 3,
      "page": 264,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1854,
      "text": "'Иҙ Да ҳ алӱ `Ала й һи Фа қ ӓлӱ Салӓмӓа н Қ ӓ ла 'И нн ӓ Ми н ку м Ұаҗил ӱ на \r",
      "numberInSurah": 52,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1855,
      "text": "Қ ӓлӱ Лӓ Таұҗал 'И нн ӓ Нубашши ру ка Би ғ ул ӓ мин `Ал ӥ ми н \r",
      "numberInSurah": 53,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1856,
      "text": "Қ ӓ ла 'Абашшартумӱнӥ `Ал á 'А н Массанийа А л-Киба ру Фабима Тубашшир ӱ на \r",
      "numberInSurah": 54,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1857,
      "text": "Қ ӓлӱ Башшарн ӓ ка Бил-Ха қ қ и Фалӓ Таку н Мина А л- Қ ӓни ҭ ӥ на \r",
      "numberInSurah": 55,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1858,
      "text": "Қ ӓ ла Ұа Ма н Йа қ на ҭ у Ми н Ра хмати Ра ббиһ и~ 'Иллӓ А đ- Đ ӓ лл ӱ на \r",
      "numberInSurah": 56,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1859,
      "text": "Қ ӓ ла Фамӓ Ҳ а ҭ буку м 'Аййуһӓ А л-Мурсал ӱ на \r",
      "numberInSurah": 57,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1860,
      "text": "Қ ӓл ӱ 'И нн ӓ 'Урсилн ӓ 'Илá Қ а ұ ми н Му җ р им ӥ на \r",
      "numberInSurah": 58,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1861,
      "text": "'Илл ӓ ' Ӓ ла Лӱ ҭ ин 'И нн ӓ Ламунаҗҗӱһу м 'А җ ма` ӥ на \r",
      "numberInSurah": 59,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1862,
      "text": "'Иллӓ А м ра 'атаһ у Қ аддарн ӓ ۙ 'И нн аһӓ Ламина А л- Ғ ӓби р ӥ на \r",
      "numberInSurah": 60,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 221,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1863,
      "text": "Фала мм ӓ Җ ӓ 'а ' Ӓ ла Лӱ ҭ ин А л-Мурсал ӱ на \r",
      "numberInSurah": 61,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1864,
      "text": "Қ ӓ ла 'И нн аку м Қ а ұ му н Му н кар ӱ на \r",
      "numberInSurah": 62,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1865,
      "text": "Қ ӓлӱ Бал Җи'н ӓ ка Бимӓ Кӓнӱ Ф ӥ һи Йа м тар ӱ на \r",
      "numberInSurah": 63,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1866,
      "text": "Ұа 'Атайн ӓ ка Бил-Ха қ қ и Ұа 'И нн ӓ Ла ҫ ӓди қ ӱ на \r",
      "numberInSurah": 64,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1867,
      "text": "Фа'ас р и Би'аһлика Би қ и ҭ `и н Мина А л-Ла й ли Ұа А ттаби` 'А д бӓ ра һу м Ұа Лӓ Йалтафит Ми н ку м 'Ахаду н Ұа А м đӱ Ха й ćу Ту'умар ӱ на \r",
      "numberInSurah": 65,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1868,
      "text": "Ұа Қ аđайн ӓ 'Ила й һи Ҙӓлика А л-'А м ра 'А нн а Дӓби р а Һ ӓ 'уул ӓ ' Ма қ ҭ ӱ `у н Му ҫ бих ӥ на \r",
      "numberInSurah": 66,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1869,
      "text": "Ұа Җ ӓ 'а 'Аһлу А л-Мадӥнати Йаста б шир ӱ на \r",
      "numberInSurah": 67,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1870,
      "text": "Қ ӓ ла 'И нн а Һ ӓ 'уул ӓ ' Đ айфӥ Фалӓ Тафđах ӱ ни \r",
      "numberInSurah": 68,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1871,
      "text": "Ұа А тта қ ӱ А лл аһа Ұа Лӓ Ту ҳ з ӱ ни \r",
      "numberInSurah": 69,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1872,
      "text": "Қ ӓл ӱ 'Аұала м Нанһака `Ани А л-`Ӓлам ӥ на \r",
      "numberInSurah": 70,
      "juz": 14,
      "manzil": 3,
      "page": 265,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1873,
      "text": "Қ ӓ ла Һ ӓ 'уул ӓ ' Банӓт ӥ 'И н Ку н ту м Фӓ`ил ӥ на \r",
      "numberInSurah": 71,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1874,
      "text": "Ла`а м ру ка 'И нн аһу м Лафӥ Сак ра тиһи м Йа`маһ ӱ на \r",
      "numberInSurah": 72,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1875,
      "text": "Фа'а ҳ аҙат/һуму А ҫ - Ҫ айхату Муш р и қ ӥ на \r",
      "numberInSurah": 73,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1876,
      "text": "Фаҗа`алнӓ `Ӓлийаһӓ Сӓфилаһӓ Ұа 'А м ҭ арнӓ `Алайһи м Хиҗӓ ра та н Ми н Сиҗҗ ӥ ли н \r",
      "numberInSurah": 74,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1877,
      "text": "'И нн а Фӥ Ҙӓлика Ла' ӓ й ӓ ти н Лилмутаұассим ӥ на \r",
      "numberInSurah": 75,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1878,
      "text": "Ұа 'И нн аһӓ Лабисаб ӥ ли н Му қ ӥ ми н \r",
      "numberInSurah": 76,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1879,
      "text": "'И нн а Фӥ Ҙӓлика Ла' ӓ йата н Лилму'умин ӥ на \r",
      "numberInSurah": 77,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1880,
      "text": "Ұа 'И н К ӓ на 'А ҫ х ӓ бу А л-'Айкати Лаžӓлим ӥ на \r",
      "numberInSurah": 78,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1881,
      "text": "Фӓ н та қ а м нӓ Минһу м Ұа 'И нн аһумӓ Лаби'им ӓ ми н Муб ӥ ни н \r",
      "numberInSurah": 79,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 222,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1882,
      "text": "Ұа Ла қ а д Каҙҙаба 'А ҫ х ӓ бу А л-Хи җ р и А л-Мурсал ӥ на \r",
      "numberInSurah": 80,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1883,
      "text": "Ұа 'Ӓтайнӓһу м 'Ӓйӓтинӓ Факӓнӱ `Анһӓ Му` р иđ ӥ на \r",
      "numberInSurah": 81,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1884,
      "text": "Ұа Кӓнӱ Йанхит ӱ на Мина А л-Җиб ӓ ли Буйӱтӓан 'Ӓмин ӥ на \r",
      "numberInSurah": 82,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1885,
      "text": "Фа'а ҳ аҙат/һуму А ҫ - Ҫ айхату Му ҫ бих ӥ на \r",
      "numberInSurah": 83,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1886,
      "text": "Фам ӓ 'А ғ нá `Анһу м Мӓ Кӓнӱ Йаксиб ӱ на \r",
      "numberInSurah": 84,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1887,
      "text": "Ұа Мӓ Ҳ ала қ нӓ А с-Самӓұ ӓ ти Ұа А л-'Арđа Ұа Мӓ Байнаһум ӓ 'Иллӓ Бил-Ха қ қ и ۗ Ұа 'И нн а А с-Сӓ`ата Ла' ӓ тийату н ۖ Фӓ ҫ фахи А ҫ - Ҫ афха А л-Җам ӥ ла \r",
      "numberInSurah": 85,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1888,
      "text": "'И нн а Ра ббака Һуұа А л- Ҳ алл ӓ қ у А л-`Ал ӥ му \r",
      "numberInSurah": 86,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1889,
      "text": "Ұа Ла қ а д 'Ӓтайн ӓ ка Са б `ӓа н Мина А л-Маćӓнӥ Ұа А л- Қ ур' ӓ на А л-`Аž ӥ ма \r",
      "numberInSurah": 87,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1890,
      "text": "Лӓ Тамудда нн а `Айна й ка 'Илá Мӓ Матта`нӓ Биһ и~ 'Азұӓҗӓа н Минһу м Ұа Лӓ Тахзан `Алайһи м Ұа А ҳ фиđ Җанӓхака Лилму'умин ӥ на \r",
      "numberInSurah": 88,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1891,
      "text": "Ұа Қ ул 'И нн ӥ 'Анӓ А н -Наҙ ӥ р у А л-Муб ӥ ну \r",
      "numberInSurah": 89,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1892,
      "text": "Кам ӓ 'А н залнӓ `Алá А л-Му қ тасим ӥ на \r",
      "numberInSurah": 90,
      "juz": 14,
      "manzil": 3,
      "page": 266,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1893,
      "text": "Ал-Лаҙ ӥ на Җа`алӱ А л- Қ ур' ӓ на `Иđ ӥ на \r",
      "numberInSurah": 91,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1894,
      "text": "Фаұа ра ббика Ланас'ала нн аһу м 'А җ ма` ӥ на \r",
      "numberInSurah": 92,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1895,
      "text": "`А мм ӓ Кӓнӱ Йа`мал ӱ на \r",
      "numberInSurah": 93,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1896,
      "text": "Фӓ ҫ да` Бимӓ Ту'ума ру Ұа 'А` р иđ `Ани А л-Муш р ик ӥ на \r",
      "numberInSurah": 94,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1897,
      "text": "'И нн ӓ Кафайн ӓ ка А л-Мустаһзи' ӥ на \r",
      "numberInSurah": 95,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1898,
      "text": "Ал-Лаҙ ӥ на Йа җ `ал ӱ на Ма`а А лл ӓһи 'Илаһӓан 'Ӓ ҳ а ра ۚ Фаса ұ фа Йа`лам ӱ на \r",
      "numberInSurah": 96,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1899,
      "text": "Ұа Ла қ а д На`ламу 'А нн ака Йаđ ӥ қ у Ҫ а д ру ка Бимӓ Йа қ ӱл ӱ на \r",
      "numberInSurah": 97,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1900,
      "text": "Фасаббих Биха м ди Ра ббика Ұа Ку н Мина А с-Сӓҗид ӥ на \r",
      "numberInSurah": 98,
      "juz": 14,
      "manzil": 3,
      "page": 267,
      "ruku": 223,
      "hizbQuarter": 106,
      "sajda": false
    },
    {
      "number": 1901,
      "text": "Ұа А `бу д Ра ббака Хаттá Йа'тийака А л-Йа қ ӥ ну \r",
      "numberInSurah": 99,
      "juz": 14,
      "manzil": 3,
      "page": 267,
