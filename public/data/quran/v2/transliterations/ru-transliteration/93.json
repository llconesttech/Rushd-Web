      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6088,
      "text": "Фа'а мм ӓ А л-Йат ӥ ма Фалӓ Та қ һар \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
