{
  "number": 91,
  "name": "سُورَةُ الشَّمۡسِ",
  "englishName": "Ash-Shams",
  "englishNameTranslation": "The Sun",
  "revelationType": "Meccan",
  "numberOfAyahs": 15,
  "ayahs": [
    {
      "number": 6044,
      "text": "Ұа А ш-Ша м си Ұа Đ ухӓһӓ \r",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6045,
      "text": "Ұа А л- Қ ама р и 'Иҙӓ Талӓһӓ \r",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6046,
      "text": "Ұа А н -Наһ ӓ р и 'Иҙӓ Җаллӓһӓ \r",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6047,
      "text": "Ұа А л-Ла й ли 'Иҙӓ Йа ғ шӓһӓ \r",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6048,
      "text": "Ұа А с-Сам ӓ 'и Ұа Мӓ Банӓһӓ \r",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6049,
      "text": "Ұа А л-'Арđи Ұа Мӓ Ҭ ахӓһӓ \r",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6050,
      "text": "Ұа Нафси н Ұа Мӓ Саұұӓһӓ \r",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6051,
      "text": "Фа'алһамаһӓ Фуҗӱ ра һӓ Ұа Та қ ұӓһӓ \r",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6052,
      "text": "Қ а д 'Афлаха Ма н Заккӓһӓ \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6053,
      "text": "Ұа Қ а д Ҳ ӓ ба Ма н Дассӓһӓ \r",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6054,
      "text": "Каҙҙабат Ćам ӱ ду Би ҭ а ғ ұӓһӓ \r",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6055,
      "text": "'Иҙи А н ба`аćа 'Аш қ ӓһӓ \r",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6056,
      "text": "Фа қ ӓ ла Лаһу м Ра с ӱ лу А лл ӓһи Нӓ қ ата А лл ӓһи Ұа Су қ йӓһӓ \r",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6057,
      "text": "Факаҙҙаб ӱ һу Фа`а қ арӱһӓ Фада м дама `Алайһи м Ра ббуһу м Биҙа н биһи м Фасаұұӓһӓ \r",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 533,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6058,
      "text": "Ұа Лӓ Йа ҳ ӓ фу `У қ бӓһӓ \r",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
