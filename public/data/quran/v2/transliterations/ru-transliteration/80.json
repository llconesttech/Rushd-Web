{
  "number": 80,
  "name": "سُورَةُ عَبَسَ",
  "englishName": "Abasa",
  "englishNameTranslation": "He frowned",
  "revelationType": "Meccan",
  "numberOfAyahs": 42,
  "ayahs": [
    {
      "number": 5759,
      "text": "`Абаса Ұа Таұаллá \r",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5760,
      "text": "'А н Җ ӓ 'аһу А л-'А` м á \r",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5761,
      "text": "Ұа Мӓ Йу д р ӥ ка Ла`аллаһ у Йаззаккá \r",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5762,
      "text": "'Аұ Йаҙҙакка ру Фата н фа`аһу А ҙ-Ҙикрá \r",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5763,
      "text": "'А мм ӓ Мани А ста ғ нá \r",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5764,
      "text": "Фа'а н та Лаһ у Та ҫ аддá \r",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5765,
      "text": "Ұа Мӓ `Ала й ка 'Аллӓ Йаззаккá \r",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5766,
      "text": "Ұа 'А мм ӓ Ма н Җ ӓ 'ака Йас`á \r",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5767,
      "text": "Ұа Һуұа Йа ҳ шá \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5768,
      "text": "Фа'а н та `Анһу Талаһһá \r",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5769,
      "text": "Калл ӓ 'И нн аһӓ Таҙки ра ту н \r",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5770,
      "text": "Фама н Ш ӓ 'а Ҙака ра һ у \r",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5771,
      "text": "Фӥ Ҫ ухуфи н Мукар ра мати н \r",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5772,
      "text": "Марфӱ`ати н Му ҭ аһһа ра ти н \r",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5773,
      "text": "Би'айдӥ Сафа ра ти н \r",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5774,
      "text": "Ки р ӓ ми н Ба ра ра ти н \r",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5775,
      "text": "Қ утила А л-'И н с ӓ ну М ӓ 'Акфа ра һ у \r",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5776,
      "text": "Мин 'А й йи Ша й 'ин Ҳ ала қ аһ у \r",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5777,
      "text": "Ми н Ну ҭ фатин Ҳ ала қ аһ у Фа қ адда ра һ у \r",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5778,
      "text": "Ćу мм а А с-Саб ӥ ла Йасса ра һ у \r",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5779,
      "text": "Ćу мм а 'Амӓтаһ у Фа'а қ ба ра һ у \r",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5780,
      "text": "Ćу мм а 'Иҙӓ Ш ӓ 'а 'А н ша ра һ у \r",
      "numberInSurah": 22,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5781,
      "text": "Каллӓ Ла мм ӓ Йа қ đи М ӓ 'Ама ра һ у \r",
      "numberInSurah": 23,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5782,
      "text": "Фалйа н žу р и А л-'И н с ӓ ну 'Илá Ҭ а`ӓмиһ и \r",
      "numberInSurah": 24,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5783,
      "text": "'А нн ӓ Ҫ аба б нӓ А л-М ӓ 'а Ҫ аббӓа н \r",
      "numberInSurah": 25,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5784,
      "text": "Ćу мм а Ша қ а қ нӓ А л-'Арđа Ша қ қ ӓа н \r",
      "numberInSurah": 26,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5785,
      "text": "Фа'а н батнӓ Фӥһӓ Хаббӓа н \r",
      "numberInSurah": 27,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5786,
      "text": "Ұа `Инабӓа н Ұа Қ аđбӓа н \r",
      "numberInSurah": 28,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5787,
      "text": "Ұа Зайтӱнӓа н Ұа На ҳ лӓа н \r",
      "numberInSurah": 29,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5788,
      "text": "Ұа Хад ӓ 'и қ а Ғ улбӓа н \r",
      "numberInSurah": 30,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5789,
      "text": "Ұа Фӓкиһата н Ұа 'Аббӓа н \r",
      "numberInSurah": 31,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5790,
      "text": "Матӓ`ӓа н Лаку м Ұа Ли'ан`ӓмику м \r",
      "numberInSurah": 32,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5791,
      "text": "Фа'иҙӓ Җ ӓ 'ати А ҫ - Ҫ ӓ ҳ ҳ ату \r",
      "numberInSurah": 33,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5792,
      "text": "Йа ұ ма Йафи р ру А л-Мар'у Мин 'А ҳ ӥ һи \r",
      "numberInSurah": 34,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5793,
      "text": "Ұа 'У мм иһ и Ұа 'Аб ӥ һи \r",
      "numberInSurah": 35,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5794,
      "text": "Ұа Ҫ ӓхибатиһ и Ұа Бан ӥ һи \r",
      "numberInSurah": 36,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5795,
      "text": "Ликулли А м р и'и н Минһу м Йаұма'иҙи н Ша'ну н Йу ғ н ӥ һи \r",
      "numberInSurah": 37,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5796,
      "text": "Ұуҗ ӱ һу н Йаұма'иҙи н Мусфи ра ту н \r",
      "numberInSurah": 38,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5797,
      "text": "Đ ӓхикату н Муста б ши ра ту н \r",
      "numberInSurah": 39,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5798,
      "text": "Ұа Ұуҗ ӱ һу н Йаұма'иҙин `Алайһӓ Ғ аба ра ту н \r",
      "numberInSurah": 40,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5799,
      "text": "Тарһа қ уһӓ Қ ата ра ту н \r",
      "numberInSurah": 41,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5800,
      "text": "'Ӱл ӓ 'ика Һуму А л-Кафа ра ту А л-Фаҗа ра ту \r",
      "numberInSurah": 42,
      "juz": 30,
      "manzil": 7,
      "page": 585,
      "ruku": 522,
      "hizbQuarter": 234,
      "sajda": false
    }
  ],
