{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "englishNameTranslation": "The Emissaries",
  "revelationType": "Meccan",
  "numberOfAyahs": 50,
  "ayahs": [
    {
      "number": 5623,
      "text": "Ұа А л-Мурсал ӓ ти `Урфӓа н \r",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5624,
      "text": "Фӓл`ӓ ҫ иф ӓ ти `А ҫ фӓа н \r",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5625,
      "text": "Ұа А н -Нӓши р ӓ ти Наш рӓ а н \r",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5626,
      "text": "Фӓлфӓ р и қ ӓ ти Фар қ ӓа н \r",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5627,
      "text": "Фӓлмул қ ий ӓ ти Ҙик рӓ а н \r",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5628,
      "text": "`Уҙ рӓ ан 'Аұ Нуҙ рӓ а н \r",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5629,
      "text": "'И нн амӓ Тӱ`ад ӱ на Лаұӓ қ и`у н \r",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5630,
      "text": "Фа'иҙӓ А н -Нуҗ ӱ му Ҭ умисат \r",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5631,
      "text": "Ұа 'Иҙӓ А с-Сам ӓ 'у Фу р и җ ат \r",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5632,
      "text": "Ұа 'Иҙӓ А л-Җиб ӓ лу Нусифат \r",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5633,
      "text": "Ұа 'Иҙӓ А р- Ру сулу 'У қ қ итат \r",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5634,
      "text": "Л'а й йи Йа ұ мин 'Уҗҗилат \r",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5635,
      "text": "Лийа ұ ми А л-Фа ҫ ли \r",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5636,
      "text": "Ұа М ӓ 'А д р ӓ ка Мӓ Йа ұ му А л-Фа ҫ ли \r",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5637,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5638,
      "text": "'Ала м Нуһлики А л-'Аұұал ӥ на \r",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5639,
      "text": "Ćу мм а Нутби`уһуму А л-'Ӓ ҳ и р ӥ на \r",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5640,
      "text": "Каҙӓлика Наф`алу Бил-Му җ р им ӥ на \r",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5641,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5642,
      "text": "'Ала м На ҳ лу қ ку м Ми н М ӓ 'и н Маһ ӥ ни н \r",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5643,
      "text": "Фаҗа`алн ӓ һу Фӥ Қ а р ӓ р и н Мак ӥ ни н \r",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5644,
      "text": "'Илá Қ ада р и н Ма`л ӱ ми н \r",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5645,
      "text": "Фа қ адарнӓ Фани`ма А л- Қ ӓдир ӱ на \r",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5646,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5647,
      "text": "'Ала м На җ `али А л-'Арđа Кифӓтӓа н \r",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5648,
      "text": "'Ахй ӓ 'а н Ұа 'А м ұӓтӓа н \r",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5649,
      "text": "Ұа Җа`алнӓ Фӥһӓ Ра ұӓсийа Шӓми ҳ ӓ ти н Ұа 'Ас қ айнӓку м М ӓ 'а н Фу рӓ тӓа н \r",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5650,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5651,
      "text": "А н ҭ али қ ӱ 'Илá Мӓ Ку н ту м Биһ и Тукаҙҙиб ӱ на \r",
      "numberInSurah": 29,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5652,
      "text": "А н ҭ али қ ӱ 'Илá Ž илли н Ҙӥ Ćалӓćи Шу`а б и н \r",
      "numberInSurah": 30,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5653,
      "text": "Лӓ Ž ал ӥ ли н Ұа Лӓ Йу ғ нӥ Мина А лл ӓһаби \r",
      "numberInSurah": 31,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5654,
      "text": "'И нн аһӓ Тармӥ Биша ра р и н Кӓл қ а ҫ р и \r",
      "numberInSurah": 32,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5655,
      "text": "Ка'а нн аһ у Җимӓлату н Ҫ уф ру н \r",
      "numberInSurah": 33,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5656,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 34,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5657,
      "text": "Һӓҙӓ Йа ұ му Лӓ Йа н ҭ и қ ӱ на \r",
      "numberInSurah": 35,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5658,
      "text": "Ұа Лӓ Йу'уҙану Лаһу м Файа`таҙир ӱ на \r",
      "numberInSurah": 36,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5659,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 37,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5660,
      "text": "Һӓҙӓ Йа ұ му А л-Фа ҫ ли ۖ Җама`нӓку м Ұа А л-'Аұұал ӥ на \r",
      "numberInSurah": 38,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5661,
      "text": "Фа'и н К ӓ на Лаку м Ка й ду н Факӥд ӱ ни \r",
      "numberInSurah": 39,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5662,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 40,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5663,
      "text": "'И нн а А л-Мутта қ ӥ на Фӥ Ž ил ӓ ли н Ұа `Уй ӱ ни н \r",
      "numberInSurah": 41,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5664,
      "text": "Ұа Фаұӓкиһа Ми мм ӓ Йаштаһ ӱ на \r",
      "numberInSurah": 42,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5665,
      "text": "Кулӱ Ұа А ш ра бӱ Һан ӥ ' ӓ а н Бимӓ Ку н ту м Та`мал ӱ на \r",
      "numberInSurah": 43,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5666,
      "text": "'И нн ӓ Каҙӓлика На җ зӥ А л-Мухсин ӥ на \r",
      "numberInSurah": 44,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5667,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 45,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5668,
      "text": "Кулӱ Ұа Таматта`ӱ Қ алӥлӓан 'И нн аку м Му җ р им ӱ на \r",
      "numberInSurah": 46,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5669,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 47,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5670,
      "text": "Ұа 'Иҙӓ Қ ӥ ла Лаһуму А рка`ӱ Лӓ Йарка` ӱ на \r",
      "numberInSurah": 48,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5671,
      "text": "Ұа й лу н Йаұма'иҙи н Лилмукаҙҙиб ӥ на \r",
      "numberInSurah": 49,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5672,
      "text": "Фаби'а й йи Хадӥćи н Ба`даһ у Йу'умин ӱ на \r",
      "numberInSurah": 50,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    }
  ],
