{
  "number": 92,
  "name": "سُورَةُ اللَّيۡلِ",
  "englishName": "Al-Lail",
  "englishNameTranslation": "The Night",
  "revelationType": "Meccan",
  "numberOfAyahs": 21,
  "ayahs": [
    {
      "number": 6059,
      "text": "Ұа А л-Ла й ли 'Иҙӓ Йа ғ шá \r",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6060,
      "text": "Ұа А н -Наһ ӓ р и 'Иҙӓ Таҗаллá \r",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6061,
      "text": "Ұа Мӓ Ҳ ала қ а А ҙ-Ҙака ра Ұа А л-'У н ćá \r",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6062,
      "text": "'И нн а Са`йаку м Лашаттá \r",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6063,
      "text": "Фа'а мм ӓ Ман 'А` ҭ á Ұа А тта қ á \r",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6064,
      "text": "Ұа Ҫ адда қ а Бил-Хуснá \r",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6065,
      "text": "Фасануйасси ру һ у Лилйусрá \r",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6066,
      "text": "Ұа 'А мм ӓ Ма н Ба ҳ ила Ұа А ста ғ нá \r",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6067,
      "text": "Ұа Каҙҙаба Бил-Хуснá \r",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6068,
      "text": "Фасануйасси ру һ у Лил`усрá \r",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6069,
      "text": "Ұа Мӓ Йу ғ нӥ `Анһу Мӓлуһ у~ 'Иҙӓ Та ра ддá \r",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6070,
      "text": "'И нн а `Алайнӓ Лалһудá \r",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6071,
      "text": "Ұа 'И нн а Ланӓ Лал' ӓ ҳ и ра та Ұа А л-'Ӱлá \r",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6072,
      "text": "Фа'а н ҙартуку м Нӓ рӓ а н Талаžžá \r",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6073,
      "text": "Лӓ Йа ҫ лӓһ ӓ 'Иллӓ А л-'Аш қ á \r",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6074,
      "text": "Ал-Лаҙӥ Каҙҙаба Ұа Таұаллá \r",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6075,
      "text": "Ұа Сайуҗа нн абуһӓ А л-'Ат қ á \r",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6076,
      "text": "Ал-Лаҙӥ Йу'утӥ Мӓлаһ у Йатазаккá \r",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6077,
      "text": "Ұа Мӓ Ли'хадин `И н даһ у Ми н Ни`мати н Ту җ зá \r",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6078,
      "text": "'Иллӓ А б ти ғ ӓ 'а Ұа җ һи Ра ббиһи А л-'А`лá \r",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6079,
      "text": "Ұа Ласа ұ фа Йарđá \r",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    }
  ],
