{
  "number": 89,
  "name": "سُورَةُ الفَجۡرِ",
  "englishName": "Al-Fajr",
  "ayahs": [
    {
      "number": 1,
      "text": "فجر کی قسم",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور دس راتوں کی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور جفت اور طاق کی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور رات کی جب جانے لگے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور بے شک یہ چیزیں عقلمندوں کے نزدیک قسم کھانے کے لائق ہیں کہ (کافروں کو ضرور عذاب ہو گا)",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "کیا تم نے نہیں دیکھا کہ تمہارے پروردگار نے عاد کے ساتھ کیا کیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "(جو) ارم (کہلاتے تھے اتنے) دراز قد",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کہ تمام ملک میں ایسے پیدا نہیں ہوئے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور ثمود کے ساتھ (کیا کیا) جو وادئِ (قریٰ) میں پتھر تراشتے تھے (اور گھر بناتے) تھے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور فرعون کے ساتھ (کیا کیا) جو خیمے اور میخیں رکھتا تھا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یہ لوگ ملکوں میں سرکش ہو رہے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور ان میں بہت سی خرابیاں کرتے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "تو تمہارے پروردگار نے ان پر عذاب کا کوڑا نازل کیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "بے شک تمہارا پروردگار تاک میں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "مگر انسان (عجیب مخلوق ہے کہ) جب اس کا پروردگار اس کو آزماتا ہے تو اسے عزت دیتا اور نعمت بخشتا ہے۔ تو کہتا ہے کہ (آہا) میرے پروردگار نے مجھے عزت بخشی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور جب (دوسری طرح) آزماتا ہے کہ اس پر روزی تنگ کر دیتا ہے تو کہتا ہے کہ (ہائے) میرے پروردگار نے مجھے ذلیل کیا",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "نہیں بلکہ تم لوگ یتیم کی خاطر نہیں کرتے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اور نہ مسکین کو کھانا کھلانے کی ترغیب دیتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور میراث کے مال سمیٹ کر کھا جاتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور مال کو بہت ہی عزیز رکھتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "تو جب زمین کی بلندی کوٹ کوٹ کو پست کر دی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اور تمہارا پروردگار (جلوہ فرما ہو گا) اور فرشتے قطار باندھ باندھ کر آ موجود ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اور دوزخ اس دن حاضر کی جائے گی تو انسان اس دن متنبہ ہو گا مگر تنبہ (سے) اسے (فائدہ) کہاں (مل سکے گا)",
      "juz": 30,
      "manzil": 7,
      "page": 593,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "کہے گا کاش میں نے اپنی زندگی (جاودانی کے لیے) کچھ آگے بھیجا ہوتا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تو اس دن نہ کوئی خدا کے عذاب کی طرح کا (کسی کو) عذاب دے گا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "اور نہ کوئی ویسا جکڑنا جکڑے گا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اے اطمینان پانے والی روح!",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اپنے پروردگار کی طرف لوٹ چل۔ تو اس سے راضی وہ تجھ سے راضی",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 29,
      "text": "تو میرے (ممتاز) بندوں میں شامل ہو جا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اور میری بہشت میں داخل ہو جا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 531,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}