{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "ayahs": [
    {
      "number": 1,
      "text": "(اے محمدﷺ) اپنے پروردگار کا نام لے کر پڑھو جس نے (عالم کو) پیدا کیا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "جس نے انسان کو خون کی پھٹکی سے بنایا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "پڑھو اور تمہارا پروردگار بڑا کریم ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "جس نے قلم کے ذریعے سے علم سکھایا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور انسان کو وہ باتیں سکھائیں جس کا اس کو علم نہ تھا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "مگر انسان سرکش ہو جاتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "جب کہ اپنے تیئں غنی دیکھتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کچھ شک نہیں کہ (اس کو) تمہارے پروردگار ہی کی طرف لوٹ کر جانا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "بھلا تم نے اس شخص کو دیکھا جو منع کرتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 10,
      "text": "(یعنی) ایک بندے کو جب وہ نماز پڑھنے لگتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 11,
      "text": "بھلا دیکھو تو اگر یہ راہِ راست پر ہو",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 12,
      "text": "یا پرہیز گاری کا حکم کرے (تو منع کرنا کیسا)",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اور دیکھو تو اگر اس نے دین حق کو جھٹلایا اور اس سے منہ موڑا (تو کیا ہوا)",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 14,
      "text": "کیااس کو معلوم نہیں کہ خدا دیکھ رہا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 15,
      "text": "دیکھو اگر وہ باز نہ آئے گا تو ہم (اس کی) پیشانی کے بال پکڑ گھسیٹیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 16,
      "text": "یعنی اس جھوٹے خطاکار کی پیشانی کے بال",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 17,
      "text": "تو وہ اپنے یاروں کی مجلس کو بلالے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ہم بھی اپنے موکلانِ دوزخ کو بلا لیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 19,
      "text": "دیکھو اس کا کہا نہ ماننا اور قربِ (خدا) حاصل کرتے رہنا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
        "id": 15,
        "recommended": false,
        "obligatory": true
      }
    }
  ]
}