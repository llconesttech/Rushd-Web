{
  "number": 1,
  "name": "سُورَةُ ٱلْفَاتِحَةِ",
  "englishName": "Al-Faatiha",
  "ayahs": [
    {
      "number": 1,
      "text": "شروع الله کا نام لے کر جو بڑا مہربان نہایت رحم والا ہے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 2,
      "text": "سب طرح کی تعریف خدا ہی کو (سزاوار) ہے جو تمام مخلوقات کا پروردگار ہے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 3,
      "text": "بڑا مہربان نہایت رحم والا",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 4,
      "text": "انصاف کے دن کا حاکم",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 5,
      "text": "(اے پروردگار) ہم تیری ہی عبادت کرتے ہیں اور تجھ ہی سے مدد مانگتے ہیں",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ہم کو سیدھے رستے چلا",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 7,
      "text": "ان لوگوں کے رستے جن پر تو اپنا فضل وکرم کرتا رہا نہ ان کے جن پر غصے ہوتا رہا اور نہ گمراہوں کے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    }
  ]
}