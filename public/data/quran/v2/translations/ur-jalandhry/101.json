{
  "number": 101,
  "name": "سُورَةُ القَارِعَةِ",
  "englishName": "Al-Qaari'a",
  "ayahs": [
    {
      "number": 1,
      "text": "کھڑ کھڑانے والی",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "کھڑ کھڑانے والی کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور تم کیا جانوں کھڑ کھڑانے والی کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "(وہ قیامت ہے) جس دن لوگ ایسے ہوں گے جیسے بکھرے ہوئے پتنگے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور پہاڑ ایسے ہو جائیں گے جیسے دھنکی ہوئی رنگ برنگ کی اون",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تو جس کے (اعمال کے) وزن بھاری نکلیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وہ دل پسند عیش میں ہو گا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور جس کے وزن ہلکے نکلیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اس کا مرجع ہاویہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور تم کیا سمجھے کہ ہاویہ کیا چیز ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "دہکتی ہوئی آگ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}