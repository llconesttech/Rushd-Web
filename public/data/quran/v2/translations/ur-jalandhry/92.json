{
  "number": 92,
  "name": "سُورَةُ اللَّيۡلِ",
  "englishName": "Al-Lail",
  "ayahs": [
    {
      "number": 1,
      "text": "رات کی قسم جب (دن کو) چھپالے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور دن کی قسم جب چمک اٹھے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور اس (ذات) کی قسم جس نے نر اور مادہ پیدا کیے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کہ تم لوگوں کی کوششں طرح طرح کی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تو جس نے (خدا کے رستے میں مال) دیا اور پرہیز گاری کی",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اور نیک بات کو سچ جانا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اس کو ہم آسان طریقے کی توفیق دیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور جس نے بخل کیا اور بےپروا بنا رہا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور نیک بات کو جھوٹ سمجھا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اسے سختی میں پہنچائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور جب وہ (دوزخ کے گڑھے میں) گرے گا تو اس کا مال اس کے کچھ کام نہ آئے گا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "ہمیں تو راہ دکھا دینا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اور آخرت او ردنیا ہماری ہی چیزیں ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "سو میں نے تم کو بھڑکتی آگ سے متنبہ کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 595,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اس میں وہی داخل ہو گا جو بڑا بدبخت ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 16,
      "text": "جس نے جھٹلایا اور منہ پھیرا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اور جو بڑا پرہیزگار ہے وہ (اس سے) بچا لیا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 18,
      "text": "جو مال دیتا ہے تاکہ پاک ہو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور (اس لیے) نہیں (دیتا کہ) اس پر کسی کا احسان (ہے) جس کا وہ بدلہ اتارتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 20,
      "text": "بلکہ اپنے خداوند اعلیٰ کی رضامندی حاصل کرنے کے لیے دیتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اور وہ عنقریب خوش ہو جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 534,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}