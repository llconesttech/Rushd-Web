{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "ayahs": [
    {
      "number": 1,
      "text": "بھلا تم کو ڈھانپ لینے والی (یعنی قیامت کا) حال معلوم ہوا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اس روز بہت سے منہ (والے) ذلیل ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "سخت محنت کرنے والے تھکے ماندے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "دہکتی آگ میں داخل ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ایک کھولتے ہوئے چشمے کا ان کو پانی پلایا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اور خار دار جھاڑ کے سوا ان کے لیے کوئی کھانا نہیں (ہو گا)",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "جو نہ فربہی لائے اور نہ بھوک میں کچھ کام آئے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور بہت سے منہ (والے) اس روز شادماں ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اپنے اعمال (کی جزا) سے خوش دل",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "بہشت بریں میں",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وہاں کسی طرح کی بکواس نہیں سنیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اس میں چشمے بہ رہے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وہاں تخت ہوں گے اونچے بچھے ہوئے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور آبخورے (قرینے سے) رکھے ہوئے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اور گاؤ تکیے قطار کی قطار لگے ہوئے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور نفیس مسندیں بچھی ہوئی",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "یہ لوگ اونٹوں کی طرف نہیں دیکھتے کہ کیسے (عجیب) پیدا کیے گئے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اور آسمان کی طرف کہ کیسا بلند کیا گیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور پہاڑوں کی طرف کہ کس طرح کھڑے کیے گئے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور زمین کی طرف کہ کس طرح بچھائی گئی",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "تو تم نصیحت کرتے رہو کہ تم نصیحت کرنے والے ہی ہو",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "تم ان پر داروغہ نہیں ہو",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "ہاں جس نے منہ پھیرا اور نہ مانا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "تو خدا اس کو بڑا عذاب دے گا",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "بےشک ان کو ہمارے پاس لوٹ کر آنا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "پھر ہم ہی کو ان سے حساب لینا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}