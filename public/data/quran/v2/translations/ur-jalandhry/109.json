{
  "number": 109,
  "name": "سُورَةُ الكَافِرُونَ",
  "englishName": "Al-Kaafiroon",
  "ayahs": [
    {
      "number": 1,
      "text": "(اے پیغمبر ان منکران اسلام سے) کہہ دو کہ اے کافرو!",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "جن (بتوں) کو تم پوچتے ہو ان کو میں نہیں پوجتا",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور جس (خدا) کی میں عبادت کرتا ہوں اس کی تم عبادت نہیں کرتے",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور (میں پھر کہتا ہوں کہ) جن کی تم پرستش کرتے ہوں ان کی میں پرستش کرنے والا نہیں ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور نہ تم اس کی بندگی کرنے والے (معلوم ہوتے) ہو جس کی میں بندگی کرتا ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تم اپنے دین پر میں اپنے دین پر",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}