{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "ayahs": [
    {
      "number": 1,
      "text": "آفتاب کی روشنی کی قسم",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور رات (کی تاریکی) کی جب چھا جائے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "کہ (اے محمدﷺ) تمہارے پروردگار نے نہ تو تم کو چھوڑا اور نہ (تم سے) ناراض ہوا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور آخرت تمہارے لیے پہلی (حالت یعنی دنیا) سے کہیں بہتر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور تمہیں پروردگار عنقریب وہ کچھ عطا فرمائے گا کہ تم خوش ہو جاؤ گے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بھلا اس نے تمہیں یتیم پا کر جگہ نہیں دی؟ (بےشک دی)",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور رستے سے ناواقف دیکھا تو رستہ دکھایا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور تنگ دست پایا تو غنی کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "تو تم بھی یتیم پر ستم نہ کرنا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور مانگنے والے کو جھڑکی نہ دینا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور اپنے پروردگار کی نعمتوں کا بیان کرتے رہنا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}