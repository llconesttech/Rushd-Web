{
  "number": 95,
  "name": "سُورَةُ التِّينِ",
  "englishName": "At-Tin",
  "ayahs": [
    {
      "number": 1,
      "text": "انجیر کی قسم اور زیتون کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور طور سینین کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور اس امن والے شہر کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کہ ہم نے انسان کو بہت اچھی صورت میں پیدا کیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر (رفتہ رفتہ) اس (کی حالت) کو (بدل کر) پست سے پست کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "مگر جو لوگ ایمان لائے اور نیک عمل کرتے رہے انکے لیے بےانتہا اجر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تو (اے آدم زاد) پھر تو جزا کے دن کو کیوں جھٹلاتا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کیا خدا سب سے بڑا حاکم نہیں ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}