{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "ayahs": [
    {
      "number": 1,
      "text": "آسمان اور رات کے وقت آنے والے کی قسم",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تم کو کیا معلوم کہ رات کے وقت آنے والا کیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وہ تارا ہے چمکنے والا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کہ کوئی متنفس نہیں جس پر نگہبان مقرر نہیں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تو انسان کو دیکھنا چاہئے کہ وہ کاہے سے پیدا ہوا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وہ اچھلتے ہوئے پانی سے پیدا ہوا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "جو پیٹھ اور سینے کے بیچ میں سے نکلتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "بےشک خدا اس کے اعادے (یعنی پھر پیدا کرنے) پر قادر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "جس دن دلوں کے بھید جانچے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "تو انسان کی کچھ پیش نہ چل سکے گی اور نہ کوئی اس کا مددگار ہو گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "آسمان کی قسم جو مینہ برساتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور زمین کی قسم جو پھٹ جاتی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "کہ یہ کلام (حق کو باطل سے) جدا کرنے والا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور بیہودہ بات نہیں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "یہ لوگ تو اپنی تدبیروں میں لگ رہے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور ہم اپنی تدبیر کر رہے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "تو تم کافروں کو مہلت دو بس چند روز ہی مہلت دو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}