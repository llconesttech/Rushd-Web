{
  "number": 100,
  "name": "سُورَةُ العَادِيَاتِ",
  "englishName": "Al-Aadiyaat",
  "ayahs": [
    {
      "number": 1,
      "text": "ان سرپٹ دوڑنے والے گھوڑوں کی قسم جو ہانپ اٹھتےہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "پھر (پتھروں پر نعل) مار کر آگ نکالتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "پھر صبح کو چھاپہ مارتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر اس میں گرد اٹھاتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر اس وقت دشمن کی فوج میں جا گھستے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "کہ انسان اپنے پروردگار کا احسان ناشناس (اور ناشکرا) ہے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور وہ اس سے آگاہ بھی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "وہ تو مال سے سخت محبت کرنے والا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "کیا وہ اس وقت کو نہیں جانتا کہ جو (مردے) قبروں میں ہیں وہ باہر نکال لیے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور جو (بھید) دلوں میں ہیں وہ ظاہر کر دیئے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "بےشک ان کا پروردگار اس روز ان سے خوب واقف ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}