{
  "number": 55,
  "name": "سُورَةُ الرَّحۡمَٰن",
  "englishName": "Ar-Rahmaan",
  "ayahs": [
    {
      "number": 1,
      "text": "(خدا جو) نہایت مہربان",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اسی نے قرآن کی تعلیم فرمائی",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اسی نے انسان کو پیدا کیا",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اسی نے اس کو بولنا سکھایا",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 5,
      "text": "سورج اور چاند ایک حساب مقرر سے چل رہے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اور بوٹیاں اور درخت سجدہ کر رہے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور اسی نے آسمان کو بلند کیا اور ترازو قائم کی",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کہ ترازو (سے تولنے) میں حد سے تجاوز نہ کرو",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور انصاف کے ساتھ ٹھیک تولو۔ اور تول کم مت کرو",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور اسی نے خلقت کے لئے زمین بچھائی",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اس میں میوے اور کھجور کے درخت ہیں جن کے خوشوں پر غلاف ہوتے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور اناج جس کے ساتھ بھس ہوتا ہے اور خوشبودار پھول",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 13,
      "text": "تو (اے گروہ جن وانس) تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اسی نے انسان کو ٹھیکرے کی طرح کھنکھناتی مٹی سے بنایا",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اور جنات کو آگ کے شعلے سے پیدا کیا",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 16,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وہی دونوں مشرقوں اور دونوں مغربوں کا مالک (ہے)",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 18,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اسی نے دو دریا رواں کئے جو آپس میں ملتے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 20,
      "text": "دونوں میں ایک آڑ ہے کہ (اس سے) تجاوز نہیں کرسکتے",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 21,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 22,
      "text": "دونوں دریاؤں سے موتی اور مونگے نکلتے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 23,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اور جہاز بھی اسی کے ہیں جو دریا میں پہاڑوں کی طرح اونچے کھڑے ہوتے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 26,
      "text": "جو (مخلوق) زمین پر ہے سب کو فنا ہونا ہے",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اور تمہارے پروردگار ہی کی ذات (بابرکات) جو صاحب جلال وعظمت ہے باقی رہے گی",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 28,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 29,
      "text": "آسمان اور زمین میں جتنے لوگ ہیں سب اسی سے مانگتے ہیں۔ وہ ہر روز کام میں مصروف رہتا ہے",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 30,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اے دونوں جماعتو! ہم عنقریب تمہاری طرف متوجہ ہوتے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 32,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اے گروہِ جن وانس اگر تمہیں قدرت ہو کہ آسمان اور زمین کے کناروں سے نکل جاؤ تو نکل جاؤ۔ اور زور کے سوا تم نکل سکنے ہی کے نہیں",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 34,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 35,
      "text": "تم پر آگ کے شعلے اور دھواں چھوڑ دیا جائے گا تو پھر تم مقابلہ نہ کرسکو گے",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 36,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 37,
      "text": "پھر جب آسمان پھٹ کر تیل کی تلچھٹ کی طرح گلابی ہوجائے گا (تو) وہ کیسا ہولناک دن ہوگا",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 38,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اس روز نہ تو کسی انسان سے اس کے گناہوں کے بارے میں پرسش کی جائے گی اور نہ کسی جن سے",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 40,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 41,
      "text": "گنہگار اپنے چہرے ہی سے پہچان لئے جائیں گے تو پیشانی کے بالوں اور پاؤں سے پکڑ لئے جائیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 42,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 43,
      "text": "یہی وہ جہنم ہے جسے گنہگار لوگ جھٹلاتے تھے",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 44,
      "text": "وہ دوزخ اور کھولتے ہوئے گرم پانی کے درمیان گھومتے پھریں گے",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 45,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 46,
      "text": "اور جو شخص اپنے پروردگار کے سامنے کھڑے ہونے سے ڈرا اس کے لئے دو باغ ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 47,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 48,
      "text": "ان دونوں میں بہت سی شاخیں (یعنی قسم قسم کے میووں کے درخت ہیں)",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 49,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 50,
      "text": "ان میں دو چشمے بہہ رہے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 51,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 52,
      "text": "ان میں سب میوے دو دو قسم کے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 53,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 54,
      "text": "(اہل جنت) ایسے بچھونوں پر جن کے استرا طلس کے ہیں تکیہ لگائے ہوئے ہوں گے۔ اور دونوں باغوں کے میوے قریب (جھک رہے) ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 55,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 56,
      "text": "ان میں نیچی نگاہ والی عورتیں ہیں جن کو اہل جنت سے پہلے نہ کسی انسان نے ہاتھ لگایا اور نہ کسی جن نے",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 57,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 58,
      "text": "گویا وہ یاقوت اور مرجان ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 59,
      "text": "تو تم اپنے پروردگار کی کون کون سی نعمت کو جھٹلاؤ گے؟",
      "juz": 27,
