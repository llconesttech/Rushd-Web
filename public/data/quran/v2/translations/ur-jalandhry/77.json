{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "ayahs": [
    {
      "number": 1,
      "text": "ہواؤں کی قسم جو نرم نرم چلتی ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 2,
      "text": "پھر زور پکڑ کر جھکڑ ہو جاتی ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور (بادلوں کو) پھاڑ کر پھیلا دیتی ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر ان کو پھاڑ کر جدا جدا کر دیتی ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر فرشتوں کی قسم جو وحی لاتے ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تاکہ عذر (رفع) کردیا جائے یا ڈر سنا دیا جائے",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 7,
      "text": "کہ جس بات کا تم سے وعدہ کیا جاتا ہے وہ ہو کر رہے گی",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 8,
      "text": "جب تاروں کی چمک جاتی رہے",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور جب آسمان پھٹ جائے",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور جب پہاڑ اُڑے اُڑے پھریں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور جب پیغمبر فراہم کئے جائیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 12,
      "text": "بھلا (ان امور میں) تاخیر کس دن کے لئے کی گئی؟",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 13,
      "text": "فیصلے کے دن کے لئے",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور تمہیں کیا خبر کہ فیصلے کا دن کیا ہے؟",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اس دن جھٹلانے والوں کے لئے خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 16,
      "text": "کیا ہم نے پہلے لوگوں کو ہلاک نہیں کر ڈالا",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 17,
      "text": "پھر ان پچھلوں کو بھی ان کے پیچھے بھیج دیتے ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ہم گنہگاروں کے ساتھ ایسا ہی کیا کرتے ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 20,
      "text": "کیا ہم نے تم کو حقیر پانی سے نہیں پیدا کیا؟",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اس کو ایک محفوظ جگہ میں رکھا",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 22,
      "text": "ایک وقت معین تک",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 23,
      "text": "پھر اندازہ مقرر کیا اور ہم کیا ہی خوب اندازہ مقرر کرنے والے ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 25,
      "text": "کیا ہم نے زمین کو سمیٹنے والی نہیں بنایا",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 26,
      "text": "یعنی) زندوں اور مردوں کو",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 27,
      "text": "(بنایا) اور اس پر اونچے اونچے پہاڑ رکھ دیئے اور تم لوگوں کو میٹھا پانی پلایا",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 29,
      "text": "جس چیز کو تم جھٹلایا کرتے تھے۔ (اب) اس کی طرف چلو",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 30,
      "text": "(یعنی) اس سائے کی طرف چلو جس کی تین شاخیں ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 31,
      "text": "نہ ٹھنڈی چھاؤں اور نہ لپٹ سے بچاؤ",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اس سے (آگ کی اتنی اتنی بڑی) چنگاریاں اُڑتی ہیں جیسے محل",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 33,
      "text": "گویا زرد رنگ کے اونٹ ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 35,
      "text": "یہ وہ دن ہے کہ (لوگ) لب تک نہ ہلا سکیں گے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اور نہ ان کو اجازت دی جائے گی کہ عذر کرسکیں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 37,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 38,
      "text": "یہی فیصلے کا دن ہے (جس میں) ہم نے تم کو اور پہلے لوگوں کو جمع کیا ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اگر تم کو کوئی داؤں آتا ہو تو مجھ سے کر لو",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 41,
      "text": "بےشک پرہیزگار سایوں اور چشموں میں ہوں گے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 42,
      "text": "اور میؤوں میں جو ان کو مرغوب ہوں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اور جو عمل تم کرتے رہے تھے ان کے بدلے میں مزے سے کھاؤ اور پیو",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 44,
      "text": "ہم نیکو کاروں کو ایسا ہی بدلہ دیا کرتے ہیں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 45,
      "text": "اس دن جھٹلانے والوں کی خرابی ہوگی",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 46,
      "text": "(اے جھٹلانے والو!) تم کسی قدر کھا لو اور فائدے اُٹھا لو تم بےشک گنہگار ہو",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 47,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 48,
      "text": "اور جب ان سے کہا جاتا ہے کہ (خدا کے آگے) جھکو تو جھکتے نہیں",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 49,
      "text": "اس دن جھٹلانے والوں کی خرابی ہے",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 50,
      "text": "اب اس کے بعد یہ کون سی بات پر ایمان لائیں گے؟",
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    }
  ]
}