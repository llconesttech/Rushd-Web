{
  "number": 78,
  "name": "سُورَةُ النَّبَإِ",
  "englishName": "An-Naba",
  "ayahs": [
    {
      "number": 1,
      "text": "(یہ) لوگ کس چیز کی نسبت پوچھتے ہیں؟",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "(کیا) بڑی خبر کی نسبت؟",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "جس میں یہ اختلاف کر رہے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "دیکھو یہ عنقریب جان لیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر دیکھو یہ عنقریب جان لیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "کیا ہم نے زمین کو بچھونا نہیں بنایا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور پہاڑوں کو (ا س کی) میخیں (نہیں ٹھہرایا؟)",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "(بے شک بنایا) اور تم کو جوڑا جوڑابھی پیدا کیا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور نیند کو تمہارے لیے (موجب) آرام بنایا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور رات کو پردہ مقرر کیا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور دن کو معاش (کا وقت) قرار دیا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور تمہارے اوپر سات مضبوط (آسمان) بنائے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اور (آفتاب کا) روشن چراغ بنایا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور نچڑتے بادلوں سے موسلا دھار مینہ برسایا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "تاکہ اس سے اناج اور سبزہ پیدا کریں",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور گھنے گھنے باغ",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "بےشک فیصلہ کا دن مقرر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "جس دن صور پھونکا جائے گا تو تم لوگ غٹ کے غٹ آ موجود ہو گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور آسمان کھولا جائے گا تو (اس میں) دروازے ہو جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور پہاڑ چلائے جائیں گے تو وہ ریت ہو کر رہ جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "بےشک دوزخ گھات میں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "(یعنی) سرکشوں کا وہی ٹھکانہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اس میں وہ مدتوں پڑے رہیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "وہاں نہ ٹھنڈک کا مزہ چکھیں گے۔ نہ (کچھ) پینا (نصیب ہو گا)",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "مگر گرم پانی اور بہتی پیپ",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "(یہ) بدلہ ہے پورا پورا",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "یہ لوگ حساب (آخرت) کی امید ہی نہیں رکھتے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اور ہماری آیتوں کو جھوٹ سمجھ کر جھٹلاتے رہتے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اور ہم نے ہر چیز کو لکھ کر ضبط کر رکھا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "سو (اب) مزہ چکھو۔ ہم تم پر عذاب ہی بڑھاتے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 582,
      "ruku": 518,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "بے شک پرہیز گاروں کے لیے کامیابی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "(یعنی) باغ اور انگور",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "اور ہم عمر نوجوان عورتیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اور شراب کے چھلکتے ہوئے گلاس",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "وہاں نہ بیہودہ بات سنیں گے نہ جھوٹ (خرافات)",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "یہ تمہارے پروردگار کی طرف سے صلہ ہے انعام کثیر",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "وہ جو آسمانوں اور زمین اور جو ان دونوں میں ہے سب کا مالک ہے بڑا مہربان کسی کو اس سے بات کرنے کا یارا نہیں ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "جس دن روح (الامین) اور فرشتے صف باندھ کر کھڑے ہوں گے تو کوئی بول نہ سکے گا مگر جس کو (خدائے رحمٰن) اجازت بخشے اور اس نے بات بھی درست کہی ہو",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "یہ دن برحق ہے۔ پس جو شخص چاہے اپنے پروردگار کے پاس ٹھکانہ بنا ئے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "ہم نے تم کو عذاب سے جو عنقریب آنے والا ہے آگاہ کر دیا ہے جس دن ہر شخص ان (اعمال) کو جو اس نے آگے بھیجے ہوں گے دیکھ لے گا اور کافر کہے گا کہ اے کاش میں مٹی ہوتا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 519,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}