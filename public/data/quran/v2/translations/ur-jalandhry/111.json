{
  "number": 111,
  "name": "سُورَةُ المَسَدِ",
  "englishName": "Al-Masad",
  "ayahs": [
    {
      "number": 1,
      "text": "ابولہب کے ہاتھ ٹوٹیں اور وہ ہلاک ہو",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "نہ تو اس کا مال ہی اس کے کچھ کام آیا اور نہ وہ جو اس نے کمایا",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "وہ جلد بھڑکتی ہوئی آگ میں داخل ہو گا",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور اس کی جورو بھی جو ایندھن سر پر اٹھائے پھرتی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اس کے گلے میں مونج کی رسّی ہو گی",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}