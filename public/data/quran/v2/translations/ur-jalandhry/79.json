{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "ayahs": [
    {
      "number": 1,
      "text": "ان (فرشتوں) کی قسم جو ڈوب کر کھینچ لیتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور ان کی جو آسانی سے کھول دیتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور ان کی جو تیرتے پھرتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر لپک کر آگے بڑھتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر (دنیا کے) کاموں کا انتظام کرتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "(کہ وہ دن آ کر رہے گا) جس دن زمین کو بھونچال آئے گا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "پھر اس کے پیچھے اور (بھونچال) آئے گا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اس دن (لوگوں) کے دل خائف ہو رہے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور آنکھیں جھکی ہوئی",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "(کافر) کہتے ہیں کیا ہم الٹے پاؤں پھر لوٹ جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "بھلا جب ہم کھوکھلی ہڈیاں ہو جائیں گے (تو پھر زندہ کئے جائیں گے)",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "کہتے ہیں کہ یہ لوٹنا تو (موجب) زیاں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وہ تو صرف ایک ڈانٹ ہوگی",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اس وقت وہ (سب) میدان (حشر) میں آ جمع ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "بھلا تم کو موسیٰ کی حکایت پہنچی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "جب اُن کے پروردگار نے ان کو پاک میدان (یعنی) طویٰ میں پکارا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "(اور حکم دیا) کہ فرعون کے پاس جاؤ وہ سرکش ہو رہا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اور (اس سے) کہو کہ کیا تو چاہتا ہے کہ پاک ہو جائے؟",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور میں تجھے تیرے پروردگار کا رستہ بتاؤں تاکہ تجھ کو خوف (پیدا) ہو",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "غرض انہوں نے اس کو بڑی نشانی دکھائی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "مگر اس نے جھٹلایا اور نہ مانا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "پھر لوٹ گیا اور تدبیریں کرنے لگا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اور (لوگوں کو) اکٹھا کیا اور پکارا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "کہنے لگا کہ تمہارا سب سے بڑا مالک میں ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تو خدا نے اس کو دنیا اور آخرت (دونوں) کے عذاب میں پکڑ لیا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "جو شخص (خدا سے) ڈر رکھتا ہے اس کے لیے اس (قصے) میں عبرت ہے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "بھلا تمہارا بنانا آسان ہے یا آسمان کا؟ اسی نے اس کو بنایا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اس کی چھت کو اونچا کیا اور پھر اسے برابر کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اور اسی نے رات کو تاریک بنایا اور (دن کو) دھوپ نکالی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اور اس کے بعد زمین کو پھیلا دیا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اسی نے اس میں سے اس کا پانی نکالا اور چارا اگایا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اور اس پر پہاڑوں کابوجھ رکھ دیا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "یہ سب کچھ تمہارے اور تمہارے چارپایوں کے فائدے کے لیے (کیا)",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "تو جب بڑی آفت آئے گی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "اس دن انسان اپنے کاموں کو یاد کرے گا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اور دوزخ دیکھنے والے کے سامنے نکال کر رکھ دی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "تو جس نے سرکشی کی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اور دنیا کی زندگی کو مقدم سمجھا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اس کا ٹھکانہ دوزخ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اور جو اپنے پروردگار کے سامنے کھڑے ہونے سے ڈرتا اور جی کو خواہشوں سے روکتا رہا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اس کا ٹھکانہ بہشت ہے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 42,
      "text": "(اے پیغمبر، لوگ) تم سے قیامت کے بارے میں پوچھتے ہیں کہ اس کا وقوع کب ہو گا؟",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 43,
      "text": "سو تم اس کے ذکر سے کس فکر میں ہو",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 44,
      "text": "اس کا منتہا (یعنی واقع ہونے کا وقت) تمہارے پروردگار ہی کو (معلوم ہے)",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 45,
      "text": "جو شخص اس سے ڈر رکھتا ہے تم تو اسی کو ڈر سنانے والے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 46,
      "text": "جب وہ اس کو دیکھیں گے (تو ایسا خیال کریں گے) کہ گویا (دنیا میں صرف) ایک شام یا صبح رہے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}