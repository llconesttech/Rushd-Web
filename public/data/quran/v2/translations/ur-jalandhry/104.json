{
  "number": 104,
  "name": "سُورَةُ الهُمَزَةِ",
  "englishName": "Al-Humaza",
  "ayahs": [
    {
      "number": 1,
      "text": "ہر طعن آمیز اشارتیں کرنے والے چغل خور کی خرابی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "جو مال جمع کرتا اور اس کو گن گن کر رکھتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "(اور) خیال کرتا ہے کہ اس کا مال اس کی ہمیشہ کی زندگی کا موجب ہو گا",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ہر گز نہیں وہ ضرور حطمہ میں ڈالا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور تم کیا سمجھے حطمہ کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وہ خدا کی بھڑکائی ہوئی آگ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "جو دلوں پر جا لپٹے گی",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "(اور) وہ اس میں بند کر دیئے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "(یعنی آگ کے) لمبے لمبے ستونوں میں",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}