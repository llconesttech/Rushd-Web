{
  "number": 99,
  "name": "سُورَةُ الزَّلۡزَلَةِ",
  "englishName": "Az-Zalzala",
  "ayahs": [
    {
      "number": 1,
      "text": "جب زمین بھونچال سے ہلا دی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور زمین اپنے (اندر) کے بوجھ نکال ڈالے گی",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور انسان کہے گا کہ اس کو کیا ہوا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اس روز وہ اپنے حالات بیان کردے گی",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "کیونکہ تمہارے پروردگار نے اس کو حکم بھیجا (ہوگا)",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اس دن لوگ گروہ گروہ ہو کر آئیں گے تاکہ ان کو ان کے اعمال دکھا دیئے جائیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تو جس نے ذرہ بھر نیکی کی ہو گی وہ اس کو دیکھ لے گا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور جس نے ذرہ بھر برائی کی ہوگی وہ اسے دیکھ لے گا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 541,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}