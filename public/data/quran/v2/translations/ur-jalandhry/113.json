{
  "number": 113,
  "name": "سُورَةُ الفَلَقِ",
  "englishName": "Al-Falaq",
  "ayahs": [
    {
      "number": 1,
      "text": "کہو کہ میں صبح کے پروردگار کی پناہ مانگتا ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ہر چیز کی بدی سے جو اس نے پیدا کی",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور شب تاریکی کی برائی سے جب اس کااندھیرا چھا جائے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور گنڈوں پر (پڑھ پڑھ کر) پھونکنے والیوں کی برائی سے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور حسد کرنے والے کی برائی سے جب حسد کرنے لگے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}