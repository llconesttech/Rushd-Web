{
  "number": 37,
  "name": "سُورَةُ الصَّافَّاتِ",
  "englishName": "As-Saaffaat",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے صف باندھنے والوں کی پرا جما کر",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 2,
      "text": "پھر ڈانٹنے والوں کی جھڑک کر",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 3,
      "text": "پھر ذکر (یعنی قرآن) پڑھنے والوں کی (غور کرکر)",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کہ تمہارا معبود ایک ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 5,
      "text": "جو آسمانوں اور زمین اور جو چیزیں ان میں ہیں سب کا مالک ہے اور سورج کے طلوع ہونے کے مقامات کا بھی مالک ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 6,
      "text": "بےشک ہم ہی نے آسمان دنیا کو ستاروں کی زینت سے مزین کیا",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور ہر شیطان سرکش سے اس کی حفاظت کی",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کہ اوپر کی مجلس کی طرف کان نہ لگاسکیں اور ہر طرف سے (ان پر انگارے) پھینکے جاتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 9,
      "text": "(یعنی وہاں سے) نکال دینے کو اور ان کے لئے دائمی عذاب ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 10,
      "text": "ہاں جو کوئی (فرشتوں کی کسی بات کو) چوری سے جھپٹ لینا چاہتا ہے تو جلتا ہوا انگارہ ان کے پیچھے لگتا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 11,
      "text": "تو ان سے پوچھو کہ ان کا بنانا مشکل ہے یا جتنی خلقت ہم نے بنائی ہے؟ انہیں ہم نے چپکتے گارے سے بنایا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 12,
      "text": "ہاں تم تو تعجب کرتے ہو اور یہ تمسخر کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اور جب ان کو نصیحت دی جاتی ہے تو نصیحت قبول نہیں کرتے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور جب کوئی نشانی دیکھتے ہیں تو ٹھٹھے کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اور کہتے ہیں کہ یہ تو صریح جادو ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 16,
      "text": "بھلا جب ہم مرگئے اور مٹی اور ہڈیاں ہوگئے تو کیا پھر اٹھائے جائیں گے؟",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اور کیا ہمارے باپ دادا بھی (جو) پہلے (ہو گزرے ہیں)",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 18,
      "text": "کہہ دو کہ ہاں اور تم ذلیل ہوگے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 19,
      "text": "وہ تو ایک زور کی آواز ہوگی اور یہ اس وقت دیکھنے لگیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور کہیں گے، ہائے شامت یہی جزا کا دن ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 21,
      "text": "(کہا جائے گا کہ ہاں) فیصلے کا دن جس کو تم جھوٹ سمجھتے تھے یہی ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 386,
      "hizbQuarter": 178,
      "sajda": false
    },
    {
      "number": 22,
      "text": "جو لوگ ظلم کرتے تھے ان کو اور ان کے ہم جنسوں کو اور جن کو وہ پوجا کرتے تھے (سب کو) جمع کرلو",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 23,
      "text": "(یعنی جن کو) خدا کے سوا (پوجا کرتے تھے) پھر ان کو جہنم کے رستے پر چلا دو",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اور ان کو ٹھیرائے رکھو کہ ان سے (کچھ) پوچھنا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 446,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 25,
      "text": "تم کو کیا ہوا کہ ایک دوسرے کی مدد نہیں کرتے؟",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 26,
      "text": "بلکہ آج تو وہ فرمانبردار ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اور ایک دوسرے کی طرف رخ کرکے سوال (وجواب) کریں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 28,
      "text": "کہیں گے کیا تم ہی ہمارے پاس دائیں (اور بائیں) سے آتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 29,
      "text": "وہ کہیں گے بلکہ تم ہی ایمان لانے والے نہ تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اور ہمارا تم پر کچھ زور نہ تھا۔ بلکہ تم سرکش لوگ تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 31,
      "text": "سو ہمارے بارے میں ہمارے پروردگار کی بات پوری ہوگئی اب ہم مزے چکھیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 32,
      "text": "ہم نے تم کو بھی گمراہ کیا (اور) ہم خود بھی گمراہ تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 33,
      "text": "پس وہ اس روز عذاب میں ایک دوسرے کے شریک ہوں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 34,
      "text": "ہم گنہگاروں کے ساتھ ایسا ہی کیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ان کا یہ حال تھا کہ جب ان سے کہا جاتا تھا کہ خدا کے سوا کوئی معبود نہیں تو غرور کرتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اور کہتے تھے کہ بھلا ہم ایک دیوانے شاعر کے کہنے سے کہیں اپنے معبودوں کو چھوڑ دینے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 37,
      "text": "بلکہ وہ حق لے کر آئے ہیں اور (پہلے) پیغمبروں کو سچا کہتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 38,
      "text": "بےشک تم تکلیف دینے والے عذاب کا مزہ چکھنے والے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 39,
      "text": "اور تم کو بدلہ ویسا ہی ملے گا جیسے تم کام کرتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 40,
      "text": "مگر جو خدا کے بندگان خاص ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 41,
      "text": "یہی لوگ ہیں جن کے لئے روزی مقرر ہے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 42,
      "text": "(یعنی) میوے اور ان کا اعزاز کیا جائے گا",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 43,
      "text": "نعمت کے باغوں میں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 44,
      "text": "ایک دوسرے کے سامنے تختوں پر (بیٹھے ہوں گے)",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 45,
      "text": "شراب لطیف کے جام کا ان میں دور چل رہا ہوگا",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 46,
      "text": "جو رنگ کی سفید اور پینے والوں کے لئے (سراسر) لذت ہوگی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 47,
      "text": "نہ اس سے دردِ سر ہو اور نہ وہ اس سے متوالے ہوں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 48,
      "text": "اور ان کے پاس عورتیں ہوں گی جو نگاہیں نیچی رکھتی ہوں گی اور آنکھیں بڑی بڑی",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 49,
      "text": "گویا وہ محفوظ انڈے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 50,
      "text": "پھر وہ ایک دوسرے کی طرف رخ کرکے سوال (وجواب) کریں گے",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 51,
      "text": "ایک کہنے والا ان میں سے کہے گا کہ میرا ایک ہم نشین تھا",
      "juz": 23,
      "manzil": 6,
      "page": 447,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 52,
      "text": "(جو) کہتا تھا کہ بھلا تم بھی ایسی باتوں کے باور کرنے والوں میں ہو",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 53,
      "text": "بھلا جب ہم مر گئے اور مٹی اور ہڈیاں ہوگئے تو کیا ہم کو بدلہ ملے گا؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 54,
      "text": "(پھر) کہے گا کہ بھلا تم (اسے) جھانک کر دیکھنا چاہتے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 55,
      "text": "(اتنے میں) وہ (خود) جھانکے گا تو اس کو وسط دوزخ میں دیکھے گا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 56,
      "text": "کہے گا کہ خدا کی قسم تُو تو مجھے ہلاک ہی کرچکا تھا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 57,
      "text": "اور اگر میرے پروردگار کی مہربانی نہ ہوتی تو میں بھی ان میں ہوتا جو (عذاب میں) حاضر کئے گئے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 58,
      "text": "کیا (یہ نہیں کہ) ہم (آئندہ کبھی) مرنے کے نہیں",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 59,
      "text": "ہاں (جو) پہلی بار مرنا (تھا سو مرچکے) اور ہمیں عذاب بھی نہیں ہونے کا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 60,
      "text": "بےشک یہ بڑی کامیابی ہے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 61,
      "text": "ایسی ہی (نعمتوں) کے لئے عمل کرنے والوں کو عمل کرنے چاہئیں",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 62,
      "text": "بھلا یہ مہمانی اچھی ہے یا تھوہر کا درخت؟",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 63,
      "text": "ہم نے اس کو ظالموں کے لئے عذاب بنا رکھا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 64,
      "text": "وہ ایک درخت ہے کہ جہنم کے اسفل میں اُگے گا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 65,
      "text": "اُس کے خوشے ایسے ہوں گے جیسے شیطانوں کے سر",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 66,
      "text": "سو وہ اسی میں سے کھائیں گے اور اسی سے پیٹ بھریں گے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 67,
      "text": "پھر اس (کھانے) کے ساتھ ان کو گرم پانی ملا کر دیا جائے گا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 68,
      "text": "پھر ان کو دوزخ کی طرف لوٹایا جائے گا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 69,
      "text": "انہوں نے اپنے باپ دادا کو گمراہ ہی پایا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 70,
      "text": "سو وہ ان ہی کے پیچھے دوڑے چلے جاتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 71,
      "text": "اور ان سے پیشتر بہت سے لوگ بھی گمراہ ہوگئے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 72,
      "text": "اور ہم نے ان میں متنبہ کرنے والے بھیجے",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 73,
      "text": "سو دیکھ لو کہ جن کو متنبہ کیا گیا تھا ان کا انجام کیسا ہوا",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 74,
      "text": "ہاں خدا کے بندگان خاص (کا انجام بہت اچھا ہوا)",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 387,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 75,
      "text": "اور ہم کو نوح نے پکارا سو (دیکھ لو کہ) ہم (دعا کو کیسے) اچھے قبول کرنے والے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 76,
      "text": "اور ہم نے ان کو اور ان کے گھر والوں کو بڑی مصیبت سے نجات دی",
      "juz": 23,
      "manzil": 6,
      "page": 448,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 77,
      "text": "اور ان کی اولاد کو ایسا کیا کہ وہی باقی رہ گئے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 78,
      "text": "اور پیچھے آنے والوں میں ان کا ذکر (جمیل باقی) چھوڑ دیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 79,
      "text": "یعنی) تمام جہان میں (کہ) نوح پر سلام",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 80,
      "text": "نیکوکاروں کو ہم ایسا ہی بدلہ دیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 81,
      "text": "بےشک وہ ہمارے مومن بندوں میں سے تھا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 82,
      "text": "پھر ہم نے دوسروں کو ڈبو دیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 179,
      "sajda": false
    },
    {
      "number": 83,
      "text": "اور ان ہی کے پیرووں میں ابراہیم تھے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 84,
      "text": "جب وہ اپنے پروردگار کے پاس (عیب سے) پاک دل لے کر آئے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 85,
      "text": "جب انہوں نے اپنے باپ سے اور اپنی قوم سے کہا کہ تم کن چیزوں کو پوجتے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 86,
      "text": "کیوں جھوٹ (بنا کر) خدا کے سوا اور معبودوں کے طالب ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 87,
      "text": "بھلا پروردگار عالم کے بارے میں تمہارا کیا خیال ہے؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 88,
      "text": "تب انہوں نے ستاروں کی طرف ایک نظر کی",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 89,
      "text": "اور کہا میں تو بیمار ہوں",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 90,
      "text": "تب وہ ان سے پیٹھ پھیر کر لوٹ گئے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 91,
      "text": "پھر ابراہیم ان کے معبودوں کی طرف متوجہ ہوئے اور کہنے لگے کہ تم کھاتے کیوں نہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 92,
      "text": "تمہیں کیا ہوا ہے تم بولتے نہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 93,
      "text": "پھر ان کو داہنے ہاتھ سے مارنا (اور توڑنا) شروع کیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 94,
      "text": "تو وہ لوگ ان کے پاس دوڑے ہوئے آئے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 95,
      "text": "انہوں نے کہا کہ تم ایسی چیزوں کو کیوں پوجتے ہو جن کو خود تراشتے ہو؟",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 96,
      "text": "حالانکہ تم کو اور جو تم بناتے ہو اس کو خدا ہی نے پیدا کیا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 97,
      "text": "وہ کہنے لگے کہ اس کے لئے ایک عمارت بناؤ پھر اس کو آگ کے ڈھیر میں ڈال دو",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 98,
      "text": "غرض انہوں نے ان کے ساتھ ایک چال چلنی چاہی اور ہم نے ان ہی کو زیر کردیا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 99,
      "text": "اور ابراہیم بولے کہ میں اپنے پروردگار کی طرف جانے والا ہوں وہ مجھے رستہ دکھائے گا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 100,
      "text": "اے پروردگار مجھے (اولاد) عطا فرما (جو) سعادت مندوں میں سے (ہو)",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 101,
      "text": "تو ہم نے ان کو ایک نرم دل لڑکے کی خوشخبری دی",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 102,
      "text": "جب وہ ان کے ساتھ دوڑنے (کی عمر) کو پہنچا تو ابراہیم نے کہا کہ بیٹا میں خواب میں دیکھتا ہوں کہ (گویا) تم کو ذبح کر رہا ہوں تو تم سوچو کہ تمہارا کیا خیال ہے؟ انہوں نے کہا کہ ابا جو آپ کو حکم ہوا ہے وہی کیجیئے خدا نے چاہا تو آپ مجھے صابروں میں پایئے گا",
      "juz": 23,
      "manzil": 6,
      "page": 449,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 103,
      "text": "جب دونوں نے حکم مان لیا اور باپ نے بیٹے کو ماتھے کے بل لٹا دیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 104,
      "text": "تو ہم نے ان کو پکارا کہ اے ابراہیم",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 105,
      "text": "تم نے خواب کو سچا کر دکھایا۔ ہم نیکوکاروں کو ایسا ہی بدلہ دیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 106,
      "text": "بلاشبہ یہ صریح آزمائش تھی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 107,
      "text": "اور ہم نے ایک بڑی قربانی کو ان کا فدیہ دیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 108,
      "text": "اور پیچھے آنے والوں میں ابراہیم کا (ذکر خیر باقی) چھوڑ دیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 109,
      "text": "کہ ابراہیم پر سلام ہو",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 110,
      "text": "نیکوکاروں کو ہم ایسا ہی بدلہ دیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 111,
      "text": "وہ ہمارے مومن بندوں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 112,
      "text": "اور ہم نے ان کو اسحاق کی بشارت بھی دی (کہ وہ) نبی (اور) نیکوکاروں میں سے (ہوں گے)",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 113,
      "text": "اور ہم نے ان پر اور اسحاق پر برکتیں نازل کی تھیں۔ اور ان دونوں اولاد کی میں سے نیکوکار بھی ہیں اور اپنے آپ پر صریح ظلم کرنے والے (یعنی گنہگار) بھی ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 388,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 114,
      "text": "اور ہم نے موسیٰ اور ہارون پر بھی احسان کئے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 115,
      "text": "اور ان کو اور ان کی قوم کو مصیبت عظیمہ سے نجات بخشی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 116,
      "text": "اور ان کی مدد کی تو وہ غالب ہوگئے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 117,
      "text": "اور ان دونوں کو کتاب واضح (المطالب) عنایت کی",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 118,
      "text": "اور ان کو سیدھا رستہ دکھایا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 119,
      "text": "اور پیچھے آنے والوں میں ان کا ذکر (خیر باقی) چھوڑ دیا",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 120,
      "text": "کہ موسیٰ اور ہارون پر سلام",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 121,
      "text": "بےشک ہم نیکوکاروں کو ایسا ہی بدلہ دیا کرتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 122,
      "text": "وہ دونوں ہمارے مومن بندوں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 123,
      "text": "اور الیاس بھی پیغمبروں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 124,
      "text": "جب انہوں نے اپنی قوم سے کہا کہ تم ڈرتے کیوں نہیں؟",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 125,
      "text": "کیا تم بعل کو پکارتے (اور اسے پوجتے) ہو اور سب سے بہتر پیدا کرنے والے کو چھوڑ دیتے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 126,
      "text": "(یعنی) خدا کو جو تمہارا اور تمہارے اگلے باپ دادا کا پروردگار ہے",
      "juz": 23,
      "manzil": 6,
      "page": 450,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 127,
      "text": "تو ان لوگوں نے ان کو جھٹلا دیا۔ سو وہ (دوزخ میں) حاضر کئے جائیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 128,
      "text": "ہاں خدا کے بندگان خاص (مبتلائے عذاب نہیں) ہوں گے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 129,
      "text": "اور ان کا ذکر (خیر) پچھلوں میں (باقی) چھوڑ دیا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 130,
      "text": "کہ اِل یاسین پر سلام",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 131,
      "text": "ہم نیک لوگوں کو ایسا ہی بدلہ دیتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 132,
      "text": "بےشک وہ ہمارے مومن بندوں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 133,
      "text": "اور لوط بھی پیغمبروں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 134,
      "text": "جب ہم نے ان کو اور ان کے گھر والوں کو سب کو (عذاب سے) نجات دی",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 135,
      "text": "مگر ایک بڑھیا کہ پیچھے رہ جانے والوں میں تھی",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 136,
      "text": "پھر ہم نے اوروں کو ہلاک کردیا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 137,
      "text": "اور تم دن کو بھی ان (کی بستیوں) کے پاس سے گزرتے رہتے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 138,
      "text": "اور رات کو بھی۔ تو کیا تم عقل نہیں رکھتے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 389,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 139,
      "text": "اور یونس بھی پیغمبروں میں سے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 140,
      "text": "جب بھاگ کر بھری ہوئی کشتی میں پہنچے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 141,
      "text": "اس وقت قرعہ ڈالا تو انہوں نے زک اُٹھائی",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 142,
      "text": "پھر مچھلی نے ان کو نگل لیا اور وہ (قابل) ملامت (کام) کرنے والے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 143,
      "text": "پھر اگر وہ (خدا کی) پاکی بیان نہ کرتے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 144,
      "text": "تو اس روز تک کہ لوگ دوبارہ زندہ کئے جائیں گے اسی کے پیٹ میں رہتے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 180,
      "sajda": false
    },
    {
      "number": 145,
      "text": "پھر ہم نے ان کو جب کہ وہ بیمار تھے فراخ میدان میں ڈال دیا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 146,
      "text": "اور ان پر کدو کا درخت اُگایا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 147,
      "text": "اور ان کو لاکھ یا اس سے زیادہ (لوگوں) کی طرف (پیغمبر بنا کر) بھیجا",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 148,
      "text": "تو وہ ایمان لے آئے سو ہم نے بھی ان کو (دنیا میں) ایک وقت (مقرر) تک فائدے دیتے رہے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 149,
      "text": "ان سے پوچھو تو کہ بھلا تمہارے پروردگار کے لئے تو بیٹیاں اور ان کے لئے بیٹے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 150,
      "text": "یا ہم نے فرشتوں کو عورتیں بنایا اور وہ (اس وقت) موجود تھے",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 151,
      "text": "دیکھو یہ اپنی جھوٹ بنائی ہوئی (بات) کہتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 152,
      "text": "کہ خدا کے اولاد ہے کچھ شک نہیں کہ یہ جھوٹے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 153,
      "text": "کیا اس نے بیٹوں کی نسبت بیٹیوں کو پسند کیا ہے؟",
      "juz": 23,
      "manzil": 6,
      "page": 451,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 154,
      "text": "تم کیسے لوگ ہو، کس طرح کا فیصلہ کرتے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 155,
      "text": "بھلا تم غور کیوں نہیں کرتے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 156,
      "text": "یا تمہارے پاس کوئی صریح دلیل ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 157,
      "text": "اگر تم سچے ہو تو اپنی کتاب پیش کرو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 158,
      "text": "اور انہوں نے خدا میں اور جنوں میں رشتہ مقرر کیا۔ حالانکہ جنات جانتے ہیں کہ وہ (خدا کے سامنے) حاضر کئے جائیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 159,
      "text": "یہ جو کچھ بیان کرتے ہیں خدا اس سے پاک ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 160,
      "text": "مگر خدا کے بندگان خالص (مبتلائے عذاب نہیں ہوں گے)",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 161,
      "text": "سو تم اور جن کو تم پوجتے ہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 162,
      "text": "خدا کے خلاف بہکا نہیں سکتے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 163,
      "text": "مگر اس کو جو جہنم میں جانے والا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 164,
      "text": "اور (فرشتے کہتے ہیں کہ) ہم میں سے ہر ایک کا ایک مقام مقرر ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 165,
      "text": "اور ہم صف باندھے رہتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 166,
      "text": "اور (خدائے) پاک (ذات) کا ذکر کرتے رہتے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 167,
      "text": "اور یہ لوگ کہا کرتے تھے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 168,
      "text": "کہ اگر ہمارے پاس اگلوں کی کوئی نصیحت (کی کتاب) ہوتی",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 169,
      "text": "تو ہم خدا کے خالص بندے ہوتے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 170,
      "text": "لیکن (اب) اس سے کفر کرتے ہیں سو عنقریب ان کو (اس کا نتیجہ) معلوم ہوجائے گا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 171,
      "text": "اور اپنے پیغام پہنچانے والے بندوں سے ہمارا وعدہ ہوچکا ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 172,
      "text": "کہ وہی (مظفرو) منصور ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 173,
      "text": "اور ہمارا لشکر غالب رہے گا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 174,
      "text": "تو ایک وقت تک ان سے اعراض کئے رہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 175,
      "text": "اور انہیں دیکھتے رہو۔ یہ بھی عنقریب (کفر کا انجام) دیکھ لیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 176,
      "text": "کیا یہ ہمارے عذاب کے لئے جلدی کر رہے ہیں",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 177,
      "text": "مگر جب وہ ان کے میدان میں آ اُترے گا تو جن کو ڈر سنا دیا گیا تھا ان کے لئے برا دن ہوگا",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 178,
      "text": "اور ایک وقت تک ان سے منہ پھیرے رہو",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 179,
      "text": "اور دیکھتے رہو یہ بھی عنقریب (نتیجہ) دیکھ لیں گے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 180,
      "text": "یہ جو کچھ بیان کرتے ہیں تمہارا پروردگار جو صاحب عزت ہے اس سے (پاک ہے)",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 181,
      "text": "اور پیغمبروں پر سلام",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    },
    {
      "number": 182,
      "text": "اور سب طرح کی تعریف خدائے رب العالمین کو (سزاوار) ہے",
      "juz": 23,
      "manzil": 6,
      "page": 452,
      "ruku": 390,
      "hizbQuarter": 181,
      "sajda": false
    }
  ]
}