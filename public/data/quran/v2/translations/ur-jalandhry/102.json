{
  "number": 102,
  "name": "سُورَةُ التَّكَاثُرِ",
  "englishName": "At-Takaathur",
  "ayahs": [
    {
      "number": 1,
      "text": "(لوگو) تم کو (مال کی) بہت سی طلب نے غافل کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "یہاں تک کہ تم نے قبریں جا دیکھیں",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "دیکھو تمہیں عنقریب معلوم ہو جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر دیکھو تمہیں عنقریب معلوم ہو جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "دیکھو اگر تم جانتے (یعنی) علم الیقین (رکھتے تو غفلت نہ کرتے)",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "تم ضرور دوزخ کو دیکھو گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "پھر اس کو (ایسا) دیکھو گے (کہ) عین الیقین (آ جائے گا)",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "پھر اس روز تم سے (شکر) نعمت کے بارے میں پرسش ہو گی",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}