{
  "number": 90,
  "name": "سُورَةُ البَلَدِ",
  "englishName": "Al-Balad",
  "ayahs": [
    {
      "number": 1,
      "text": "ہمیں اس شہر (مکہ) کی قسم",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تم اسی شہر میں تو رہتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور باپ (یعنی آدم) اور اس کی اولاد کی قسم",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کہ ہم نے انسان کو تکلیف (کی حالت) میں (رہنے والا) بنایا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 5,
      "text": "کیا وہ خیال رکھتا ہے کہ اس پر کوئی قابو نہ پائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6,
      "text": "کہتا ہے کہ میں نے بہت سا مال برباد کیا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 7,
      "text": "کیا اسے یہ گمان ہے کہ اس کو کسی نے دیکھا نہیں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 8,
      "text": "بھلا ہم نےاس کو دو آنکھیں نہیں دیں؟",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور زبان اور دو ہونٹ (نہیں دیئے)",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 10,
      "text": "(یہ چیزیں بھی دیں) اور اس کو (خیر و شر کے) دونوں رستے بھی دکھا دیئے",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 11,
      "text": "مگر وہ گھاٹی پر سے ہو کر نہ گزرا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور تم کیا سمجھے کہ گھاٹی کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 13,
      "text": "کسی (کی) گردن کا چھڑانا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 14,
      "text": "یا بھوک کے دن کھانا کھلانا",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 15,
      "text": "یتیم رشتہ دار کو",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 16,
      "text": "یا فقیر خاکسار کو",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 17,
      "text": "پھر ان لوگوں میں بھی (داخل) ہو جو ایمان لائے اور صبر کی نصیحت اور (لوگوں پر) شفقت کرنے کی وصیت کرتے رہے",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 18,
      "text": "یہی لوگ صاحب سعادت ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور جنہوں نے ہماری آیتوں کو نہ مانا وہ بدبخت ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 20,
      "text": "یہ لوگ آگ میں بند کر دیئے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 594,
      "ruku": 532,
      "hizbQuarter": 238,
      "sajda": false
    }
  ]
}