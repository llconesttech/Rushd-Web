{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "ayahs": [
    {
      "number": 1,
      "text": "(اے پیغمبر) اپنے پروردگار جلیل الشان کے نام کی تسبیح کرو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "جس نے (انسان کو) بنایا پھر (اس کے اعضاء کو) درست کیا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور جس نے (اس کا) اندازہ ٹہرایا (پھر اس کو) رستہ بتایا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور جس نے چارہ اگایا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر اس کو سیاہ رنگ کا کوڑا کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ہم تمہیں پڑھا دیں گے کہ تم فراموش نہ کرو گے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "مگر جو خدا چاہے۔ وہ کھلی بات کو بھی جانتا ہے اور چھپی کو بھی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "ہم تم کو آسان طریقے کی توفیق دیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "سو جہاں تک نصیحت (کے) نافع (ہونے کی امید) ہو نصیحت کرتے رہو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "جو خوف رکھتا ہے وہ تو نصیحت پکڑے گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور (بےخوف) بدبخت پہلو تہی کرے گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "جو (قیامت کو) بڑی (تیز) آگ میں داخل ہو گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "پھر وہاں نہ مرے گا اور نہ جئے گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "بے شک وہ مراد کو پہنچ گیا جو پاک ہوا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اور اپنے پروردگار کے نام کا ذکر کرتا رہا اور نماز پڑھتا رہا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "مگر تم لوگ تو دنیا کی زندگی کو اختیار کرتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "حالانکہ آخرت بہت بہتر اور پائندہ تر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "یہ بات پہلے صحیفوں میں (مرقوم) ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "(یعنی) ابراہیم اور موسیٰ کے صحیفوں میں",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}