{
  "number": 114,
  "name": "سُورَةُ النَّاسِ",
  "englishName": "An-Naas",
  "ayahs": [
    {
      "number": 1,
      "text": "کہو کہ میں لوگوں کے پروردگار کی پناہ مانگتا ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "(یعنی) لوگوں کے حقیقی بادشاہ کی",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "لوگوں کے معبود برحق کی",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "(شیطان) وسوسہ انداز کی برائی سے جو (خدا کا نام سن کر) پیچھے ہٹ جاتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "جو لوگوں کے دلوں میں وسوسے ڈالتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "وہ جنّات میں سے (ہو) یا انسانوں میں سے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}