{
  "number": 97,
  "name": "سُورَةُ القَدۡرِ",
  "englishName": "Al-Qadr",
  "ayahs": [
    {
      "number": 1,
      "text": "ہم نے اس (قرآن) کو شب قدر میں نازل (کرنا شروع) کیا",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تمہیں کیا معلوم کہ شب قدر کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "شب قدر ہزار مہینے سے بہتر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اس میں روح (الامین) اور فرشتے ہر کام کے (انتظام کے) لیے اپنے پروردگار کے حکم سے اترتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "یہ (رات) طلوع صبح تک (امان اور) سلامتی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}