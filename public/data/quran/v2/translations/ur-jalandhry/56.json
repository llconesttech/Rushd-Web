{
  "number": 56,
  "name": "سُورَةُ الوَاقِعَةِ",
  "englishName": "Al-Waaqia",
  "ayahs": [
    {
      "number": 1,
      "text": "جب واقع ہونے والی واقع ہوجائے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اس کے واقع ہونے میں کچھ جھوٹ نہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 3,
      "text": "کسی کو پست کرے کسی کو بلند",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 4,
      "text": "جب زمین بھونچال سے لرزنے لگے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور پہاڑ ٹوٹ کر ریزہ ریزہ ہوجائیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 6,
      "text": "پھر غبار ہو کر اُڑنے لگیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور تم لوگ تین قسم ہوجاؤ",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 8,
      "text": "تو داہنے ہاتھ والے (سبحان الله) داہنے ہاتھ والے کیا (ہی چین میں) ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اور بائیں ہاتھ والے (افسوس) بائیں ہاتھ والے کیا (گرفتار عذاب) ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور جو آگے بڑھنے والے ہیں (ان کا کیا کہنا) وہ آگے ہی بڑھنے والے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 11,
      "text": "وہی (خدا کے) مقرب ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 12,
      "text": "نعمت کے بہشتوں میں",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وہ بہت سے تو اگلے لوگوں میں سے ہوں گے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور تھوڑے سے پچھلوں میں سے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 15,
      "text": "(لعل و یاقوت وغیرہ سے) جڑے ہوئے تختوں پر",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 16,
      "text": "آمنے سامنے تکیہ لگائے ہوئے",
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 17,
      "text": "نوجوان خدمت گزار جو ہمیشہ (ایک ہی حالت میں) رہیں گے ان کے آس پاس پھریں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 18,
      "text": "یعنی آبخورے اور آفتابے اور صاف شراب کے گلاس لے لے کر",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اس سے نہ تو سر میں درد ہوگا اور نہ ان کی عقلیں زائل ہوں گی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 20,
      "text": "اور میوے جس طرح کے ان کو پسند ہوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 21,
      "text": "اور پرندوں کا گوشت جس قسم کا ان کا جی چاہے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اور بڑی بڑی آنکھوں والی حوریں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 23,
      "text": "جیسے (حفاظت سے) تہہ کئے ہوئے (آب دار) موتی",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 24,
      "text": "یہ ان اعمال کا بدلہ ہے جو وہ کرتے تھے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 25,
      "text": "وہاں نہ بیہودہ بات سنیں گے اور نہ گالی گلوچ",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 26,
      "text": "ہاں ان کا کلام سلام سلام (ہوگا)",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 27,
      "text": "اور داہنے ہاتھ والے (سبحان الله) داہنے ہاتھ والے کیا (ہی عیش میں) ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 28,
      "text": "(یعنی) بےخار کی بیریوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اور تہہ بہ تہہ کیلوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اور لمبے لمبے سایوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اور پانی کے جھرنوں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اور میوہ ہائے کثیرہ (کے باغوں) میں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 33,
      "text": "جو نہ کبھی ختم ہوں اور نہ ان سے کوئی روکے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 34,
      "text": "اور اونچے اونچے فرشوں میں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 35,
      "text": "ہم نے ان (حوروں) کو پیدا کیا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 36,
      "text": "تو ان کو کنواریاں بنایا",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 37,
      "text": "(اور شوہروں کی) پیاریاں اور ہم عمر",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 38,
      "text": "یعنی داہنے ہاتھ والوں کے لئے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 469,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 39,
      "text": "(یہ) بہت سے اگلے لوگوں میں سے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اور بہت سے پچھلوں میں سے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 41,
      "text": "اور بائیں ہاتھ والے (افسوس) بائیں ہاتھ والے کیا (ہی عذاب میں) ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 42,
      "text": "(یعنی دوزخ کی) لپٹ اور کھولتے ہوئے پانی میں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 43,
      "text": "اور سیاہ دھوئیں کے سائے میں",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 44,
      "text": "(جو) نہ ٹھنڈا (ہے) نہ خوشنما",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 45,
      "text": "یہ لوگ اس سے پہلے عیشِ نعیم میں پڑے ہوئے تھے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 46,
      "text": "اور گناہ عظیم پر اڑے ہوئے تھے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 47,
      "text": "اور کہا کرتے تھے کہ بھلا جب ہم مرگئے اور مٹی ہوگئے اور ہڈیاں (ہی ہڈیاں رہ گئے) تو کیا ہمیں پھر اُٹھنا ہوگا؟",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 48,
      "text": "اور کیا ہمارے باپ دادا کو بھی؟",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 49,
      "text": "کہہ دو کہ بےشک پہلے اور پچھلے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 50,
      "text": "(سب) ایک روز مقرر کے وقت پر جمع کئے جائیں گے",
      "juz": 27,
      "manzil": 7,
      "page": 535,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 51,
      "text": "پھر تم اے جھٹلانے والے گمرا ہو!",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 52,
      "text": "تھوہر کے درخت کھاؤ گے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 53,
      "text": "اور اسی سے پیٹ بھرو گے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 54,
      "text": "اور اس پر کھولتا ہوا پانی پیو گے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 55,
      "text": "اور پیو گے بھی تو اس طرح جیسے پیاسے اونٹ پیتے ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 56,
      "text": "جزا کے دن یہ ان کی ضیافت ہوگی",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 57,
      "text": "ہم نے تم کو (پہلی بار بھی تو) پیدا کیا ہے تو تم (دوبارہ اُٹھنے کو) کیوں سچ نہیں سمجھتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 58,
      "text": "دیکھو تو کہ جس (نطفے) کو تم (عورتوں کے رحم میں) ڈالتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 59,
      "text": "کیا تم اس (سے انسان) کو بناتے ہو یا ہم بناتے ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 60,
      "text": "ہم نے تم میں مرنا ٹھہرا دیا ہے اور ہم اس (بات) سے عاجز نہیں",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 61,
      "text": "کہ تمہاری طرح کے اور لوگ تمہاری جگہ لے آئیں اور تم کو ایسے جہان میں جس کو تم نہیں جانتے پیدا کر دیں",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 62,
      "text": "اور تم نے پہلی پیدائش تو جان ہی لی ہے۔ پھر تم سوچتے کیوں نہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 63,
      "text": "بھلا دیکھو تو کہ جو کچھ تم بوتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 64,
      "text": "تو کیا تم اسے اُگاتے ہو یا ہم اُگاتے ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 65,
      "text": "اگر ہم چاہیں تو اسے چورا چورا کردیں اور تم باتیں بناتے رہ جاؤ",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 66,
      "text": "(کہ ہائے) ہم تو مفت تاوان میں پھنس گئے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 67,
      "text": "بلکہ ہم ہیں ہی بےنصیب",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 68,
      "text": "بھلا دیکھو تو کہ جو پانی تم پیتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 69,
      "text": "کیا تم نے اس کو بادل سے نازل کیا ہے یا ہم نازل کرتے ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 70,
      "text": "اگر ہم چاہیں تو ہم اسے کھاری کردیں پھر تم شکر کیوں نہیں کرتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 71,
      "text": "بھلا دیکھو تو جو آگ تم درخت سے نکالتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 72,
      "text": "کیا تم نے اس کے درخت کو پیدا کیا ہے یا ہم پیدا کرتے ہیں؟",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 73,
      "text": "ہم نے اسے یاد دلانے اور مسافروں کے برتنے کو بنایا ہے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 74,
      "text": "تو تم اپنے پروردگار بزرگ کے نام کی تسبیح کرو",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 470,
      "hizbQuarter": 214,
      "sajda": false
    },
    {
      "number": 75,
      "text": "ہمیں تاروں کی منزلوں کی قسم",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 76,
      "text": "اور اگر تم سمجھو تو یہ بڑی قسم ہے",
      "juz": 27,
      "manzil": 7,
      "page": 536,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 77,
      "text": "کہ یہ بڑے رتبے کا قرآن ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 78,
      "text": "(جو) کتاب محفوظ میں (لکھا ہوا ہے)",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 79,
      "text": "اس کو وہی ہاتھ لگاتے ہیں جو پاک ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 80,
      "text": "پروردگار عالم کی طرف سے اُتارا گیا ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 81,
      "text": "کیا تم اس کلام سے انکار کرتے ہو؟",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 82,
      "text": "اور اپنا وظیفہ یہ بناتے ہو کہ (اسے) جھٹلاتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 83,
      "text": "بھلا جب روح گلے میں آ پہنچتی ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 84,
      "text": "اور تم اس وقت کی (حالت کو) دیکھا کرتے ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 85,
      "text": "اور ہم اس (مرنے والے) سے تم سے بھی زیادہ نزدیک ہوتے ہیں لیکن تم کو نظر نہیں آتے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 86,
      "text": "پس اگر تم کسی کے بس میں نہیں ہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 87,
      "text": "تو اگر سچے ہو تو روح کو پھیر کیوں نہیں لیتے؟",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 88,
      "text": "پھر اگر وہ (خدا کے) مقربوں میں سے ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 89,
      "text": "تو (اس کے لئے) آرام اور خوشبودار پھول اور نعمت کے باغ ہیں",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 90,
      "text": "اور اگر وہ دائیں ہاتھ والوں میں سے ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 91,
      "text": "تو (کہا جائے گا کہ) تجھ پر داہنے ہاتھ والوں کی طرف سے سلام",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 92,
      "text": "اور اگر وہ جھٹلانے والے گمراہوں میں سے ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 93,
      "text": "تو (اس کے لئے) کھولتے پانی کی ضیافت ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 94,
      "text": "اور جہنم میں داخل کیا جانا",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 95,
      "text": "یہ (داخل کیا جانا یقیناً صحیح یعنی) حق الیقین ہے",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    },
    {
      "number": 96,
      "text": "تو تم اپنے پروردگار بزرگ کے نام کی تسبیح کرتے رہو",
      "juz": 27,
      "manzil": 7,
      "page": 537,
      "ruku": 471,
      "hizbQuarter": 215,
      "sajda": false
    }
  ]
}