{
  "number": 105,
  "name": "سُورَةُ الفِيلِ",
  "englishName": "Al-Fil",
  "ayahs": [
    {
      "number": 1,
      "text": "کیا تم نے نہیں دیکھا کہ تمہارے پروردگار نے ہاتھی والوں کے ساتھ کیا کیا",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "کیا ان کا داؤں غلط نہیں کیا؟ (گیا)",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور ان پر جھلڑ کے جھلڑ جانور بھیجے",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "جو ان پر کھنگر کی پتھریاں پھینکتے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "تو ان کو ایسا کر دیا جیسے کھایا ہوا بھس",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}