{
  "number": 94,
  "name": "سُورَةُ الشَّرۡحِ",
  "englishName": "Ash-Sharh",
  "ayahs": [
    {
      "number": 1,
      "text": "(اے محمدﷺ) کیا ہم نے تمہارا سینہ کھول نہیں دیا؟ (بےشک کھول دیا)",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تم پر سے بوجھ بھی اتار دیا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "جس نے تمہاری پیٹھ توڑ رکھی تھی",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور تمہارا ذکر بلند کیا",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ہاں ہاں مشکل کے ساتھ آسانی بھی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "(اور) بے شک مشکل کے ساتھ آسانی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "تو جب فارغ ہوا کرو تو (عبادت میں) محنت کیا کرو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور اپنے پروردگار کی طرف متوجہ ہو جایا کرو",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}