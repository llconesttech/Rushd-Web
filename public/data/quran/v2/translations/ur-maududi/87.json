{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "ayahs": [
    {
      "number": 1,
      "text": "(اے نبیؐ) اپنے رب برتر کے نام کی تسبیح کرو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "جس نے پیدا کیا اور تناسب قائم کیا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "جس نے تقدیر بنائی پھر راہ دکھائی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "جس نے نباتات اگائیں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر اُن کو سیاہ کوڑا کرکٹ بنا دیا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ہم تمہیں پڑھوا دیں گے، پھر تم نہیں بھولو گے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "سوائے اُس کے جو اللہ چاہے، وہ ظاہر کو بھی جانتا ہے اور جو کچھ پوشیدہ ہے اُس کو بھی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور ہم تمہیں آسان طریقے کی سہولت دیتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "لہٰذا تم نصیحت کرو اگر نصیحت نافع ہو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "جو شخص ڈرتا ہے وہ نصیحت قبول کر لے گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور اس سے گریز کریگا وہ انتہائی بد بخت",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "جو بڑی آگ میں جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "پھر نہ اس میں مرے گا نہ جیے گا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "فلاح پا گیا وہ جس نے پاکیزگی اختیار کی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "اور اپنے رب کا نام یاد کیا پھر نماز پڑھی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "مگر تم لوگ دنیا کی زندگی کو ترجیح دیتے ہو",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "حالانکہ آخرت بہتر ہے اور باقی رہنے والی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "یہی بات پہلے آئے ہوئے صحیفوں میں بھی کہی گئی تھی",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "ابراہیمؑ اور موسیٰؑ کے صحیفوں میں",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}