{
  "number": 79,
  "name": "سُورَةُ النَّازِعَاتِ",
  "englishName": "An-Naazi'aat",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے اُن (فرشتوں کی) جو ڈوب کر کھینچتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور آہستگی سے نکال لے جاتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور (اُن فرشتوں کی جو کائنات میں) تیزی سے تیرتے پھرتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر (حکم بجا لانے میں) سبقت کرتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر (احکام الٰہی کے مطابق) معاملات کا انتظام چلاتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 6,
      "text": "جس روز ہلا مارے گا زلزلے کا جھٹکا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور اس کے پیچھے ایک اور جھٹکا پڑے گا",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کچھ دل ہوں گے جو اُس روز خوف سے کانپ رہے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 9,
      "text": "نگاہیں اُن کی سہمی ہوئی ہوں گی",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 10,
      "text": "یہ لوگ کہتے ہیں \"کیا واقعی ہم پلٹا کر پھر واپس لائے جائیں گے؟",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 11,
      "text": "کیا جب ہم کھوکھلی بوسیدہ ہڈیاں بن چکے ہوں گے؟\"",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 12,
      "text": "کہنے لگے \"یہ واپسی تو پھر بڑے گھاٹے کی ہوگی!\"",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 13,
      "text": "حالانکہ یہ بس اتنا کام ہے کہ ایک زور کی ڈانٹ پڑے گی",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور یکایک یہ کھلے میدان میں موجود ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 15,
      "text": "کیا تمہیں موسیٰؑ کے قصے کی خبر پہنچی ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 583,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 16,
      "text": "جب اس کے رب نے اُسے طویٰ کی مقدس وادی میں پکارا تھا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 17,
      "text": "کہ \"فرعون کے پاس جا، وہ سرکش ہو گیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اور اس سے کہہ کیا تو اِس کے لیے تیار ہے کہ پاکیزگی اختیار کرے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 19,
      "text": "اور میں تیرے رب کی طرف تیری رہنمائی کروں تو (اُس کا) خوف تیرے اندر پیدا ہو؟\"",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 20,
      "text": "پھر موسیٰؑ نے (فرعون کے پاس جا کر) اُس کو بڑی نشانی دکھائی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 21,
      "text": "مگر اُس نے جھٹلا دیا اور نہ مانا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 22,
      "text": "پھر چالبازیاں کرنے کے لیے پلٹا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اور لوگوں کو جمع کر کے اس نے پکار کر کہا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 24,
      "text": "\"میں تمہارا سب سے بڑا رب ہوں\"",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 25,
      "text": "آخرکار اللہ نے اسے آخرت اور دنیا کے عذاب میں پکڑ لیا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 26,
      "text": "درحقیقت اِس میں بڑی عبرت ہے ہر اُس شخص کے لیے جو ڈرے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 520,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 27,
      "text": "کیا تم لوگوں کی تخلیق زیادہ سخت کام ہے یا آسمان کی؟ اللہ نے اُس کو بنایا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 28,
      "text": "اُس کی چھت خوب اونچی اٹھائی پھر اُس کا توازن قائم کیا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اور اُس کی رات ڈھانکی اور اُس کا دن نکالا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 30,
      "text": "اِس کے بعد زمین کو اس نے بچھایا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 31,
      "text": "اُس کے اندر سے اُس کا پانی اور چارہ نکالا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 32,
      "text": "اور پہاڑ اس میں گاڑ دیے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 33,
      "text": "سامان زیست کے طور پر تمہارے لیے اور تمہارے مویشیوں کے لیے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 34,
      "text": "پھر جب وہ ہنگامہ عظیم برپا ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 35,
      "text": "جس روز انسان اپنا سب کیا دھرا یاد کرے گا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 36,
      "text": "اور ہر دیکھنے والے کے سامنے دوزخ کھول کر رکھ دی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 37,
      "text": "تو جس نے سرکشی کی تھی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 38,
      "text": "اور دنیا کی زندگی کو ترجیح دی تھی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 39,
      "text": "دوزخ ہی اس کا ٹھکانا ہوگی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 40,
      "text": "اور جس نے اپنے رب کے سامنے کھڑے ہونے کا خوف کیا تھا اور نفس کو بری خواہشات سے باز رکھا تھا",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 41,
      "text": "جنت اس کا ٹھکانا ہوگی",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 42,
      "text": "یہ لوگ تم سے پوچھتے ہیں کہ \"آخر وہ گھڑی کب آ کر ٹھیرے گی؟\"",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 43,
      "text": "تمہارا کیا کام کہ اس کا وقت بتاؤ",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 44,
      "text": "اس کا علم تو اللہ پر ختم ہے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 45,
      "text": "تم صرف خبردار کرنے والے ہو ہر اُس شخص کو جو اُس کا خوف کرے",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    },
    {
      "number": 46,
      "text": "جس روز یہ لوگ اسے دیکھ لیں گے تو انہیں یوں محسوس ہوگا کہ (یہ دنیا میں یا حالت موت میں) بس ایک دن کے پچھلے پہر یا اگلے پہر تک ٹھیرے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 584,
      "ruku": 521,
      "hizbQuarter": 233,
      "sajda": false
    }
  ]
}