{
  "number": 100,
  "name": "سُورَةُ العَادِيَاتِ",
  "englishName": "Al-Aadiyaat",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے اُن (گھوڑوں) کی جو پھنکارے مارتے ہوئے دوڑتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "پھر (اپنی ٹاپوں سے) چنگاریاں جھاڑتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "پھر صبح سویرے چھاپہ مارتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "پھر اس موقع پر گرد و غبار اڑاتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر اِسی حالت میں کسی مجمع کے اندر جا گھستے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "حقیقت یہ ہے کہ انسان اپنے رب کا بڑا ناشکرا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور وہ خود اِس پر گواہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور وہ مال و دولت کی محبت میں بری طرح مبتلا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "تو کیا وہ اُس حقیقت کو نہیں جانتا جب قبروں میں جو کچھ (مدفون) ہے اُسے نکال لیا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 599,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور سینوں میں جو کچھ (مخفی) ہے اُسے برآمد کر کے اس کی جانچ پڑتال کی جائے گی؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "یقیناً اُن کا رب اُس روز اُن سے خوب باخبر ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 542,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}