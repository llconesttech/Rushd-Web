{
  "number": 101,
  "name": "سُورَةُ القَارِعَةِ",
  "englishName": "Al-Qaari'a",
  "ayahs": [
    {
      "number": 1,
      "text": "عظیم حادثہ!",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "کیا ہے وہ عظیم حادثہ؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "تم کیا جانو کہ وہ عظیم حادثہ کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "وہ دن جب لوگ بکھرے ہوئے پروانوں کی طرح",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور پہاڑ رنگ برنگ کے دھنکے ہوئے اون کی طرح ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "پھر جس کے پلڑے بھاری ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "وہ دل پسند عیش میں ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور جس کے پلڑے ہلکے ہوں گے",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "اس کی جائے قرار گہری کھائی ہوگی",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور تمہیں کیا خبر کہ وہ کیا چیز ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 11,
      "text": "بھڑکتی ہوئی آگ",
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 543,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}