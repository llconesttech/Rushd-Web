{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے آسمان کی اور رات کو نمودار ہونے والے کی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تم کیا جانو کہ وہ رات کو نمودار ہونے والا کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "چمکتا ہوا تارا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کوئی جان ایسی نہیں ہے جس کے اوپر کوئی نگہبان نہ ہو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر ذرا انسان یہی دیکھ لے کہ وہ کس چیز سے پیدا کیا گیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ایک اچھلنے والے پانی سے پیدا کیا گیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "جو پیٹھ اور سینے کی ہڈیوں کے درمیان سے نکلتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "یقیناً وہ (خالق) اُسے دوبارہ پیدا کرنے پر قادر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "جس روز پوشیدہ اسرار کی جانچ پڑتال ہوگی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اُس وقت انسان کے پاس نہ خود اپنا کوئی زور ہوگا اور نہ کوئی اس کی مدد کرنے والا ہوگا",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "قسم ہے بارش برسانے والے آسمان کی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور (نباتات اگتے وقت) پھٹ جانے والی زمین کی",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "یہ ایک جچی تلی بات ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "ہنسی مذاق نہیں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "یہ لوگ چالیں چل رہے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور میں بھی ایک چال چل رہا ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "پس چھوڑ دو اے نبیؐ، اِن کافروں کو اک ذرا کی ذرا اِن کے حال پر چھوڑ دو",
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}