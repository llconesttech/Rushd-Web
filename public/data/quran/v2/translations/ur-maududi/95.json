{
  "number": 95,
  "name": "سُورَةُ التِّينِ",
  "englishName": "At-Tin",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے انجیر اور زیتون کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور طور سینا کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور اِس پرامن شہر (مکہ) کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ہم نے انسان کو بہترین ساخت پر پیدا کیا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "پھر اُسے الٹا پھیر کر ہم نے سب نیچوں سے نیچ کر دیا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "سوائے اُن لوگوں کے جو ایمان لائے اور نیک عمل کرتے رہے کہ ان کے لیے کبھی ختم نہ ہونے والا اجر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "پس (اے نبیؐ) اس کے بعد کون جزا و سزا کے معاملہ میں تم کو جھٹلا سکتا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "کیا اللہ سب حاکموں سے بڑا حاکم نہیں ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}