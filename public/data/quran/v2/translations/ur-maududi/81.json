{
  "number": 81,
  "name": "سُورَةُ التَّكۡوِيرِ",
  "englishName": "At-Takwir",
  "ayahs": [
    {
      "number": 1,
      "text": "جب سورج لپیٹ دیا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور جب تارے بکھر جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور جب پہاڑ چلائے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور جب دس مہینے کی حاملہ اونٹنیاں اپنے حال پر چھوڑ دی جائیں گی",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور جب جنگلی جانور سمیٹ کر اکٹھے کر دیے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 6,
      "text": "اور جب سمندر بھڑکا دیے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور جب جانیں (جسموں سے) جوڑ دی جائیں گی",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور جب زندہ گاڑی ہوئی لڑکی سے پوچھا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 9,
      "text": "کہ وہ کس قصور میں ماری گئی؟",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 10,
      "text": "اور جب اعمال نامے کھولے جائیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 11,
      "text": "اور جب آسمان کا پردہ ہٹا دیا جائے گا",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 12,
      "text": "اور جب جہنم دہکائی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 13,
      "text": "اور جب جنت قریب لے آئی جائے گی",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اُس وقت ہر شخص کو معلوم ہو جائے گا کہ وہ کیا لے کر آیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 15,
      "text": "پس نہیں، میں قسم کھاتا ہوں",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 16,
      "text": "پلٹنے اور چھپ جانے والے تاروں کی",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 17,
      "text": "اور رات کی جبکہ وہ رخصت ہوئی",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 18,
      "text": "اور صبح کی جبکہ اس نے سانس لیا",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 19,
      "text": "یہ فی الواقع ایک بزرگ پیغام بر کا قول ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 20,
      "text": "جو بڑی توانائی رکھتا ہے، عرش والے کے ہاں بلند مرتبہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 21,
      "text": "وہاں اُس کا حکم مانا جاتا ہے، وہ با اعتماد ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اور (اے اہل مکہ) تمہارا رفیق مجنون نہیں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 23,
      "text": "اُس نے اُس پیغام بر کو روشن افق پر دیکھا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 24,
      "text": "اور وہ غیب (کے اِس علم کو لوگوں تک پہنچانے) کے معاملے میں بخیل نہیں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 25,
      "text": "اور یہ کسی شیطان مردود کا قول نہیں ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 26,
      "text": "پھر تم لوگ کدھر چلے جا رہے ہو؟",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 27,
      "text": "یہ تو سارے جہان والوں کے لیے ایک نصیحت ہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 28,
      "text": "تم میں سے ہر اُس شخص کے لیے جو راہ راست پر چلنا چاہتا ہو",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    },
    {
      "number": 29,
      "text": "اور تمہارے چاہنے سے کچھ نہیں ہوتا جب تک اللہ رب العالمین نہ چاہے",
      "juz": 30,
      "manzil": 7,
      "page": 586,
      "ruku": 523,
      "hizbQuarter": 234,
      "sajda": false
    }
  ]
}