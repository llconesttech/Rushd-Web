{
  "number": 85,
  "name": "سُورَةُ البُرُوجِ",
  "englishName": "Al-Burooj",
  "ayahs": [
    {
      "number": 1,
      "text": "قسم ہے مضبوط قلعوں والے آسمان کی",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور اُس دن کی جس کا وعدہ کیا گیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور دیکھنے والے کی اور دیکھی جانے والی چیز کی",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 4,
      "text": "کہ مارے گئے گڑھے والے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5,
      "text": "(اُس گڑھے والے) جس میں خوب بھڑکتے ہوئے ایندھن کی آگ تھی",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 6,
      "text": "جبکہ وہ اُس گڑھے کے کنارے بیٹھے ہوئے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اور جو کچھ وہ ایمان لانے والوں کے ساتھ کر رہے تھے اُسے دیکھ رہے تھے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 8,
      "text": "اور اُن اہل ایمان سے اُن کی دشمنی اِس کے سوا کسی وجہ سے نہ تھی کہ وہ اُس خدا پر ایمان لے آئے تھے جو زبردست اور اپنی ذات میں آپ محمود ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 9,
      "text": "جو آسمانوں اور زمین کی سلطنت کا مالک ہے، اور وہ خدا سب کچھ دیکھ رہا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 10,
      "text": "جن لوگوں نے مومن مردوں اور عورتوں پر ظلم و ستم توڑا اور پھر اس سے تائب نہ ہوئے، یقیناً اُن کے لیے جہنم کا عذاب ہے اور ان کے لیے جلائے جانے کی سزا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 11,
      "text": "جو لوگ ایمان لائے اور جنہوں نے نیک عمل کیے، یقیناً اُن کے لیے جنت کے باغ ہیں جن کے نیچے نہریں بہتی ہوں گی، یہ ہے بڑی کامیابی",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 12,
      "text": "درحقیقت تمہارے رب کی پکڑ بڑی سخت ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 13,
      "text": "وہی پہلی بار پیدا کرتا ہے اور وہی دوبارہ پیدا کرے گا",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 14,
      "text": "اور وہ بخشنے والا ہے، محبت کرنے والا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 15,
      "text": "عرش کا مالک ہے، بزرگ و برتر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اور جو کچھ چاہے کر ڈالنے والا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 17,
      "text": "کیا تمہیں لشکروں کی خبر پہنچی ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 18,
      "text": "فرعون اور ثمود (کے لشکروں) کی؟",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 19,
      "text": "مگر جنہوں نے کفر کیا ہے وہ جھٹلانے میں لگے ہوئے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 20,
      "text": "حالانکہ اللہ نے ان کو گھیرے میں لے رکھا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 21,
      "text": "(اُن کے جھٹلانے سے اِس قرآن کا کچھ نہیں بگڑتا) بلکہ یہ قرآن بلند پایہ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 22,
      "text": "اُس لوح میں (نقش ہے) جو محفوظ ہے",
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    }
  ]
}