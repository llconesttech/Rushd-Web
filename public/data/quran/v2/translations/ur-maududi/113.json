{
  "number": 113,
  "name": "سُورَةُ الفَلَقِ",
  "englishName": "Al-Falaq",
  "ayahs": [
    {
      "number": 1,
      "text": "کہو، میں پناہ مانگتا ہوں صبح کے رب کی",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "ہر اُس چیز کے شر سے جو اُس نے پیدا کی ہے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "اور رات کی تاریکی کے شر سے جب کہ وہ چھا جائے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "اور گرہوں میں پھونکنے والوں (یا والیوں) کے شر سے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "اور حاسد کے شر سے جب کہ وہ حسد کرے",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
