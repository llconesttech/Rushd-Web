{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "ayahs": [
    {
      "number": 1,
      "text": "پڑھو (اے نبیؐ) اپنے رب کے نام کے ساتھ جس نے پیدا کیا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "جمے ہوئے خون کے ایک لوتھڑے سے انسان کی تخلیق کی",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "پڑھو، اور تمہارا رب بڑا کریم ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "جس نے قلم کے ذریعہ سے علم سکھایا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "انسان کو وہ علم دیا جسے وہ نہ جانتا تھا",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ہرگز نہیں، انسان سرکشی کرتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اِس بنا پر کہ وہ اپنے آپ کو بے نیاز دیکھتا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "پلٹنا یقیناً تیرے رب ہی کی طرف ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 9,
      "text": "تم نے دیکھا اُس شخص کو",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 10,
      "text": "جو ایک بندے کو منع کرتا ہے جبکہ وہ نماز پڑھتا ہو؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 11,
      "text": "تمہارا کیا خیال ہے اگر (وہ بندہ) راہ راست پر ہو",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 12,
      "text": "یا پرہیزگاری کی تلقین کرتا ہو؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 13,
      "text": "تمہارا کیا خیال ہے اگر (یہ منع کرنے والا شخص حق کو) جھٹلاتا اور منہ موڑتا ہو؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 14,
      "text": "کیا وہ نہیں جانتا کہ اللہ دیکھ رہا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 15,
      "text": "ہرگز نہیں، اگر وہ باز نہ آیا تو ہم اس کی پیشانی کے بال پکڑ کر اسے کھینچیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 16,
      "text": "اُس پیشانی کو جو جھوٹی اور سخت خطا کار ہے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 17,
      "text": "وہ بلا لے اپنے حامیوں کی ٹولی کو",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 18,
      "text": "ہم بھی عذاب کے فرشتوں کو بلا لیں گے",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 19,
      "text": "ہرگز نہیں، اُس کی بات نہ مانو اور سجدہ کرو اور (اپنے رب کا) قرب حاصل کرو",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
        "id": 15,
        "recommended": false,
        "obligatory": true
      }
    }
  ]
}