{
  "number": 97,
  "name": "سُورَةُ القَدۡرِ",
  "englishName": "Al-Qadr",
  "ayahs": [
    {
      "number": 1,
      "text": "ہم نے اِس (قرآن) کو شب قدر میں نازل کیا ہے",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "اور تم کیا جانو کہ شب قدر کیا ہے؟",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "شب قدر ہزار مہینوں سے زیادہ بہتر ہے",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "فرشتے اور روح اُس میں اپنے رب کے اذن سے ہر حکم لے کر اترتے ہیں",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "وہ رات سراسر سلامتی ہے طلوع فجر تک",
      "juz": 30,
      "manzil": 7,
      "page": 598,
      "ruku": 539,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
