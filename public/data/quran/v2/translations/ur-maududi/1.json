{
  "number": 1,
  "name": "سُورَةُ ٱلْفَاتِحَةِ",
  "englishName": "Al-Faatiha",
  "ayahs": [
    {
      "number": 1,
      "text": "اللہ کے نام سے جو رحمان و رحیم ہے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 2,
      "text": "تعریف اللہ ہی کے لیے ہے جو تمام کائنات کا رب ہے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 3,
      "text": "رحمان اور رحیم ہے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 4,
      "text": "روز جزا کا مالک ہے",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ہم تیری ہی عبادت کرتے ہیں اور تجھی سے مدد مانگتے ہیں",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 6,
      "text": "ہمیں سیدھا راستہ دکھا",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 7,
      "text": "اُن لوگوں کا راستہ جن پر تو نے انعام فرمایا، جو معتوب نہیں ہوئے، جو بھٹکے ہوئے نہیں ہیں",
      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    }
  ]
}