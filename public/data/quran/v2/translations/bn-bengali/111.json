{
  "number": 111,
  "name": "سُورَةُ المَسَدِ",
  "englishName": "Al-Masad",
  "ayahs": [
    {
      "number": 1,
      "text": "আবু লাহাবের হস্তদ্বয় ধ্বংস হোক এবং ধ্বংস হোক সে নিজে,",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "কোন কাজে আসেনি তার ধন-সম্পদ ও যা সে উপার্জন করেছে।",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "সত্বরই সে প্রবেশ করবে লেলিহান অগ্নিতে",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "এবং তার স্ত্রীও-যে ইন্ধন বহন করে,",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "তার গলদেশে খর্জুরের রশি নিয়ে।",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 553,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}