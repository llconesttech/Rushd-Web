{
  "number": 113,
  "name": "سُورَةُ الفَلَقِ",
  "englishName": "Al-Falaq",
  "ayahs": [
    {
      "number": 1,
      "text": "বলুন, আমি আশ্রয় গ্রহণ করছি প্রভাতের পালনকর্তার,",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "তিনি যা সৃষ্টি করেছেন, তার অনিষ্ট থেকে,",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "অন্ধকার রাত্রির অনিষ্ট থেকে, যখন তা সমাগত হয়,",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "গ্রন্থিতে ফুঁৎকার দিয়ে জাদুকারিনীদের অনিষ্ট থেকে",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "এবং হিংসুকের অনিষ্ট থেকে যখন সে হিংসা করে।",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}