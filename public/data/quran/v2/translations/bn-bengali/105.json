{
  "number": 105,
  "name": "سُورَةُ الفِيلِ",
  "englishName": "Al-Fil",
  "ayahs": [
    {
      "number": 1,
      "text": "আপনি কি দেখেননি আপনার পালনকর্তা হস্তীবাহিনীর সাথে কিরূপ ব্যবহার করেছেন?",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "তিনি কি তাদের চক্রান্ত নস্যাৎ করে দেননি?",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "তিনি তাদের উপর প্রেরণ করেছেন ঝাঁকে ঝাঁকে পাখী,",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "যারা তাদের উপর পাথরের কংকর নিক্ষেপ করছিল।",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "অতঃপর তিনি তাদেরকে ভক্ষিত তৃণসদৃশ করে দেন।",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 547,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}