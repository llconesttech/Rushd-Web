{
  "number": 95,
  "name": "سُورَةُ التِّينِ",
  "englishName": "At-Tin",
  "ayahs": [
    {
      "number": 1,
      "text": "শপথ আঞ্জীর (ডুমুর) ও যয়তুনের,",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "এবং সিনাই প্রান্তরস্থ তূর পর্বতের,",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "এবং এই নিরাপদ নগরীর।",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "আমি সৃষ্টি করেছি মানুষকে সুন্দরতর অবয়বে।",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "অতঃপর তাকে ফিরিয়ে দিয়েছি নীচ থেকে নীচে।",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "কিন্তু যারা বিশ্বাস স্থাপন করেছে ও সৎকর্ম করেছে, তাদের জন্যে রয়েছে অশেষ পুরস্কার।",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "অতঃপর কেন তুমি অবিশ্বাস করছ কেয়ামতকে?",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "আল্লাহ কি বিচারকদের মধ্যে শ্রেষ্টতম বিচারক নন?",
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}