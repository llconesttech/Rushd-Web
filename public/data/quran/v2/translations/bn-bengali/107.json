{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "ayahs": [
    {
      "number": 1,
      "text": "আপনি কি দেখেছেন তাকে, যে বিচারদিবসকে মিথ্যা বলে?",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "সে সেই ব্যক্তি, যে এতীমকে গলা ধাক্কা দেয়",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "এবং মিসকীনকে অন্ন দিতে উৎসাহিত করে না।",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "অতএব দুর্ভোগ সেসব নামাযীর,",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "যারা তাদের নামায সম্বন্ধে বে-খবর;",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "যারা তা লোক-দেখানোর জন্য করে",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "এবং নিত্য ব্যবহার্য্য বস্তু অন্যকে দেয় না।",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}