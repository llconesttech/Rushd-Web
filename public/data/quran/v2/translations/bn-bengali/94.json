{
  "number": 94,
  "name": "سُورَةُ الشَّرۡحِ",
  "englishName": "Ash-Sharh",
  "ayahs": [
    {
      "number": 1,
      "text": "আমি কি আপনার বক্ষ উম্মুক্ত করে দেইনি?",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 2,
      "text": "আমি লাঘব করেছি আপনার বোঝা,",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 3,
      "text": "যা ছিল আপনার জন্যে অতিশয় দুঃসহ।",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 4,
      "text": "আমি আপনার আলোচনাকে সমুচ্চ করেছি।",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 5,
      "text": "নিশ্চয় কষ্টের সাথে স্বস্তি রয়েছে।",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6,
      "text": "নিশ্চয় কষ্টের সাথে স্বস্তি রয়েছে।",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 7,
      "text": "অতএব, যখন অবসর পান পরিশ্রম করুন।",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 8,
      "text": "এবং আপনার পালনকর্তার প্রতি মনোনিবেশ করুন।",
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    }
  ]
}