{
  "number": 114,
  "name": "سُورَةُ النَّاسِ",
  "englishName": "An-Naas",
  "ayahs": [
    {
      "number": 1,
      "text": "বলুন, আমি আশ্রয় গ্রহণ করিতেছি মানুষের পালনকর্তার,",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "মানুষের অধিপতির,",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "মানুষের মা’বুদের",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "তার অনিষ্ট থেকে, যে কুমন্ত্রণা দেয় ও আত্নগোপন করে,",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "যে কুমন্ত্রণা দেয় মানুষের অন্তরে",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "জ্বিনের মধ্য থেকে অথবা মানুষের মধ্য থেকে।",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 556,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}