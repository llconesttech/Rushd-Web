{
  "number": 109,
  "name": "سُورَةُ الكَافِرُونَ",
  "englishName": "Al-Kaafiroon",
  "ayahs": [
    {
      "number": 1,
      "text": "বলুন, হে কাফেরকূল,",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "আমি এবাদত করিনা, তোমরা যার এবাদত কর।",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "এবং তোমরাও এবাদতকারী নও, যার এবাদত আমি করি",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "এবং আমি এবাদতকারী নই, যার এবাদত তোমরা কর।",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "তোমরা এবাদতকারী নও, যার এবাদত আমি করি।",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "তোমাদের কর্ম ও কর্মফল তোমাদের জন্যে এবং আমার কর্ম ও কর্মফল আমার জন্যে।",
      "juz": 30,
      "manzil": 7,
      "page": 603,
      "ruku": 551,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}