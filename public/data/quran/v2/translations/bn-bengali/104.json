{
  "number": 104,
  "name": "سُورَةُ الهُمَزَةِ",
  "englishName": "Al-Humaza",
  "ayahs": [
    {
      "number": 1,
      "text": "প্রত্যেক পশ্চাতে ও সম্মুখে পরনিন্দাকারীর দুর্ভোগ,",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "যে অর্থ সঞ্চিত করে ও গণনা করে",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "সে মনে করে যে, তার অর্থ চিরকাল তার সাথে থাকবে!",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "কখনও না, সে অবশ্যই নিক্ষিপ্ত হবে পিষ্টকারীর মধ্যে।",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "আপনি কি জানেন, পিষ্টকারী কি?",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "এটা আল্লাহর প্রজ্জ্বলিত অগ্নি,",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "যা হৃদয় পর্যন্ত পৌছবে।",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 8,
      "text": "এতে তাদেরকে বেঁধে দেয়া হবে,",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 9,
      "text": "লম্বা লম্বা খুঁটিতে।",
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}