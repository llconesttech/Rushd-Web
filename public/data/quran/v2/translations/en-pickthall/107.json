{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "ayahs": [
    {
      "number": 1,
      "text": "Hast thou observed him who belieth religion?",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "That is he who repelleth the orphan,",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "And urgeth not the feeding of the needy.",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "Ah, woe unto worshippers",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "Who are heedless of their prayer;",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6,
      "text": "Who would be seen (at worship)",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 7,
      "text": "Yet refuse small kindnesses!",
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}