{
  "number": 88,
  "name": "سُورَةُ الغَاشِيَةِ",
  "englishName": "Al-Ghaashiya",
  "ayahs": [
    {
      "number": 1,
      "text": "Hath there come unto thee tidings of the Overwhelming?",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 2,
      "text": "On that day (many) faces will be downcast,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 3,
      "text": "Toiling, weary,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 4,
      "text": "Scorched by burning fire,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5,
      "text": "Drinking from a boiling spring,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 6,
      "text": "No food for them save bitter thorn-fruit",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 7,
      "text": "Which doth not nourish nor release from hunger.",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 8,
      "text": "In that day other faces will be calm,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 9,
      "text": "Glad for their effort past,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 10,
      "text": "In a high Garden",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 11,
      "text": "Where they hear no idle speech,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 12,
      "text": "Wherein is a gushing spring,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 13,
      "text": "Wherein are couches raised",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 14,
      "text": "And goblets set at hand",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 15,
      "text": "And cushions ranged",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 16,
      "text": "And silken carpets spread.",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 17,
      "text": "Will they not regard the camels, how they are created?",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 18,
      "text": "And the heaven, how it is raised?",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 19,
      "text": "And the hills, how they are set up?",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 20,
      "text": "And the earth, how it is spread?",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 21,
      "text": "Remind them, for thou art but a remembrancer,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 22,
      "text": "Thou art not at all a warder over them.",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 23,
      "text": "But whoso is averse and disbelieveth,",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 24,
      "text": "Allah will punish him with direst punishment.",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 25,
      "text": "Lo! unto Us is their return",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 26,
      "text": "And Ours their reckoning.",
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 530,
      "hizbQuarter": 237,
      "sajda": false
    }
  ]
}