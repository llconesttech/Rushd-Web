{
  "number": 113,
  "name": "سُورَةُ الفَلَقِ",
  "englishName": "Al-Falaq",
  "ayahs": [
    {
      "number": 1,
      "text": "قل -أيها الرسول-: أعوذ وأعتصم برب الفلق، وهو الصبح.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 2,
      "text": "من شر جميع المخلوقات وأذاها.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 3,
      "text": "ومن شر ليل شديد الظلمة إذا دخل وتغلغل، وما فيه من الشرور والمؤذيات.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 4,
      "text": "ومن شر الساحرات اللاتي ينفخن فيما يعقدن من عُقَد بقصد السحر.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 5,
      "text": "ومن شر حاسد مبغض للناس إذا حسدهم على ما وهبهم الله من نعم، وأراد زوالها عنهم، وإيقاع الأذى بهم.",
      "juz": 30,
      "manzil": 7,
      "page": 604,
      "ruku": 555,
      "hizbQuarter": 240,
      "sajda": false
    }
  ]
}